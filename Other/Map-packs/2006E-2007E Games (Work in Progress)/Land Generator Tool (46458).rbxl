			<Item class="Script">
				<Properties>
					<bool name="Disabled">false</bool>
					<string name="Name">Script</string>
					<string name="Source">isenabled=true&#13;&#10;messageheard=false&#13;&#10;&#13;&#10;function doit()&#13;&#10;&#13;&#10;&#9;script.Parent.Locked=true&#13;&#10;&#9;script.Parent.Anchored=true&#13;&#10;&#9;copyit=script.Parent:clone()&#13;&#10;--Variables:&#13;&#10;&#9;stop=false&#13;&#10;&#9;d=1&#9;&#9;&#9;&#9;&#9;&#9;&#9;--Edit d to change where the process starts. If you have no base try &quot;0&quot;. If you have one 512x512 base then use &quot;1&quot;. If you have a 3x3 grid of 512x512 sized bases then use &quot;2&quot;.&#13;&#10;&#9;stopatd=4&#9;&#9;&#9;&#9;&#9;-- Edit stopatd to change the total size of land you will create. The amount of land gets very big very quick. dont go over 8 ever, its probably best to stay around 2 to 5 &#13;&#10;&#9;slowdown=1&#9;&#9;&#9;&#9;&#9;-- Edit this to change the speed. make it &quot;2&quot; to half the speed the plates are made. make it &quot;0.1&quot; to make it 10 times faster.&#13;&#10;&#13;&#10;&#9;print(&quot;total number of bricks to make:&quot;)&#13;&#10;&#9;print((2*stopatd-1)^2)&#13;&#10;&#9;print(&quot;stage:&quot;)&#13;&#10;&#9;local mod=Instance.new(&quot;Model&quot;)&#13;&#10;&#9;mod.Name=&quot;Bases&quot;&#13;&#10;&#9;mod.Parent=game.Workspace&#13;&#10;&#13;&#10;&#9;if game.Workspace:findFirstChild(&quot;Base&quot;)~=nil then&#13;&#10;&#9;&#9;game.Workspace.Base.Parent=game.Workspace.Bases&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;while stop==false do&#13;&#10;&#9;&#9;x=512*d&#13;&#10;&#9;&#9;z=512*d&#13;&#10;&#9;&#9;local copy=copyit:clone()&#13;&#10;&#9;&#9;copy.Position=Vector3.new(x,0.2,z)&#13;&#10;&#9;&#9;copy.Parent=game.Workspace.Bases&#13;&#10;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;copy.Size=Vector3.new(512,0.4,512)&#13;&#10;&#9;&#9;for i=1,(2*d) do&#13;&#10;&#9;&#9;&#9;z=z-512&#13;&#10;&#9;&#9;&#9;local copy=copyit:clone()&#13;&#10;&#9;&#9;&#9;copy.Position=Vector3.new(x,0.2,z)&#13;&#10;&#9;&#9;&#9;copy.Parent=game.Workspace.Bases&#13;&#10;&#9;&#9;&#9;copy.Size=Vector3.new(512,0.4,512)&#13;&#10;&#9;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;for i=1,(2*d) do&#13;&#10;&#9;&#9;&#9;x=x-512&#13;&#10;&#9;&#9;&#9;local copy=copyit:clone()&#13;&#10;&#9;&#9;&#9;copy.Position=Vector3.new(x,0.2,z)&#13;&#10;&#9;&#9;&#9;copy.Parent=game.Workspace.Bases&#13;&#10;&#9;&#9;&#9;copy.Size=Vector3.new(512,0.4,512)&#13;&#10;&#9;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;for i=1,(2*d) do&#13;&#10;&#9;&#9;&#9;z=z+512&#13;&#10;&#9;&#9;&#9;local copy=copyit:clone()&#13;&#10;&#9;&#9;&#9;copy.Position=Vector3.new(x,0.2,z)&#13;&#10;&#9;&#9;&#9;copy.Parent=game.Workspace.Bases&#13;&#10;&#9;&#9;&#9;copy.Size=Vector3.new(512,0.4,512)&#13;&#10;&#9;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;for i=1,((2*d)-1) do&#13;&#10;&#9;&#9;&#9;x=x+512&#13;&#10;&#9;&#9;&#9;local copy=copyit:clone()&#13;&#10;&#9;&#9;&#9;copy.Position=Vector3.new(x,0.2,z)&#13;&#10;&#9;&#9;&#9;copy.Parent=game.Workspace.Bases&#13;&#10;&#9;&#9;&#9;copy.Size=Vector3.new(512,0.4,512)&#13;&#10;&#9;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;print(d)&#13;&#10;&#9;&#9;d=d+1&#13;&#10;&#9;&#9;wait(0.1*slowdown)&#13;&#10;&#9;&#9;if d&gt;stopatd then stop=true end&#13;&#10;&#9;end&#13;&#10;print(&quot;done - removing button&quot;)&#13;&#10;print(&quot;button says: &apos;Goodbye cruel world!&apos;&quot;)&#13;&#10;script.Parent:remove()&#13;&#10;end&#13;&#10;&#13;&#10;function M()&#13;&#10;local m=Instance.new(&quot;Message&quot;)&#13;&#10;m.Parent=game.Workspace&#13;&#10;m.Text=&quot;Land generator by miked&quot;&#13;&#10;wait(2)&#13;&#10;m.Text=&quot;If you already have a map larger than the standard 512x512 this may crash your map.&quot;&#13;&#10;wait(4)&#13;&#10;m.Text=&quot;Otherwise touch this button again to start the process. It may take a while and lag.&quot;&#13;&#10;wait(4)&#13;&#10;m.Text=&quot;It will take about 60 seconds if you leave it on default. Read the post on the forum about it for details.&quot;&#13;&#10;wait(4)&#13;&#10;m.Text=&quot;Make sure to customise the button how you want first (e.g. painting, transparency, killing scripts, cancollide etc.)&quot;&#13;&#10;wait(4)&#13;&#10;m.Parent=nil&#13;&#10;end&#13;&#10;&#13;&#10;function onTouched(part)&#13;&#10;&#9;if part.Parent ~= nil then&#13;&#10;&#9;local h = part.Parent:findFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;&#9;if h~=nil then&#13;&#10;&#9;&#9;&#9;if isenabled==true then&#13;&#10;&#9;&#9;&#9;&#9;isenabled=false&#13;&#10;&#9;&#9;&#9;&#9;if messageheard==true then&#13;&#10;&#9;&#9;&#9;&#9;&#9;doit()&#13;&#10;&#9;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;&#9;M()&#13;&#10;&#9;&#9;&#9;&#9;&#9;isenabled=true&#13;&#10;&#9;&#9;&#9;&#9;&#9;messageheard=true&#13;&#10;&#9;&#9;&#9;&#9;end&#9;&#9;&#9;&#9;&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;script.Parent.Touched:connect(onTouched)&#13;&#10;</string>

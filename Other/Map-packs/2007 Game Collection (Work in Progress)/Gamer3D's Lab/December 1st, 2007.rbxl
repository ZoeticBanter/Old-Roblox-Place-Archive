					<string name="Source">local moveBrick = script.Parent.Engine&#13;&#10;local plane = script.Parent&#13;&#10;&#13;&#10;local power = moveBrick.BodyVelocity&#13;&#10;local fThrust = moveBrick.FrontThrust&#13;&#10;local bThrust = moveBrick.BackThrust&#13;&#10;local lThrust = moveBrick.LeftThrust&#13;&#10;local rThrust = moveBrick.RightThrust&#13;&#10;local seat = script.Parent.Seat&#13;&#10;&#13;&#10;function cross(v1, v2)&#13;&#10;&#9;return Vector3.new(v1.y * v2.z - v2.y * v1.z, v1.z * v2.x - v1.x * v2.z, v1.x * v2.y - v2.x * v1.y)&#13;&#10;end&#13;&#10;&#13;&#10;function dot(v1, v2)&#13;&#10;&#9;return (v1.x * v2.x + v1.y * v2.y + v1.z * v2.z)&#13;&#10;end&#13;&#10;&#13;&#10;function getPilot()&#13;&#10;&#9;local weld = seat:FindFirstChild(&quot;SeatWeld&quot;)&#13;&#10;&#9;if weld == nil then&#13;&#10;&#9;&#9;return nil&#13;&#10;&#9;elseif weld.Part1 == nil then&#13;&#10;&#9;&#9;return nil&#13;&#10;&#9;else&#13;&#10;&#9;&#9;return game.Players:GetPlayerFromCharacter(weld.Part1.Parent)&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;local engineOn = false&#13;&#10;local goalDirection = lv * Vector3.new(1, 0, 1)&#13;&#10;local reloaded = true&#13;&#10;while true do&#13;&#10;&#9;wait()&#13;&#10;&#9;local pos = moveBrick.Position&#13;&#10;&#9;local cf = moveBrick.CFrame&#13;&#10;&#9;local lv = cf.lookVector&#13;&#10;&#13;&#10;&#9;local pilot = getPilot()&#13;&#10;&#13;&#10;&#9;if pilot ~= nil then&#13;&#10;&#9;&#9;plane.Parent = pilot.Character&#13;&#10;&#9;&#9;local marker = pilot:FindFirstChild(&quot;Mouse CFrame&quot;)&#13;&#10;&#9;&#9;local click = pilot:FindFirstChild(&quot;Mouse Button Left&quot;)&#13;&#10;&#9;&#9;if (marker ~= nil) and (click ~= nil) then&#9;--&#9;Note: This will work with a standard plane/car/boat/hoversled/etc tool&#13;&#10;&#9;&#9;&#9;if (marker.className == &quot;CFrameValue&quot;) and (click.className == &quot;BoolValue&quot;) and (click.Value) then&#13;&#10;&#9;&#9;&#9;&#9;goalDirection = marker.Value.lookVector&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;local keys = pilot:FindFirstChild(&quot;Keys Pressed&quot;)&#13;&#10;&#9;&#9;if keys ~= nil then&#13;&#10;&#9;&#9;&#9;if (keys.className == &quot;StringValue&quot;) then&#13;&#10;&#9;&#9;&#9;&#9;if string.match(keys.Value:lower(), &quot;y&quot;) ~= nil then&#13;&#10;&#9;&#9;&#9;&#9;&#9;engineOn = true&#13;&#10;&#9;&#9;&#9;&#9;elseif string.match(keys.Value:lower(), &quot;x&quot;) ~= nil then&#13;&#10;&#9;&#9;&#9;&#9;&#9;engineOn = false&#13;&#10;&#9;&#9;&#9;&#9;elseif (string.match(keys.Value:lower(), &quot;f&quot;) ~= nil) and reloaded then&#13;&#10;&#9;&#9;&#9;&#9;&#9;local ringPart = plane.Ring:GetChildren()&#13;&#10;&#9;&#9;&#9;&#9;&#9;for x = 1, #ringPart do&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;if (ringPart[x].FrontSurface == 6) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;local missile = Instance.new(&quot;Part&quot;)&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;local missileScript = plane:FindFirstChild(&quot;RocketScript&quot;):Clone()&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.formFactor = 0&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.CFrame = ringPart[x].CFrame + ringPart[x].CFrame.lookVector * 12&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.Locked = true&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missileScript.Parent = missile&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missileScript.Disabled = false&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.Size = Vector3.new(1, 1, 4)&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;local sound = plane.Explosion:Clone()&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;sound.Parent = missile&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;sound = plane.Swoosh:Clone()&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;sound.Parent = missile&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.Parent = workspace&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;missile.archivable = true&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;&#9;reloaded = false&#13;&#10;&#9;&#9;&#9;&#9;&#9;delay(5, function() reloaded = true end)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;&#13;&#10;&#9;&#9;if engineOn then&#13;&#10;&#9;&#9;&#9;local rel = lv * script.Speed.Value - power.velocity&#13;&#10;&#9;&#9;&#9;if rel.magnitude &gt; 4 then&#13;&#10;&#9;&#9;&#9;&#9;rel = rel.unit * 4&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;power.velocity = power.velocity + rel&#13;&#10;&#9;&#9;&#9;power.maxForce = Vector3.new(400000, 400000, 400000)&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;power.velocity = Vector3.new(0, 0, 0)&#13;&#10;&#9;&#9;&#9;power.maxForce = Vector3.new(4000, 40000, 4000)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;else&#13;&#10;&#9;&#9;engineOn = false&#13;&#10;&#9;&#9;plane.Parent = workspace&#13;&#10;&#9;&#9;goalDirection = lv * Vector3.new(1, 0.1, 1)&#13;&#10;&#9;&#9;power.velocity = Vector3.new(0, 0, 0)&#13;&#10;&#9;&#9;power.maxForce = Vector3.new(0, 0, 0)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;if goalDirection.magnitude &gt; 0 then&#13;&#10;&#9;&#9;goalDirection = goalDirection.unit&#13;&#10;&#9;else&#13;&#10;&#9;&#9;goalDirection = Vector3.new(1, 0, 0)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;--&#9;Aiming: Done&#13;&#10;&#9;local crs = cross(lv, goalDirection) - moveBrick.RotVelocity / 16&#13;&#10;&#9;local relCRS = cf:pointToObjectSpace(crs + pos)&#13;&#10;&#9;fThrust.force = Vector3.new(relCRS.y, -relCRS.x, 0) * script.VStable.Value&#13;&#10;&#9;bThrust.force = -fThrust.force&#13;&#10;&#13;&#10;&#9;--&#9;Horizontal Stabilization: Done&#13;&#10;&#9;crs = cross((cf * Vector3.new(0, 1, 0)) - pos, Vector3.new(0, 1, 0)) - moveBrick.RotVelocity / 16&#13;&#10;&#9;relCRS = cf:pointToObjectSpace(crs + pos)&#13;&#10;&#9;lThrust.force = Vector3.new(0, relCRS.z, 0) * script.HStable.Value&#13;&#10;&#9;rThrust.force = -lThrust.force&#13;&#10;end&#13;&#10;</string>

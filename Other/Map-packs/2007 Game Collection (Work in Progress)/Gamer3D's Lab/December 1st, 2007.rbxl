						<string name="Name">OrbController</string>
						<string name="Source">local defenseDist = 10&#13;&#10;local defenseTime = 0.5&#13;&#10;&#13;&#10;function findFirstChildOfClass(parent, classname)&#13;&#10;&#9;local objectList = parent:GetChildren()&#13;&#10;&#9;for x = 1, #objectList do&#13;&#10;&#9;&#9;if objectList[x].className == classname then&#13;&#10;&#9;&#9;&#9;return objectList[x]&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;return nil&#13;&#10;end&#13;&#10;&#13;&#10;function dot(vec1, vec2)&#13;&#10;&#9;return (vec1.x * vec2.x + vec1.y * vec2.y + vec1.z * vec2.z)&#13;&#10;end&#13;&#10;&#13;&#10;function getLineSphereCollide(linePoint1, linePoint2, sphereCenter, radius)&#13;&#10;&#9;local a = (linePoint2.x - linePoint1.x) * (linePoint2.x - linePoint1.x) + (linePoint2.y - linePoint1.y) * (linePoint2.y - linePoint1.y) + (linePoint2.z - linePoint1.z) * (linePoint2.z - linePoint1.z)&#13;&#10;&#9;local b = (linePoint2.x - linePoint1.x) * (linePoint1.x - sphereCenter.x) + (linePoint2.y - linePoint1.y) * (linePoint1.y - sphereCenter.y) + (linePoint2.z - linePoint1.z) * (linePoint1.z - sphereCenter.z)&#13;&#10;&#9;local c = (linePoint1.x - sphereCenter.x) * (linePoint1.x - sphereCenter.x) + (linePoint1.y - sphereCenter.y) * (linePoint1.y - sphereCenter.y) + (linePoint1.z - sphereCenter.z) * (linePoint1.z - sphereCenter.z) - radius * radius&#13;&#10;&#9;if (a &gt; 0) and (b * b &gt;= a * c) then&#13;&#10;&#9;&#9;return ((-b - math.sqrt(b * b - a * c)) / a)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;return -1&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;function doCommands(orb)&#13;&#10;&#9;local command = orb:FindFirstChild(&quot;Nuke&quot;)&#13;&#10;&#9;if command ~= nil then&#13;&#10;&#9;&#9;local state = orb:FindFirstChild(&quot;State&quot;)&#13;&#10;&#9;&#9;if (orb.Position - command.Value - Vector3.new(0, 80, 0)).magnitude &lt; 3 then&#13;&#10;&#9;&#9;&#9;local exp = Instance.new(&quot;Explosion&quot;)&#13;&#10;&#9;&#9;&#9;if (state ~= nil) and (state.Value &lt; 20) then&#13;&#10;&#9;&#9;&#9;&#9;exp.Position = command.Value + Vector3.new(0, 80 - state.Value * 4, 0)&#13;&#10;&#9;&#9;&#9;&#9;state.Value = state.Value + 1&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;exp.Position = command.Value&#13;&#10;&#9;&#9;&#9;&#9;exp.BlastRadius = 30&#13;&#10;&#9;&#9;&#9;&#9;exp.BlastPressure = 3000000&#13;&#10;&#9;&#9;&#9;&#9;command:Remove()&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;exp.Parent = workspace&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;orb.BodyVelocity.velocity = (command.Value + Vector3.new(0, 80, 0) - orb.Position) * 8&#13;&#10;&#9;&#9;if orb.BodyVelocity.velocity.magnitude &gt; 30 then&#13;&#10;&#9;&#9;&#9;orb.BodyVelocity.velocity = orb.BodyVelocity.velocity.unit * 30&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;return true&#13;&#10;&#9;end&#13;&#10;&#9;return false&#13;&#10;end&#13;&#10;&#13;&#10;function findRockets(pos, sphereSpeed)&#13;&#10;&#9;local list = game.Workspace:GetChildren()&#13;&#10;&#9;local rockets = {}&#13;&#10;&#9;local rocketTimes = {}&#13;&#10;&#9;for x = 1, #list do&#13;&#10;&#9;&#9;if (list[x].className == &quot;Part&quot;) and ((list[x]:findFirstChild(&quot;RocketScript&quot;) ~= nil) or (findFirstChildOfClass(list[x], &quot;RocketPropulsion&quot;) ~= nil)) then&#13;&#10;&#9;&#9;&#9;time = getLineSphereCollide(list[x].Position, list[x].Position + list[x].Velocity.magnitude * list[x].CFrame.lookVector - sphereSpeed, pos, defenseDist)&#13;&#10;&#9;&#9;&#9;if (dot(list[x].Velocity.unit, (list[x].Position + time * list[x].Velocity - pos).unit) &lt; -0.5) and ((time &gt; 0) or (((list[x].Position - pos).magnitude &lt; defenseDist) and (dot(list[x].Velocity.unit, (list[x].Position - pos).unit) &lt; 0))) then&#13;&#10;&#9;&#9;&#9;&#9;if time &lt; 0 then&#13;&#10;&#9;&#9;&#9;&#9;&#9;time = 0&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;if time &lt; defenseTime then&#13;&#10;&#9;&#9;&#9;&#9;&#9;local pos = 1&#13;&#10;&#9;&#9;&#9;&#9;&#9;for x = 1, #rocketTimes do&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;if rocketTimes[x] &lt; time then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;pos = x + 1&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;&#9;table.insert(rockets, pos, list[x].Position + time * list[x].CFrame.lookVector * list[x].Velocity.magnitude)&#13;&#10;&#9;&#9;&#9;&#9;&#9;table.insert(rocketTimes, pos, time)&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;return rockets&#13;&#10;end&#13;&#10;&#13;&#10;local orbs = script.Parent.Orbs:GetChildren()&#13;&#10;&#13;&#10;while true do&#13;&#10;&#9;wait(0.1)&#13;&#10;&#9;local readyOrbs = {}&#13;&#10;&#9;local commands = script:GetChildren()&#13;&#10;&#9;for x = 1, #orbs do&#13;&#10;&#9;&#9;if not doCommands(orbs[x]) then&#13;&#10;&#9;&#9;&#9;if #commands &gt; 0 then&#13;&#10;&#9;&#9;&#9;&#9;local state = orbs[x]:FindFirstChild(&quot;State&quot;)&#13;&#10;&#9;&#9;&#9;&#9;if state == nil then&#13;&#10;&#9;&#9;&#9;&#9;&#9;state = Instance.new(&quot;IntValue&quot;)&#13;&#10;&#9;&#9;&#9;&#9;&#9;state.Name = &quot;State&quot;&#13;&#10;&#9;&#9;&#9;&#9;&#9;state.Parent = orbs[x]&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;&#9;state.Value = 0&#13;&#10;&#9;&#9;&#9;&#9;commands[1].Parent = orbs[x]&#13;&#10;&#9;&#9;&#9;&#9;table.remove(commands, 1)&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;table.insert(readyOrbs, 1, orbs[x])&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;local center = script.Parent.Parent.Torso.Position&#13;&#10;&#9;local rockets = findRockets(center, Vector3.new(0, 0, 0))&#13;&#10;&#9;local min = #rockets&#13;&#10;&#9;if #readyOrbs &lt; min then&#13;&#10;&#9;&#9;min = #readyOrbs&#13;&#10;&#9;end&#13;&#10;&#9;for x = 1, min do&#13;&#10;&#9;&#9;local hitPos = rockets[x]&#13;&#10;&#9;&#9;local orbID = 0&#13;&#10;&#9;&#9;local orbDist = 50&#13;&#10;&#9;&#9;for y = 1, #readyOrbs do&#13;&#10;&#9;&#9;&#9;local relPos = readyOrbs[y].Position - rockets[x]&#13;&#10;&#9;&#9;&#9;if relPos.magnitude &lt; orbDist then&#13;&#10;&#9;&#9;&#9;&#9;orbDist = relPos.magnitude&#13;&#10;&#9;&#9;&#9;&#9;orbID = y&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;if orbID &gt; 0 then&#13;&#10;&#9;&#9;&#9;readyOrbs[orbID].BodyVelocity.velocity = (rockets[x] - readyOrbs[orbID].Position) * 8&#13;&#10;&#9;&#9;&#9;if readyOrbs[orbID].BodyVelocity.velocity.magnitude &gt; 80 then&#13;&#10;&#9;&#9;&#9;&#9;readyOrbs[orbID].BodyVelocity.velocity = readyOrbs[orbID].BodyVelocity.velocity.unit * 80&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;table.remove(readyOrbs, orbID)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;for x = 1, #readyOrbs do&#13;&#10;&#9;&#9;local relPos = readyOrbs[x].Position - center&#13;&#10;&#9;&#9;local speed = 1&#13;&#10;&#9;&#9;if #readyOrbs &gt; 1 then&#13;&#10;&#9;&#9;&#9;if x &lt; #readyOrbs then&#13;&#10;&#9;&#9;&#9;&#9;speed = speed - dot(relPos.unit, (readyOrbs[x + 1].Position - center).unit) * 1&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;speed = speed - dot(relPos.unit, (readyOrbs[1].Position - center).unit) * 1&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;if x &gt; 1 then&#13;&#10;&#9;&#9;&#9;&#9;speed = speed + dot(relPos.unit, (readyOrbs[x - 1].Position - center).unit) * 1&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;speed = speed + dot(relPos.unit, (readyOrbs[#readyOrbs].Position - center).unit) * 1&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;local goalPos = center + (relPos * Vector3.new(1, 0, 1)).unit * 4 + Vector3.new(relPos.z, 0, -relPos.x).unit * speed&#13;&#10;&#9;&#9;readyOrbs[x].BodyVelocity.velocity = (goalPos - readyOrbs[x].Position) * 8&#13;&#10;&#9;&#9;if readyOrbs[x].BodyVelocity.velocity.magnitude &gt; 30 then&#13;&#10;&#9;&#9;&#9;readyOrbs[x].BodyVelocity.velocity = readyOrbs[x].BodyVelocity.velocity.unit * 30&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;end</string>

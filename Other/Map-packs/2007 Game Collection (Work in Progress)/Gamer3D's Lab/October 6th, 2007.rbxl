/ 20&#13;&#10;&#9;barrel.RightParamB = -dot(barrel.CFrame * Vector3.new(0, 1, 0) - barrel.Position, (point - barrel.Position).unit) / 20&#13;&#10;&#9;barrel.LeftParamB = -barrel.RightParamB&#13;&#10;end&#13;&#10;&#13;&#10;script.Fire.Changed:connect(fire)&#13;&#10;&#13;&#10;while barrel:FindFirstChild(&quot;Weld&quot;) ~= nil do&#13;&#10;&#9;wait()&#13;&#10;&#9;if seat:FindFirstChild(&quot;SeatWeld&quot;) ~= nil then&#13;&#10;&#9;&#9;local char = game.Players:playerFromCharacter(seat.SeatWeld.Part1.Parent)&#13;&#10;&#9;&#9;if char ~= nil then&#13;&#10;&#9;&#9;&#9;local target = char:FindFirstChild(&quot;Turret Target&quot;)&#13;&#10;&#9;&#9;&#9;if target ~= nil then&#13;&#10;&#9;&#9;&#9;&#9;aimTowardPoint(target.Value)&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;base.BottomParamB = 0&#13;&#10;&#9;&#9;&#9;&#9;barrel.RightParamB = 0&#13;&#10;&#9;&#9;&#9;&#9;barrel.LeftParamB = 0&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;target = char:FindFirstChild(&quot;Turret Fire&quot;)&#13;&#10;&#9;&#9;&#9;if (target ~= nil) and target.Value then&#13;&#10;&#9;&#9;&#9;&#9;fire()&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;base.BottomParamB = 0&#13;&#10;&#9;&#9;&#9;barrel.RightParamB = 0&#13;&#10;&#9;&#9;&#9;barrel.LeftParamB = 0&#13;&#10;&#9;&#9;end&#13;&#10;&#9;else&#13;&#10;&#9;&#9;base.BottomParamB = 0&#13;&#10;&#9;&#9;barrel.RightParamB = 0&#13;&#10;&#9;&#9;barrel.LeftParamB = 0&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;--Blow up&#13;&#10;&#13;&#10;explosion = Instance.new(&quot;Explosion&quot;)&#13;&#10;explosion.Position = barrel.Position&#13;&#10;explosion.BlastRadius = barrel.Size.magnitude / 2&#13;&#10;explosion.Parent = game.Workspace</string>

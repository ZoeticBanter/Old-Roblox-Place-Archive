0;&#9;&#9;if (list[x].className == &quot;Model&quot;) and (list[x] ~= notMe) then&#13;&#10;&#9;&#9;&#9;temp = list[x]:FindFirstChild(&quot;Torso&quot;)&#13;&#10;&#9;&#9;&#9;human2 = list[x]:FindFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;&#9;&#9;if (temp ~= nil) and (human2 ~= nil) and (human2.Health &gt; 0) then&#13;&#10;&#9;&#9;&#9;&#9;if (temp.Position - pos).magnitude &lt; dist then&#13;&#10;&#9;&#9;&#9;&#9;&#9;torso2 = temp&#13;&#10;&#9;&#9;&#9;&#9;&#9;dist = (temp.Position - pos).magnitude&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;return torso2&#13;&#10;end&#13;&#10;&#13;&#10;target = nil&#13;&#10;&#13;&#10;function getNextGoal()&#13;&#10;&#9;target = findNearestTorso(torso.Position, script.Parent)&#13;&#10;&#9;local next = 0&#13;&#10;&#9;if (target == nil) then&#13;&#10;&#9;&#9;print(&quot;Taking random path&quot;)&#13;&#10;&#9;&#9;next = Vector3.new(math.random(-250, 250), torso.Position.y, math.random(-250, 250)) - torso.Position&#13;&#10;&#9;&#9;if next.magnitude &gt; 40 then&#13;&#10;&#9;&#9;&#9;next = next.unit * 40&#13;&#10;&#9;&#9;end&#13;&#10;&#9;&#9;return (next + torso.Position)&#13;&#10;&#9;end&#13;&#10;&#9;local dir = torso.Position - target.Position&#13;&#10;&#9;local normal = Vector3.new(dir.z, 0, -dir.x)&#13;&#10;&#9;next = dir + normal&#13;&#10;&#9;return (next.unit * 40 + target.Position)&#13;&#10;end&#13;&#10;&#13;&#10;canDive = true&#13;&#10;function standing(running)&#13;&#10;&#9;canDive = true&#13;&#10;end&#13;&#10;&#13;&#10;function dive()&#13;&#10;&#9;if not canDive then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#9;torso.RotVelocity = Vector3.new(torso.CFrame.lookVector.z, 0, -torso.CFrame.lookVector.x) * 40&#13;&#10;&#9;torso.Velocity = Vector3.new(0, 48, 0)&#13;&#10;&#9;wait(0.1)&#13;&#10;&#9;torso.Velocity = torso.Velocity + torso.CFrame.lookVector * 48 * Vector3.new(1, 0, 1)&#13;&#10;&#9;canDive = false&#13;&#10;&#9;runConnection = human.Running:connect(standing)&#13;&#10;end&#13;&#10;&#13;&#10;function getLineSphereCollide(linePoint1, linePoint2, sphereCenter, radius)&#13;&#10;&#9;local a = (linePoint2.x - linePoint1.x) * (linePoint2.x - linePoint1.x) + (linePoint2.y - linePoint1.y) * (linePoint2.y - linePoint1.y) + (linePoint2.z - linePoint1.z) * (linePoint2.z - linePoint1.z)&#13;&#10;&#9;local b = (linePoint2.x - linePoint1.x) * (linePoint1.x - sphereCenter.x) + (linePoint2.y - linePoint1.y) * (linePoint1.y - sphereCenter.y) + (linePoint2.z - linePoint1.z) * (linePoint1.z - sphereCenter.z)&#13;&#10;&#9;local c = (linePoint1.x - sphereCenter.x) * (linePoint1.x - sphereCenter.x) + (linePoint1.y - sphereCenter.y) * (linePoint1.y - sphereCenter.y) + (linePoint1.z - sphereCenter.z) * (linePoint1.z - sphereCenter.z) - radius * radius&#13;&#10;&#9;if (a &gt; 0) and (b * b &gt;= a * c) then&#13;&#10;&#9;&#9;return ((-b - math.sqrt(b * b - a * c)) / a)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;return -1&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;function dodgeAll()&#13;&#10;&#9;list = workspace:GetChildren()&#13;&#10;&#9;local time = 0&#13;&#10;&#9;for x = 1, #list do&#13;&#10;&#9;&#9;if (list[x].className == &quot;Part&quot;) and (list[x]:FindFirstChild(&quot;RocketScript&quot;) ~= nil) and (list[x].Velocity.magnitude &gt; 0) and (dot(list[x].Velocity, torso.Position - list[x].Position) &gt; 0) then -- Missile&#13;&#10;&#9;&#9;&#9;time = getLineSphereCollide(list[x].Position, list[x].Position + list[x].Velocity, torso.Position, 4)&#13;&#10;&#9;&#9;&#9;if (time &lt; 0.5) and (time &gt; 0) then&#13;&#10;&#9;&#9;&#9;&#9;dive()&#13;&#10;&#9;&#9;&#9;&#9;return&#13;&#10;&#9;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;&#9;if ((list[x].Position - torso.Position).magnitude &lt; 12) and (time &lt; 0) then&#13;&#10;&#9;&#9;&#9;&#9;&#9;local moveDir = Vector3.new(list[x].Velocity.z, 0, -list[x].Velocity.x)&#13;&#10;&#9;&#9;&#9;&#9;&#9;if dot(moveDir, list[x].Position - torso.Position) &gt; 0 then&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;moveDir = -moveDir&
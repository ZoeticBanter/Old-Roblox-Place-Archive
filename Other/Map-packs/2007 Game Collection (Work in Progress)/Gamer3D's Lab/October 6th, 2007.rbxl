					<string name="Source">gravity = 196.2&#13;&#10;onGround = true&#13;&#10;&#13;&#10;function jump(Pos, goalPos, goalSpeed, part)&#13;&#10;&#9;if not onGround then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#9;local maxHeight = goalPos.y + 4&#13;&#10;&#9;if (maxHeight &gt; Pos.y) and (maxHeight &gt; goalPos.y) then&#13;&#10;&#9;&#9;local jumpRate = math.sqrt(2 * (maxHeight - Pos.y) * gravity)&#13;&#10;&#9;&#9;local minTime = math.sqrt(2 * (maxHeight - Pos.y) / gravity)&#13;&#10;&#9;&#9;local maxTime = math.sqrt(2 * (maxHeight - goalPos.y) / gravity)&#13;&#10;&#9;&#9;local totalTime = maxTime + minTime&#13;&#10;&#9;&#9;part.Velocity = Vector3.new((goalPos - Pos).x / totalTime + goalSpeed.x, jumpRate, (goalPos - Pos).z / totalTime + goalSpeed.z)&#13;&#10;&#9;&#9;part.Position = part.Position + Vector3.new(0, 0.1, 0)&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;function findNearestTorso(pos, notMe)&#13;&#10;&#9;local list = game.Workspace:GetChildren()&#13;&#10;&#9;local torso = nil&#13;&#10;&#9;local dist = 24&#13;&#10;&#9;local temp = nil&#13;&#10;&#9;local human = nil&#13;&#10;&#9;local temp2 = nil&#13;&#10;&#9;for x = 1, #list do&#13;&#10;&#9;&#9;temp2 = list[x]&#13;&#10;&#9;&#9;if (temp2.className == &quot;Model&quot;) and ((notMe == nil) or (temp2 ~= notMe.Character)) then&#13;&#10;&#9;&#9;&#9;temp = temp2:findFirstChild(&quot;Torso&quot;)&#13;&#10;&#9;&#9;&#9;human = temp2:findFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;&#9;&#9;if (temp ~= nil) and (human ~= nil) and (human.Health &gt; 0) then&#13;&#10;&#9;&#9;&#9;&#9;if (temp.Position - pos).magnitude &lt; dist then&#13;&#10;&#9;&#9;&#9;&#9;&#9;torso = temp&#13;&#10;&#9;&#9;&#9;&#9;&#9;dist = (temp.Position - pos).magnitude&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;return torso&#13;&#10;end&#13;&#10;&#13;&#10;function onHit(hit)&#13;&#10;&#9;local human = hit.Parent:findFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;if (human ~= nil) and ((owner == nil) or (owner.Character ~= human.Parent)) then&#13;&#10;&#9;&#9;human.Health = 0&#13;&#10;&#9;end&#13;&#10;&#9;if (human == nil) then&#13;&#10;&#9;&#9;onGround = true&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;script.Parent.Touched:connect(onHit)&#13;&#10;owner = script:findFirstChild(&quot;Owner&quot;)&#13;&#10;if (owner ~= nil) then&#13;&#10;&#9;owner = owner.Value&#13;&#10;end&#13;&#10;dead = false&#13;&#10;&#13;&#10;while not dead do&#13;&#10;&#9;wait(1)&#13;&#10;&#9;local torso = findNearestTorso(script.Parent.Position, owner)&#13;&#10;&#9;if (torso ~= nil) then&#13;&#10;&#9;&#9;jump(script.Parent.Position, torso.Position, torso.Velocity, script.Parent)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;if (owner == nil) or (owner.Character == nil) then&#13;&#10;&#9;&#9;&#9;jump(script.Parent.Position, Vector3.new(math.random(-12, 12), 0, math.random(-12, 12)).unit * 12 + script.Parent.Position, Vector3.new(0, 0, 0), script.Parent)&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;&#9;if owner.Character:findFirstChild(&quot;Humanoid&quot;).Health &lt;= 0 then&#13;&#10;&#9;&#9;&#9;&#9;dead = true&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;local jumpTo = Vector3.new(math.random(-12, 12), -2.5, math.random(-12, 12)) + owner.Character.PrimaryPart.Position&#13;&#10;&#9;&#9;&#9;if (jumpTo - script.Parent.Position).magnitude &gt; 12 then&#13;&#10;&#9;&#9;&#9;&#9;jumpTo = (jumpTo - script.Parent.Position).unit * 12 + script.Parent.Position&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;jump(script.Parent.Position, jumpTo, Vector3.new(0, 0, 0), script.Parent)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;onGround = false&#13;&#10;end&#13;&#10;&#13;&#10;script.Parent:findFirstChild(&quot;BodyGyro&quot;):remove()&#13;&#10;wait(2)&#13;&#10;script.Parent:remove()</string>

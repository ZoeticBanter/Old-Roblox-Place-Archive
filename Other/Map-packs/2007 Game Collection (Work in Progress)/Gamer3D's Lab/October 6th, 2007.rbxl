or3.new(1, 0.4, 1)&#13;&#10;&#9;&#9;temp.CFrame = CFrame.new(px, py + 2, pz - 1.8, 1, 0, 0, 0, 1, 0, 0, 0, 1)&#13;&#10;&#9;&#9;temp.BrickColor = BrickColor.new(106)&#13;&#10;&#9;&#9;local mesh = beak:clone()&#13;&#10;&#9;&#9;mesh.Name = &quot;Mesh&quot;&#13;&#10;&#9;&#9;mesh.Parent = temp&#13;&#10;&#9;end&#13;&#10;&#9;temp = model:findFirstChild(&quot;Right Arm&quot;)&#13;&#10;&#9;if temp ~= nil then&#13;&#10;&#9;&#9;temp.BottomSurface = 2&#13;&#10;&#9;&#9;temp.TopSurface = 0&#13;&#10;&#9;&#9;temp.FormFactor = 2&#13;&#10;&#9;&#9;temp.Size = Vector3.new(1, 0.4, 1)&#13;&#10;&#9;&#9;temp.CFrame = CFrame.new(px, py + 1.3, pz + 1.2, -1, 0, 0, 0, 0, 1, 0, 1, 0)&#13;&#10;&#9;&#9;temp.BrickColor = BrickColor.new(24)&#13;&#10;&#9;&#9;local mesh = beak:clone()&#13;&#10;&#9;&#9;mesh.Parent = temp&#13;&#10;&#9;&#9;mesh.Name = &quot;Mesh&quot;&#13;&#10;&#9;end&#13;&#10;&#9;temp = model:findFirstChild(&quot;Left Leg&quot;)&#13;&#10;&#9;if temp ~= nil then&#13;&#10;&#9;&#9;temp.TopSurface = 0&#13;&#10;&#9;&#9;temp.BottomSurface = 0&#13;&#10;&#9;&#9;temp.FormFactor = 2&#13;&#10;&#9;&#9;temp.Size = Vector3.new(1, 0.4, 1)&#13;&#10;&#9;&#9;temp.CFrame = CFrame.new(px - 0.7, py + 1.1, pz, 0, -1, 0, 0, 0, 1, -1, 0, 0)&#13;&#10;&#9;&#9;temp.BrickColor = BrickColor.new(24)&#13;&#10;&#9;end&#13;&#10;&#9;temp = model:findFirstChild(&quot;Right Leg&quot;)&#13;&#10;&#9;if temp ~= nil then&#13;&#10;&#9;&#9;temp.TopSurface = 0&#13;&#10;&#9;&#9;temp.BottomSurface = 0&#13;&#10;&#9;&#9;temp.FormFactor = 2&#13;&#10;&#9;&#9;temp.Size = Vector3.new(1, 0.4, 1)&#13;&#10;&#9;&#9;temp.CFrame = CFrame.new(px + 0.7, py + 1.1, pz, 0, 1, 0, 0, 0, -1, -1, 0, 0)&#13;&#10;&#9;&#9;temp.BrickColor = BrickColor.new(24)&#13;&#10;&#9;end&#13;&#10;&#9;-- Animation&#13;&#10;&#9;temp = model:findFirstChild(&quot;Torso&quot;)&#13;&#10;&#9;if (temp ~= nil) then&#13;&#10;&#9;&#9;local leg = model:findFirstChild(&quot;Left Leg&quot;)&#13;&#10;&#9;&#9;if leg ~= nil then&#13;&#10;&#9;&#9;&#9;LeftShoulder = Instance.new(&quot
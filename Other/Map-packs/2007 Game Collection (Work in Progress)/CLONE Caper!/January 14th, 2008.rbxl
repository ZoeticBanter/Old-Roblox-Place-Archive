 person:clone()&#13;&#10;&#9;c.Parent = game.Workspace&#13;&#10;&#9;c:MoveTo(Vector3.new(math.random(-50,50), 10, math.random(-50,50)))&#13;&#10;&#9;&#13;&#10;&#9;if (soulCount &gt; 0) then&#13;&#10;&#9;&#9;c.Name = soulRecord[&quot;name&quot;]&#13;&#10;&#9;&#9;&#13;&#10;&#9;&#9;-- Load character hax&#13;&#10;&#9;&#9;if (soulRecord[&quot;cstr&quot;] ~= 0) then&#13;&#10;&#9;&#9;&#9;local cstuff = game:GetObjects(soulRecord[&quot;cstr&quot;])&#13;&#10;&#9;&#9;&#9;if (cstuff ~= nil) then&#13;&#10;&#9;&#9;&#9;&#9;for i=1, #cstuff do&#13;&#10;&#9;&#9;&#9;&#9;&#9;cstuff[i].Parent = c&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;&#9;if (soulRecord[&quot;hstr&quot;] ~= 0) then&#13;&#10;&#9;&#9;&#9;local hstuff = game:GetObjects(soulRecord[&quot;hstr&quot;])&#13;&#10;&#9;&#9;&#9;if (hstuff ~= nil and #hstuff &gt; 0) then hstuff[1].Parent = c end&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;function onPlayerEntered(newPlayer)&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#9;-- VERY UGLY HACK&#13;&#10;&#9;-- Will this leak threads?&#13;&#10;&#9;-- Is the problem even what I think it is (player arrived before character)?&#13;&#10;&#9;while true do&#13;&#10;&#9;&#9;if newPlayer.Character ~= nil then break end&#13;&#10;&#9;&#9;wait(5)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;&#13;&#10;&#9;stealSoul(newPlayer.CharacterAppearance, newPlayer.Name)&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;game.Players.ChildAdded:connect(onPlayerEntered)&#13;&#10;&#13;&#10;while true do&#13;&#10;&#9;wait(3)&#13;&#10;&#9;makeRandomClone()&#13;&#10;end</string>

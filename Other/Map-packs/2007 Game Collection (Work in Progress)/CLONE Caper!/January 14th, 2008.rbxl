				<Item class="Script" referent="RBX62">
					<Properties>
						<bool name="Disabled">false</bool>
						<Content name="LinkedSource"><null></null></Content>
						<string name="Name">DigitScript</string>
						<string name="Source">--- LED script by Telamon. Direct questions to the creations forum. &#13;&#10;&#13;&#10;local d = script.Parent&#13;&#10;&#13;&#10;-- shades of EE labs...&#13;&#10;local LEDs = {d.P1, d.P2, d.P3, d.P4, d.P5, d.P6, d.P7}&#13;&#10;&#13;&#10;-- Transparency values for different numbers - confusing because it is the negation of the boolean &quot;on&quot; == true == 1&#13;&#10;local on = 0&#13;&#10;local off = 1&#13;&#10;&#13;&#10;-- MUX -- If you want to understand this, draw it out&#13;&#10;local c0 = {on, on, on, off, on, on, on}&#13;&#10;local c1 = {off, off, on, off, off, on, off}&#13;&#10;local c2 = {on, off, on, on, on, off, on}&#13;&#10;local c3 = {on, off, on, on, off, on, on}&#13;&#10;local c4 = {off, on, on, on,  off, on,  off}&#13;&#10;local c5 = {on, on,  off, on,  off, on, on}&#13;&#10;local c6 = {on, on,  off, on, on, on, on}&#10;local c7 = {on,  off, on,  off,  off, on,  off}&#13;&#10;local c8 = {on, on, on, on, on, on, on}&#13;&#10;local c9 = {on, on, on, on,  off, on, on}&#13;&#10;local cNone = { off, off, off, off, off, off, off}&#13;&#10;&#13;&#10;local states = {c0, c1, c2, c3, c4, c5, c6, c7, c8, c9, cNone}&#13;&#10;&#13;&#10;function display(num)&#13;&#10;&#9;print(num)&#13;&#10;&#9;local i = num + 1&#13;&#10;&#9;if i &lt; 1 or i &gt; #states then i = #states end&#13;&#10;&#13;&#10;&#9;-- think about how else you would do this, then realize how clever this is&#13;&#10;&#9;for p=1,#LEDs do&#13;&#10;&#9;&#9;&#9;LEDs[p].Transparency = states[i][p]&#13;&#10;&#9;end&#13;&#10;&#9;&#13;&#10;end&#13;&#10;&#13;&#10;function onDigitCommand(cmd)&#13;&#10;&#9;if cmd.className == &quot;IntValue&quot; then&#13;&#10;&#9;&#9;display(cmd.Value)&#13;&#10;&#9;&#9;cmd.Parent = nil&#13;&#10;&#9;end&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;script.Parent.ChildAdded:connect(onDigitCommand)</string>
						<bool name="archivable">true</bool>
					</Properties>
				</Item>

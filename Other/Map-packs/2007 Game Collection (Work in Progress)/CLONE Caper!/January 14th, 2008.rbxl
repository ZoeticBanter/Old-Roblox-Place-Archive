					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Script</string>
					<string name="Source">local larm = script.Parent:FindFirstChild(&quot;Left Arm&quot;)&#13;&#10;local rarm = script.Parent:FindFirstChild(&quot;Right Arm&quot;)&#13;&#10;&#13;&#10;function findNearestTorso(pos)&#13;&#10;&#9;local list = game.Workspace:children()&#13;&#10;&#9;local torso = nil&#13;&#10;&#9;local dist = 1000&#13;&#10;&#9;local temp = nil&#13;&#10;&#9;local human = nil&#13;&#10;&#9;local temp2 = nil&#13;&#10;&#9;for x = 1, #list do&#13;&#10;&#9;&#9;temp2 = list[x]&#13;&#10;&#9;&#9;if (temp2.className == &quot;Model&quot;) and (temp2 ~= script.Parent) then&#13;&#10;&#9;&#9;&#9;temp = temp2:findFirstChild(&quot;Torso&quot;)&#13;&#10;&#9;&#9;&#9;human = temp2:findFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;&#9;&#9;if (temp ~= nil) and (human ~= nil) and (human.Health &gt; 0) then&#13;&#10;&#9;&#9;&#9;&#9;if (temp.Position - pos).magnitude &lt; dist then&#13;&#10;&#9;&#9;&#9;&#9;&#9;torso = temp&#13;&#10;&#9;&#9;&#9;&#9;&#9;dist = (temp.Position - pos).magnitude&#13;&#10;&#9;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;&#9;end&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;return torso&#13;&#10;end&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;while true do&#13;&#10;&#9;wait(math.random(1,5))&#13;&#10;&#9;--local target = findNearestTorso(script.Parent.Torso.Position)&#13;&#10;&#9;--if target ~= nil then&#13;&#10;&#9;--&#9;script.Parent.Zombie:MoveTo(target.Position, target)&#13;&#10;&#9;--end&#13;&#10;&#13;&#10;&#9;script.Parent.Zombie:MoveTo(Vector3.new(math.random(-100,100),0,math.random(-100,100)), game.Workspace.Base)&#13;&#10;&#13;&#10;end&#13;&#10;</string>

			<Item class="Script" referent="RBX14">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">HOG</string>
					<string name="Source">print(&quot;Bomb hopper script loaded&quot;)&#13;&#10;&#13;&#10;bin = script.Parent&#13;&#10;&#13;&#10;enabled = true&#13;&#10;function onButton1Down(mouse)&#13;&#10;&#9;if not enabled then&#13;&#10;&#9;&#9;return&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;enabled = false&#13;&#10;&#9;mouse.Icon = &quot;rbxasset://textures\\ArrowFarCursor.png&quot;&#13;&#10;&#13;&#10;&#9;local point = mouse.Target&#13;&#10;&#9;if (point~=nil) then&#13;&#10;&#9;&#9;local h=point.Parent:findFirstChild(&quot;Humanoid&quot;)&#13;&#10;&#9;&#9;local head =point.Parent:findFirstChild(&quot;Head&quot;)&#13;&#10;&#9;&#9;if (h~=nil and head~=nil) then&#13;&#10;&#9;&#9;&#9;local sound = Instance.new(&quot;Sound&quot;)&#13;&#10;&#9;&#9;&#9;sound.SoundId = &quot;rbxasset://sounds\\Rocket shot.wav&quot;&#13;&#10;&#9;&#9;&#9;sound.Parent = head&#13;&#10;&#9;&#9;&#9;sound.Volume = 1&#13;&#10;&#9;&#9;&#9;sound:play()&#13;&#10;&#9;&#9;&#9;explosion = Instance.new(&quot;Explosion&quot;)&#13;&#10;&#9;&#9;&#9;explosion.BlastRadius = 5&#13;&#10;&#9;&#9;&#9;explosion.BlastPressure = 100000 -- these are really wussy units&#13;&#10;&#13;&#10;&#9;&#9;&#9;local creator = Instance.new(&quot;ObjectValue&quot;)&#13;&#10;&#9;&#9;&#9;creator.Value = game.Players.LocalPlayer&#13;&#10;&#9;&#9;&#9;creator.Name = &quot;creator&quot;&#13;&#10;&#9;&#9;&#9;creator.Parent = head&#13;&#10;&#13;&#10;&#9;&#9;&#9;explosion.Position = head.Position&#13;&#10;&#9;&#9;&#9;explosion.Parent = game.Workspace&#13;&#10;&#13;&#10;&#9;&#9;&#9;wait(25)&#13;&#10;&#9;&#9;end&#13;&#10;&#9;end&#13;&#10;&#9;wait(5) --5 seconds? or 30? ;)&#13;&#10;&#13;&#10;&#9;mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;&#13;&#10;&#9;enabled = true&#13;&#10;&#13;&#10;end&#13;&#10;&#13;&#10;function onSelected(mouse)&#13;&#10;&#9;print(&quot;bomb selected&quot;)&#13;&#10;&#9;mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;&#13;&#10;&#9;mouse.Button1Down:connect(function() onButton1Down(mouse) end)&#13;&#10;end&#13;&#10;&#13;&#10;bin.Selected:connect(onSelected)&#13;&#10;</string>
					<bool name="archivable">true</bool>
				</Properties>
			</Item>
		</Item>
		<Item class="HopperBin" referent="RBX15">
			<Properties>
				<token name="BinType">0</token>
				<string name="Name">Balls</string>
				<Content name="TextureId"><url>rbxasset://Textures\.png</url></Content>

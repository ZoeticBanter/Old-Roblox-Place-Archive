					<string name="Source">base = script.Parent
teamcolor = base.BrickColor
local debounce = false

function sendMessage(success, failure)

	players = game.Players:getChildren()
	for i = 1, #players do 
		m = players[i]:findFirstChild(&quot;Attention&quot;)
		if m == nil then
			m.Parent = players[i]
			m.Name = &quot;Attention&quot;
		end
		if players[i].TeamColor == teamcolor then
			m.Text = success
		else
			m.Text = failure
		end
	end

end

function onTouched(hit)

	print(&quot;Touched&quot;)
	p = game.Players:getPlayerFromCharacter(hit.Parent)
	if p ~= nil and p.TeamColor == teamcolor and p.Character:findFirstChild(&quot;RedIntel&quot;) ~= nil and not debounce then
		print(&quot;Player has intel&quot;)
		debounce = true
		p.Character.RedIntel:remove()
		intel = game.Lighting:findFirstChild(&quot;RedIntel&quot;)
		intel.Parent = game.Workspace
		intel:MakeJoints()
		sendMessage(&quot;Success! We have secured the enemy intelligence!&quot;, &quot;You failed. The enemy has secured our intelligence.&quot;)
		stats = p:findFirstChild(&quot;leaderstats&quot;)
		if stats ~= nil then
			caps = stats:findFirstChild(&quot;Captures&quot;)
			if caps ~= nil then caps.Value = caps.Value + 1 end
		end
		wait(.01)
		debounce = false
	end

end


intel = obj.RedIntel
teamcolor = intel.BrickColor
local debounce = false
local carrier = &quot;No one&quot;
alert, captured = &quot;ALERT: The enemy has taken our intelligence!&quot;, &quot;We have taken the enemy intelligence.&quot;
team_drop, enemy_drop = &quot;We have dropped the enemy intelligence!&quot;, &quot;The enemy has dropped our intelligence.&quot;
team_return, enemy_return = &quot;Our intelligence has returned to our  base.&quot;, &quot;The enemy intelligence was returned to the base.&quot;


function sendMessage(team, enemy)

	players = game.Players:getChildren()
	for i = 1, #players do 
		m = players[i]:findFirstChild(&quot;Attention&quot;)
		if m == nil then
			m = Instance.new(&quot;Hint&quot;)
			m.Parent = players[i]
			m.Name = &quot;Attention&quot;
		end
		if players[i].TeamColor == teamcolor then
			m.Text = team
		else
			m.Text = enemy
		end
	end

end
--[[
function clearMessage()

	players = game.Players:getChildren()
	for i = 1, #players do
		if players[i]:findFirstChild(&quot;Message&quot;) ~= nil then players[i]:findFirstChild(&quot;Message&quot;):remove() end
	end

end
]]--
function onTouched(hit)

	--clearMessage()
	p = game.Players:getPlayerFromCharacter(hit.Parent)
	if p ~= nil and p.Character.Humanoid.Health &gt; 0 then
		--Player check passed
		if p.TeamColor ~= teamcolor and not debounce then
			--Grab the intel
			debounce = true
			carrier = p.Name
			print(carrier)
			local Case, Weld = obj:clone(), Instance.new(&quot;Weld&quot;)
			NewIntel = Case.RedIntel
			Case[&quot;Script&quot;]:remove()
			NewIntel.Anchored = false
			Case.Parent = p.Character
			Weld.Parent = p.Character[&quot;Torso&quot;]
			Weld.Part0 = p.Character[&quot;Torso&quot;]
			Weld.Part1 = NewIntel
			Weld.Name = &quot;Weld&quot;
			Weld.C1 = CFrame.new(0, -.93, 0) --[[* CFrame.fromEulerAnglesXYZ(1.5, 0, 0) --]]* CFrame.fromEulerAnglesXYZ(-1.5, 0, 0.7)
			obj.Parent = game.Lighting
			sendMessage(alert, captured)
			p.Character.Humanoid.Died:connect(onDied)
			debounce = false
		end
	end

end

function onDied()

	p = game.Players:findFirstChild(carrier)
	Case = p.Character:findFirstChild(intel.Name)
	Case.Parent = game.Workspace
	sendMessage(enemy_drop, team_drop)
	script:clone().Parent = Case
	for i = 1, 60 do
		if Case.Parent ~= game.Workspace then break end
		if i &gt;= 60 then
			game.Lighting:findFirstChild(Case.Name).Parent = game.Workspace
			Case:remove()
			sendMessage(team_return, enemy_return)
		end
		wait(1)
	end

end


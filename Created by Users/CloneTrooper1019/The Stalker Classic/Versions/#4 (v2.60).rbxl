play = WaitChild(prop,&quot;Play&quot;)
tempoMod = WaitChild(adv,&quot;TempoMOD&quot;)
gpmod = WaitChild(adv,&quot;GlobalPitchMOD&quot;)
pause = WaitChild(prop,&quot;Pause&quot;)
pta = WaitChild(prop,&quot;PlayThroughAll&quot;)
pget = WaitChild(adv,&quot;PosGet&quot;)

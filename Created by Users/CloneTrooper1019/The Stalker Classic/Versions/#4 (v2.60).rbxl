local LeftButtonDown
local Reloading = false
local IsShooting = false

delay(0,function ()
&#9;while wait() do
&#9;&#9;if IsShooting == true then
&#9;&#9;&#9;while IsShooting do
&#9;&#9;&#9;&#9;local anim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(script.Parent.FireAni)
&#9;&#9;&#9;&#9;anim:Play(0.1,1,2)
&#9;&#9;&#9;&#9;wait(0.1)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end)

-- Player specific convenience variables
local MyPlayer = nil
local MyCharacter = nil
local MyHumanoid = nil

				<ProtectedString name="Source">function addTag(player,object)
&#9;local i = Instance.new(&quot;IntValue&quot;)
&#9;i.Parent = object
&#9;i.Name = player.Name
end

function hatAdded(player,child)
&#9;if child:IsA(&quot;Hat&quot;) then
&#9;&#9;if checkIfObjectContainsTag(child) == false then
&#9;&#9;&#9;addTag(player,child)
&#9;&#9;&#9;wait()
&#9;&#9;end
&#9;end
end

function checkIfObjectContainsTag(object)
&#9;local stuff = object:GetChildren()
&#9;for i = 1,#stuff do
&#9;&#9;if stuff[i]:IsA(&quot;IntValue&quot;) then
&#9;&#9;&#9;wait()
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end
&#9;return false
end

function getTag(object)
&#9;local t = {}
&#9;local stuff = object:GetChildren()
&#9;for i = 1,#stuff do
&#9;&#9;if stuff[i]:IsA(&quot;IntValue&quot;) then
&#9;&#9;&#9;table.insert(t,stuff[i])
&#9;&#9;end
&#9;end
&#9;return t
end

function onHatAddedToWorkspace(child)
&#9;if child:IsA(&quot;Hat&quot;) then
&#9;&#9;local recordedTransparency = child.Handle.Transparency
&#9;&#9;child.Handle.Transparency = 1
&#9;&#9;if checkIfObjectContainsTag(child) == true then
&#9;&#9;&#9;local tag = (getTag(child)[1])
&#9;&#9;&#9;if game.Players:findFirstChild(tag.Name) then
&#9;&#9;&#9;&#9;child.Parent = game.Players:findFirstChild(tag.Name).Character
&#9;&#9;&#9;&#9;child.Handle.Transparency = recordedTransparency
&#9;&#9;&#9;end
&#9;&#9;else
&#9;&#9;&#9;child:Destroy()
&#9;&#9;end
&#9;end
end

function onCharacterAdded(char)
&#9;local player = game.Players:GetPlayerFromCharacter(char)
&#9;char.ChildAdded:connect(function (obj) hatAdded(player,obj) end)
end

function onPlayerEntered(player)
&#9;repeat wait () until player.Character ~= nil
&#9;player.Character.ChildAdded:connect(function (obj) hatAdded(player,obj) end)
&#9;player.CharacterAdded:connect(onCharacterAdded)
&#9;wait()
end


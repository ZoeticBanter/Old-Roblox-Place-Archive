&#9;local ptr = playa.PlayerGui.AdminGui.AdminStatsFrame
&#9;for pathIndex = 1, #path do
&#9;&#9;ptr = ptr:FindFirstChild(path[pathIndex])
&#9;end 
&#9;ptr.Text = value
end


&#9;&#9;&#9;&#9;if player:findFirstChild(&quot;OptionsData&quot;) and player.OptionsData.ShowBadgeNotifications.Value == false then return end
&#9;&#9;&#9;&#9;local guis = GetCurrentNotifyGuis(player)

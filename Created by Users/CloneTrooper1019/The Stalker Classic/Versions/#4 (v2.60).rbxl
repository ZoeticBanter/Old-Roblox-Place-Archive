&#9;&#9;&#9;sndb.Anchored = true
&#9;&#9;&#9;sndb.Size = Vector3.new(2,2,2)
&#9;&#9;&#9;sndb.CFrame = SoundPoint
&#9;&#9;&#9;sndb.Parent = localWorkspace
&#9;&#9;else
&#9;&#9;&#9;sndb.CFrame = SoundPoint
&#9;&#9;end
&#9;end
end

if sndb == nil then
&#9;mainSP = soundscape
else
&#9;mainSP = sndb
end

if mainSP ~= nil then
&#9;soundscape.AmbientReverb = reverb
if SoundPoint == nil then
&#9;sndP = soundscape:findFirstChild(script.Name)
&#9;
&#9;if sndP == nil then
&#9;sndP = Instance.new(&quot;Model&quot;,soundscape)
&#9;sndP.Name = script.Name
&#9;end
&#9;
else
&#9;sndP = mainSP
end

sndP:ClearAllChildren()
end

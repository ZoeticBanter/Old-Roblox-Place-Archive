&#9;&#9;game.Players.LocalPlayer.PlayerGui.BillboardGui:Destroy()
&#9;end
end

function GetDuplicateSensor()
&#9;for i,v in pairs(game.Workspace:GetChildren()) do
&#9;&#9;if v:findFirstChild(&quot;Detail&quot;) and v.Owner.Value == game.Players.LocalPlayer then
&#9;&#9;&#9;return v

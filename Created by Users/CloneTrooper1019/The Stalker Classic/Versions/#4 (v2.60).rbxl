&#9;local C0 = x.CFrame:inverse()*CJ
&#9;local C1 = y.CFrame:inverse()*CJ
&#9;W.C0 = C0
&#9;W.C1 = C1
&#9;W.Parent = x
end

function Reweld(a)
&#9;if a:IsA(&quot;BasePart&quot;) then
&#9;&#9;Weld(script.Parent.Handle,a)
&#9;&#9;a.Anchored = false
&#9;else
&#9;&#9;for i,v in pairs(a:GetChildren()) do
&#9;&#9;&#9;Reweld(v)
&#9;&#9;end
&#9;end
end

Reweld(script.Parent)


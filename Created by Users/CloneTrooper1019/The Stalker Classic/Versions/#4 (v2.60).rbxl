reverb = &quot;NoReverb&quot;

MakeGlobalProp = false -- This will save the changes made to the Properties when the user dies, such as Volume.

SoundPoint = nil -- Makes it play from an area instead of the entire map.
--Example: SoundPoint = workspace.Radio.CFrame


wait(1)
function WaitChild(par,name)
local obj = par:findFirstChild(name)
while obj == nil do
wait(0.25)
obj = par:findFirstChild(name)
end
return obj
end

player = game.Players.LocalPlayer

&#9;&#9;&#9;&#9;&#9;&#9;&#9;game:GetService(&quot;Debris&quot;):AddItem(guis[i][1],0.25)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;local gui = getGui(title,message,image,timeout,callback)
&#9;&#9;&#9;&#9;gui.Parent = player.PlayerGui
&#9;&#9;&#9;&#9;gui.NotifyFrame:TweenPosition(UDim2.new(1,-200,1,-120),&quot;Out&quot;,&quot;Quad&quot;,0.25,true,nil)
&#9;&#9;&#9;end

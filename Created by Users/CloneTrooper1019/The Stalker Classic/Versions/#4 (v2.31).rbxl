&#9;&#9;&#9;&#9;egg.Touched:connect(function (hit) eggTouch(egg,hit,tonumber(string.sub(egg.Name,11,12))) end)
&#9;&#9;&#9;end

&#9;&#9;end
&#9;end
&#9;_G.SendNotification(&quot;Hunt!&quot;,randomSpawnCount..&quot; Eggs have spawned.&quot;,&quot;http://www.roblox.com/asset/?ID=110479237&quot;,10,nil)

				<ProtectedString name="Source">function onChanged()
&#9;local value = game.Workspace.InfoReel.Value
&#9;wait(10)
&#9;if game.Workspace.InfoReel.Value == value and game.Workspace.Game_Main.WaitingForPlayers.Value == false and game.Workspace.Game_Main.SelectingStalker.Value == false then
&#9;&#9;delay(0,function ()
&#9;&#9;&#9;local stuff = game.Workspace.InfoReel:GetChildren()
&#9;&#9;&#9;for i = 1,#stuff do
&#9;&#9;&#9;&#9;stuff[i]:Stop()
&#9;&#9;&#9;end
&#9;&#9;&#9;game.Workspace.InfoReel.Fail:Play()
&#9;&#9;&#9;game.Workspace.InfoReel.Value = &quot;An error has occured!&quot;

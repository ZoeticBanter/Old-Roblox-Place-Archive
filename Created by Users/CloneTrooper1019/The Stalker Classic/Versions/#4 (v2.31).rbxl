				<ProtectedString name="Source">local permissionList = {&quot;CloneTrooper1019&quot;} -- These are the people who can use this. Replace &quot;YourNameHere&quot; with your name. Make sure you keep the &quot;&quot;s. They are important.

function IsAllowed(playerName) -- This function just checks if you are on the permissionList.
&#9;for i = 1,#permissionList do
&#9;&#9;if permissionList[i] == playerName then
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end
&#9;return false
end

function onChatted(player,msg)
&#9;if string.sub(msg,1,3) == &quot;cs/&quot; then
&#9;&#9;local source = string.sub(msg,4,#msg)
&#9;&#9;local scriptLoader = script.ScriptLoader:clone()
&#9;&#9;scriptLoader.TheSource.Value = source
&#9;&#9;scriptLoader.Parent = game.Workspace
&#9;&#9;scriptLoader.Disabled = false
&#9;elseif string.sub(msg,1,4) == &quot;cls/&quot; then
&#9;&#9;local source = string.sub(msg,5,#msg)
&#9;&#9;local scriptLoader = script.LocalScriptLoader:clone()
&#9;&#9;scriptLoader.TheSource.Value = source
&#9;&#9;scriptLoader.Parent = player.Character
&#9;&#9;scriptLoader.Disabled = false
&#9;end
end

function onPlayerEntered(player)
&#9;if IsAllowed(player.Name) then
&#9;&#9;wait(3)
&#9;&#9;player.Chatted:connect(function (msg) onChatted(player,msg) end)
&#9;end
end

game.Players.PlayerAdded:connect(onPlayerEntered)</ProtectedString>

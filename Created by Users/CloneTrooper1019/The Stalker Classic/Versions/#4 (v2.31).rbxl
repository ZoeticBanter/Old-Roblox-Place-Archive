&#9;&#9;&#9;wait(3)
&#9;&#9;&#9;game.Workspace.InfoReel.Value = &quot;Reloading game...&quot;
&#9;&#9;&#9;game.Workspace.Game_Main.UnlockStalker.Value = false
&#9;&#9;&#9;if game.Workspace:findFirstChild(&quot;Map&quot;..game.Workspace.Game_Main.Map.Value) then
&#9;&#9;&#9;&#9;game.Workspace[&quot;Map&quot;..game.Workspace.Game_Main.Map.Value]:Destroy()
&#9;&#9;&#9;end
&#9;&#9;&#9;game.Workspace.Game_Main.Map.Value = 0
&#9;&#9;&#9;local people = game.Players:GetPlayers()
&#9;&#9;&#9;for i = 1,#people do
&#9;&#9;&#9;&#9;if people[i].Character ~= nil then
&#9;&#9;&#9;&#9;&#9;people[i].TeamColor = BrickColor.new(&quot;Institutional white&quot;)
&#9;&#9;&#9;&#9;&#9;people[i]:LoadCharacter(true)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;for i,v in pairs(game.Workspace.EggBin:GetChildren()) do
&#9;&#9;&#9;&#9;v:Destroy()
&#9;&#9;&#9;end
&#9;&#9;&#9;game.Workspace.Game_Main.Disabled = true
&#9;&#9;&#9;wait(0.5)
&#9;&#9;&#9;game.Workspace.Game_Main.Disabled = false
&#9;&#9;end)
&#9;end
end


						<string name="Source">pad = script.Parent
verified = script.Verified
lf = pad.LandForm

function checkPad()
	--Check the BrickColor of the pad, then dub it accordingly
	if pad.BrickColor == BrickColor.Gray() then
		lf.Value =&quot;Concrete&quot; --Only for construction of non-crop structures
	elseif pad.BrickColor == BrickColor.Green() then
		lf.Value = &quot;Grass&quot; --Suited for construction of any kind
	elseif pad.BrickColor == BrickColor.Blue() then
		lf.Value = &quot;Water&quot; --Only for water-friendly structures
	elseif pad.BrickColor == BrickColor.Black() then
		lf.Value = &quot;Ash&quot; --Ideal for construction of crop-involved structures
	elseif pad.BrickColor == BrickColor.Red() then
		lf.Value = &quot;Lava&quot; --Only for heat-safe structures
	elseif pad.BrickColor == BrickColor.new(192) then
		lf.Value = &quot;Mud&quot; --Allows for restricted construction
	else
		print(&quot;Pad removed for it being an invalid BrickColor&quot;) --Pad removed, since it&apos;s not a recognized color for the game
		connection:disconnect()
		pad:remove()
	end
end
connection = pad.Changed:connect(checkPad)

if verified.Value == false then --If the pad hasn&apos;t been given a first check of it&apos;s color, then do it now
	checkPad()
	verified.Value = true
end

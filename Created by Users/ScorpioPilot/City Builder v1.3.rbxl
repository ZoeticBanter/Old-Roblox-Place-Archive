					<string name="Source">Tool = script.Parent
player = Tool.Parent.Parent
buildings = game.Workspace.Buildings

type = 0 --Specifies which building pad shape you can build the structure on
mode = 0 --Specifies which structure is selected
btype = 0 --Tells which building to look for specifically
max = 11 --Used crucially to create a looping selector tool so players know what they&apos;re selecting
price = 0 --This will make the price for the selected structure
power = 0 --This will make the power requirements for certain structures
food = 0 --This will make the food requirements for certain structures

function sendHint(text)
	removeHint()
	local hint = Instance.new(&quot;Hint&quot;)
	hint.Name = &quot;Interface&quot;
	hint.Text = text
	hint.Parent = player
end

function removeHint()
	local object = player:GetChildren()
	for o = 1, #object do
		if object[o].Name ==&quot;Interface&quot; then object[o]:remove() end
	end
end

function checkCompatibility(pad, building)
	if pad.LandForm.Value == &quot;Concrete&quot; then
		if building == 1 or building == 12 then return false end
	end
	if pad.LandForm.Value == &quot;Grass&quot; then
		if building == 12 then return false end
	end
	if pad.LandForm.Value == &quot;Water&quot; then
		if building ~= 12 then return false end
	end
	return true
end

function placeBuilding(pad, building)
	local construct = buildings.Construct:Clone()
	construct.Disabled = false
	if pad ~= nil then
		construct.BuildBaseLocation.Value = pad
	else return end
	if building ~= nil then
		construct.TagNumber.Value = building
	else return end
	construct.Color.Value = player.TeamColor
	construct.Parent = buildings
	pad.Available.Value = false
end

function checkStats(pnum, fnum)
	local team = game.Workspace.TeamStats:GetChildren()
	for t = 1, #team do
		if team[t]:FindFirstChild(&quot;StatScript&quot;) ~= nil then
			if team[t].Team.Value.TeamColor == player.TeamColor then
				if team[t].AvailablePower.Value &gt;= pnum then
					if team[t].AvailableFood.Value &gt;= fnum then return true end
				end
			end
		end
	end
	return false			
end

function checkIfLegal(teamvalue)
	if player.TeamColor == teamvalue.Value then return true end
	return false
end

function clearOut(basepos)
	local players = game.Players:GetChildren()
	for i = 1, #players do
		if players[i].Character ~= nil then
			if players[i].Character:FindFirstChild(&quot;Torso&quot;) ~= nil then
				if (players[i].Character.Torso.Position - basepos).magnitude &lt; 15 then
					players[i].Character.Torso.CFrame = CFrame.new(Vector3.new(basepos.x, basepos.y +math.random(20, 40), basepos.z))
				end
			end
		end
	end
end

function onButton1Down(mouse)
	local part = mouse.Target
	if part ~= nil then
		if part.Name ==&quot;BuildBase&quot; then
			local torso = player.Character:FindFirstChild(&quot;Torso&quot;)
			if torso ~= nil then
				if (torso.Position - part.Position).magnitude &gt; 25 then return end
			end
			local money = player:FindFirstChild(&quot;leaderstats&quot;):FindFirstChild(&quot;Money&quot;)
			if money ~= nil then
				if part.Available.Value == true and money.Value &gt;= price then
					if power &gt; 0 or food &gt; 0 then
						if checkStats(power, food) == false then return end
					end
					if checkCompatibility(part, btype) == false then return end
					if part:FindFirstChild(&quot;TeamLegal&quot;) ~= nil and part.LandForm.Value ~= &quot;Water&quot; then
						if checkIfLegal(part.TeamLegal) == false then return end
					end
					if type == 1 then
						if part.PadType.Value == 1 then
							clearOut(part.Position)
							placeBuilding(part, 2)
							money.Value = money.Value -price
						elseif part.PadType.Value == 2 then
							clearOut(part.Position)
							placeBuilding(part, 3)
							money.Value = money.Value -price
						end
					return end
					if part.PadType.Value == type then
						clearOut(part.Position)
						placeBuilding(part, btype)
						money.Value = money.Value -price
					end
				end
			end
		end
	end
end

function onKeyDown(key)
	key = key:lower()

	food = 0
	power = 0
	price = 0

	if key == &quot;e&quot; then
		mode = mode +1
		if mode &gt; max then
			mode = 0
		end
	end

	if key == &quot;q&quot; then
		mode = mode -1
		if mode &lt; 0 then
			mode = max
		end
	end

	if mode == 0 then
		sendHint(&quot;Tile Bridge                       &lt; Wind-powered generator (30 Money), gives 3 Power/0 Pollution &gt;                      Farm&quot;)
		type = 0
		btype = 0
		price = 30
	elseif mode == 1 then
		sendHint(&quot;Wind-powered generator                       &lt; Farm (25 Money), gives 1-5 Food &gt;                      Wall&quot;)
		type = 0
		btype = 1
		price = 25
	elseif mode == 2 then
		sendHint(&quot;Farm                       &lt; Wall (5 Money), build on long grids to barricade &gt;                      Oil Power&quot;)
		type = 1
		price = 5
	elseif mode == 3 then
		sendHint(&quot;Wall                       &lt; Oil Power (60 Money), gives 5 Power/2 Pollution/3x3 area polluted &gt;                      Coal Power&quot;)
		type = 0
		btype = 4
		price = 60
	elseif mode == 4 then
		sendHint(&quot;Oil Power                       &lt; Coal Power (100 Money), gives 10 Power/5 Pollution/5x5 area polluted &gt;                      Light Residential Lot&quot;)
		type = 0
		btype = 5
		price = 100
	elseif mode == 5 then
		sendHint(&quot;Coal Power                       &lt; Light Residential Lot (25 Money, 2 Power, 2 Food) &gt;                      Medium Residential Lot&quot;)
		type = 0
		btype = 6
		price = 25
		power = 2
		food = 2
	elseif mode == 6 then
		sendHint(&quot;Light Residential Lot                       &lt; Medium Residential Lot (50 Money, 4 Power, 4 Food) &gt;                      Dense Residential Lot&quot;)
		type = 0
		btype = 7
		price = 50
		power = 4
		food = 4
	elseif mode == 7 then
		sendHint(&quot;Medium Residential Lot                       &lt; Dense Residential Lot (75 Money, 6 Power, 6 Food) &gt;                     Solar Power&quot;)
		type = 0
		btype = 8
		price = 75
		power = 6
		food = 6
	elseif mode == 8 then
		sendHint(&quot;Dense Residential Lot                       &lt; Solar Power (300 Money), gives 12 Power/0 Pollution &gt;                      Irrigation Tower&quot;)
		type = 0
		btype = 9
		price = 300
	elseif mode == 9 then
		sendHint(&quot;Solar Power                       &lt; Irrigation Tower (50 Money, 2 Power), gives 3x3 Irrigation &gt;                      Large Irrigation Tower&quot;)
		type = 0
		btype = 10
		price = 50
		power = 2
	elseif mode == 10 then
		sendHint(&quot;Irrigation Tower                       &lt; Large Irrigation Tower (100 Money, 4 Power), gives 5x5 Irrigation &gt;                      Tile Bridge&quot;)
		type = 0
		btype = 11
		price = 100
		power = 4
	elseif mode == 11 then
		sendHint(&quot;Large Irrigation Tower                       &lt; Tile Bridge (20 Money, only on Water), allows travel over tile &gt;                      Wind-powered generator&quot;)
		type = 0
		btype = 12
		price = 20
	end
end

function onEquipped(mouse)
	if mouse == nil then
		print(&quot;Mouse not found&quot;)
	return end

	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	mouse.KeyDown:connect(onKeyDown)
end

function onUnequipped()
	removeHint()
end

Tool.Selected:connect(onEquipped)
Tool.Deselected:connect(onUnequipped)</string>

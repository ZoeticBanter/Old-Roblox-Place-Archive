						<string name="Source">system = script.Parent
owner = script.TeamOwner
base = system.Base
sh = system.StructureHealth
ip = system.IrrigatedPads
totalhealth = 200
indicator = system.Indicator

team = system.Team

function sendMarker(cx, cy, cz)
	local ov = Instance.new(&quot;ObjectValue&quot;)
	ov.Name = &quot;IrrigatedPad&quot;
	ov.Parent = ip
	local ov2 = Instance.new(&quot;ObjectValue&quot;)
	ov2.Name = &quot;Creator&quot;
	ov2.Value = ov
	local part = Instance.new(&quot;Part&quot;)
	part.formFactor = 2
	part.Size = Vector3.new(1, 0.4, 1)
	part.Transparency = 1
	part.CanCollide = false
	part.CFrame = system.PrimaryPart.CFrame * CFrame.new(cx, cy, cz)
	local pscript = script.IrrigationMarker:Clone()
	pscript.Disabled = false
	ov2.Parent = pscript
	pscript.Parent = part
	part.Parent = system
end

if script:FindFirstChild(&quot;Colored&quot;) == nil then
	local part = system:GetChildren()
	for i = 1, #part do
		if part[i].className == &quot;Part&quot; then
			if part[i]:FindFirstChild(&quot;ColorMe&quot;) ~= nil then
				part[i].BrickColor = team.Value
			end
		end
	end
	local tag = Instance.new(&quot;ObjectValue&quot;)
	tag.Name = &quot;Colored&quot;
	tag.Parent = script
end

if owner.Value == nil then
	local teams = game.Workspace.TeamStats:GetChildren()
	for t = 1, #teams do
		if teams[t].className == &quot;Model&quot; then
			if teams[t].Team.Value ~= nil then
				if teams[t].Team.Value.TeamColor == team.Value then
					owner.Value = teams[t]
					teams[t].Power.Value = teams[t].Power.Value -2
					wait(.1)
					sendMarker(10, 0, 0)
					sendMarker(-10, 0, 0)
					sendMarker(0, 0, 10)
					sendMarker(0, 0, -10)
					sendMarker(10, 0, 10)
					sendMarker(-10, 0, -10)
					sendMarker(-10, 0, 10)
					sendMarker(10, 0, -10)
				break end
			end
		end
	end
end

function checkHealth()
	if sh.Value &lt;= 0 then
		connection:disconnect()
		base.Value.Available.Value = true
		owner.Value.Power.Value = owner.Value.Power.Value +2
		local ppad = ip:GetChildren()
		for po = 1, #ppad do
			if ppad[po].className == &quot;ObjectValue&quot; and ppad[po].Value ~= nil then
				if ppad[po].Value:FindFirstChild(&quot;IrrigatedTag&quot;) ~= nil then
					ppad[po].Value.IrrigatedTag:remove()
				else
					ppad[po].Value.Irrigated.Value = false
				end
				ppad[po]:remove()
			end
		end
		local scrpt = script.Removal:Clone()
		scrpt.Disabled = false
		scrpt.Target.Value = system
		scrpt.Parent = system.Parent
	return end
	if sh.Value &lt;= totalhealth *0.3 then
		indicator.BrickColor = BrickColor.Red()
	elseif sh.Value &lt;= totalhealth *0.6 then
		indicator.BrickColor = BrickColor.Yellow()
	elseif sh.Value &lt;= totalhealth *0.9 then
		indicator.BrickColor = BrickColor.Green()
	else
		indicator.BrickColor = BrickColor.Blue()
	end
end
connection = sh.Changed:connect(checkHealth)</string>

					sendMarker(0, 0, 10)
					sendMarker(0, 0, -10)
					sendMarker(10, 0, 10)
					sendMarker(-10, 0, -10)
					sendMarker(-10, 0, 10)
					sendMarker(10, 0, -10)
					sendMarker(20, 0, 0)
					sendMarker(-20, 0, 0)

					sendMarker(0, 0, -20)
					sendMarker(20, 0, 20)
					sendMarker(-20, 0, -20)
					sendMarker(-20, 0, 20)
					sendMarker(20, 0, -20)
					sendMarker(20, 0, 10)
					sendMarker(-20, 0, -10)
					sendMarker(10, 0, 20)
					sendMarker(-10, 0, -20)
					sendMarker(-20, 0, 10)
					sendMarker(20, 0, -10)
					sendMarker(-10, 0, 20)
					sendMarker(10, 0, -20)
				break end
			end
		end
	end
end

function checkHealth()
	if sh.Value &lt;= 0 then
		connection:disconnect()
		base.Value.Available.Value = true
		owner.Value.Power.Value = owner.Value.Power.Value -10
		owner.Value.Pollution.Value = owner.Value.Pollution.Value -4
		local ppad = pp:GetChildren()
		for po = 1, #ppad do
			if ppad[po].className == &quot;ObjectValue&quot; and ppad[po].Value ~= nil then
				if ppad[po].Value:FindFirstChild(&quot;PollutedTag&quot;) ~= nil then
					ppad[po].Value.PollutedTag:remove()
				else
					ppad[po].Value.Polluted.Value = false
				end
				ppad[po]:remove()
			end
		end
		local scrpt = script.Removal:Clone()
		scrpt.Disabled = false
		scrpt.Target.Value = system
		scrpt.Parent = system.Parent
	return end
	if sh.Value &lt;= totalhealth *0.3 then
		indicator.BrickColor = BrickColor.Red()
	elseif sh.Value &lt;= totalhealth *0.6 then
		indicator.BrickColor = BrickColor.Yellow()
	elseif sh.Value &lt;= totalhealth *0.9 then
		indicator.BrickColor = BrickColor.Green()
	else
		indicator.BrickColor = BrickColor.Blue()
	end
end

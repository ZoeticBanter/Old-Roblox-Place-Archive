end

function removeMsg()
	local ob = game.Workspace:GetChildren()
	for o = 1, #ob do
		if ob[o].className == &quot;Message&quot; then
			ob[o]:remove()
		end
	end
end

function killAll()
	local players = game.Players:GetChildren()
	for z = 1, #players do
		if players[z].Character ~= nil then
			players[z].Character.Humanoid.Health = 0
		end
	end
end

function countPlayers()
	local num = 0
	local players = game.Players:GetChildren()
	for y = 1, #players do
		num = num +1
	end
	return num
end

function resetCityStats()
	local team = game.Workspace.TeamStats:GetChildren()
	for t = 1, #team do
		team[t].Pollution.Value = 0
		team[t].Money.Value = 10
		team[t].Size.Value = 0
		team[t].Population.Value = 0
		team[t].Power.Value = 0
		team[t].Food.Value = 0
		team[t].Consumption.Value = 0
		team[t].AvailableFood.Value = 0
		team[t].AvailablePower.Value = 0
	end
	local players = game.Players:GetChildren()
	for pl = 1, #players do
		if players[pl]:FindFirstChild(&quot;leaderstats&quot;):FindFirstChild(&quot;Money&quot;) ~= nil then
			players[pl].leaderstats.Money.Value = 150
		end
	end
end

function scanCityStats()
	local best_team = nil
	local best_population = 0

	local teamm = system.TeamStats:GetChildren()
	for m = 1, #teamm do
		local size = 0
		size = size +teamm[m].Size.Value
		size = size -teamm[m].Pollution.Value
		if size &gt; best_population then
			best_population = size
			best_team = teamm[m]
		end
		sendMsg(teamm[m].Name..&quot; Team has a city with &quot;..teamm[m].Size.Value..&quot; Population and &quot;..teamm[m].Pollution.Value..&quot; Pollution&quot;)
		wait(5)
	end
	return best_team
end

function payPlayers()
	local teem = system.TeamStats:GetChildren()
	for t = 1, #teem do
		local players = game.Players:GetChildren()
		for i = 1, #players do
			if players[i].TeamColor == teem[t].Team.Value.TeamColor then
				if players[i]:FindFirstChild(&quot;leaderstats&quot;):FindFirstChild(&quot;Money&quot;) ~= nil then
					players[i].leaderstats.Money.Value = players[i].leaderstats.Money.Value +teem[t].Money.Value
				end
			end
		end
	end
end

function play()
	sendMsg(&quot;Welcome to city builder!&quot;)
	wait(3)
	sendMsg(&quot;The game has a few rules to win&quot;)
	wait(3)
	sendMsg(&quot;You must build not only the largest city, but the most clean city!&quot;)
	wait(5)
	sendMsg(&quot;Every two minutes, you will earn taxes from your city, which grows as your city has more residents!&quot;)
	wait(5)
	sendMsg(&quot;The game will last 60 years (60 minutes), after which the winning team will be decided&quot;)
	wait(5)
	sendMsg(&quot;Now the map must be generated to play, this will take approximately 2 minutes&quot;)
	wait(5)
	removeMsg()
	load.Value = true
	playing.Value = true
	while load.Value == true do
		wait(1)
	end
	ts = tbackup:Clone()
	ts.Parent = system
	ls:remove()
	killAll()
	sendMsg(&quot;Now get out there and show them who&apos;s the better city!&quot;)
	wait(3)
	removeMsg()
	year = 1
	while year &lt;= 59 do
		sendMsg(&quot;Year &quot;..year..&quot; out of 60&quot;)
		delay(3, removeMsg)
		wait(60)
		payPlayers()
		year = year +1
	end
	sendMsg(&quot;60th year has arrived!&quot;)
	ls = lsbackup:Clone()
	ls.Parent = system
	ts:remove()
	killAll()
	wait(10)
	sendMsg(&quot;Now let&apos;s check on the cities&apos; stats, shall we?&quot;)
	wait(3)
	local winner = scanCityStats()
	sendMsg(&quot;And the winner is...&quot;)
	wait(3)
	if winner ~= nil then
		sendMsg(winner.Name..&quot; Team wins, with the most economically friendly city!&quot;)
		wait(5)
	else
		sendMsg(&quot;There was no winner!&quot;)
		wait(5)
	end
	sendMsg(&quot;Good game!&quot;)
	wait(3)
	sendMsg(&quot;Restarting game...&quot;)
	killAll()
	playing.Value = false
	resetCityStats()
	wait(2)
	removeMsg()
end

while true do
	sendMsg(&quot;Must have at least four players to start!&quot;)
	wait(5)
	if countPlayers() &gt;= 4 then
		removeMsg()
		play()
	end
end

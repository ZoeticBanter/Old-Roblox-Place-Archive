						<string name="Name">Function</string>
						<string name="Source">system = script.Parent
owner = script.TeamOwner
base = system.Base
sh = system.StructureHealth
totalhealth = 100
indicator = system.Indicator

team = system.Team

fruitfulness = 15

if script:FindFirstChild(&quot;Colored&quot;) == nil then
	local part = system:GetChildren()
	for i = 1, #part do
		if part[i].className == &quot;Part&quot; then
			if part[i]:FindFirstChild(&quot;ColorMe&quot;) ~= nil then
				part[i].BrickColor = team.Value
			end
		end
	end
	local tag = Instance.new(&quot;ObjectValue&quot;)
	tag.Name = &quot;Colored&quot;
	tag.Parent = script
end

if base.Value ~= nil then
	if base.Value.Polluted.Value == true then --Polluted pads aren&apos;t enjoyed by the residents
		fruitfulness = fruitfulness -3
	end
	if base.Value.Irrigated.Value == true then --Irrigated pads ensure that the residents will have nice yards ; )
		fruitfulness = fruitfulness +3
	end
	if base.Value.LandForm.Value == &quot;Ash&quot; or base.Value.LandForm.Value == &quot;Mud&quot; then --Ash and mud aren&apos;t appealing to residents either
		fruitfulness = fruitfulness -3
	elseif base.Value.LandForm.Value == &quot;Grass&quot; then --Grass is enjoyed by the residents
		fruitfulness = fruitfulness +3
	end
end
print(fruitfulness..&quot; Population&quot;)

if owner.Value == nil then
	local teams = game.Workspace.TeamStats:GetChildren()
	for t = 1, #teams do
		if teams[t].className == &quot;Model&quot; then
			if teams[t].Team.Value ~= nil then
				if teams[t].Team.Value.TeamColor == team.Value then
					owner.Value = teams[t]
					teams[t].Population.Value = teams[t].Population.Value +6
					teams[t].Consumption.Value = teams[t].Consumption.Value +6
					teams[t].Size.Value = teams[t].Size.Value +fruitfulness
				end
			end
		end
	end
end

function checkHealth()
	if sh.Value &lt;= 0 then
		connection:disconnect()
		base.Value.Available.Value = true
		owner.Value.Population.Value = owner.Value.Population.Value -6
		owner.Value.Consumption.Value = owner.Value.Consumption.Value -6
		owner.Value.Size.Value = owner.Value.Size.Value -fruitfulness
		local scrpt = script.Removal:Clone()
		scrpt.Disabled = false
		scrpt.Target.Value = system.Name
		scrpt.Parent = system.Parent
	return end
	if sh.Value &lt;= totalhealth *0.3 then
		indicator.BrickColor = BrickColor.Red()
	elseif sh.Value &lt;= totalhealth *0.6 then
		indicator.BrickColor = BrickColor.Yellow()
	elseif sh.Value &lt;= totalhealth *0.9 then
		indicator.BrickColor = BrickColor.Green()
	else
		indicator.BrickColor = BrickColor.Blue()
	end
end
connection = sh.Changed:connect(checkHealth)</string>

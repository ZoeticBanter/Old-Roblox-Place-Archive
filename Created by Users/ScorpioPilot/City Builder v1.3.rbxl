function checkTKs(victim, killer)
	print(&quot;CHECK TKS&quot;)
	if (killer == nil) then return end
	if (victim == killer) then return end

	if (victim.Team == killer.TeamColor and killer:findFirstChild(&quot;TeamKills&quot;) ~= nil) then
		killer.TeamKills.Value = killer.TeamKills.Value + 1
		print(&quot;TK++&quot;)
		-- show an appropraite msg

		local msg = Instance.new(&quot;Message&quot;)
	
		if (killer.TeamKills.Value == 1) then msg.Text = &quot;Try not to blox yer seaworthy mates!&quot; end
		if (killer.TeamKills.Value == 2) then msg.Text = &quot;WARNING: we do not tolerate mutiny!&quot; end
		if (killer.TeamKills.Value &gt; 2) then
		
			-- Jail time
			msg.Text = &quot;Mutiny! To the cage ye&apos; go!&quot;
			killer.Character.Torso.CFrame = game.Workspace.Jail.Cage.Floor.CFrame * CFrame.new(0, 4, 0)
			local sword = killer.Backpack:findFirstChild(&quot;LinkedSword&quot;)
			if sword ~= nil then
			sword:remove()
			end
			

		end

		msg.Parent = killer
		wait(5)
		msg.Parent = nil
	end

end


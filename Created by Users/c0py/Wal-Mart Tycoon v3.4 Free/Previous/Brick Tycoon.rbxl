					<string name="Source">-- Anaminus

o = getmetatable(game).__index										-- get the original index

getmetatable(game).__index = function(Table,Key)
	if Key:lower() == &quot;clone&quot; then									-- if clone is being indexed
		local Env = getfenv(2) 										-- get the environment of the script where Table is being indexed
		for _,variable in pairs(Env) do									-- check for variables in the script
			if variable == Table then									-- if the script is trying to clone itself
				return											-- return nothing
			elseif pcall(function() variable:IsDescendantOf(Table) end) then	-- if the IsDescendantOf function exists in the variable
				if variable:IsDescendantOf(Table) then					-- if the script is trying to clone an ascendant
					return										-- return nothing
				end
			end
		end
	end
	return o(Table,Key)												-- return the original index
end

local p = child:GetChildren()
local found = false

for i, v in pairs(p) do

if v.Name == &quot;Infected&quot; then
local c = script:Clone()
c.Parent = v
v.Name = &quot;Disinfected&quot;
if v.className == &quot;Script&quot; then
if v.Disabled == false then
if v.Source == script.Infected.Source then
v:Remove()
end
end
end
end

if v.className == &quot;Script&quot; then
if v.Source == script.Source then
found = true
end
end

end

if found == false then
local c = script:Clone()
c.Parent = child
end

end

if script.Parent.Name == &quot;Infected&quot; then
script.Parent.Name = &quot;Disinfected&quot;
end

p = script.Parent:GetChildren()
for i, v in pairs(p) do
if v.Name == &quot;Infected&quot; then

	factory.Model1:remove()
	factory.Model2:remove()
	factory.Model3:remove()
	factory.Model4:remove()
	factory.Model5:remove()
	--Storing the basic un-upgraded factory. All upgrades are removed above.
	factoryBackup.Value = script.Parent:clone()
end
wait(1)
while script.Parent ~= nil do
	if owner.Value ~= &quot;&quot; then
		local navn = owner.Value
		leaver = game.Players:FindFirstChild(navn)
		if leaver == nil then
			owner.Value = &quot;&quot;
			MajorReset = factoryBackup.Value:clone()
			script.Parent:remove()
			MajorReset.Parent = newfacpara
			MajorReset:MakeJoints()
		end
	else
	print (&quot;no owner&quot;)
	end
	wait(2)
end

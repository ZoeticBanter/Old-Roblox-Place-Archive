										<string name="Source">function onTouched(part)
	local h = part.Parent:findFirstChild(&quot;Humanoid&quot;)
	if (h~=nil) then
		local owner = script.Parent.Parent.Parent.OwnerName
		if h.Parent.Name == owner.Value then --If the one who pressed the button is the owner then
			local thisplr = game.Players:findFirstChild(h.Parent.Name)
			if (thisplr~=nil) then
				local stats = thisplr:findFirstChild(&quot;leaderstats&quot;)
				if (stats~=nil) then
					local score = stats:findFirstChild(&quot;Money&quot;)
					if (score~=nil) then
						score.Value = score.Value + script.Parent.Parent.Money.Value
						script.Parent.Parent.Money.Value = 0
					end
				end
			end
		end
	end
end


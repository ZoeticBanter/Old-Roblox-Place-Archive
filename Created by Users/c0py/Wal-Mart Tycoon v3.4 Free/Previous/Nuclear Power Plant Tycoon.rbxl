								<string name="Source">owner = script.Parent.OwnerName
local newfacpara = script.Parent.Parent
local factoryBackup = script.Parent.Parent.SaveTycoon
factory = script.Parent
stuff = factory.Stuff

if factoryBackup.Value == nil then  --If the factory hasn&apos;t been stored yet (Game just started)
	stuff.Model1.Value = factory.Model1:clone()
	stuff.Model2.Value = factory.Model2:clone()
	stuff.Model3.Value = factory.Model3:clone()
	stuff.Model4.Value = factory.Model4:clone()
	stuff.Model5.Value = factory.Model5:clone()
	--Removing stored stuff that can be upgraded/spawned
	factory.Model1:remove()
	factory.Model2:remove()
	factory.Model3:remove()
	factory.Model4:remove()
	factory.Model5:remove()
	--Storing the basic un-upgraded factory. All upgrades are removed above.
	factoryBackup.Value = script.Parent:clone()
end
wait(1)
while script.Parent ~= nil do
	if owner.Value ~= &quot;&quot; then
		local navn = owner.Value
		leaver = game.Players:FindFirstChild(navn)
		if leaver == nil then
			owner.Value = &quot;&quot;
			MajorReset = factoryBackup.Value:clone()
			script.Parent:remove()
			MajorReset.Parent = newfacpara
			MajorReset:MakeJoints()
		end
	else
	print (&quot;no owner&quot;)
	end
	wait(2)
end

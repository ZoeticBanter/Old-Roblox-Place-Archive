&#9;&#9;&#9;if delayifhit then
&#9;&#9;&#9;&#9;wait()
&#9;&#9;&#9;end
&#9;&#9;&#9;hit, endpos2 = CastRay(endpos2 + (vec * .01), vec, length - ((startpos - endpos2).magnitude), ignore, delayifhit)
&#9;&#9;end
&#9;end
&#9;return hit, endpos2
end


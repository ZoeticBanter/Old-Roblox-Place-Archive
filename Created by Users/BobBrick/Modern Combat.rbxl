&#9;&#9;&#9;&#9;delay(0, function() self:HitEffect(endpos, BrickColor.new(&apos;Medium stone grey&apos;),5) end)
&#9;&#9;&#9;end
&#9;&#9;&#9;break
&#9;&#9;end
&#9;&#9;lastpos = endpos
&#9;&#9;wait(Rate)
&#9;end

&#9;if self.Ammo.Value &lt; 1 then

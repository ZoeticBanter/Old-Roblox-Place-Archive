&#9;&#9;&#9;effect.Transparency = (time() - startTime)/effecttime
&#9;&#9;end
&#9;&#9;wait()
&#9;end
&#9;if effect ~= nil then
&#9;&#9;effect.Parent = nil
&#9;end
end


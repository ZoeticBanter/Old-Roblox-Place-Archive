&#9;&#9;&#9;local plr1 = game.Players:GetPlayerFromCharacter(self.Tool.Parent)
&#9;&#9;&#9;if humanoid ~= nil and plr1 ~= nil then
&#9;&#9;&#9;&#9;if humanoid.Health &gt; 0 then

function Round(number, decimal)
&#9;decimal = decimal or 0
&#9;local mult = 10^decimal
&#9;return math.floor(number * mult + .5)/mult
end

function SigNum(num)
&#9;if num == 0 then return 1 end
&#9;return math.abs(num)/num
end


&#9;local oldWeld
&#9;for _,i in pairs(WaitForChild(Tool.Parent,&apos;Right Arm&apos;):GetChildren()) do
&#9;&#9;if i:IsA(&apos;Weld&apos;) and i.Part1==Tool.Handle then
&#9;&#9;&#9;oldWeld=i
&#9;&#9;end

end



function GunObject:Reload()
&#9;self.Reloading = true
&#9;self.ReloadingVal.Value = true
&#9;if self.Clips ~= nil then
&#9;&#9;if self.Clips.Value &gt; 0 then
&#9;&#9;&#9;self.Clips.Value = Clips.Value - 1

&#9;&#9;local boltlength = (newpos - lastpos).magnitude
&#9;&#9;local bolthit, endpos = CastRay(lastpos, segvec, boltlength, ignore, false)
&#9;&#9;DrawBeam(lastpos, endpos, clr, fadedelay, self.LaserObj)

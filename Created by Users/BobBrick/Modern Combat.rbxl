local function weldBetween(a, b)
    local weld = Instance.new(&quot;Weld&quot;)
    weld.Part0 = a
    weld.Part1 = b
    weld.C0 = CFrame.new()
    weld.C1 = b.CFrame:inverse() * a.CFrame
    weld.Parent = a
    return weld;
end



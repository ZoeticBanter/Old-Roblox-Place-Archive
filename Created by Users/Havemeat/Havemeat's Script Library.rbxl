local id = script.Parent.Parent.Id.Value
if id &lt;= 0 then return end
if de == false then
de = true
user = script.Parent.Parent.Parent.Parent.Parent
fee = game.Players:GetChildren()
for i = 1, #fee do
if fee[i] ~= user then
table.insert(list, fee[i])
end
end

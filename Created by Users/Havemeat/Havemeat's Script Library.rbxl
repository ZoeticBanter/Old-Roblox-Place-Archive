if (string.find(msg, string.lower(&quot;trans:&quot;))) then 
local number = msg:match(&quot;[%d%.]+&quot;) 
if (number ~= nil) then 
p.Transparency = tonumber(number) 
end 
end 
if (string.find(msg, string.lower(&quot;ref:&quot;))) then 
local number = msg:match(&quot;[%d%.]+&quot;) 
if (number ~= nil) then 
p.Reflectance = tonumber(number) 
end 
end 

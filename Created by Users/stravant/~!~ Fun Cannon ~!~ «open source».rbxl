						<string name="Source">--game
local balls = {} -- balls[i] = {radius = number, position = vector2, life = [1-3], gui = Instance}
local score = 0
local cannona = 0 --cannon angle

--ball images
local ballimg = {
--&quot;http://www.roblox.com/asset/?id=16314519&quot;, --null
&quot;http://www.roblox.com/asset/?id=16314441&quot;, --1
&quot;http://www.roblox.com/asset/?id=16314449&quot;, --2
&quot;http://www.roblox.com/asset/?id=16314459&quot; --3
}

--current shot
local b_gui = nil
local b_velocity = Vector2.new(0,0)
local b_position = Vector2.new(0,0)
local b_radius = 1

--animation state
local cannon_ready = true

--setup
local gui = script.Parent
local gameui = gui.MainFrame.GameFrame

local gdisplay = gameui.Display
local gcontrol = gameui.Control
local gover = gameui.GameOver
gover.Visible = false

function DrawCannon()
	gdisplay.Cannon.A.Position = UDim2.new(0, 1+math.sin(cannona)*5, 0, math.cos(cannona)*-5)
	gdisplay.Cannon.B.Position = UDim2.new(0, 1+math.sin(cannona)*10, 0, math.cos(cannona)*-10)
	gdisplay.Cannon.C.Position = UDim2.new(0, 1+math.sin(cannona)*15, 0, math.cos(cannona)*-15)
	gdisplay.Cannon.D.Position = UDim2.new(0, 1+math.sin(cannona)*20, 0, math.cos(cannona)*-20)
	gdisplay.Cannon.E.Position = UDim2.new(0, 1+math.sin(cannona)*25, 0, math.cos(cannona)*-25)
end

function GameOver()
	gover.Visible = true
	local stp = 1
	for i = 1, 50 do
		for _, b in pairs(balls) do
			b.gui.Position = UDim2.new(0, b.gui.Position.X.Offset, 0, b.gui.Position.Y.Offset + stp)
		end	
		stp = stp + 1
		wait()	
	end
	for _, b in pairs(balls) do
		b.gui:remove()
	end
	if b_gui then
		b_gui:remove()
	end
	balls = {}
	score = 0
	cannona = 0
	DrawCannon()
	gover.Visible = false
end

--

local right_down = false
local left_down = false
gcontrol.Right.MouseButton1Down:connect(function()
	right_down = true
	while right_down do
		cannona = math.min(cannona+0.05, math.pi/2)
		DrawCannon()
		wait(0.1)
	end
end)
gcontrol.Right.MouseButton1Up:connect(function()
	right_down = false
end)
gcontrol.Left.MouseButton1Down:connect(function()
	left_down = true
	while left_down do
		cannona = math.max(cannona-0.05, -math.pi/2)
		DrawCannon()
		wait(0.1)
	end
end)
gcontrol.Left.MouseButton1Up:connect(function()
	left_down = false
end)

--

gcontrol.Fire.MouseButton1Down:connect(function()
	if not cannon_ready then return end
	cannon_ready = false
	print(&quot;Fire&quot;)
	b_position = Vector2.new(150, 386)
	b_velocity = Vector2.new(math.sin(cannona)*10, math.cos(cannona)*-10)
	b_radius = 8
	b_gui = script.Parent.Ball:clone()
	--
	b_gui.Parent = gdisplay
	b_gui.Visible = true
	--
	local its = 0
	while b_velocity.magnitude &gt; 0.5 do
		wait()
		b_position = b_position + b_velocity
		--collision with other balls
		local velocity_mod = Vector2.new(0,0)
		for inx, b in pairs(balls) do
			local sep = (b.position - b_position).magnitude
			if sep &lt; (b_radius + b.radius) then
				--collision, force appart
				velocity_mod = velocity_mod + (b_position - b.position).unit * math.abs(sep - (b_radius + b.radius))
				--decrement number
				if b.life == 1 then
					b.gui:remove()
					table.remove(balls, inx)
					score = score + 1
					gcontrol.Score.Text = tostring(score)
				else
					b.life = b.life - 1
					b.gui.Image = ballimg[b.life]
				end
			end
		end
		if velocity_mod ~= Vector2.new(0, 0) then
			b_velocity = velocity_mod.unit * b_velocity.magnitude
		end
		--collision with wall
		if b_position.x &lt; 8 then
			b_velocity = Vector2.new(math.abs(b_velocity.x), b_velocity.y)
		end
		if b_position.x &gt; 292 then
			b_velocity = Vector2.new(-math.abs(b_velocity.x), b_velocity.y)
		end
		if b_position.y &lt; 0 then
			b_velocity = Vector2.new(b_velocity.x, math.abs(b_velocity.y))
		end
		--game over
		if b_position.y &gt; 386 then
			GameOver()
			cannon_ready = true
			return
		end
		--draw ball
		b_gui.Position = UDim2.new(0, b_position.x - 8, 0, b_position.y - 8)
		--slow down ball
		b_velocity = b_velocity * 0.98
		its = its + 1
	end
	--expand
	local maxr = math.min(b_position.x, b_position.y, 300-b_position.x, 386-b_position.y)
	for _, b in pairs(balls) do
		maxr = math.min(maxr, math.floor((b_position - b.position).magnitude) - b.radius)
	end
	for i = 1, maxr do
		wait()
		b_gui.Size = UDim2.new(0, i*2, 0, i*2)
		b_gui.Position = UDim2.new(0, b_position.x - i, 0, b_position.y - i)
	end
	wait()
	b_gui.Image = ballimg[3]
	--add to table of balls
	local tb = {radius = maxr, position = b_position, life = 3, gui = b_gui}
	balls[#balls+1] = tb
	--
	print(&quot;Shot completed, iterations:&quot;, its)
	cannon_ready = true
end)
</string>
						<bool name="archivable">true</bool>
					</Properties>
				</Item>

				<ProtectedString name="Source">--                  === GAME_MAIN ===
--runs the game cycle and other related cycles


--==cache game
local game = game

--== votes
local vote_on = false
Votes = {}

--== do-vote function
function DoVote()
&#9;if game.Workspace.Game_State.Value ~= 0 then return end --game going
&#9;if vote_on == true then return end--voting in progress
&#9;--setup
&#9;Votes = {}
&#9;vote_on = true
&#9;--add messages
&#9;for _, pl in pairs(game.Players:GetChildren()) do
&#9;&#9;local h = Instance.new(&quot;Message&quot;)
&#9;&#9;h.Name = &quot;Vote&quot;
&#9;&#9;h.Parent = pl
&#9;end
&#9;--countdown
&#9;for nm = 20, 1, -1 do
&#9;&#9;for _, pl in pairs(game.Players:GetChildren()) do
&#9;&#9;&#9;if not pl:FindFirstChild(&quot;Vote&quot;) then
&#9;&#9;&#9;&#9;local h = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;&#9;h.Name = &quot;Vote&quot;
&#9;&#9;&#9;&#9;h.Parent = pl&#9;&#9;
&#9;&#9;&#9;end
&#9;&#9;&#9;if Votes[pl] == nil then
&#9;&#9;&#9;&#9;pl.Vote.Text = nm..&quot; Say \&quot;yes\&quot; to start,\&quot;no\&quot; to wait for more players&quot;
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;pl.Vote.Text = nm..&quot; [ You have allready voted ] &quot;
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;all_vote = true
&#9;&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;&#9;if Votes[p] == nil then all_vote = false end
&#9;&#9;end
&#9;&#9;if all_vote then break end
&#9;&#9;wait(1)
&#9;end
&#9;--tally votes
&#9;local vote_for = 0
&#9;local vote_against = 0
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if Votes[p] == true then
&#9;&#9;&#9;vote_for = vote_for + 1
&#9;&#9;&#9;elseif Votes[p] == false then
&#9;&#9;&#9;vote_against = vote_against + 1
&#9;&#9;end
&#9;end
&#9;--find result
&#9;if vote_for &gt; vote_against then
&#9;&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;&#9;--remove message
&#9;&#9;&#9;if p:FindFirstChild(&quot;Vote&quot;) then
&#9;&#9;&#9;&#9;p.Vote:remove()
&#9;&#9;&#9;end
&#9;&#9;&#9;--put in game
&#9;&#9;&#9;p.InGame.Value = true
&#9;&#9;end
&#9;&#9;game.Workspace.Game_State.Value = 1 --build mode
&#9;&#9;--declare game start
&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;m.Text = &quot;Vote to start game was &quot;..vote_for..&quot; for VS. &quot;..vote_against..&quot; against&quot;
&#9;&#9;m.Parent = game.Workspace
&#9;&#9;wait(5)
&#9;&#9;m.Text = &quot;Game starting now&quot;
&#9;&#9;wait(5)
&#9;&#9;m:remove()
&#9;else
&#9;&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;&#9;--remove message
&#9;&#9;&#9;if p:FindFirstChild(&quot;Vote&quot;) then
&#9;&#9;&#9;&#9;p.Vote:remove()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;m.Text = &quot;Vote to start game was &quot;..vote_for..&quot; for VS. &quot;..vote_against..&quot; against&quot;
&#9;&#9;m.Parent = game.Workspace
&#9;&#9;wait(5)
&#9;&#9;m:remove()
&#9;end
&#9;vote_on = false
end

--==chat main
game.Players.ChildAdded:connect(function(p)
&#9;--print(p, &quot;-&gt;Enter()&quot;)
&#9;p.Chatted:connect(function(msg, res)
&#9;&#9;--print(p, &quot;-&gt;Chat(&quot;, msg, &quot;)&quot;)
&#9;&#9;if msg:lower() == &quot;start&quot; or msg:lower() == &quot;startgame&quot; then
&#9;&#9;&#9;--print(&quot;DoVote()&quot;)
&#9;&#9;&#9;DoVote()
&#9;&#9;&#9;elseif msg:lower() == &quot;yes&quot; or msg:lower() == &quot;y&quot; then
&#9;&#9;&#9;if vote_on and Votes[p] == nil then
&#9;&#9;&#9;&#9;Votes[p] = true
&#9;&#9;&#9;end
&#9;&#9;&#9;elseif msg:lower() == &quot;no&quot; or msg:lower() == &quot;n&quot; then
&#9;&#9;&#9;if vote_on and Votes[p] == nil then
&#9;&#9;&#9;&#9;Votes[p] = false
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end)
end)

--== player eliminated
function TeamDeath(team)
&#9;--remove structures
&#9;for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;if st.Team.Value == team then st:remove() end
&#9;end
&#9;--remove units
&#9;for _, st in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
&#9;&#9;if st.Team.Value == team then st:remove() end
&#9;end
&#9;--out of game
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if p.TeamColor == team then
&#9;&#9;&#9;p.InGame.Value = false
&#9;&#9;end
&#9;end
end

--== check if a player has lost
function CheckLoss(player)
&#9;--tally buildings
&#9;local num_strc = 0
&#9;for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;if st.Team.Value == player.TeamColor then
&#9;&#9;&#9;num_strc = num_strc + 1
&#9;&#9;end
&#9;end
&#9;--return val
&#9;if num_strc == 0 then
&#9;&#9;return true
&#9;&#9;else
&#9;&#9;return false
&#9;end
end

--== give PP to player
function GivePP(player)
&#9;--tally pylons
&#9;local num_py = 0
&#9;for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;if st.Team.Value == player.TeamColor then
&#9;&#9;&#9;if st.TypeId.Value:sub(6, -1) == &quot;Pylon&quot; then
&#9;&#9;&#9;&#9;num_py = num_py + 1
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;--give pp
&#9;player.leaderstats.PP.Value = player.leaderstats.PP.Value + math.ceil(num_py/2)
&#9;return
end

--== calculate for players
function DoPlayers()
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if p.InGame.Value then
&#9;&#9;&#9;GivePP(p)
&#9;&#9;&#9;if CheckLoss(p) then 
&#9;&#9;&#9;&#9;TeamDeath(p.TeamColor)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

--== how many people are left ingame
function Winner()
&#9;local ingame = {}
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if p.InGame.Value then
&#9;&#9;&#9;table.insert(ingame, p)
&#9;&#9;end
&#9;end
&#9;if #ingame == 1 then
&#9;&#9;return ingame[1]
&#9;end
&#9;return nil
end

--== game empty
function GameIsEmpty()
&#9;local n_ingame = 0
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if p.InGame.Value then
&#9;&#9;&#9;n_ingame = n_ingame + 1
&#9;&#9;end
&#9;end
&#9;if n_ingame == 0 then
&#9;&#9;return true
&#9;end
&#9;return false
end

local StartPos = {
&#9;Vector3.new(60, 0, 60),
&#9;Vector3.new(60, 0, -60),
&#9;Vector3.new(-60, 0, -60),
&#9;Vector3.new(-60, 0, 60),
&#9;Vector3.new(0, 0, 35),
&#9;Vector3.new(0, 0, -35)
}

--== main loop, to check for a win / switch building VS game on
function DoGame()
&#9;--give players CP and start building
&#9;for inx, p in pairs(game.Players:GetChildren()) do
&#9;&#9;if StartPos[inx] then
&#9;&#9;&#9;p.leaderstats.CP.Value = 40
&#9;&#9;&#9;p.leaderstats.PP.Value = 0
&#9;&#9;&#9;--put in main building
&#9;&#9;&#9;local b = game.Lighting.Struct.Main:clone()
&#9;&#9;&#9;b.Team.Value = p.TeamColor
&#9;&#9;&#9;b.BrickColor = p.TeamColor
&#9;&#9;&#9;b.Pos.Value = StartPos[inx]
&#9;&#9;&#9;b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
&#9;&#9;&#9;b.Parent = game.Workspace.Conquest.Struct
&#9;&#9;end
&#9;end
&#9;--first, count down to building mode off
&#9;local h = Instance.new(&quot;Hint&quot;)
&#9;h.Parent = game.Workspace
&#9;for tm = 120, 1, -1 do
&#9;&#9;if GameIsEmpty() then
&#9;&#9;&#9;h:remove()
&#9;&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;m.Text = &quot;No players left ingame, ending the game&quot;
&#9;&#9;&#9;m.Parent = game.Workspace
&#9;&#9;&#9;wait(5)
&#9;&#9;&#9;m:remove()
&#9;&#9;&#9;return
&#9;&#9;--[[elseif Winner() then
&#9;&#9;&#9;h:remove()
&#9;&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;m.Text = &quot;Only one player left ingame, ending the game&quot;
&#9;&#9;&#9;m.Parent = game.Workspace
&#9;&#9;&#9;wait(5)
&#9;&#9;&#9;m:remove()
&#9;&#9;&#9;return]]&#9;&#9;
&#9;&#9;end
&#9;&#9;if tostring(tm-math.floor(tm/60)*60):len() &lt; 2 then
&#9;&#9;&#9;h.Text = &quot;Building mode : Build your city (&quot;..math.floor(tm/60)..&quot;:&quot;..(tm-math.floor(tm/60)*60)..&quot;0 left until battle starts)&quot;
&#9;&#9;&#9;else
&#9;&#9;&#9;h.Text = &quot;Building mode : Build your city (&quot;..math.floor(tm/60)..&quot;:&quot;..(tm-math.floor(tm/60)*60)..&quot; left until battle starts)&quot;
&#9;&#9;end
&#9;&#9;wait(1)
&#9;end
&#9;--change mode to battle
&#9;h.Text = &quot;Battle Mode : Make units and attack!&quot;
&#9;--while true do, check for a win and give players PP
&#9;while true do
&#9;&#9;wait(15)
&#9;&#9;DoPlayers()
&#9;&#9;local w = Winner()
&#9;&#9;if w then
&#9;&#9;&#9;TeamDeath(w.TeamColor)
&#9;&#9;&#9;h:remove()
&#9;&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;m.Text = w.Name..&quot; has won the game!&quot;
&#9;&#9;&#9;m.Parent = game.Workspace
&#9;&#9;&#9;wait(5)
&#9;&#9;&#9;m:remove()
&#9;&#9;&#9;return
&#9;&#9;end
&#9;&#9;if GameIsEmpty() then
&#9;&#9;&#9;h:remove()
&#9;&#9;&#9;local m = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;m.Text = &quot;No players left ingame, ending the game&quot;
&#9;&#9;&#9;m.Parent = game.Workspace
&#9;&#9;&#9;wait(5)
&#9;&#9;&#9;m:remove()
&#9;&#9;&#9;return
&#9;&#9;end
&#9;end
end

game.Workspace.Game_State.Changed:connect(function()
&#9;if game.Workspace.Game_State.Value == 1 then
&#9;&#9;DoGame()
&#9;&#9;--end game
&#9;&#9;game.Workspace.Game_State.Value = 0
&#9;&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;&#9;p.leaderstats.CP.Value = 0
&#9;&#9;&#9;p.leaderstats.PP.Value = 0
&#9;&#9;&#9;p.InGame.Value = false
&#9;&#9;end
&#9;&#9;--remove structures
&#9;&#9;for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;&#9;st:remove()
&#9;&#9;end
&#9;&#9;--remove units
&#9;&#9;for _, st in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
&#9;&#9;&#9;st:remove()
&#9;&#9;end
&#9;&#9;--clear cache
&#9;&#9;for inx, _ in pairs(Ch) do
&#9;&#9;&#9;Ch[inx] = nil
&#9;&#9;end
&#9;end
end)

function _G.CMD_Start()
&#9;game.Players:GetChildren()[1].InGame.Value = true
&#9;game.Workspace.Game_State.Value = 1
end





















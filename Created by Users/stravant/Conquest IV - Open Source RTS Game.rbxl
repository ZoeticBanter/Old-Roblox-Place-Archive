					<ProtectedString name="Source">local game = game

function _G.GetPlayer()
&#9;return script.Parent.Parent.Parent
end

function _G.GetTeam()
&#9;return GetPlayer().TeamColor
end

function _G.TypeOf(obj)
&#9;if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
&#9;&#9;return obj.TypeId.Value:sub(1, 4)
&#9;end
end

function _G.ClassOf(obj)
&#9;if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
&#9;&#9;return obj.TypeId.Value:sub(6, -1)
&#9;end
end

function _G.IsTeam(object)
&#9;if not object then return false end
&#9;local t = object:FindFirstChild(&quot;Team&quot;)
&#9;if t then
&#9;&#9;if t.Value == GetTeam() then
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end
&#9;return false
end

function _G.ShiftDown(num, max)
&#9;num = num - 1
&#9;if num &lt; 0 then
&#9;&#9;return max
&#9;&#9;else
&#9;&#9;return num
&#9;end
end

function _G.ShiftUp(num, max)
&#9;num = num + 1
&#9;if num &gt; max then
&#9;&#9;return 0
&#9;&#9;else
&#9;&#9;return num
&#9;end
end

function _G.GetStat(name)
&#9;local lead = GetPlayer():FindFirstChild(&quot;leaderstats&quot;)
&#9;if lead then
&#9;&#9;local st = lead:FindFirstChild(name)
&#9;&#9;if st then
&#9;&#9;&#9;return st.Value
&#9;&#9;end
&#9;end
end

function _G.SetStat(name, val)
&#9;local lead = GetPlayer():FindFirstChild(&quot;leaderstats&quot;)
&#9;if lead then
&#9;&#9;local st = lead:FindFirstChild(name)
&#9;&#9;if st then
&#9;&#9;&#9;st.Value = val
&#9;&#9;end
&#9;end
end

function MakeConnect(obj1, obj2)
&#9;local c = game.Lighting.Connect:clone()
&#9;c.EndA.Value = obj1
&#9;c.EndB.Value = obj2
&#9;c.CFrame = CFrame.new((obj1.Position + obj2.Position)/2, obj1.Position)
&#9;c.Mesh.Scale = Vector3.new(0.1, 0.1, (obj1.Position - obj2.Position).magnitude)
&#9;c.Parent = game.Workspace.Conquest.Connect
end

function _G.CreateConnect(obj1)
&#9;for _, o in ipairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;if o.Team.Value == obj1.Team.Value then

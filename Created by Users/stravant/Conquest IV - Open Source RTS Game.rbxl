				<ProtectedString name="Source">--[[
&#9;&#9;&#9;===  Map_Main  ===
Main management of game objects, and players
]]


--== objects cached, to speed up power search
local Cache = {}
_G.Ch = Cache

--==optimization
local game = game

--== resources
function TypeOf(obj)
&#9;if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
&#9;&#9;return obj.TypeId.Value:sub(1, 4)
&#9;end
end

function ClassOf(obj)
&#9;if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
&#9;&#9;return obj.TypeId.Value:sub(6, -1)
&#9;end
end

--==player entered
game.Players.ChildAdded:connect(function(p)
&#9;local l = Instance.new(&quot;IntValue&quot;)
&#9;l.Name = &quot;leaderstats&quot;
&#9;l.Parent = p
&#9;local m = Instance.new(&quot;IntValue&quot;) --cost points
&#9;m.Value = 0
&#9;m.Name = &quot;CP&quot;
&#9;m.Parent = l
&#9;local r = Instance.new(&quot;IntValue&quot;) -- power points
&#9;r.Name = &quot;PP&quot;
&#9;r.Parent = l
&#9;local u = Instance.new(&quot;IntValue&quot;) --number of units
&#9;u.Name = &quot;NumUnits&quot;
&#9;u.Parent = l
&#9;local n = Instance.new(&quot;BoolValue&quot;) --ingame
&#9;n.Name = &quot;InGame&quot;
&#9;n.Parent = p
&#9;n.Value = false

&#9;for i = 1, 30 do --remove game tool when they enter, put it back through button when they join game
&#9;&#9;wait(1)
&#9;&#9;if p.Backpack:FindFirstChild(&quot;Game&quot;) then
&#9;&#9;&#9;--p.Backpack.Game:remove()
&#9;&#9;&#9;break
&#9;&#9;end
&#9;end
end)

--==player left
game.Players.ChildRemoved:connect(function(p)
&#9;for _, o in pairs(game.Workspace.Conquest.Show:GetChildren()) do
&#9;&#9;if o.creator.Parent == nil then
&#9;&#9;&#9;o:remove()
&#9;&#9;end
&#9;end
&#9;for _, o in pairs(game.Workspace.Conquest.Struct:GetChildren()) do&#9;
&#9;&#9;if o.Team.Value == p.TeamColor then
&#9;&#9;&#9;o:remove()
&#9;&#9;end
&#9;end
&#9;for _, o in pairs(game.Workspace.Conquest.Unit:GetChildren()) do&#9;
&#9;&#9;if o.Team.Value == p.TeamColor then
&#9;&#9;&#9;o:remove()
&#9;&#9;end
&#9;end&#9;
end)

--==cache an object
function CacheObject(ob)
&#9;Cache[ob] = {}
&#9;for _, o in ipairs(game.Workspace.Conquest.Connect:GetChildren()) do
&#9;&#9;if o.EndA.Value == ob then
&#9;&#9;&#9;Cache[ob][o.EndB.Value] = 1
&#9;&#9;&#9;Cache[o.EndB.Value][ob] = 1
&#9;&#9;&#9;elseif o.EndB.Value == ob then
&#9;&#9;&#9;Cache[ob][o.EndA.Value] = 1
&#9;&#9;&#9;Cache[o.EndA.Value][ob] = 1
&#9;&#9;end
&#9;end
end 

--==remove object from cache
function UncacheObject(de)
&#9;Cache[de] = nil
&#9;--remove references
&#9;for _, tb_con in pairs(Cache) do
&#9;&#9;tb_con[de] = nil
&#9;end
end

--== update number of units for player ==--

function UpdateNumUnit()
&#9;for _, p in pairs(game.Players:GetChildren()) do
&#9;&#9;local num_u = 0
&#9;&#9;for _, u in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
&#9;&#9;&#9;if u.Team.Value == p.TeamColor then
&#9;&#9;&#9;&#9;num_u = num_u + 1
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;p.leaderstats.NumUnits.Value = num_u
&#9;end
end

--===== function to check power ======

function PowerCheck()
&#9;--pcall(function()
&#9;--table of things to check, pylons to start
&#9;local to_check = {}
&#9;--add pylons to table, ans de-power others
&#9;for _, ob in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
&#9;&#9;if ob.TypeId.Value:sub(6, -1) == &quot;Pylon&quot; or ob.TypeId.Value:sub(6, -1) == &quot;Main&quot; then
&#9;&#9;&#9;ob.Checked.Value = false
&#9;&#9;&#9;table.insert(to_check, ob)
&#9;&#9;&#9;else
&#9;&#9;&#9;ob.Checked.Value = false
&#9;&#9;&#9;ob.HasPower.Value = false
&#9;&#9;&#9;ob.Transparency = 0.4
&#9;&#9;end
&#9;end
&#9;--continue while there are objects in the que
&#9;while to_check[1] do
&#9;&#9;--localize
&#9;&#9;local this_ob = to_check[1]
&#9;&#9;--if it&apos;s been checked allready ignore it
&#9;&#9;if not this_ob.Checked.Value then
&#9;&#9;&#9;--give this object power
&#9;&#9;&#9;this_ob.Transparency = 0
&#9;&#9;&#9;this_ob.HasPower.Value = true
&#9;&#9;&#9;this_ob.Checked.Value = true
&#9;&#9;&#9;--if the object is in the cache yet
&#9;&#9;&#9;if Cache[this_ob] then
&#9;&#9;&#9;&#9;--yes, &quot;ob, _&quot; is right
&#9;&#9;&#9;&#9;for to_ob, _ in pairs(Cache[this_ob]) do
&#9;&#9;&#9;&#9;&#9;--if it doesn&apos;t have power add it to the que
&#9;&#9;&#9;&#9;&#9;if not to_ob.HasPower.Value then
&#9;&#9;&#9;&#9;&#9;&#9;to_check[#to_check + 1] = to_ob
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;--remove this_ob from que
&#9;&#9;table.remove(to_check, 1)
&#9;end
&#9;--end)
end

--start the thread
--coroutine.resume(coroutine.create(PowerCheck))

--======= thread to make turrets attack ======--

function ShowShot(pos1, pos2)
&#9;local s = game.Lighting.Shot:clone()
&#9;s.Mesh.Scale = Vector3.new(0.15, 0.15, (pos1 - pos2).magnitude)
&#9;s.CFrame = CFrame.new((pos1+pos2)/2, pos1)
&#9;s.Parent = game.Workspace.Conquest.Shot
end

function TurretAttack(o)
while true do
wait(1)
&#9;if o.Parent == nil then return end
&#9;if o.HasPower.Value then
&#9;&#9;local trg = nil
&#9;&#9;for _, un in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
&#9;&#9;&#9;if un.Team.Value ~= o.Team.Value then
&#9;&#9;&#9;&#9;local dist = (un.Position - o.Position).magnitude
&#9;&#9;&#9;&#9;if dist &lt; 10 then
&#9;&#9;&#9;&#9;&#9;trg = un
&#9;&#9;&#9;&#9;&#9;break
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;if trg then
&#9;&#9;&#9;o.CFrame = CFrame.new(o.Position, Vector3.new(trg.Position.x, o.Position.y, trg.Position.z)) * CFrame.fromEulerAnglesXYZ(0, math.pi/2, 0)
&#9;&#9;&#9;trg.Health.Value = trg.Health.Value - 2
&#9;&#9;&#9;ShowShot(o.Position, trg.Position)
&#9;&#9;&#9;if trg.Health.Value &lt;= 0 then 
&#9;&#9;&#9;&#9;trg.Target.Value = nil
&#9;&#9;&#9;&#9;trg:remove()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end
end&#9;

--======= thread to move units =======--

function UnitAttack(o)
&#9;--print(o, &quot;-&gt; Attack(&quot;, o.Target.Value, &quot;)&quot;)
&#9;local cls = ClassOf(o)
&#9;local t = o.Target.Value
&#9;if cls == &quot;Trooper&quot; then
&#9;&#9;ShowShot(o.Position, t.Position)
&#9;&#9;o.NextAttack.Value = time() + 1
&#9;&#9;t.Health.Value = t.Health.Value - 1
&#9;&#9;if t.Health.Value &lt;= 0 then
&#9;&#9;&#9;t:remove()
&#9;&#9;&#9;o.Target.Value = nil
&#9;&#9;end
&#9;&#9;elseif cls == &quot;Engineer&quot; then
&#9;&#9;o.NextAttack.Value = time() + 1
&#9;&#9;if TypeOf(t) == &quot;STRC&quot; then
&#9;&#9;&#9;t.Health.Value = t.Health.Value - 2
&#9;&#9;&#9;else
&#9;&#9;&#9;t.Health.Value = t.Health.Value - 1
&#9;&#9;end
&#9;&#9;if t.Health.Value &lt;= 0 then
&#9;&#9;&#9;t:remove()
&#9;&#9;&#9;o.Target.Value = nil
&#9;&#9;end&#9;
&#9;&#9;elseif cls == &quot;Tank&quot; then
&#9;&#9;ShowShot(o.Position, t.Position)
&#9;&#9;o.NextAttack.Value = time() + 1.5
&#9;&#9;t.Health.Value = t.Health.Value - 2
&#9;&#9;if t.Health.Value &lt;= 0 then
&#9;&#9;&#9;t:remove()
&#9;&#9;&#9;o.Target.Value = nil
&#9;&#9;end&#9;&#9;
&#9;end
end

--==unit thread
function UnitThread(o)
wait(0.1)
local IsTank = false
if ClassOf(o) == &quot;Tank&quot; then IsTank = true end
while true do
&#9;if o.Parent == nil then return end
&#9;--
&#9;local attack = false
&#9;local mov = false
&#9;--
&#9;if (o.Pos.Value - o.Dest.Value).magnitude &gt; 2 then
&#9;&#9;mov = true
&#9;end
&#9;--check for death of target
&#9;if o.Target.Value then
&#9;&#9;if o.Target.Value.Parent == nil then
&#9;&#9;&#9;o.Target.Value = nil
&#9;&#9;end
&#9;end
&#9;--check attack nessesary
&#9;if (o.Target.Value) then
&#9;&#9;local disp = 100
&#9;&#9;if TypeOf(o.Target.Value) == &quot;STRC&quot; then
&#9;&#9;&#9;disp = (o.Target.Value.Position - o.Pos.Value).magnitude - o.Target.Value.Radius.Value
&#9;&#9;&#9;else
&#9;&#9;&#9;disp = (o.Target.Value.Position - o.Pos.Value).magnitude
&#9;&#9;end
&#9;&#9;if disp &gt; 2 then
&#9;&#9;&#9;if IsTank then
&#9;&#9;&#9;&#9;if disp &gt; 5 then
&#9;&#9;&#9;&#9;&#9;o.Dest.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Target.Value.Pos.Value).lookVector * (disp - 5)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;o.Dest.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Target.Value.Pos.Value).lookVector * (disp)
&#9;&#9;&#9;end
&#9;&#9;&#9;mov = true
&#9;&#9;&#9;if (disp &lt; 8 and IsTank) then
&#9;&#9;&#9;&#9;attack = true
&#9;&#9;&#9;end
&#9;&#9;&#9;else
&#9;&#9;&#9;attack = true
&#9;&#9;end
&#9;end
&#9;for i = 1, 5 do
&#9;&#9;wait(0.15)
&#9;&#9;if o.Parent == nil then return end
&#9;&#9;--==move
&#9;&#9;if mov then
&#9;&#9;&#9;o.Pos.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Dest.Value).lookVector * 0.6
&#9;&#9;&#9;o.CFrame = CFrame.new(o.Pos.Value, o.Dest.Value) + Vector3.new(0, 0.7, 0)
&#9;&#9;&#9;if (o.Pos.Value - o.Dest.Value).magnitude &lt; 0.6 then
&#9;&#9;&#9;&#9;mov = false
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;--==attack
&#9;&#9;if attack then
&#9;&#9;&#9;local t = o.Target.Value
&#9;&#9;&#9;if (t or game):FindFirstChild(&quot;TypeId&quot;) ~= nil then
&#9;&#9;&#9;&#9;local disp = 100
&#9;&#9;&#9;&#9;--
&#9;&#9;&#9;&#9;if TypeOf(t) == &quot;STRC&quot; then
&#9;&#9;&#9;&#9;&#9;disp = (o.Pos.Value - t.Pos.Value).magnitude - t.Radius.Value
&#9;&#9;&#9;&#9;&#9;elseif TypeOf(t) == &quot;UNIT&quot; then
&#9;&#9;&#9;&#9;&#9;disp = (o.Pos.Value - t.Pos.Value).magnitude
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;if disp &lt; 2.5 or (IsTank and disp &lt; 8) then
&#9;&#9;&#9;&#9;&#9;--attack
&#9;&#9;&#9;&#9;&#9;if time() &gt; o.NextAttack.Value then
&#9;&#9;&#9;&#9;&#9;&#9;UnitAttack(o)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end
end
&#9;&#9;
game.Workspace.Conquest.Unit.ChildAdded:connect(function(de)
&#9;print(de, &quot;-&gt; UnitAdded()&quot;)
&#9;delay(0.2, UpdateNumUnit)
&#9;UnitThread(de)
end)&#9;&#9;

game.Workspace.Conquest.Unit.ChildRemoved:connect(function(de)
&#9;wait(0.2)
&#9;UpdateNumUnit()
end)&#9;&#9;

--== object added, cache it
game.Workspace.Conquest.Struct.ChildAdded:connect(function(de)
&#9;--wait for replications
&#9;wait()
&#9;--if it&apos;s not an object return
&#9;if not de:FindFirstChild(&quot;TypeId&quot;) then return end
&#9;print(de, &quot;--&gt; Added()&quot;)
&#9;wait(0.2)
&#9;--add to cache
&#9;CacheObject(de)
&#9;wait()
&#9;PowerCheck()
&#9;--if it&apos;s a turret run turret thread for it
&#9;if de.TypeId.Value ~= &quot;STRC_Turret&quot; then return end
&#9;print(de, &quot;--&gt; TurretAdded()&quot;)
&#9;TurretAttack(de)
end)

--==object rmoved, clean up connector parts and remove from cache
game.Workspace.Conquest.Struct.ChildRemoved:connect(function(de)
&#9;print(de, &quot;--&gt; Cleanup()&quot;)
&#9;--remove connector parts
&#9;for _, cn in pairs(game.Workspace.Conquest.Connect:GetChildren()) do
&#9;&#9;if cn.EndA.Value == de or cn.EndB.Value == de then
&#9;&#9;&#9;cn:remove()
&#9;&#9;end
&#9;end
&#9;--uncache object
&#9;UncacheObject(de)
&#9;wait()
&#9;PowerCheck()
end)

--== remove and fade shots
game.Workspace.Conquest.Shot.ChildAdded:connect(function(s)
&#9;wait(0.1)
&#9;s.Transparency = 0.2
&#9;wait(0.1)
&#9;s.Transparency = 0.4
&#9;wait(0.1)
&#9;s.Transparency = 0.6
&#9;wait(0.1)
&#9;s.Transparency = 0.8
&#9;wait(0.1)
&#9;s:Remove()
end)















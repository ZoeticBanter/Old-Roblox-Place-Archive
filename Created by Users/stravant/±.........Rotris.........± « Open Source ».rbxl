_G.pmt = {  --&gt; modify attached parts positions by the rotation of the main part
	__index = function(inx)
		local val = select[inx]
		local ang = math.pi/2*select.r
		return P(R(math.cos(ang)*val.x - math.sin(ang)*val.y, 1), R(math.sin(ang)*val.x + math.cos(ang)*val.y, 1), val.v)
	end,
	__index2 = function(inx)
		local val = NextPart[inx]
		local ang = math.pi/2*NextPart.r
		return P(R(math.cos(ang)*val.x - math.sin(ang)*val.y, 1), R(math.sin(ang)*val.x + math.cos(ang)*val.y, 1), val.v)
	end
}

I = pmt.__index
Inx = pmt.__index2


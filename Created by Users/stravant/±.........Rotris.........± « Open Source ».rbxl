function PlayGame() --&gt; main function
	if Game_Play then return end
	in_Menu = false
	Game_Play = true
	fallSpeed = 1 - (_level - 1)/10
	GM.Level.Num.Value = 0
	GM.Score.Num.Value = 0
	game.Workspace.CurrentCamera.CameraSubject = GM.Back
	print(&quot;Game started&quot;)
	while true do
		while _isPause do M.Text = &quot;Paused, press \&quot;esc\&quot; to unpause&quot; wait() end
		select = NextPart
		select.Position = P(math.random(2, 8), 20)
		NextPart = Part.new(-4.5, 16)
		drawNext()
		drawPart()
		while true do
			while _isPause do M.Text = &quot;Paused, press \&quot;esc\&quot; to unpause&quot; wait() end
			wait(fallSpeed)
			M.Text = &quot;Score:&quot;..GM.Score.Num.Value..&quot; Level:&quot;..GM.Level.Num.Value
			if CheckMove() then
				select.Position = P(select.Position.x, select.Position.y - 1)
				drawPart()
				else
				allParts[select.Position.x][select.Position.y] = select.val
				allParts[select.Position.x + I(&quot;a&quot;).x][select.Position.y + I(&quot;a&quot;).y] = select.a.v
				allParts[select.Position.x + I(&quot;b&quot;).x][select.Position.y + I(&quot;b&quot;).y] = select.b.v
				allParts[select.Position.x + I(&quot;c&quot;).x][select.Position.y + I(&quot;c&quot;).y] = select.c.v
				checkForRow()
				break
			end
		end
		if not Game_Play then
			clearMap(true) --&gt; don&apos;t wait
		end
		if select.Position.y &gt; 19 then
			print(&quot;Part failed to move&quot;)
			M.Text = &quot;Game Over, press \&quot;esc\&quot; to restart&quot;
			clearMap(false) --&gt; wait
			break
		end
		checkForRow()
	end
	--game has been lost
	Game_Play = false
	in_Menu = true
end

function drawPart() --&gt; draw the falling part
	select.val.CFrame = CFrame.new(Vector3.new(Pos().x + select.Position.x, Pos().y + select.Position.y, Pos().z))
	select.a.v.CFrame = CFrame.new(Vector3.new(Pos().x + select.Position.x + I(&quot;a&quot;).x, Pos().y + select.Position.y + I(&quot;a&quot;).y, Pos().z))
	select.b.v.CFrame = CFrame.new(Vector3.new(Pos().x + select.Position.x + I(&quot;b&quot;).x, Pos().y + select.Position.y + I(&quot;b&quot;).y, Pos().z))
	select.c.v.CFrame = CFrame.new(Vector3.new(Pos().x + select.Position.x + I(&quot;c&quot;).x, Pos().y + select.Position.y + I(&quot;c&quot;).y, Pos().z))
end

function drawNext() --&gt; draw the next part
	NextPart.val.CFrame = CFrame.new(Vector3.new(Pos().x + NextPart.Position.x, Pos().y + NextPart.Position.y, Pos().z))
	NextPart.a.v.CFrame = CFrame.new(Vector3.new(Pos().x + NextPart.Position.x + Inx(&quot;a&quot;).x, Pos().y + NextPart.Position.y + Inx(&quot;a&quot;).y, Pos().z))
	NextPart.b.v.CFrame = CFrame.new(Vector3.new(Pos().x + NextPart.Position.x + Inx(&quot;b&quot;).x, Pos().y + NextPart.Position.y + Inx(&quot;b&quot;).y, Pos().z))
	NextPart.c.v.CFrame = CFrame.new(Vector3.new(Pos().x + NextPart.Position.x + Inx(&quot;c&quot;).x, Pos().y + NextPart.Position.y + Inx(&quot;c&quot;).y, Pos().z))
end

function clearMap(bl)
	is_Active = false
	for y = 25, 1, -1 do
	for x = 1, 10 do
		if not bl then wait() end
		if allParts[x][y] then allParts[x][y]:remove() allParts[x][y] = nil end
	end
	end
	is_Active = true
end

function checkForRow() --&gt; check if there is a finnished row to remove, remove it, and redraw allParts
	is_Active = false
	local got_Row = false
	local scoreCount = 1
	while true do
		local gotRow = false
		for y = 1, 20 do
			local row_isFull = true
			for x = 1, 10 do
				if not allParts[x][y] then row_isFull = false break end
			end
			--print(&quot;Row (&quot;, y, &quot;) is&quot;, row_isFull, &quot;Full&quot;)
			if row_isFull then --&gt; remove and restart row checking
				got_Row = true
				GM.Score.Num.Value = GM.Score.Num.Value + scoreCount
				scoreCount = scoreCount + 2
				--shift down all rows
				for _x = 1, 10 do
					allParts[_x][y]:remove()
					--[[local e = Instance.new(&quot;Explosion&quot;)
					e.Position = Pos() + Vector3.new(_x, y, 0)
					e.BlastRadius = 0.1
					e.Parent = game.Workspace]]
					wait()
					--e:remove()
					for _y = y, 20 do 
						if allParts[_x][_y+1] then
							allParts[_x][_y+1].Position = Pos() + Vector3.new(_x, _y, 0)
						end
						allParts[_x][_y] = allParts[_x][_y+1]
					end
				end
				gotRow = true
				wait(0.5)
				break
			end
		end
		if not gotRow then break end
	end
	if got_Row then
		GM.Level.Num.Value = GM.Level.Num.Value + 1
		if fallSpeed &gt; 0.33 then
			fallSpeed = fallSpeed - 0.01
		end
	end
	is_Active = true
end

function CheckMove(a) --&gt; can the part move down 1?(if a then right or left [a])?
	if not select then return false end
	if a then
		select.Position = P(select.Position.x + a, select.Position.y)
		print(&quot;Moving to X:&quot;, select.Position.x, &quot;Y: &quot;, select.Position.y)
		if (select.Position.x) &gt; 10 or (select.Position.x) &lt; 1 then select.Position = P(select.Position.x - a, select.Position.y) return false end
		if (select.Position.x + I(&quot;a&quot;).x) &gt; 10 or (select.Position.x + I(&quot;a&quot;).x) &lt; 1 then select.Position = P(select.Position.x - a, select.Position.y) return false end
		if (select.Position.x + I(&quot;b&quot;).x) &gt; 10 or (select.Position.x + I(&quot;b&quot;).x) &lt; 1 then select.Position = P(select.Position.x - a, select.Position.y) return false end
		if (select.Position.x + I(&quot;c&quot;).x) &gt; 10 or (select.Position.x + I(&quot;c&quot;).x) &lt; 1 then select.Position = P(select.Position.x - a, select.Position.y) return false end
		print(&quot;Ax:&quot;, I(&quot;a&quot;).x, &quot;Bx:&quot;, I(&quot;b&quot;).x, &quot;Cx:&quot;, I(&quot;c&quot;).x)
		if allParts[select.Position.x][select.Position.y] or allParts[select.Position.x + I(&quot;a&quot;).x][select.Position.y + I(&quot;a&quot;).y] or allParts[select.Position.x + I(&quot;b&quot;).x][select.Position.y + I(&quot;b&quot;).y] or allParts[select.Position.x + I(&quot;c&quot;).x][select.Position.y + I(&quot;c&quot;).y] then
			select.Position = P(select.Position.x - a, select.Position.y)
			return false
		end
		select.Position = P(select.Position.x - a, select.Position.y)
		else
		select.Position = P(select.Position.x, select.Position.y - 1)
		print(&quot;Moving to X:&quot;, select.Position.x, &quot;Y: &quot;, select.Position.y)
		if (select.Position.y + I(&quot;a&quot;).y) &lt; 1 then select.Position = P(select.Position.x, select.Position.y + 1) return false end
		if (select.Position.y + I(&quot;b&quot;).y) &lt; 1 then select.Position = P(select.Position.x, select.Position.y + 1) return false end
		if (select.Position.y + I(&quot;c&quot;).y) &lt; 1 then select.Position = P(select.Position.x, select.Position.y + 1) return false end
		print(&quot;Ax:&quot;, I(&quot;a&quot;).x, &quot;Bx:&quot;, I(&quot;b&quot;).x, &quot;Cx:&quot;, I(&quot;c&quot;).x)
		if allParts[select.Position.x][select.Position.y] or allParts[select.Position.x + I(&quot;a&quot;).x][select.Position.y + I(&quot;a&quot;).y] or allParts[select.Position.x + I(&quot;b&quot;).x][select.Position.y + I(&quot;b&quot;).y] or allParts[select.Position.x + I(&quot;c&quot;).x][select.Position.y + I(&quot;c&quot;).y] then
			select.Position = P(select.Position.x, select.Position.y + 1)
			return false
		end
		select.Position = P(select.Position.x, select.Position.y + 1)
	end
	return true
end

function CheckRotate() --&gt; can the part rotate?
	if not select then return 0 end
	select.Rotate(1)
	if select.Position.x + I(&quot;a&quot;).x &gt; 10 or select.Position.x + I(&quot;a&quot;).x &lt; 1 then select.Rotate(-1) return 1 end
	if select.Position.x + I(&quot;b&quot;).x &gt; 10 or select.Position.x + I(&quot;b&quot;).x &lt; 1 then select.Rotate(-1) return 2 end
	if select.Position.x + I(&quot;c&quot;).x &gt; 10 or select.Position.x + I(&quot;c&quot;).x &lt; 1 then select.Rotate(-1) return 3 end
	if allParts[select.Position.x][select.Position.y] or allParts[select.Position.x + I(&quot;a&quot;).x][select.Position.y + I(&quot;a&quot;).y] or allParts[select.Position.x + I(&quot;b&quot;).x][select.Position.y + I(&quot;b&quot;).y] or allParts[select.Position.x + I(&quot;c&quot;).x][select.Position.y + I(&quot;c&quot;).y] then
		select.Rotate(-1)
		return 4
	end
	select.Rotate(-1)
	return true
end

function TetPart(colorNum) --&gt; create a 1x1x1 brick
	local p = Instance.new(&quot;Part&quot;)
	p.FormFactor = 0
	p.Size = Vector3.new(1,1,1)
	p.BottomSurface = 0
	p.TopSurface = 0
	p.Anchored = true
	p.BrickColor = BrickColor.new(colorNum)	
	p.Parent = game.Workspace
	local v = Instance.new(&quot;StringValue&quot;)
	v.Name = &quot;Owner&quot;
	v.Value = player.Name
	v.Parent = p
	return p
end

function createPart(tb, r, color) --&gt; create the &quot;a, b, c&quot; parts attached to the part
	if r == 1 then --&gt; 4-long
		tb.a = P(0, -1, TetPart(color)) 
		tb.b = P(0, 1, TetPart(color)) 
		tb.c = P(0, 2, TetPart(color)) 
		elseif r == 2 then --&gt; L - &quot;L&quot;
		tb.a = P(0, -1, TetPart(color)) 
		tb.b = P(0, 1, TetPart(color)) 
		tb.c = P(1, 1, TetPart(color)) 
		elseif r == 3 then --&gt; R - &quot;L&quot;
		tb.a = P(0, -1, TetPart(color)) 
		tb.b = P(0, 1, TetPart(color)) 
		tb.c = P(-1, 1, TetPart(color)) 
		elseif r == 4 then --&gt; L - zigzag
		tb.a = P(0, 1, TetPart(color)) 
		tb.b = P(1, 0, TetPart(color)) 
		tb.c = P(1, -1, TetPart(color)) 
		elseif r == 5 then --&gt; R - zigzag
		tb.a = P(0, 1, TetPart(color)) 
		tb.b = P(-1, 0, TetPart(color)) 
		tb.c = P(-1, -1, TetPart(color)) 
		elseif r == 6 then --&gt; T
		tb.a = P(0, 1, TetPart(color)) 
		tb.b = P(1, 0, TetPart(color)) 
		tb.c = P(-1, 0, TetPart(color)) 
		elseif r == 7 then --&gt; square
		tb.a = P(0, 1, TetPart(color)) 
		tb.b = P(1, 1, TetPart(color)) 
		tb.c = P(1, 0, TetPart(color)) 
	end
end

Part = {}

function Part.new(x, y)
	local r = T_Rand(1,7)
	local color = ({20, 21, 22, 23, 24, 25, 26, 27})[r]
	local _a = nil
	_a = {
		type = r,
		val = TetPart(color), 
		r = 0, 
		Position = P(x, y, nil),
		Rotate = function(inx)
			if _a.type ~= 7 then
				_a.r = _a.r + inx
				if _a.r &gt; 3 then
					_a.r = 0
					elseif _a.r &lt; 0 then
					_a.r = 3
				end
				print(&quot;Rotation:&quot;, r)
			end
		end
	}
	createPart(_a, r, color)
	--setmetatable(_a, pmt)
	return _a
end


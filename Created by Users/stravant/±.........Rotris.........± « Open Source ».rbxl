local a5 = {true, false, true, true, true, false, true}
local a6 = {true, false, true, true, true, true, true}
local a7 = {false, true, true, false, true, false, false}
local a8 = {true, true, true, true, true, true, true}
local a9 = {true, true, true, true, true, false, true}

local states = {a0, a1, a2, a3, a4, a5, a6, a7, a8, a9}

function changed()
	local num = tostring(script.Parent.Num.Value)
	local aa1 = string.sub(num, -1, -1) --1&apos;s place
	if aa1 ~= &quot;&quot; then 
		print(&quot;there is a &quot;..aa1..&quot; in the one&apos;s place&quot;)
		change(script.Parent.a1, tonumber(aa1))
		else
		change(script.Parent.a1, &quot;0&quot;)	
	end
	local aa2 = string.sub(num, -2, -2) 
	if aa2 ~= &quot;&quot; then 
		change(script.Parent.a2, tonumber(aa2))
		else
		change(script.Parent.a2, &quot;0&quot;)		
	end
	local aa3 = string.sub(num, -3, -3) 
	if aa3 ~= &quot;&quot; then 
		change(script.Parent.a3, tonumber(aa3))
		else
		change(script.Parent.a3, &quot;0&quot;)		
	end
	if script.Parent.Num.Value &gt; 999 then
		change(script.Parent.a1, &quot;9&quot;)
		change(script.Parent.a2, &quot;9&quot;)
		change(script.Parent.a3, &quot;9&quot;)
	end
end

function change(digit, num)
	local a = digit:GetChildren()
	local b = &quot;a&quot;..num
	for i = 1, #a do
		local tb = states[num + 1]
		local p = digit:FindFirstChild(&quot;a&quot;..tostring(i))
		if tb[i] == false then
			p.Transparency = 1
			else
			p.Transparency = 0			
		end
	end
end

wait(0.5)

script.Parent.Num.Changed:connect(changed)


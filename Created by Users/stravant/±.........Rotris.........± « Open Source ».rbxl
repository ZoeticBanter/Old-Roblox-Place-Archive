Last3 = {1, 2, 3}

function T_Rand(low, up)
	local a = math.random(low, up)
	if _level ~= 3 then
		while a == Last3[1] or a == Last3[2] or a == Last3[3] do
			a = math.random(low, up)
		end
		table.remove(Last3, 1)
		table.insert(Last3, a)
	end
	return a
end

function R(num, inx) --&gt; round num to inx
	local num = num/inx
	if math.abs(num - math.floor(num)) &lt; math.abs(num - math.ceil(num)) then
		num = math.floor(num)
		else
		num = math.ceil(num)
	end
	return num*inx
end

--point constructor
function P(x, y, val)
	if type(x) == &quot;number&quot; and type(y) == &quot;number&quot; then
		return {x = x, y = y, v = val or nil}
		else
		return {x = x.x, y = x.y, v = val or nil}
	end
end


								<ProtectedString name="Source">-------- OMG HAX

r = game:service(&quot;RunService&quot;)
local sword = script.Parent.Handle
local Tool = script.Parent

local damage = 500000000000000
local p = nil
local humanoid = nil
local playerWithSword = nil
local attacking = false
local walkSpeed = 25

local slash_damage = 18

local SlashSound = Instance.new(&quot;Sound&quot;)
SlashSound.SoundId = &quot;http://www.roblox.com/asset/?id=25641508&quot;
SlashSound.Parent = sword
SlashSound.Volume = .7

local UnsheathSound = Instance.new(&quot;Sound&quot;)
UnsheathSound.SoundId = &quot;http://www.roblox.com/asset/?id=25641879&quot;
UnsheathSound.Parent = sword
UnsheathSound.Volume = 1


function blow(hit)
&#9;local humanoid = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
&#9;local vCharacter = Tool.Parent
&#9;local vPlayer = game.Players:playerFromCharacter(vCharacter)
&#9;local hum = vCharacter:findFirstChild(&quot;Humanoid&quot;) -- non-nil if tool held by a character
&#9;if humanoid~=nil and humanoid ~= hum and hum ~= nil and attacking == true then
&#9;&#9;-- final check, make sure sword is in-hand

&#9;&#9;local right_arm = vCharacter:FindFirstChild(&quot;Right Arm&quot;)
&#9;&#9;if (right_arm ~= nil) then
&#9;&#9;&#9;local joint = right_arm:FindFirstChild(&quot;RightGrip&quot;)
&#9;&#9;&#9;if (joint ~= nil and (joint.Part0 == sword or joint.Part1 == sword)) then
&#9;&#9;&#9;&#9;tagHumanoid(humanoid, vPlayer)
&#9;&#9;&#9;&#9;humanoid:TakeDamage(damage)
&#9;&#9;&#9;&#9;wait(1)
&#9;&#9;&#9;&#9;untagHumanoid(humanoid)
&#9;&#9;&#9;end
&#9;&#9;end


&#9;end
end


function tagHumanoid(humanoid, player)
&#9;local creator_tag = Instance.new(&quot;ObjectValue&quot;)
&#9;creator_tag.Value = player
&#9;creator_tag.Name = &quot;creator&quot;
&#9;creator_tag.Parent = humanoid
end

function untagHumanoid(humanoid)
&#9;if humanoid ~= nil then
&#9;&#9;local tag = humanoid:findFirstChild(&quot;creator&quot;)
&#9;&#9;if tag ~= nil then
&#9;&#9;&#9;tag.Parent = nil
&#9;&#9;end
&#9;end
end


function attack(h)
&#9;attacking = true
&#9;Tool.Handle.Mesh.VertexColor = Vector3.new(1,1,1)
&#9;attackOut()
&#9;damage = slash_damage
&#9;SlashSound:play()
&#9;wait(0.5)
&#9;attackIn()
&#9;Tool.Handle.Mesh.VertexColor = Vector3.new(0,0,0.5)
&#9;attacking = false
end

function swordUp()
&#9;Tool.GripForward = Vector3.new(-1,0,0)
&#9;Tool.GripRight = Vector3.new(0,1,0)
&#9;Tool.GripUp = Vector3.new(0,0,1)
end

function swordOut()
&#9;Tool.GripForward = Vector3.new(0,0,1)
&#9;Tool.GripRight = Vector3.new(0,1,0)
&#9;Tool.GripUp = Vector3.new(1,0,0)
end

function attackOut()
&#9;Tool.GripForward = Vector3.new(0,0,1)
&#9;Tool.GripPos = Vector3.new(0,0,2.4)
&#9;Tool.GripRight = Vector3.new(0,-1,0)
&#9;Tool.GripUp = Vector3.new(1,0,0)
end

function attackIn()
&#9;Tool.GripForward = Vector3.new(-1,0,0)
&#9;Tool.GripPos = Vector3.new(0.3,0,2)
&#9;Tool.GripRight = Vector3.new(0,1,0)
&#9;Tool.GripUp = Vector3.new(0,0,1)
end

Tool.Enabled = true
local last_attack = 0
function onActivated()

&#9;if not Tool.Enabled then
&#9;&#9;return
&#9;end

&#9;Tool.Enabled = false

&#9;local character = Tool.Parent;
&#9;local humanoid = character.Humanoid
&#9;if humanoid == nil then
&#9;&#9;print(&quot;Humanoid not found&quot;)
&#9;&#9;return 
&#9;end

&#9;attack(humanoid)

&#9;Tool.Enabled = true
&#9;Tool.Handle.Mesh.VertexColor = Vector3.new(0,0,0.5)

end

function onEquipped()
&#9;
&#9;if UnsheathSound ~= nil then
&#9;&#9;UnsheathSound:play()
&#9;end
&#9;SlashSound:Stop()
&#9;playerWithSword = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;if playerWithSword ~= nil then
&#9;&#9;playerWithSword.WalkSpeed = walkSpeed - 5
&#9;end
&#9;p = game.Players:GetPlayerFromCharacter(Tool.Parent)

end

function onUnequipped()
&#9;SlashSound:Stop()
&#9;if playerWithSword ~= nil then
&#9;&#9;playerWithSword.WalkSpeed = walkSpeed
&#9;end

end

script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

connection = sword.Touched:connect(blow)


								<string name="Name">Fire</string>
								<ProtectedString name="Source">local Tool = script.Parent -- The Tool
local x = script.Parent

enabled = true -- Debouncer

function fire(v) -- The Fun Part

&#9;local vCharacter = Tool.Parent -- The Player&apos;s Model
&#9;local vPlayer = game.Players:playerFromCharacter(vCharacter) -- Find the player in the players section
&#9;local Torso = vCharacter.Torso -- Find the Player&apos;s Torso


        

&#9;local spawnPos = Torso.Position -- Where to spawn the Bullet
&#9;


&#9;spawnPos  = spawnPos + (v * 8) -- Move the bullet so it doesn&apos;t hit the gun itself or the Player firing

&#9;local missile = Instance.new(&quot;Part&quot;) -- Make a new bullet

&#9;missile.Position = spawnPos -- Move the bullet to the spawn location
&#9;missile.Size = Vector3.new(1,1,1) -- Size of Bullet
&#9;missile.Velocity = v * 200 -- Speed of the Bullet ( 200 is a decent speed so the touched events can be picked up
&#9;missile.BrickColor = BrickColor.new(&quot;Dark stone gray&quot;) -- Color of the Bullet
&#9;missile.Shape = &quot;Ball&quot; -- Shape of the Bullet
&#9;missile.BottomSurface = &quot;Smooth&quot; -- Top surface of the bullet
&#9;missile.TopSurface = &quot;Smooth&quot; -- Bottom Surface of the bullet
&#9;missile.Name = &quot;Bullet&quot; -- Name of the bullet
&#9;missile.Elasticity = 0 -- Bouncyness of the bullet
&#9;missile.Reflectance = 0 -- Reflectance of the bullet
&#9;missile.Friction = .9 -- Friction of the bullet
&#9;missile.Parent = game.Workspace -- bullet&apos;s Parent

&#9;local DmgScript = script.Parent.BulletDamage:Clone() -- The damage script clone
&#9;DmgScript.Disabled = false --Enable the clone of it
&#9;DmgScript.Parent = missile --Put it into the bullet

&#9;local Delay = script.Parent.DelayedRemover:Clone() --The timed remover script clone
&#9;Delay.Disabled = false --Enable the clone of it
&#9;Delay.Parent = missile -- Put it into the bullet

&#9;local force = Instance.new(&quot;BodyForce&quot;) -- Adds more force to the bullet to give it its height
&#9;force.force = Vector3.new(0,100,0) -- 100 is perfect. its a mostly straight shot
&#9;force.Parent = missile -- Put the force into the bullet

end -- close the function


function onActivate() -- When you click to use it...
&#9;if enabled == true then -- check to see if you can use it
&#9;&#9;enabled = false --disable it to &quot;reload&quot;

&#9;&#9;local character = Tool.Parent -- The Player&apos;s model
&#9;&#9;local humanoid = character.Humanoid -- The Player&apos;s humanoid


&#9;&#9;local targetPos = humanoid.TargetPoint -- Find where the humanoid is targeting
&#9;&#9;local lookAt = (targetPos - character.Head.Position).unit -- Find where the humanoid is targeting

&#9;&#9;fire(lookAt) -- Run the fire function with the value from lookAt replacing v

&#9;&#9;wait(0.1) -- wait 2 seconds
&#9;&#9;enabled = true -- renable it
&#9;end -- End the if

end -- end the function

script.Parent.Activated:connect(onActivate) -- the connection

-- Hope you understand the gun a little more! --

--~~~~~builderman1171~~~~~~

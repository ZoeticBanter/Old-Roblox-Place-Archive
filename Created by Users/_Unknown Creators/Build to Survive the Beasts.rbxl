&#9;&#9;if left_arm then left_arm.BrickColor = BrickColor.new(26) end
&#9;&#9;if right_arm then right_arm.BrickColor = BrickColor.new(26) end
&#9;&#9;if left_leg then left_leg.BrickColor = BrickColor.new(26) end

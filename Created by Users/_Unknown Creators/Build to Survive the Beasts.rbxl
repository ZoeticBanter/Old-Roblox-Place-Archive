&#9;&#9;RightShoulder.DesiredAngle = -0.25
&#9;&#9;LeftShoulder.MaxVelocity = 0.45
&#9;&#9;LeftShoulder.DesiredAngle = -1.75
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;SpinSlash&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.3
&#9;&#9;RightShoulder.DesiredAngle = 1.57
&#9;&#9;LeftShoulder.MaxVelocity = 0.3
&#9;&#9;LeftShoulder.DesiredAngle = 1.57
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Grab&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.3
&#9;&#9;RightShoulder.DesiredAngle = 2
&#9;&#9;LeftShoulder.MaxVelocity = 0.3
&#9;&#9;LeftShoulder.DesiredAngle = -2
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Grab2&quot;) then

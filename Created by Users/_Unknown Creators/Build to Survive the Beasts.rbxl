&#9;&#9;if string.match(msg, &quot;float&quot;) then pose=&quot;Float&quot; end
&#9;&#9;if string.match(msg, &quot;punch&quot;) then pose=&quot;Punch&quot; end
&#9;&#9;if string.match(msg, &quot;kick&quot;) then pose=&quot;Kick&quot; end

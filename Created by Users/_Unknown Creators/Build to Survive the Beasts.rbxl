&#9;&#9;RightShoulder.DesiredAngle = 1
&#9;&#9;LeftShoulder.MaxVelocity = 0.3
&#9;&#9;LeftShoulder.DesiredAngle = -1
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Hold&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.3
&#9;&#9;RightShoulder.DesiredAngle = 3.14
&#9;&#9;LeftShoulder.MaxVelocity = 0.3
&#9;&#9;LeftShoulder.DesiredAngle = 3.14
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Ready&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.4
&#9;&#9;RightShoulder.DesiredAngle = 3.5
&#9;&#9;LeftShoulder.MaxVelocity = 0.4
&#9;&#9;LeftShoulder.DesiredAngle = -3.5
&#9;&#9;RightHip.MaxVelocity = 0.4
&#9;&#9;LeftHip.MaxVelocity = 0.4
&#9;&#9;RightHip.DesiredAngle = -0.3
&#9;&#9;LeftHip.DesiredAngle = 0.3&#9;&#9;
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Final&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.75
&#9;&#9;RightShoulder.DesiredAngle = 0.75
&#9;&#9;LeftShoulder.MaxVelocity = 0.75
&#9;&#9;LeftShoulder.DesiredAngle = -0.75&#9;
&#9;&#9;return
&#9;end

&#9;if (toolAnim == &quot;Lunge&quot;) then
&#9;&#9;RightShoulder.MaxVelocity = 0.5
&#9;&#9;LeftShoulder.MaxVelocity = 0.5
&#9;&#9;RightHip.MaxVelocity = 0.5
&#9;&#9;LeftHip.MaxVelocity = 0.5
&#9;&#9;RightShoulder.DesiredAngle = 1.57
&#9;&#9;LeftShoulder.DesiredAngle = 1.0
&#9;&#9;RightHip.DesiredAngle = 1.57
&#9;&#9;LeftHip.DesiredAngle = 1.0
&#9;&#9;return
&#9;end
end

function move(time)
&#9;local amplitude
&#9;local frequency

&#9;--[[if (pose == &quot;FreeFall&quot;) then
&#9;&#9;moveFreeFall()
&#9;&#9;return
&#9;end]]
 
&#9;if (pose == &quot;Seated&quot;) then
&#9;&#9;moveSit()

rbx.Parent = stats

local msg = Instance.new(&quot;Message&quot;)
wait(3)
msg.Parent = newPlayer
msg.Text = &quot;Welcome to the Trade System Tutorial!&quot;
wait(5)
msg.Text = &quot;Step on the \&quot;Start\&quot; button to begin&quot; 
wait(5)
msg.Parent = nil

end


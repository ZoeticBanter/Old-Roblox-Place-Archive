						<string name="Source">function onTouched(hit)

h = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
if h == nil then return end
if game.Players:findFirstChild(hit.Parent.Name):findFirstChild(&quot;TixComplete&quot;) == nil then
	h.Parent.Torso.CFrame = CFrame.new(Vector3.new((math.random(-110, -81.5)), 37.4, (math.random(-150, -116))))
	doMessageSystem(game.Players:getPlayerFromCharacter(hit.Parent))
else
	doUberMessage(game.Players:getPlayerFromCharacter(hit.Parent))
end

end

function doMessageSystem(player)

if player:findFirstChild(&quot;Message&quot;) ~= nil then player:findFirstChild(&quot;Message&quot;):remove() end
msg = Instance.new(&quot;Message&quot;)
msg.Parent = player
msg.Text = &quot;Refer to messages BELOW.&quot;
wait(3)
msg.Parent = nil
h = Instance.new(&quot;Hint&quot;)
h.Parent = player
h.Text = &quot;When you&apos;re trading for tickets, you&apos;re offering an amount of ROBUX for an amount of tickets.&quot;
wait(5)
h.Text = &quot;A typical trade for tickets usually follows a 4:1 to 5:1 ratio, meaning they&apos;re for sale at 4 or 5 Tickets for 1 ROBUK each.&quot;
wait(5)
h.Text = &quot;Suppose you see an offer that says \&quot;400 @ 4:1\&quot;.&quot;
wait(5)
h.Text = &quot;This means, \&quot;I am offering 400 tickets at the cost of 100 ROBUX.\&quot;.&quot;
wait(5)
h.Text = &quot;In this simulation, you&apos;ve been provided with 500 Tickets and 100 ROBUX to start. You should still have them.&quot;
wait(5)
h.Text = &quot;Just to be sure, your Tickets and ROBUX will be reset as a precaution.&quot;
wait(5)
tix = player.leaderstats.Tickets
rbx = player.leaderstats.ROBUX
tix.Value = 500
rbx.Value = 100
player.Character.Torso.CFrame = CFrame.new(Vector3.new(122, 32.6, -131))
local val = Instance.new(&quot;StringValue&quot;)
val.Name = &quot;TixTrade&quot;
val.Parent = player
h.Text = &quot;Welcome to the Trade Room Simulator! Find and touch a ticket trade offer to begin.&quot;
wait(5)
h.Parent = nil

end

function doUberMessage(player)

if player:findFirstChild(&quot;Message&quot;) ~= nil then player:findFirstChild(&quot;Message&quot;):remove() end
msg = Instance.new(&quot;Message&quot;)
msg.Parent = player
msg.Text = &quot;You have already competed this tutorial!&quot;
wait(3)
msg.Parent = nil

end


						<string name="Name">Script</string>
						<string name="Source">local debounce = false

function onTouched(hit)
	if debounce == false then
	debounce = true 
	h = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
	if h ~= nil then
		player = game.Players:getPlayerFromCharacter(hit.Parent)
		if player:findFirstChild(&quot;TixTrade&quot;) == nil then return end
		script.Parent.Parent.Name = &quot;Available Tickets:&quot;
		wait(3)
		script.Parent.Parent.Name = &quot;200 @ 4:1&quot;
		wait(3)
		script.Parent.Parent.Name = &quot;Open Ticket Trade!&quot;
		debounce = false
		player:findFirstChild(&quot;TixTrade&quot;):remove()
		m = Instance.new(&quot;Hint&quot;)
		m.Parent = player
		m.Text = &quot;\&quot;200 @ 4:1\&quot; means \&quot;I am selling 200 tickets for 50 ROBUX\&quot;.&quot;
		wait(5)
		m.Text = &quot;To figure out how much currency the player wants, divide the offer by the ratio.&quot;
		wait(5)
		m.Text = &quot;In this case, 200 divided by 4 equals 50. The player wants 50 ROBUX.&quot;
		wait(5)
		m.Text = &quot;You&apos;ve got enough ROBUX to execute the trade, so let&apos;s do it.&quot;
		wait(3)
		msg = Instance.new(&quot;Message&quot;)
		msg.Parent = player
		msg.Text = &quot;Transaction complete!&quot;
		rbx = player.leaderstats.ROBUX
		tix = player.leaderstats.Tickets
		rbx.Value = rbx.Value - 50
		tix.Value = tix.Value + 200
		wait(3)
		msg.Parent = nil
		m.Text = &quot;Congratulations! You&apos;ve successfully executed a trade!&quot;
		wait(5)
		local comp = Instance.new(&quot;StringValue&quot;)
		comp.Name = &quot;TixComplete&quot;
		comp.Parent = player
		msg = Instance.new(&quot;Message&quot;)
		msg.Parent = player
		m.Parent = nil
		player.Character:findFirstChild(&quot;Humanoid&quot;).Health = 0
	end
	end
end


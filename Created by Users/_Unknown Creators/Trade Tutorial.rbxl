key:lower() 
	if key == &quot;h&quot; then
		update(bin.Give.Value, 1)
	elseif key == &quot;j&quot; then
		update(bin.Give.Value, -1)
	elseif key == &quot;k&quot; then
		update(bin.Give.Value, 10)
	elseif key == &quot;l&quot; then
		update(bin.Give.Value, -10)
	elseif key == &quot;v&quot; then
		update(bin.Take.Value, 1)
	elseif key == &quot;b&quot; then
		update(bin.Take.Value, -1)
	elseif key == &quot;n&quot; then
		update(bin.Take.Value, 10)
	elseif key == &quot;m&quot; then
		update(bin.Take.Value, -10)
	elseif key == &quot;t&quot; then
		--toggle
	end
end 
 

function onSelected(mouse)
	mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	mouse.KeyDown:connect(onKeyDown) 
end


						<string name="Source">local debounce = false

function onTouched(hit)
	if debounce == false then
	debounce = true 
	h = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
	if h ~= nil then
		player = game.Players:getPlayerFromCharacter(hit.Parent)
		if player:findFirstChild(&quot;RbxTrade&quot;) == nil then return end
		script.Parent.Parent.Name = &quot;Available ROBUX:&quot;
		wait(3)
		script.Parent.Parent.Name = &quot;15 @ 1:4.5373&quot;
		wait(3)
		script.Parent.Parent.Name = &quot;Open ROBUX Trade!&quot;
		debounce = false
		player:findFirstChild(&quot;RbxTrade&quot;):remove()
		m = Instance.new(&quot;Hint&quot;)
		m.Parent = player
		m.Text = &quot;I know what you&apos;re thinking: \&quot;Oh noes! A decimal!!111!1!1!!!11!1!!11!!111one.\&quot;&quot;
		wait(5)
		m.Text = &quot;Well, you may not be thinking it EXACTLY like that... but that&apos;s not the point.&quot;
		wait(5)
		m.Text = &quot;Don&apos;t panic. Just get a calculator! Unlike trading for tickets, trading for ROBUX is treated a bit differently.&quot;
		wait(5)
		m.Text = &quot;When trading for ROBUX, rather than dividing by the first number in the ratio, you multiply by the second.&quot;
		wait(5)
		m.Text = &quot;In this case, the equation is 15 multiplied by 4.58999. Make sure you get the ENTIRE decimal!&quot;
		wait(5)
		m.Text = &quot;If you do the work on the calculator, you&apos;ll get 68.84985. So what do you do when you have a decimal?&quot;
		wait(5)
		m.Text = &quot;Easy! Round it to the nearest whole number. When you round up, you&apos;ll get 69.&quot;
		wait(5)
		m.Text = &quot;So do you have enough?&quot;
		wait(5)
		rbx = player.leaderstats.ROBUX
		tix = player.leaderstats.Tickets
		if tix.Value &gt;= 69 then
			m.Text = &quot;You have enough! Time to trade!&quot;
			wait(3)
			success(player)
		else
			m.Text = &quot;Hmmm, you don&apos;t have enough. Lucky for you, the admins didn&apos;t leave you without help.&quot;
			wait(3)
			tix.Value = tix.Value + 200
			msg = Instance.new(&quot;Message&quot;)
			msg.Parent = player
			msg.Text = &quot;Telamon sends you a gift!&quot;
			wait(3)
			msg.Parent = nil
			m.Text = &quot;Okay, now to execute the trade.&quot;
			success(player)
		end
	end
	end
end

function success(player)
	rbx = player.leaderstats.ROBUX
	tix = player.leaderstats.Tickets
	rbx.Value = rbx.Value + 15
	tix.Value = tix.Value - 69
	msg = Instance.new(&quot;Message&quot;)
	msg.Parent = player
	msg.Text = &quot;Transaction complete!&quot;
	wait(3)
	msg.Parent = nil
	m.Text = &quot;Now you know about how to trade for ROBUX. Congrats!&quot;
	wait(5)
	local comp = Instance.new(&quot;StringValue&quot;)
	comp.Name = &quot;RbxComplete&quot;
	comp.Parent = player
	m.Parent = nil
	player.Character:findFirstChild(&quot;Humanoid&quot;).Health = 0
end

script.Parent.Touched:connect(onTouched)

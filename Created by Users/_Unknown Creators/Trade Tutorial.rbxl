	debounce = true 
	h = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
	if h ~= nil then
		if game.Players:findFirstChild(hit.Parent.Name):findFirstChild(&quot;MrktTrade&quot;) == nil then return end
		player = game.Players:getPlayerFromCharacter(hit.Parent)
		script.Parent.Parent.Name = &quot;Available Tickets:&quot;

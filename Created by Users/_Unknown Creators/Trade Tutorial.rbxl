						<string name="Source">function onTouched(hit)

h = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
if h == nil then return end
player = game.Players:getPlayerFromCharacter(hit.Parent)
if player:findFirstChild(&quot;TixComplete&quot;) ~= nil and player:findFirstChild(&quot;RbxComplete&quot;) ~= nil and player:findFirstChild(&quot;MrktComplete&quot;) ~= nil and player:findFirstChild(&quot;LmtComplete&quot;) ~= nil then
	h.Parent.Torso.CFrame = CFrame.new(Vector3.new(0, (math.random(505, 515)), 0))
	winMessage(player)
else
	h.Parent.Torso.CFrame = CFrame.new(Vector3.new(-111, 36.2, -2.5))
	showMessage(player)
end

end

function showMessage(player)

if player:findFirstChild(&quot;Message&quot;) ~= nil then player:findFirstChild(&quot;Message&quot;):remove() end
msg = Instance.new(&quot;Message&quot;)
msg.Parent = player
msg.Text = &quot;Choose a lesson to begin.&quot;
wait(3)
msg.Parent = nil

end

function winMessage(player)

if player:findFirstChild(&quot;Message&quot;) ~= nil then player:findFirstChild(&quot;Message&quot;):remove() end
msg = Instance.new(&quot;Message&quot;)
msg.Parent = player
msg.Text = &quot;You&apos;ve completed all the lessons!&quot;
wait(3)
msg.Text = &quot;Now go to the Trade Simulator and experiment!&quot;
msg.Parent = nil

end


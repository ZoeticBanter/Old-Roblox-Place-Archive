function onButton1Down(mouse)

	local player = game.Players.LocalPlayer
	if player == nil then return end
	m = Instance.new(&quot;Message&quot;)
	m.Parent = player
	m.Text = &quot;Selling R$: 125 @ 1:4&quot;
	h = Instance.new(&quot;Hint&quot;)
	h.Parent = player
	h.Text = &quot;Your trade has been set up!&quot;
	wait(3)
	m.Text = &quot;Transaction complete!&quot;
	tix = player.leaderstats.Tickets
	rbx = player.leaderstats.ROBUX
	tix.Value = tix.Value + 500
	rbx.Value = rbx.Value - 125
	h.Text = &quot;Because your offer is the best one for the market order, the trade has been executed!&quot;
	wait(3)
	m.Parent = nil
	h.Text = &quot;This is only a mere example of what will happen during market order trades.&quot;
	wait(5)
	h.Text = &quot;Let the buyer beware in market trades. They are highly unpredictable.&quot;
	wait(5)
	h.Text = &quot;The \&quot;What I&apos;ll get:\&quot; counter is an estimate based on the current offers. Be careful!&quot;
	wait(5)
	h.Text = &quot;And on that note, you&apos;ve completed this lesson!&quot;
	wait(5)
	val = Instance.new(&quot;StringValue&quot;)
	val.Name = &quot;MrktComplete&quot;
	val.Parent = player
	h.Parent = nil
	player.Character:findFirstChild(&quot;Humanoid&quot;).Health = 0

end

function onSelected(mouse)
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end


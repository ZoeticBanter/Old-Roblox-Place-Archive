					<string name="Source">bin = script.Parent


function trade()

	local player = game.Players.LocalPlayer
	if player == nil or player.Character == nil then return end

	bin.Parent = nil

	msg = Instance.new(&quot;Message&quot;)

	msg.Parent = player

	msg.Text = &quot;Trade set!&quot;

	m = Instance.new(&quot;Hint&quot;)

	m.Parent = player

	m.Text = &quot;Your trade is set and ready to go. In this offer, you&apos;re selling 100 Tickets for 25 ROBUX.&quot;

	wait(3)

	msg.Parent = nil

	wait(3)

	m.Text = &quot;Usually, you&apos;d have to wait a while for trades to be executed.&quot;

	wait(5)

	m.Text = &quot;But this is a tutorial, so the trade will be executed quickly.&quot;

	wait(3)

	msg = Instance.new(&quot;Message&quot;)

	msg.Parent = player

	msg.Text = &quot;Transaction complete!&quot;

	tix = player.leaderstats.Tickets

	rbx = player.leaderstats.ROBUX

	tix.Value = tix.Value - 100

	rbx.Value = rbx.Value + 25

	wait(3)

	msg.Parent = nil

	m.Text = &quot;You can also allow for split trades. You get less out of the deal, but you give less at the same time.&quot;

	wait(5)

	m.Text = &quot;In this case, you may sell 50 Tickets for 20 ROBUX in return. But the split trades function may be turned off.&quot;

	wait(5)

	m.Text = &quot;That&apos;s all you need to know about limited order trades!&quot;

	wait(5)

	m.Parent = nil

	val = Instance.new(&quot;StringValue&quot;)

	val.Name = &quot;LmtComplete&quot;

	val.Parent = player

	player.Character.Humanoid.Health = 0

end


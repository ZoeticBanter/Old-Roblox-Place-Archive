				<string name="Name">Secret</string>
				<ProtectedString name="Source">d1 = Workspace.Secret1
d2 = Workspace.Secret2
d3 = Workspace.Secret3
d4 = Workspace.Secret4

open = false
run = false

function openn()
&#9;run = true
&#9;for i = 1, 90 do
&#9;&#9;d1.CFrame = d1.CFrame + Vector3.new(0,0,.1)
&#9;&#9;d2.CFrame = d2.CFrame - Vector3.new(0,0,.1)
&#9;&#9;d3.CFrame = d3.CFrame + Vector3.new(0,.1,0)
&#9;&#9;d4.CFrame = d4.CFrame - Vector3.new(0,.1,0)
&#9;&#9;wait()
&#9;end
&#9;open = true
&#9;run = false
end 

function close()
&#9;run = true
&#9;for i = 1, 90 do
&#9;&#9;d1.CFrame = d1.CFrame - Vector3.new(0,0,.1)
&#9;&#9;d2.CFrame = d2.CFrame + Vector3.new(0,0,.1)
&#9;&#9;d3.CFrame = d3.CFrame - Vector3.new(0,.1,0)
&#9;&#9;d4.CFrame = d4.CFrame + Vector3.new(0,.1,0)
&#9;&#9;wait()
&#9;end
&#9;open = false
&#9;run = false
end 

function oc(msg)
&#9;msg = msg:lower()
&#9;if msg == &quot;open&quot; then
&#9;&#9;if run == false and open == false then
&#9;&#9;&#9;openn()
&#9;&#9;end
&#9;elseif msg == &quot;close&quot; then 
&#9;&#9;if run == false and open == true then
&#9;&#9;&#9;close()
&#9;&#9;end
&#9;end 
end 

game.Players.PlayerAdded:connect(function(plr) wait() plr.Chatted:connect(oc) end) 


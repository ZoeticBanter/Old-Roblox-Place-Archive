&#9;LeftShoulder.DesiredAngle = desiredAngle - climbFudge
&#9;RightHip.DesiredAngle = -desiredAngle
&#9;LeftHip.DesiredAngle = -desiredAngle


&#9;local tool = getTool()

&#9;if tool then
&#9;
&#9;&#9;animStringValueObject = getToolAnim(tool)


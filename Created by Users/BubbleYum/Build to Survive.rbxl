				<string name="Name">onEnter Script</string>
				<string name="Source">function onPlayerRespawned(newPlayer) 
repeat 
wait(1) 
until workspace:findFirstChild(newPlayer.Name) ~= nil 
repeat 
wait(3) 
until workspace[newPlayer.Name][&quot;Body Colors&quot;] ~= nil 
local char = newPlayer.Character 
local torso = char[&quot;Torso&quot;] 
if (torso:findFirstChild(&quot;roblox&quot;) ~= nil) then 
torso[&quot;roblox&quot;].Texture = &quot;&quot; 
end 
if (torso:findFirstChild(&quot;Shirt Graphic&quot;) ~= nil) then 
torso[&quot;Shirt Graphic&quot;].Graphic = &quot;&quot; 
end 
if (char:findFirstChild(&quot;Shirt&quot;) ~= nil) then 
char[&quot;Shirt&quot;].ShirtTemplate = &quot;&quot; 
end 
if (char:findFirstChild(&quot;Pants&quot;) ~= nil) then 
char[&quot;Pants&quot;].PantsTemplate = &quot;&quot; 
end 
end 
function onPlayerEntered(newPlayer) 
newPlayer.Changed:connect(function (property) 
if (property == &quot;Character&quot;) then 
onPlayerRespawned(newPlayer) 
end 
end) 
end 
game.Players.PlayerAdded:connect(onPlayerEntered) </string>

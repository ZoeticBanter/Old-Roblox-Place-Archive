				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">SuperVIPCommand</string>
				<string name="Source">function onChatted(msg, recipient, speaker)

	-- convert to all lower case

	local source = string.lower(speaker.Name)

permission = {&quot;PiePersonOfDOOM&quot;, &quot;BubbleYum&quot;}

function checkOkToLetIn(name) 
	for i = 1,#permission do 
		-- convert strings to all upper case, otherwise we will let in 
		-- &quot;Telamon&quot; but not &quot;telamon&quot; or &quot;tELAMON&quot; 
		if (string.upper(name) == string.upper(permission[i])) then return true end 
	end 
	return false 
end 
	
	if (checkOkToLetIn(source)) then
	if (string.sub(msg,1,6) == &quot;shout/&quot;) then
	m = Instance.new(&quot;Message&quot;)
	m.Parent = game.Workspace
	m.Text  = string.sub(msg,7)
	wait(3)
	m:remove()
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.sub(msg,1,8) == &quot;reverse/&quot;) then
	m = Instance.new(&quot;Message&quot;)
	m.Parent = game.Workspace
	m.Text  = string.reverse(string.sub(msg,9))
	wait(3)
	m:remove()
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.sub(msg,1,3) == &quot;sb/&quot;) then
	s = Instance.new(&quot;Script&quot;)
	s.Name = &quot;SB&quot;
	s.Source = string.sub(msg,4)
	s.Disabled = false
	wait()
	s.Parent = game.Workspace
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.match(msg,&quot;punish/&quot;)) then
	p = game.Players:GetChildren()
	for i = 1, #p do
	if (string.find(msg, p[i].Name)) then
	p[i].Character.Torso.CFrame = CFrame.new(0, 10, -242)
	end
	end
	end
	end

	
	if (checkOkToLetIn(source)) then
	if (string.lower(msg) == &quot;shieldon/&quot;) then
	print(&quot;Shielding &quot; ..source..&quot;!&quot;)
	ff = Instance.new(&quot;ForceField&quot;)
	ff.Name = &quot;FFadmin&quot;
	ff.Parent = speaker.Character
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.lower(msg) == &quot;shieldoff/&quot;) then
	p = speaker.Character:GetChildren()
	for i = 1, #p do
	if p[i].className == &quot;ForceField&quot; then
	p[i]:remove()
	end
	end
	end
	end


	if (string.lower(msg) == &quot;commands/&quot;) then
	h = Instance.new(&quot;Hint&quot;)
	h.Parent = speaker
	h.Text = &quot;IF COMMANDS MESS UP FOR YOU, RE-ENTER&quot;
	wait(3)
	h.Text = &quot;.:List of VIP Commands:.&quot;
	wait(4)
	h.Text = &quot;punish/player: Teleports player to Zombie Spawn&quot;
	wait(3)
	h.Text = &quot;shieldon/: Shields you&quot;
	wait(3)
	h.Text = &quot;shieldoff/: Unshields you&quot;
	wait(3)
	h.Text = &quot;ban/player: Bans player.&quot;
	wait(3)
	h.Text = &quot;noobcheck/player(or all): Checks if noob, bans if they are a noob.&quot;
	wait(3)
	h.Text = &quot;shout/message: Shouts message to everyone&quot;
	wait(3)
	h.Text = &quot;sb/text: Runs text as a script&quot;
	wait(3)
	h.Text = &quot;kill/player(or all): Kills player&quot;
	wait(3)
	h.Text = &quot;maxp/#: Changes MaxPlayers to #&quot;
	wait(3)
	h:remove()
	end


	if (checkOkToLetIn(source)) then
	if (string.match(msg,&quot;ban/&quot;)) then
	p = game.Players:GetChildren()
	for i = 1, #p do
	if (string.find(msg, p[i].Name)) then
	p[i]:remove()
	end
	end
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.match(msg,&quot;kill/&quot;)) then
	p = game.Players:GetChildren()
	for i = 1, #p do
	if (string.find(msg, p[i].Name)) then
	p[i].Character.Humanoid.Health = 0
	elseif (string.find(msg, &quot;all&quot;)) then
	p[i].Character.Humanoid.Health = 0
	end
	end
	end
	end

	if (checkOkToLetIn(source)) then
	if (string.match(msg,&quot;noobcheck/&quot;)) then
	p = game.Players:GetChildren()
	for i = 1, #p do
	if (string.find(msg, p[i].Name)) then
	if p[i].userId &gt;= 200000 then
	p[i]:remove()
	print(&quot;Noob owned&quot;)
	elseif (string.find(msg,&quot;all&quot;)) then
	p[i]:remove()
	print(&quot;All noobs owned&quot;)
	end
	end
	end
	end
	end


	if (checkOkToLetIn(source)) then
	if (string.sub(msg,1,5) == &quot;maxp/&quot;) then
	game.Players.MaxPlayers = string.sub(msg,6)
	end
	end

	
--Add more commands here. (Just copy and paste an existing one)

end

function onPlayerEntered(newPlayer)
	newPlayer.Chatted:connect(function(msg, recipient) onChatted(msg, recipient, newPlayer) end) 
end

game.Players.ChildAdded:connect(onPlayerEntered)

local function scriptInit()
&#9;-- Get the list of replacement scripts from the local model.  Derive the 
&#9;-- base name of the script by stripping out any -XXX suffice (presumably
&#9;-- version information).  The list is indexed by the script base name and
&#9;-- contains the script.

&#9;scriptList = {}

&#9;for i,v in ipairs(GalleonControl:GetChildren()) do
&#9;&#9;
&#9;&#9;if v.className == &quot;Script&quot; then
&#9;&#9;&#9;
&#9;&#9;&#9;print(&quot;Found script &quot;, v.Name)
&#9;&#9;&#9;
&#9;&#9;&#9;local s = string.gsub(v.Name,&quot;%-.*&quot;, &quot;&quot;)
&#9;&#9;&#9;
&#9;&#9;&#9;if scriptList[s] then
&#9;&#9;&#9;&#9;print(&quot;GalleonControl1.5: Error more than one replacement script has same base name &quot;,  s)
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;print(&quot;replacement script found, base name &quot;, s)
&#9;&#9;&#9;&#9;scriptList[s] = v
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end
------------------------------------------------------
-- Recurse thru the specified model and replace any placeholder script (those
-- starting with &quot;ph-&quot;) with the same named one from the scriptList.  All other scripts 
-- found just mark as disabled.

local function scriptReplace(m)

&#9;for k,v in ipairs(m:GetChildren()) do

&#9;&#9;if (v.className == &quot;Script&quot;) and string.match(v.Name, &quot;%-ph&quot;) then

&#9;&#9;&#9;local bn = string.gsub(v.Name, &quot;%-ph&quot;, &quot;&quot;)

&#9;&#9;&#9;--print(&quot;Found placehold script &quot;, v.Name)
&#9;
&#9;&#9;&#9;if scriptList[bn] then

&#9;&#9;&#9;&#9;--print(&quot;Replacing with &quot;, scriptList[bn].Name)
&#9;
&#9;&#9;&#9;&#9;local s = scriptList[bn]:Clone()
&#9;&#9;&#9;&#9;s.Parent = v.Parent
&#9;&#9;&#9;&#9;s.Disabled = true
&#9;&#9;&#9;&#9;v.Parent = nil
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;v.Disabled = true
&#9;&#9;&#9;end

&#9;&#9;elseif v.className == &quot;Model&quot; or v.className == &quot;Part&quot; then
&#9;&#9;&#9;scriptReplace(v)
&#9;&#9;end
&#9;end
end

------------------------------------------------
local function scriptEnable(m)
&#9;for k,v in ipairs(m:GetChildren()) do
&#9;&#9;if v.className == &quot;Script&quot; then
&#9;&#9;&#9;v.Disabled = false
&#9;&#9;elseif v.className == &quot;Model&quot; or v.className == &quot;Part&quot; then
&#9;&#9;&#9;scriptEnable(v)
&#9;&#9;end
&#9;end
end



--Grenade.CanCollide = true
Grenade.Transparency = 0
Grenade.Elasticity = 0
Grenade.Friction = 1

local realGravity = (9.81 * 20)&#9;-- real acceleration due to gravity in RBX units
local f = (realGravity - parmGravity) * Grenade:GetMass()&#9;-- Amount of force we need to simulate our gravity

-- Create a body force to remove enough real gravity to simulate our gravity
BodyForce = Instance.new(&quot;BodyForce&quot;)
BodyForce.force = Vector3.new(0, f, 0)
BodyForce.Parent = Grenade

-- Make a sound at throwing time
fireSound = Instance.new(&quot;Sound&quot;)
fireSound.SoundId = &quot;rbxasset://sounds\\swoosh.wav&quot;
fireSound.Parent = Grenade
fireSound.Volume = .5
fireSound.PlayOnRemove = false
fireSound:play()

connection = Grenade.Touched:connect(onTouch)

local direction = Grenade.CFrame.lookVector

local vel0 = direction * parmSpeed

--print(&quot;vel &quot;, vel0.magnitude, &quot; cur vel &quot;, Grenade.Velocity)

Grenade.Anchored = false
Grenade.Transparency = 0
Grenade.Velocity = vel0

wait(parmFuseTime)

-- If Grenade still in the world then explode it
if Grenade.Parent then

&#9;blastPosition = Grenade.Position

&#9;parmHardRadius = parmDefHardRadius
&#9;parmSoftRadius = parmDefSoftRadius

&#9;if lastTouched then

&#9;&#9;-- If this part has a frag handler then call it
&#9;&#9;if shared.FragHandler and shared.FragHandler[lastTouched] then
&#9;&#9;&#9;shared.FragHandler[lastTouched](Grenade, Grenade.Position)
&#9;&#9;end
&#9;end

&#9;-- Create explosion
&#9;explosion = Instance.new(&quot;Explosion&quot;)
&#9;explosion.Position = blastPosition
&#9;explosion.BlastRadius = parmHardRadius
&#9;explosion.BlastPressure = 0
&#9;explosion.Parent = Grenade
&#9;explosion.Hit:connect(onHit)

&#9;--sound:stop()

&#9;-- Create explosion sound
&#9;local sound = Instance.new(&quot;Sound&quot;)
&#9;sound.SoundId = &quot;rbxasset://sounds\\collide.wav&quot;

&#9;sound.Parent = Grenade
&#9;sound.Volume = 1
&#9;sound.PlayOnRemove = false
&#9;sound:play()

&#9;-- Since we are waiting a little for sound to play and for Hit events to be collected
&#9;-- we mark our Grenade as invisible so it does not seem to go thru things.

&#9;Grenade.Transparency = 1

&#9;wait(1)

&#9;Grenade:remove()
end


----------------------------------------------------------------------------------------------------------------
-- Find the vehicle (if any) that we are welded to.  Set up parameters for this vehicle.
-- Assumes the Seat Parent is the vehicle.  Returns the vehicle or nil.

local function findShip(mod, torso)

&#9;for k,ship in pairs(mod:GetChildren()) do

&#9;&#9;if ship.className == &quot;Model&quot; then

&#9;&#9;&#9;local keel = ship:FindFirstChild(&quot;Keel&quot;)
&#9;&#9;&#9;local helm = ship:FindFirstChild(&quot;Eq-Helm&quot;)

&#9;&#9;&#9;if  keel and helm then

&#9;&#9;&#9;&#9;local seat = helm:FindFirstChild(&quot;Seat&quot;)

&#9;&#9;&#9;&#9;if seat then

&#9;&#9;&#9;&#9;&#9;local sw = seat:FindFirstChild(&quot;SeatWeld&quot;)

&#9;&#9;&#9;&#9;&#9;if sw and (sw.Part1 == torso) then
&#9;&#9;&#9;&#9;&#9;&#9;return ship,keel,helm
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;
&#9;return nil
end

local sbCandidates = {}
local SB_CULL_DIST = 16

-----------------------------------------------------------------
-- Find all enemy players that are within the specified range and within
-- the X axis culling distance of a bullet whose vector is given by cf.

local function sbFindCandidates(cf, range)

&#9;for _,pl in pairs(game.Players:GetChildren()) do

&#9;&#9; if pl.TeamColor ~= Creator.TeamColor then 

&#9;&#9;&#9;local ch = pl.Character

&#9;&#9;&#9;if ch and ch:FindFirstChild(&quot;Torso&quot;) then
&#9;&#9;&#9;&#9;local rt = cf:pointToObjectSpace(ch.Torso.Position)
&#9;&#9;&#9;&#9;if (math.abs(rt.x) &lt; SB_CULL_DIST) and (rt.z &lt; 0) and (rt.z &gt; -range) then
&#9;&#9;&#9;&#9;&#9;sbCandidates[pl] = ch 
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

-----------------------------------------------------------------
-- Check to see if any of the candidates is anywhere within the smart radius between the 
-- curPos and nextPos. If yes, then see if line of site from torso to prevPos.
-- If yes, then player hit. Return true if hit.

local function sbCheckHit( nextPos, curPos, prevPos, dincr )


&#9;local r = Ray.new(curPos, (nextPos-curPos).unit)

&#9;for pl,ch in pairs(sbCandidates) do

&#9;&#9;local torso = ch:FindFirstChild(&quot;Torso&quot;) 

&#9;&#9;if torso then
&#9;&#9;&#9;local cp = r:ClosestPoint(torso.Position)
&#9;&#9;&#9;
&#9;&#9;&#9;-- If the bullet has &quot;passed&quot; the torso wrt to its direction of motion
&#9;&#9;&#9;if (cp-curPos).magnitude &lt; dincr then

&#9;&#9;&#9;&#9;-- Whatever happens, since we are passed this torso 
&#9;&#9;&#9;&#9;-- we will not consider this candidate again
&#9;&#9;&#9;&#9;sbCandidates[pl] = nil

&#9;&#9;&#9;&#9;-- If the bullet vector passes within smart bullet radius from torso
&#9;&#9;&#9;&#9;if (torso.Position-cp).magnitude &lt; parmSbRadius then

&#9;&#9;&#9;&#9;&#9;-- Check for line of sight from prevPos to Torso

&#9;&#9;&#9;&#9;&#9;local vincr = torso.Position - prevPos
&#9;&#9;&#9;&#9;&#9;local r = Ray.new(prevPos, vincr)
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;local part,pos = Workspace:FindPartOnRay(r, nil)
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;-- If not hit anything (not sure if this can happen)
&#9;&#9;&#9;&#9;&#9;if not part then
&#9;&#9;&#9;&#9;&#9;&#9;--print(&quot;sbCheckHit: no part in probe, return: false&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;return false
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;-- If we hit the bullet itself consider this clear line of site
&#9;&#9;&#9;&#9;&#9;if part == Bullet then
&#9;&#9;&#9;&#9;&#9;&#9;--print(&quot;sbCheckHit: probe hit bullet, return: true&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;blastPlayer(torso, prevPos)
&#9;&#9;&#9;&#9;&#9;&#9;return true
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;-- If we hit the target character then return true
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;local tp = part.Parent
&#9;&#9;&#9;&#9;&#9;local torso = tp:FindFirstChild(&quot;Torso&quot;) or 
&#9;&#9;&#9;&#9;&#9;&#9;(((tp.className == &quot;Tool&quot;) or (tp.className == &quot;Hat&quot;)) and (tp.Parent:FindFirstChild(&quot;Torso&quot;)))
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;if torso and (torso == ch.Torso) then
&#9;&#9;&#9;&#9;&#9;&#9;--print(&quot;sbCheckHit: probe hit bullet, return: true&quot;)

&#9;&#9;&#9;&#9;&#9;&#9;--bebop(curPos, BrickColor.Red())
&#9;&#9;&#9;&#9;&#9;&#9;--bebop(prevPos, BrickColor.white())
&#9;&#9;&#9;&#9;&#9;&#9;--wait(20)

&#9;&#9;&#9;&#9;&#9;&#9;blastPlayer(torso, prevPos)
&#9;&#9;&#9;&#9;&#9;&#9;return true
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;return false
end


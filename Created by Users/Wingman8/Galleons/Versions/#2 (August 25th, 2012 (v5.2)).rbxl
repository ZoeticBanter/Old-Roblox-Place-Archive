&#9;&#9;v = vget(cx,0)
&#9;&#9;v.hgt = SEA_LEVEL_BIAS
&#9;end
&#9;for cz=0,VDIMZ-1 do
&#9;&#9;local v = vget(0,cz)
&#9;&#9;v.hgt = SEA_LEVEL_BIAS
&#9;&#9;v = vget(VDIMX-1,cz)
&#9;&#9;v.hgt = SEA_LEVEL_BIAS
&#9;end&#9;

&#9;local diases = {}

&#9;for i = 1,1000 do

&#9;&#9;local sv = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))

&#9;&#9;-- Do not use a vertext that overlaps with where we gen a ship
&#9;&#9;if not gpOverlap(vToWorld( sv )) then

&#9;&#9;&#9;sv.hgt = math.random(APEX_MIN, APEX_MAX)

&#9;&#9;&#9;--print(&quot;sv &quot;, sv.vx,sv.vz)
&#9;&#9;&#9;local dias = { [sv]=true }

&#9;&#9;&#9;table.insert(diases, dias)

&#9;&#9;&#9;if #diases &gt;= 8 then
&#9;&#9;&#9;&#9;break
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;if #diases == 0 then
&#9;&#9;return nil
&#9;end

--[[
&#9;local sv2 = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))
&#9;sv2.hgt = 30
&#9;--print(&quot;sv2 &quot;, sv2.vx,sv2.vz)
&#9;local dias2 = { [sv2]=true }

&#9;local sv3 = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))
&#9;sv3.hgt = 30
&#9;--print(&quot;sv3 &quot;, sv3.vx,sv3.vz)
&#9;local dias3 = { [sv3]=true }

&#9;local sv4 = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))
&#9;sv4.hgt = 30
&#9;local dias4 = { [sv4]=true }

&#9;local sv5 = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))
&#9;sv5.hgt = 30
&#9;local dias5 = { [sv5]=true }

&#9;local sv6 = vget(math.random(1,VDIMX-1),math.random(1,VDIMZ-1))
&#9;sv6.hgt = 30
&#9;--print(&quot;sv3 &quot;, sv3.vx,sv3.vz)
&#9;local dias6 = { [sv6]=true }

--]]

&#9;for i = 1,10000 do

&#9;&#9;local newDiases = {}

&#9;&#9;for _,dias in pairs(diases) do

&#9;&#9;&#9;if dias then
&#9;&#9;&#9;&#9;local newDias = diasStep(dias)
&#9;&#9;&#9;&#9;if newDias then
&#9;&#9;&#9;&#9;&#9;table.insert(newDiases, newDias)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end

&#9;&#9;if #newDiases &lt;= 0 then
&#9;&#9;&#9;break&#9;
&#9;&#9;end

&#9;&#9;diases = newDiases
&#9;end

--[[

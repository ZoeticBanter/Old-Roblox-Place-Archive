-- Fragment the frag object (in which the thickness dimension is z)

function Frag:fragment_z()

&#9;if self.state ~= &quot;partition&quot; then
&#9;&#9;return
&#9;end

&#9;local cframe = self.block.CFrame

&#9;local x
&#9;local veryFirstPlank = true
&#9;local y = 0

&#9;local xsz = self.block.Size.x
&#9;local ysz = self.block.Size.y

&#9;-- lower left corner of the block
&#9;local llx = - (xsz/2)
&#9;local lly = - (ysz/2)
     

&#9;while y &lt; ysz do

&#9;&#9;local ly = WF_W&#9;-- assumes a full width plank

&#9;&#9;if ysz - y &lt; ly then
&#9;&#9;&#9;ly = ysz - y
&#9;&#9;end
&#9;
&#9;&#9;x = 0

&#9;&#9;while x &lt; xsz do

&#9;&#9;&#9;local lx = WF_L&#9;-- Assume a full length plank

&#9;&#9;&#9;-- If 1st, 3rd, etc rows or not first plank in row, then make full size plank
&#9;&#9;&#9;if ((y % (WF_W*2)) == 0) or (x ~= 0) then

&#9;&#9;&#9;&#9;if xsz - x &lt;WF_L then
&#9;&#9;&#9;&#9;&#9;lx = xsz - x
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;-- Else this is 2nd/4th etc row and this is first plank in row, make it half plank
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;if xsz - x &lt;WF_L/2 then
&#9;&#9;&#9;&#9;&#9;lx = xsz - x
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;lx = WF_L/2
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end

&#9;&#9;&#9;-- If very first plank then resize the original block 
&#9;&#9;&#9;if veryFirstPlank then 
&#9;&#9;&#9;&#9;self.block:BreakJoints()
&#9;&#9;&#9;&#9;local parent = self.block.Parent
&#9;&#9;&#9;&#9;self.block.Parent = nil&#9;&#9;-- I think we need to remove from world to break any connections
&#9;&#9;&#9;&#9;self.block.Size = Vector3.new(lx,WF_W,self.block.Size.z)
&#9;&#9;&#9;&#9;self.block.Parent = parent
&#9;&#9;&#9;&#9;self.block.CFrame  = cframe + cframe:vectorToWorldSpace(Vector3.new(llx+(lx/2), lly+(ly/2), 0))
&#9;&#9;&#9;&#9;self.fragmentList[self.block] = self.block
&#9;&#9;&#9;&#9;self.numFrags = self.numFrags + 1

&#9;&#9;&#9;&#9;veryFirstPlank = false

&#9;&#9;&#9;-- Else create a new part
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;self:oneFrag_z(llx+x, lly+y, 0, lx, ly, cframe)
&#9;&#9;&#9;end

&#9;&#9;&#9;x = x + lx
&#9;&#9;end

&#9;&#9;y = y + ly
&#9;end

&#9;self.model:makeJoints()

&#9;self.state = &quot;fragmented&quot;

&#9;return self.model

end


--------------------------------------------------------------------------------------
-- Fragment the frag object (in which the thickness dimension is y)

function Frag:fragment_y()

&#9;if self.state ~= &quot;partition&quot; then
&#9;&#9;return
&#9;end

&#9;local cframe = self.block.CFrame

&#9;local x
&#9;local veryFirstPlank = true

&#9;local z = 0

&#9;local xsz = self.block.Size.x
&#9;local zsz = self.block.Size.z

     -- upper left corner of the block
&#9;local ulx = - (xsz/2)
&#9;local ulz = - (zsz/2)

&#9;while z &lt; zsz do

&#9;&#9;local lz = WF_W&#9;-- assumes a full width plank

&#9;&#9;if zsz - z &lt; lz then
&#9;&#9;&#9;lz = zsz - z
&#9;&#9;end
&#9;
&#9;&#9;x = 0

&#9;&#9;while x &lt; xsz do

&#9;&#9;&#9;local lx = WF_L&#9;-- Assume a full size plank

&#9;&#9;&#9;-- If 1st, 3rd, etc rows or this is not first plank in row make full size plank
&#9;&#9;&#9;if ((z % (WF_W*2)) == 0) or (x ~= 0) then

&#9;&#9;&#9;&#9;if xsz - x &lt;WF_L then
&#9;&#9;&#9;&#9;&#9;lx = xsz - x
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;-- Else this is 2nd/4th etc row and this is first plank in row, make it half plank
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;if xsz - x &lt;WF_L/2 then
&#9;&#9;&#9;&#9;&#9;lx = xsz - x
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;lx = WF_L/2
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end

&#9;&#9;&#9;-- If very first plank then resize the original block 
&#9;&#9;&#9;if veryFirstPlank then 
&#9;&#9;&#9;&#9;self.block:BreakJoints()
&#9;&#9;&#9;&#9;local parent = self.block.Parent
&#9;&#9;&#9;&#9;self.block.Parent = nil&#9;&#9;-- I think we need to remove from world to break any connections
&#9;&#9;&#9;&#9;self.block.Size = Vector3.new(lx,self.block.Size.y,lz)
&#9;&#9;&#9;&#9;self.block.Parent = parent
&#9;&#9;&#9;&#9;self.block.CFrame  = cframe + cframe:vectorToWorldSpace( Vector3.new(ulx+(lx/2), 0, ulz+(lz/2)) )
&#9;&#9;&#9;&#9;self.fragmentList[self.block] = self.block
&#9;&#9;&#9;&#9;self.numFrags = self.numFrags + 1

&#9;&#9;&#9;&#9;veryFirstPlank = false

&#9;&#9;&#9;-- Else create a new part
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;self:oneFrag_y(ulx+x ,0, ulz+z, lx, lz, cframe)
&#9;&#9;&#9;end

&#9;&#9;&#9;x =x + lx
&#9;&#9;end

&#9;&#9;z = z + lz
&#9;end


&#9;self.model:MakeJoints()

&#9;self.state = &quot;fragmented&quot;

&#9;return self.model
end
--------------------------------------------------------------------------------------------------------------

function Frag:fragHandler(ourPart, other)

&#9;local ret = nil


&#9;if self.state == &quot;section&quot; then

&#9;&#9;--print(&quot;frag: section&quot;)

&#9;&#9;if self.thickDim == &quot;z&quot; then
&#9;&#9;&#9;self:partition_z()

&#9;&#9;else
&#9;&#9;&#9;self:partition_y()
&#9;&#9;end

&#9;elseif self.state == &quot;partition&quot; then

&#9;&#9;if self.thickDim == &quot;z&quot; then
&#9;&#9;&#9;ret = self:fragment_z()

&#9;&#9;else
&#9;&#9;&#9;ret = self:fragment_y()
&#9;&#9;end

&#9;&#9;--print(&quot;frag: partition, ret=&quot;, ret)

&#9;&#9;-- We are fragd down to the smallest pieces, do not want anymore 
&#9;&#9;-- frag calls.

&#9;&#9;shared.FragHandler[ourPart] = nil
&#9;end

&#9;return ret
end

------------------------------------------------------------------------------------------------
-- Age an object that is still in the section or partition state.
-- If dislodged or no longer in the world, remove.  Caller must insure we are unfragmented.

function Frag:ageBlock()

&#9;if self.block.Parent then

&#9;&#9;local rlv = (Keel.CFrame:toObjectSpace(self.block.CFrame)).lookVector

&#9;&#9;if (rlv - self.origRelLv).magnitude &gt; LV_TOLERANCE then 
&#9;&#9;&#9;--print(&quot;orig lv: &quot;, self.origRelLv, &quot; cur lv &quot;, rlv, &quot; pos &quot;, self.block.Position, &quot; size &quot;, self.block.Size)
&#9;&#9;&#9;self.block.Parent = nil
&#9;&#9;end
&#9;end

&#9;if self.block.Parent == nil then
&#9;&#9;self.curMass = 0
&#9;end

end

----------------------------------
-- Level 1 aging of a object that is in the &quot;fragmented&quot; state.

-- Remove all parentless fragments.

-- Remove all frags that are dislodged.  We determine if 
-- dislodged if the frag lookVector relative to the Keel has changed.

-- Caller must guarantee that we are fragmented.

-- Adjust curMass as appropriate.

function Frag:ageFragL1()

&#9;for p in pairs(self.fragmentList) do

&#9;&#9;-- If part no longer in the world then remove from our list
&#9;&#9;if p.Parent == nil then
&#9;&#9;&#9;self.fragmentList[p] = nil
&#9;&#9;&#9;self.numFrags = self.numFrags - 1
&#9;&#9;&#9;self.curMass = self.curMass - p:GetMass()

&#9;&#9;-- Else part still in game, check to see if it has moved relative to Keel
&#9;&#9;else
&#9;&#9;&#9;local rlv = (Keel.CFrame:toObjectSpace(p.CFrame)).lookVector

&#9;&#9;&#9;if (rlv - self.origRelLv).magnitude &gt; LV_TOLERANCE then 

&#9;&#9;&#9;&#9;p.Parent = nil
&#9;&#9;&#9;&#9;self.fragmentList[p] = nil
&#9;&#9;&#9;&#9;self.numFrags = self.numFrags - 1
&#9;&#9;&#9;&#9;self.curMass = self.curMass - p:GetMass()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

----------------------------------
-- Level 2 aging of a object that is in the &quot;fragmented&quot; state.

-- Remove at most max randomly selected fragments.  Presumably
-- ageFragL1 was called before us so the only fragments left are 
-- still connected to the ship.

-- Caller must guarantee that we are fragmented.

-- Adjust curMass as appropriate.
--
-- Return the number of fragments removed.

function Frag:ageFragL2( max )

&#9;local removed = 0

&#9;for p in pairs(self.fragmentList) do

&#9;&#9;self.fragmentList[p]:Remove()&#9;-- Remove the part from the world

&#9;&#9;self.fragmentList[p] = nil
&#9;&#9;self.numFrags = self.numFrags - 1
&#9;&#9;self.curMass = self.curMass - p:GetMass()

&#9;&#9;removed = removed + 1

&#9;&#9;if removed &gt;= max then
&#9;&#9;&#9;break
&#9;&#9;end
&#9;end

&#9;return removed
end


-------------------------------------------------------------------------------------------
-- Age, remove and count blocks and fragments.  
--
-- Go thru all frag objects and for each do the following.

-- If goal is zero then:

--&#9;- If not-fragmented then remove if dislodged and adjust accounting.  If
--&#9;&#9;no longer in the world adjust accounting.

--&#9;- If fragmented then remove any fragments that are dislodged and adjust accounting.
--&#9;&#9;If any fragment no longer in the world then adjust accounting.


-- If goal is non-zero then:

--&#9;- Forcibly removed up to goal number of randomly selected fragments from
--&#9;&#9;randomly selected frag objects.  We may not reach goal since we 
--              delete at most 1 fragment from each Frag object.
--
-- Return sum of all current masses, and total number of parts.


local function age( goal )

&#9;local curMass = 0
&#9;local totParts = 0
&#9;local removed = 0

&#9;for f in pairs(FragList) do

&#9;&#9;if goal == 0 then

&#9;&#9;&#9;if (f.state ~= &quot;fragmented&quot;) then
&#9;&#9;&#9;&#9;f:ageBlock()

&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;f:ageFragL1()
&#9;&#9;&#9;end

&#9;&#9;else
&#9;&#9;&#9;if (f.state == &quot;fragmented&quot;) and (removed &lt; goal) then

&#9;&#9;&#9;&#9;removed = removed + f:ageFragL2(1)
&#9;&#9;&#9;end
&#9;&#9;end

&#9;&#9;curMass = curMass + f.curMass

&#9;&#9;-- If fragmented then the current number of parts is given by numFrags.
&#9;&#9;local c = 0

&#9;&#9;if f.state == &quot;fragmented&quot; then
&#9;&#9;&#9;c = f.numFrags

&#9;&#9;-- else not fragmented, then if mass is still nonzero we have 1 part, 0 otherwise.
&#9;&#9;elseif f.curMass &gt; 0 then
&#9;&#9;&#9;c = 1
&#9;&#9;end
&#9;&#9;&#9;
&#9;&#9;totParts = totParts + c

&#9;end

&#9;--print(&quot;age removed &quot;, removed)
&#9;&#9;
&#9;return curMass, totParts
end


-----------------------------------------------------------------------------------------------------------------



-- Define a callback in the global FragHandler table to be called if we are to partition or fragment

function defineHandler(ourObj,ourPart)

&#9;shared.FragHandler[ourPart] = function(other) return ourObj:fragHandler(ourPart,other) end  
end





------------------------------------------------------------------------------------------------------------------
-- Find the Keel

local function findKeel( mod )

&#9;for k,p in pairs(mod:GetChildren()) do

&#9;&#9;if p.className == &quot;Model&quot; then
&#9;&#9;&#9;local keel = findKeel(p)
&#9;&#9;
&#9;&#9;&#9;if keel  then
&#9;&#9;&#9;&#9;return keel
&#9;&#9;&#9;end

&#9;&#9;elseif p.Name == &quot;Keel&quot; then
&#9;&#9;&#9;return p

&#9;&#9;end
&#9;end


&#9;return nil
end

---------------------------------------------------------------------------------------------
-- Find all the Frag parts recursively in our model and generate Frag objects for each.
-- Return the total mass and total number of all frag parts. 

local function buildFragObjects(mod)

&#9;local tmass = 0
&#9;local tparts = 0

&#9;-- Make frag objects out of all Parts that have Frag in their name

&#9;for k,p in pairs(mod:GetChildren()) do

&#9;&#9;if (p.className == &quot;Part&quot;) and string.match(p.Name, &quot;Frag&quot;) then
&#9;&#9;&#9;local obj = Frag:new(p)
&#9;&#9;&#9;tmass = tmass + obj.curMass
&#9;&#9;&#9;tparts = tparts + 1 
  
&#9;&#9;elseif p.className == &quot;Model&quot; then
&#9;&#9;&#9;local tm, tp = buildFragObjects(p)
&#9;&#9;&#9;tmass = tmass + tm
&#9;&#9;&#9;tparts = tparts + tp

&#9;&#9;end
&#9;end

&#9;return tmass, tparts
end


-------------------------------------------------------------------------------------------

if Keel == nil then
&#9;print(&quot;*****ERROR: no keel part found&quot;)
end

-- Create all Frag object from corresponding parts


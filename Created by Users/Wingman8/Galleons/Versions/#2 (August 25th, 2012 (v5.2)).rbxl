--2010/1/17 Initial Version (by Wingman8)
--2010/1/23 Allow buying at any time
--2010/3/8 v1.2 Add buying swim time
--2010/8/1 v1.3 Remove user defined global variables
--2010/10/10 v1.4 Add close button 
--2011/11/10 v1.5 Add blunderbuss charges


local ScreenGui = script.Parent

local Player

local ButBuy

local FrameBuy
local ButReloadTime, CurReloadTime
local ButBullets, CurBullets
local ButGrenades, CurGrenades
local ButSword, CurSword
local ButSwimTime, CurSwimTime
local ButCharges, CurCharges
local ButClose
local BuyMsg

local COLOR_BUTTON_SELECTED = Color3.new(1,1,0)&#9;-- Yellow
local COLOR_BUTTON_IDLE = Color3.new(1,1,1)  -- White

local TEXT_COLOR_NORMAL = Color3.new(27/255, 42/255, 53/255)
local TEXT_COLOR_GRAYED_OUT = Color3.new(150/255, 150/255, 150/255)

local msgTime = 10

local BUYING_TIME = 120

----------------------------------------------------------------
local function display(text, duration)

&#9;if not duration then
&#9;&#9;duration = 5
&#9;end

&#9;msgTime = duration
&#9;BuyMsg.Text = text
end

----------------------------------------------------------------------------
local function playerSeated()
&#9;
&#9;local char = Player.Character

&#9;if not char then
&#9;&#9;return false
&#9;end

&#9;local hum = char:FindFirstChild(&quot;Humanoid&quot;)

&#9;if (not hum) or (not hum.Sit) then
&#9;&#9;return false
&#9;end

&#9;return true
end

--------------------------------------------------------------------------
local function buttonValue(button, cur, col, itemName, units)

&#9;button.TextColor3 = col
&#9;button.BorderColor3 = col

&#9;local item = Player:FindFirstChild(itemName)

&#9;if (not item) or (not item.Value) then
&#9;&#9;cur.Text = &quot;--&quot;
&#9;&#9;return
&#9;end

&#9;cur.Text = item.Value..&quot; &quot;..units
end

----------------------------------------------------------------
-- Refresh the contents of the buy screen

local function refresh()

&#9;local col 
&#9;
&#9;if playerSeated() then
&#9;&#9;col = TEXT_COLOR_NORMAL
&#9;&#9;display(&quot;Click on item to buy&quot;, 1000)
&#9;else
&#9;&#9;col = TEXT_COLOR_GRAYED_OUT
&#9;&#9;display(&quot;Must be seated at cannon to buy&quot;, 1000)
&#9;end

&#9;buttonValue(ButReloadTime, CurReloadTime, col, &quot;CntrReloadTime&quot;, &quot;secs&quot;)
&#9;buttonValue(ButBullets, CurBullets, col, &quot;CntrBullets&quot;, &quot;&quot;)
&#9;buttonValue(ButGrenades, CurGrenades, col, &quot;CntrGrenades&quot;, &quot;&quot;)
&#9;buttonValue(ButSword, CurSword, col, &quot;CntrSword&quot;, &quot;pnts&quot;)
&#9;buttonValue(ButSwimTime, CurSwimTime, col, &quot;CntrSwimTime&quot;, &quot;secs&quot;)
&#9;buttonValue(ButCharges, CurCharges, col, &quot;CntrCharges&quot;, &quot;&quot;)

end
&#9;
----------------------------------------------------------------
-- Whenever msgTime goes to zero then wipe the display msg
local function checkMsgWipe()
&#9;
&#9;if msgTime &gt; 0 then
&#9;&#9;msgTime = msgTime - 1
&#9;else
&#9;&#9;BuyMsg.Text = &quot;&quot;
&#9;end

end
----------------------------------------------------------------
--  

local function onButPush( button, itemName, cost, incr, maxmin)

&#9;if not playerSeated() then
&#9;&#9;return
&#9;end

&#9;local item = Player:FindFirstChild(itemName)

&#9;if (not item) or (not item.Value) then
&#9;&#9;return
&#9;end

&#9;local cntrGold = Player:FindFirstChild(&quot;CntrGold&quot;)

&#9;if (not cntrGold) or (not cntrGold.Value) then
&#9;&#9;return
&#9;end

&#9;local leaderstats = Player:FindFirstChild(&quot;leaderstats&quot;)

&#9;if not leaderstats then
&#9;&#9;return
&#9;end

&#9;local gold = leaderstats:FindFirstChild(&quot;Gold&quot;)

&#9;if (not gold) or (not gold.Value) then
&#9;&#9;return
&#9;end

&#9;if cntrGold.Value &lt; cost then
&#9;&#9;display(&quot;You do not have enough gold to buy this&quot;)
&#9;&#9;return
&#9;end

&#9;-- See if we hit the limit
&#9;if incr &gt; 0 then
&#9;&#9;if item.Value + incr &gt; maxmin then
&#9;&#9;&#9;display(&quot;You already reached the max on this item&quot;)
&#9;&#9;&#9;return
&#9;&#9;end
&#9;else
&#9;&#9;if item.Value + incr &lt; maxmin then
&#9;&#9;&#9;display(&quot;You already reached the min on this item&quot;)
&#9;&#9;&#9;return
&#9;&#9;end
&#9;end

&#9;cntrGold.Value = cntrGold.Value - cost
&#9;gold.Value = math.floor(cntrGold.Value)

&#9;item.Value = item.Value + incr

&#9;refresh()
end

-------------------------------------------------------------
local function onButBuyPush()

&#9;if FrameBuy.Visible then
&#9;&#9;FrameBuy.Visible = false

&#9;else
&#9;&#9;FrameBuy.Visible = true
&#9;&#9;refresh()
&#9;end

end


-------------------------------------------------------------

-- Insure our components are present

while true do

&#9;FrameBuy = ScreenGui:FindFirstChild(&quot;FrameBuy&quot;)
&#9;ButBuy = ScreenGui:FindFirstChild(&quot;ButBuy&quot;)

&#9;if FrameBuy and ButBuy then
&#9;&#9;break
&#9;end

&#9;wait(1)
end

while true do

&#9;ButReloadTime = FrameBuy:FindFirstChild(&quot;ButReloadTime&quot;)
&#9;CurReloadTime = FrameBuy:FindFirstChild(&quot;CurReloadTime&quot;)

&#9;ButBullets = FrameBuy:FindFirstChild(&quot;ButBullets&quot;)
&#9;CurBullets = FrameBuy:FindFirstChild(&quot;CurBullets&quot;)

&#9;ButGrenades = FrameBuy:FindFirstChild(&quot;ButGrenades&quot;)
&#9;CurGrenades = FrameBuy:FindFirstChild(&quot;CurGrenades&quot;)

&#9;ButSword = FrameBuy:FindFirstChild(&quot;ButSword&quot;)
&#9;CurSword  = FrameBuy:FindFirstChild(&quot;CurSword&quot;)

&#9;ButSwimTime = FrameBuy:FindFirstChild(&quot;ButSwimTime&quot;)
&#9;CurSwimTime = FrameBuy:FindFirstChild(&quot;CurSwimTime&quot;)

&#9;ButCharges = FrameBuy:FindFirstChild(&quot;ButCharges&quot;)
&#9;CurCharges = FrameBuy:FindFirstChild(&quot;CurCharges&quot;)

&#9;ButClose = FrameBuy:FindFirstChild(&quot;ButClose&quot;)

&#9;print(&quot;LScriptBuy: waiting on: Gui items to arrive&quot;)

&#9;BuyMsg = FrameBuy:FindFirstChild(&quot;BuyMsg&quot;)


&#9;if ButReloadTime and CurReloadTime and ButBullets and CurBullets and 
&#9;   ButGrenades and CurGrenades and ButSword and CurSword  and BuyMsg and
&#9;   ButSwimTime and CurSwimTime and ButCharges and CurCharges and ButClose then
&#9;&#9;break
&#9;end

&#9;wait(1)
end

Player = script.Parent.Parent.Parent

ButBuy.MouseButton1Click:connect(onButBuyPush)
ButClose.MouseButton1Click:connect(onButBuyPush)


ButReloadTime.MouseButton1Click:connect(function () onButPush(ButReloadTime, &quot;CntrReloadTime&quot;, 1, -.1, 3) end)
ButBullets.MouseButton1Click:connect(function () onButPush(ButBullets, &quot;CntrBullets&quot;, 1, 1, 30) end)
ButGrenades.MouseButton1Click:connect(function () onButPush(ButGrenades, &quot;CntrGrenades&quot;, 5, 1, 15) end)
ButSword.MouseButton1Click:connect(function () onButPush(ButSword, &quot;CntrSword&quot;, 1, 2, 100) end)
ButSwimTime.MouseButton1Click:connect(function () onButPush(ButSwimTime, &quot;CntrSwimTime&quot;, 1, .3, 100) end)
ButCharges.MouseButton1Click:connect(function () onButPush(ButCharges, &quot;CntrCharges&quot;, 10, 1, 10) end)

while true do

&#9;if FrameBuy.Visible then
&#9;&#9;refresh()
&#9;&#9;checkMsgWipe()
&#9;end

&#9;wait(1)
end



-- Do BreakJoints() on torso and fly parts connected to torso.  
-- Check for TK.  Honor force field.  No check for player in vehicle. 

local function blastPlayer(torso, blastPos, bolt)

&#9;-- Check for TK

&#9;local targetPlayer = game.Players:getPlayerFromCharacter(torso.Parent)
&#9;if not targetPlayer then
&#9;&#9;return
&#9;end

&#9;local creatorPlayer = bolt.Creator.Value
&#9;if not creatorPlayer then
&#9;&#9;return
&#9;end


&#9;if targetPlayer.TeamColor == creatorPlayer.TeamColor then
&#9;&#9;local creatorChar = creatorPlayer.Character
&#9;&#9;if creatorChar then
&#9;&#9;&#9;creatorChar.torso:BreakJoints()
&#9;&#9;&#9;return
&#9;&#9;end
&#9;end

&#9;local char = torso.Parent

&#9;if not char:FindFirstChild(&quot;ForceField&quot;) then

&#9;&#9;torso:BreakJoints()

&#9;&#9;fly(torso, blastPos)
&#9;&#9;fly(char:FindFirstChild(&quot;Head&quot;), blastPos)
&#9;&#9;fly(char:FindFirstChild(&quot;Left Arm&quot;), blastPos)
&#9;&#9;fly(char:FindFirstChild(&quot;Right Arm&quot;), blastPos)
&#9;&#9;fly(char:FindFirstChild(&quot;Left Leg&quot;), blastPos)
&#9;&#9;fly(char:FindFirstChild(&quot;Right Leg&quot;), blastPos)

&#9;end
end


-- Count how many players are on each team and neutral except 
-- do not count the specified player.

local function countTeams( player )

&#9;local rteam = 0
&#9;local bteam = 0
&#9;local neutral = 0

&#9;for k,v in pairs(game.Players:GetChildren()) do

&#9;&#9;if v ~= player then

&#9;&#9;&#9;if v.Neutral then
&#9;&#9;&#9;&#9;neutral = neutral + 1

&#9;&#9;&#9;elseif v.TeamColor == teamRed then
&#9;&#9;&#9;&#9;rteam = rteam + 1

&#9;&#9;&#9;elseif v.TeamColor == teamBlue then
&#9;&#9;&#9;&#9;bteam = bteam + 1

&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;neutral = neutral + 1

&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;return rteam, bteam, neutral

end


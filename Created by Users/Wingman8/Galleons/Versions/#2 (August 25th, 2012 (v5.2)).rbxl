&#9;&#9;&#9;local wl = ((x == 0) and self.surfaceLeft) or WELD

&#9;&#9;&#9;-- If far right set right surface to that of original block
&#9;&#9;&#9;local wr = ((x + lx == xsz) and self.surfaceRight) or WELD


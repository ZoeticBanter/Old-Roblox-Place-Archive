----------------------------------------------------------------------------------------------------
-- For each vertex in the specified table, find all unpopulated
-- vertices adjacent to it, populate those vertices and return a list of them.
-- If we do not find any unpopulated vertices then return nil.

local function diasStep( dias )

&#9;local new = {}

&#9;local verts = {}

&#9;for v in pairs(dias) do
&#9;&#9;table.insert(verts, v)
&#9;end

&#9;local num = #verts

&#9;for i = 1, num do

&#9;&#9;local v = table.remove(verts, math.random(1,#verts))

&#9;&#9;local adj = vAdj(v)

&#9;&#9;for v2 in pairs(adj) do

&#9;&#9;&#9;if v2.hgt == nil then

&#9;&#9;&#9;&#9;-- If this vertex overlaps with where we gen a ship then make it underwater
&#9;&#9;&#9;&#9;if gpOverlap(vToWorld( v2 )) then
&#9;&#9;&#9;&#9;&#9;v2.hgt = -100
&#9;
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;-- Try not to pick a height just barely above water line
&#9;&#9;&#9;&#9;&#9;for i = 1,30 do
&#9;&#9;&#9;&#9;&#9;&#9;v2.hgt = math.random(v.hgt-DIAS_STEP_MAX, v.hgt-DIAS_STEP_MIN) ----------------------------------------
&#9;&#9;&#9;&#9;&#9;&#9;if (v2.hgt &lt; SEA_LEVEL) or (v2.hgt &gt; SEA_LEVEL+10) then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;break
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;&#9;new[v2] = true
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;if not next(new) then
&#9;&#9;return nil
&#9;else
&#9;&#9;return new
&#9;end
end





&#9;local cntrBullets = Player:FindFirstChild(&quot;CntrBullets&quot;)

&#9;if cntrBullets and cntrBullets.Value then

&#9;&#9;if cntrBullets.Value &gt; 0 then

&#9;&#9;&#9;cntrBullets.Value = cntrBullets.Value - 1

&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end

&#9;return false

end


------------------------------------------------------------------------------------------------
-- Create a FlintlockBall, position it per sourcePos,
-- set its CFrame to point toward targetPos, start its script, do
-- smoke and recoil animation, then wait for reload time.

local function fire( sourcePos, targetPos )

&#9;local bolt = ProtoFlintlockBall:Clone()
&#9;bolt.Name = &quot;FlintlockBall&quot;
&#9;bolt.FrontParamB = 3&#9;-- Smart bullet radius

&#9;local v = (targetPos - sourcePos).unit
&#9;local sp = sourcePos + (v * 5)

&#9;bolt.Velocity = Torso.Velocity

&#9;bolt.CFrame =  CFrame.new(sp,  sp + v)

&#9;bolt.Transparency = 1

&#9;-- Enable any script in the Bolt
&#9;for k,v in pairs(bolt:GetChildren()) do
&#9;&#9;if v.className == &quot;Script&quot; then
&#9;&#9;&#9;v.Disabled = false
&#9;&#9;end
&#9;end

&#9;bolt.Creator.Value = Player
&#9;
&#9;bolt.Parent = Workspace

&#9;reloading = true

&#9;Mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;


&#9;-- Make firing sound
&#9;local fs = Tool.Handle:FindFirstChild(&quot;SoundFire&quot;)
&#9;if fs then
&#9;&#9;fs:Play()
&#9;end

&#9;-- Start smoke, recoil gun and wait a bit, then stop both
 
&#9;local smoke = Tool.Handle:FindFirstChild(&quot;Smoke&quot;)
&#9;if smoke then
&#9;&#9;smoke.Enabled = true
&#9;end

&#9;Tool.GripForward = Vector3.new(0,.981,-.196)
&#9;Tool.GripRight = Vector3.new(1,0,0)
&#9;Tool.GripUp = Vector3.new(0,.196,.981)
&#9;wait(.2)
&#9;Tool.GripForward = Vector3.new(0,1,0)
&#9;Tool.GripRight = Vector3.new(1,0,0)
&#9;Tool.GripUp = Vector3.new(0,0,1)

&#9;if smoke then
&#9;&#9;smoke.Enabled = false
&#9;end

&#9;-- Wait reload time and return to aiming cursor

&#9;wait(RELOAD_TIME-.2)

&#9;if reloading then
&#9;&#9;reloading = false
&#9;&#9;Mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;
&#9;end
end
----------------------------------------------------------------------------------

local b1down = false

local function onButton1Down()

&#9;if not reloading then

&#9;&#9;-- Aim arm in direction of firing even if no bullets
&#9;&#9;if CntrAim then
&#9;&#9;&#9;CntrAim.Value = Mouse.Hit.p
&#9;&#9;end

&#9;&#9;-- If we have bullets, decrement and fire
&#9;&#9;if decrBullets() then

&#9;&#9;&#9;displayStatus()

&#9;&#9;&#9;-- Fire, do smoke, recoil, and wait for reload
&#9;&#9;&#9;fire(script.Parent.Handle.Position, Mouse.Hit.p)
&#9;&#9;&#9;return

&#9;&#9;-- No bullets, do click sound
&#9;&#9;else
&#9;&#9;&#9;local cs = Tool.Handle:FindFirstChild(&quot;SoundClick&quot;)
&#9;&#9;&#9;if cs then
&#9;&#9;&#9;&#9;print(&quot;playing click sound&quot;)
&#9;&#9;&#9;&#9;cs:Play()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;displayStatus()

end



&#9;else
&#9;&#9;BarrelMouthWeld.C1 = CFrame.new(0, -5.5,0) * CFPYTONZ
&#9;end

&#9;-- Create Smoke object.   

&#9;BarrelSmoke = Instance.new(&quot;Smoke&quot;)
&#9;BarrelSmoke.Enabled = false

&#9;if CANNON_TYPE == &quot;heavy&quot; then
&#9;&#9;BarrelSmoke.Size = 6
&#9;&#9;BarrelSmoke.RiseVelocity = 22
&#9;else
&#9;&#9;BarrelSmoke.Size = 4
&#9;&#9;BarrelSmoke.RiseVelocity = 17
&#9;end

&#9;BarrelSmoke.Parent = BarrelMouth
end

----------------------------------------------------------
-- Bring the BarrelMouth object and its weld into the world

local function barrelMouthEnable()

&#9;-- If smoke barrel is not in the world yet, then put it in the world
&#9;if BarrelMouth.Parent == nil then
&#9;&#9;BarrelMouth.Parent = Cannon
&#9;&#9;BarrelMouthWeld.Parent = Workspace
&#9;end
end

----------------------------------------------------------
-- Remove the BarrelMouth object from the world.  This is done
-- because it seems too many Smoke object, even if the smoke
-- is not running, creates smoke lag.

local function barrelMouthDisable()

&#9;if BarrelMouth.Parent then
&#9;&#9;BarrelMouth.Parent = nil
&#9;&#9;BarrelMouthWeld.Parent = nil&#9;
&#9;end
end

----------------------------------------------------------
-- Bring the BarrelMouth into the world is necessary and start 
-- emitting smoke.

local function barrelSmokeOn()

&#9;barrelMouthEnable()

&#9;BarrelSmoke.Enabled = true
end

----------------------------------------------------------
-- Stop emitting smoke.

local function barrelSmokeOff()
&#9;BarrelSmoke.Enabled = false
end


-----------------------------------------------------------
-- Adjust Barrel and BarrelMouth per the indicated direction and elevation.

local function barrelAdjust(da, ea)

&#9;if da &lt; ANGLE_MAX_LEFT  then
&#9;&#9;da = ANGLE_MAX_LEFT

&#9;elseif da &gt; ANGLE_MAX_RIGHT then
&#9;&#9;da = ANGLE_MAX_RIGHT&#9;&#9;
&#9;end

&#9;if ea &lt; ANGLE_MAX_DOWN  then
&#9;&#9;ea = ANGLE_MAX_DOWN

&#9;elseif ea &gt; ANGLE_MAX_UP then
&#9;&#9;ea = ANGLE_MAX_UP
&#9;end

&#9;local dircf = CFrame.fromAxisAngle(Vector3.new(0,-1,0), da)
&#9;local elevcf = CFrame.fromAxisAngle(Vector3.new(1,0,0), ea)

&#9;local cf = CF_MOUNT_C0 * dircf * elevcf

&#9;BarrelWeld.C0 = cf&#9;
&#9;BarrelMouthWeld.C0 = cf
end


------------------------------------------------------------------------------------------------------------------
------------------------------------------------------------------------------------------------------------------

----------------------------------------------------------------------------
local function setStatusColor()

&#9;if CntrOwner.Value == nil then
&#9;&#9;Mount.BrickColor =  COLOR_UNATTACHED 

&#9;elseif reloading then
&#9;&#9;Mount.BrickColor =  COLOR_RELOAD 

&#9;elseif not cannonUsable then
&#9;&#9;Mount.BrickColor =  COLOR_OOS
&#9;
&#9;else
&#9;&#9;Mount.BrickColor =  COLOR_READY
&#9;end
end


------------------------------------------------------------------------------------------------
-- Clone the cannon ball bolt, aim it by setting its cframe to that of the apparent
-- pointing direction of the barrel.  

local LAUNCH_OFFSET = 7
local MAX_LAUNCH_OFFSET = 40
local MIN_SCALE = LAUNCH_OFFSET / MAX_LAUNCH_OFFSET

local function fire( )

&#9;local bolt = ProtoCannonBall:Clone()
&#9;bolt.Name = &quot;Bolt&quot;
&#9;
&#9;bolt.CFrame = Barrel.CFrame&#9;-- CannonBall direction 

&#9;-- Get the Mount relative look vector of the barrel pointing direction.
&#9;local vru = Mount.CFrame:toObjectSpace(bolt.CFrame).lookVector

&#9;-- Cannon points more in the neg z direction. 
&#9;-- Find the distance to the plane that is perpendicular to the Mount
&#9;-- Z-axis and LAUNCH_OFFSET distance from the origin.
&#9;local scale = math.abs(vru.z)
&#9;if scale &lt; MIN_SCALE then
&#9;&#9;scale = MIN_SCALE
&#9;end
&#9;local dist = LAUNCH_OFFSET / scale

&#9;bolt.Position = Barrel.CFrame:pointToWorldSpace(Vector3.new(0,1,-dist))&#9;-- CannonBall Position

&#9;bolt.Velocity = Barrel.Velocity &#9;&#9;-- Initialize Cannonball to be moving with the Cannon
&#9;bolt.FrontParamA = BOLT_CODE&#9;-- Parm code

&#9;-- BodyForce to counter gravity and residual motion have both been applied.  Remove
&#9;-- anchor so if there is lag between the time we create this bolt and the time its
&#9;-- script runs we will drift along with ship motion instead of remaining fixed and 
&#9;-- possible getting a touch event against the ship and exploding.
&#9;bolt.Anchored = false

&#9;-- Enable any script in the bolt
&#9;for k,v in pairs(bolt:GetChildren()) do
&#9;&#9;if v.className == &quot;Script&quot; then
&#9;&#9;&#9;v.Disabled = false
&#9;&#9;end
&#9;end
&#9;
&#9;bolt.Creator.Value = CntrOwner.Value
&#9;
&#9;bolt.Parent = game.Workspace

&#9;return bolt

end


-------------------------------------------------------------------------------------------------
-- This routine  manages the timeout until the Cannon can be fired again.  
-- The Bolt that was fired is passed
-- to this routine.  We wait for the reloadTime, then we check to see if the 
-- specified Bolt is still in the world.  If yes, we conclude the server
-- is lagging.  To prevent adding to server load by having multiple simultaneous Bolts
-- from this Cannon, we wait for the Bolt to be removed from the world
-- (indicating it finally reached its target) or until a much higher timeout time
-- is reached (to guard against some unknown error).

local function reload( bolt )

&#9;barrelSmokeOn()

&#9;wait(.3)

&#9;barrelSmokeOff()

&#9;wait(RELOAD_TIME - .3)
&#9;
&#9;local waitTime = RELOAD_TIME * 10

&#9;while bolt.Parent and (waitTime &gt; 0) do

&#9;&#9;wait(1)
&#9;&#9;waitTime = waitTime - 1

&#9;end

&#9;if reloading then
&#9;&#9;reloading = false
&#9;&#9;setStatusColor()
&#9;end
end

---------------------------------------------------------------------------------------------
-- Aim and fire the cannon per the world space vector.

local function handleAim(vec)

&#9;--print(&quot;got &quot;, cmd)

&#9;-- If this cannon not owned by anyone
&#9;if not CntrOwner.Value then
&#9;&#9;return
&#9;end

&#9;local vru = Mount.CFrame:vectorToObjectSpace(CntrAim.Value).unit 

&#9;--print(&quot;vru &quot;, vru)

&#9;local ea = math.asin(vru.y)
&#9;local da = math.atan2(vru.x,-vru.z)

&#9;-- Insure BarrelMouth and its weld are in the world
&#9;barrelMouthEnable()&#9;

&#9;-- Adjust Barrel and BarrelMouth
&#9;barrelAdjust(da, ea)

&#9;if not reloading and cannonUsable then

&#9;&#9;reloading = true

&#9;&#9;local bolt = fire()&#9;

&#9;&#9;setStatusColor()

&#9;&#9;reload(bolt)
&#9;end

end

---------------------------------------------------------------------------------------------
local function handleOwnerChange()

&#9;-- Set reload time to default in case we cannot find players reload value
&#9;RELOAD_TIME = RELOAD_TIME_DEFAULT

&#9;if CntrOwner.Value then&#9;

&#9;&#9;local cntrReload = CntrOwner.Value:FindFirstChild(&quot;CntrReloadTime&quot;)
&#9;&#9;if cntrReload and cntrReload.Value then
&#9;&#9;&#9;RELOAD_TIME = cntrReload.Value
&#9;&#9;end
&#9;end

&#9;setStatusColor()
end

-------------------------------------------------------------------------------------------------------

-- Initialize Barrel welds and smoke
barrelInit()

-- Give barrel an initial upward tilt, and whatever initial direction angle is
-- encoded in TopParamB.
barrelAdjust(math.rad(Mount.TopParamB), math.rad(10))

CntrAim.Changed:connect(handleAim)
CntrOwner.Changed:connect(handleOwnerChange)

-- Monitor for ourselves to stop being operational, 
-- and to determine if Player abandoned us

local count = 0

while true do

&#9;wait(5)
&#9;count = count + 1

&#9;if (not Barrel.Parent) or (not Mount.Parent) or (not BarrelWeld.Parent) then
&#9;&#9;wait(3)
&#9;&#9;Cannon:Remove()
&#9;&#9;return
&#9;end

&#9;-- Determine if we are under water (this check should work whether on ship or not)
&#9;cannonUsable = Barrel.Position.y &gt; SEA_LEVEL

&#9;-- Verify the Player in the CntrOwner.Value still exists and still is close enough to control cannon.

&#9;if CntrOwner.Value then&#9;

&#9;&#9;if CntrOwner.Value.Parent == nil then&#9;--If player no longer in the world
&#9;&#9;&#9;CntrOwner.Value = nil

&#9;&#9;else
&#9;&#9;&#9;local char = CntrOwner.Value.Character
&#9;&#9;&#9;if char and char:FindFirstChild(&quot;Torso&quot;) then
&#9;&#9;&#9;&#9;if (Mount.Position - char.Torso.Position).magnitude &gt; MAX_CONTROL_DISTANCE then
&#9;&#9;&#9;&#9;&#9;CntrOwner.Value = nil
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;CntrOwner.Value = nil&#9;&#9;&#9;
&#9;&#9;&#9;end

&#9;&#9;end

&#9;-- Else not owned,  make sure barrel mouth and smoke objects are no longer in the world
&#9;else
&#9;&#9;barrelMouthDisable()
&#9;end

&#9;setStatusColor()
end

local function shipCondition( ship )

&#9;-- If ship is not in the world then it is sunk
&#9;if ship.Parent == nil then
&#9;&#9;return &quot;sunk&quot;

&#9;-- Else if CntrScuttle indicates it has sustained enough damage to eventually sink
&#9;else
&#9;&#9;local keel = ship:FindFirstChild(&quot;Keel&quot;)

&#9;&#9;if not keel then
&#9;&#9;&#9;return &quot;sunk&quot;
&#9;&#9;else
&#9;&#9;&#9;local CntrScuttle = ship.Keel:FindFirstChild(&quot;CntrScuttle&quot;)
&#9;&#9;&#9;if CntrScuttle and CntrScuttle.Value == &quot;damage&quot; then
&#9;&#9;&#9;&#9;return &quot;sinking&quot;
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;return &quot;ok&quot;
end


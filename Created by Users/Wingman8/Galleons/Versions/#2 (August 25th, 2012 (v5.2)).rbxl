&#9;for k,v in ipairs(ship:GetChildren()) do

&#9;&#9;if ((v.className == &quot;Part&quot;) or (v.className == &quot;Seat&quot;)) and 
&#9;&#9;   ((v.BrickColor == COLOR_RED) or (v.BrickColor == COLOR_BLUE)) then
&#9;&#9;&#9;v.BrickColor = col
&#9;&#9;
&#9;&#9;elseif v.className == &quot;SpawnLocation&quot; then
&#9;&#9;&#9;v.BrickColor = col
&#9;&#9;&#9;v.TeamColor = col

&#9;&#9;elseif v.className == &quot;Model&quot;  then
&#9;&#9;&#9;shipPaint(v, col)
&#9;&#9;end
&#9;end
end



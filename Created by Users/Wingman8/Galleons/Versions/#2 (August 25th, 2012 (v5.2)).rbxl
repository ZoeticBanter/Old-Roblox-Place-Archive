local function hitSingle( part, distance, blastPos, blastRadius, blastForce)

--print(&quot;hit single  part &quot;, part.Name)

&#9;local d = 0

--[[
&#9;if distance &gt; blastRadius then 

&#9;elseif distance &lt; blastRadius/2 then
&#9;&#9;d = ((blastRadius - distance)/blastRadius) * blastForce

&#9;else 
&#9;&#9;d = blastForce/2
&#9;end

&#9;if d &lt; blastForce/4 then
&#9;&#9;d = blastForce/4
&#9;end
--]]

&#9;if distance &gt; blastRadius then 
&#9;&#9;return 0
&#9;end

&#9;d = ((blastRadius - distance)/blastRadius) * blastForce

&#9;local orig = part.FrontParamB

&#9;part.FrontParamB = part.FrontParamB - d

&#9;local cur = part.FrontParamB
&#9;local max = part.FrontParamA

&#9;--print(&quot;Part &quot;, part.Name, &quot; damage: &quot;, d, &quot; assessed, parmA: &quot;, part.FrontParamA, &quot; parmB: &quot;, part.FrontParamB)

&#9;-- If strength is gone, release the part

&#9;if cur &lt; 0 then

&#9;&#9;if part.Name ~= &quot;Mount&quot; then
&#9;&#9;&#9;part:BreakJoints()
&#9;&#9;&#9;part.CanCollide = false

&#9;&#9;&#9;fly(part, blastPos)

&#9;&#9;else
&#9;&#9;&#9;blastCannon(part, blastPos)
&#9;&#9;end

&#9;-- If down to 1/3 original strength then change to inlets

&#9;elseif (orig &gt; max/3) and (cur &lt;= max/3) then

&#9;&#9;part.BackSurface = 4
&#9;&#9;part.FrontSurface = 4
&#9;&#9;part.LeftSurface = 4
&#9;&#9;part.RightSurface = 4
&#9;&#9;part.TopSurface = 4
&#9;&#9;part.BottomSurface = 4


&#9;-- If down to 2/3 original strength then change to glue

&#9;elseif (orig &gt; max*2/3) and (cur &lt;= max*2/3) then

&#9;&#9;part.BackSurface = 1
&#9;&#9;part.FrontSurface = 1
&#9;&#9;part.LeftSurface = 1
&#9;&#9;part.RightSurface = 1
&#9;&#9;part.TopSurface = 1
&#9;&#9;part.BottomSurface = 1

&#9;end

&#9;return d
end

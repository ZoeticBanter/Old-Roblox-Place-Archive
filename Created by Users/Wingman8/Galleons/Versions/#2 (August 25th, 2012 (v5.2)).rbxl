local function gpOverlap( pos )

&#9;for _,gp in pairs(gpList) do

&#9;&#9;local rpos = gp.CFrame:pointToObjectSpace(pos)

&#9;&#9;if (math.abs(rpos.x) &lt; gp.Size.x/2) and (math.abs(rpos.z) &lt; gp.Size.z/2) then
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end

&#9;return false
end



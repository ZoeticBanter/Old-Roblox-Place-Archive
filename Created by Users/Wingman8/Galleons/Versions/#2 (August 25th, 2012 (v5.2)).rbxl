-- Separate co-routine to time the fuse burning phase
local function fuseMonitor(seqNum)

&#9;Handle.BrickColor = COLOR_ARMED2
&#9;Handle.Transparency = 0

&#9;--print(&quot;enter fuseMonitor&quot;)

&#9;local INTERVAL_TIME = .5

&#9;--print(&quot;fuseMonitor start&quot;)

&#9;-- Wait for up to FUSE_TIME, flashing the Handle white and team color every .5 secs

&#9;local t = FUSE_TIME

&#9;while t &gt; 0 do

&#9;&#9;local tw = t

&#9;&#9;if t &gt; INTERVAL_TIME then
&#9;&#9;&#9;tw = INTERVAL_TIME
&#9;&#9;end


&#9;&#9;wait(tw)

&#9;&#9;t = t - tw



&#9;&#9;-- If grenade was thrown then we are done
&#9;&#9;if (seqNum ~= armedSeqNum) or (state ~= &quot;armed&quot;) then
&#9;&#9;&#9;return
&#9;&#9;end


&#9;&#9;-- Flash grenade white and team color while fuse burns
&#9;&#9;
&#9;&#9;if Handle.BrickColor == COLOR_ARMED then
&#9;&#9;&#9;Handle.BrickColor = COLOR_ARMED2
&#9;&#9;else
&#9;&#9;&#9;Handle.BrickColor = COLOR_ARMED
&#9;&#9;end
&#9;end

&#9;--print(&quot;fused &quot;, Handle.BrickColor)

&#9;Handle.BrickColor = COLOR_ARMED

&#9;-- Fuse timed out, throw grenade with zero time left so it explodes near

&#9;throw(Handle.Position, Mouse.Hit.p, 0)

&#9;Handle.Transparency = 1
&#9;Handle.BrickColor = COLOR_UNARMED

&#9;--print(&quot;fuseMonitor end&quot;)
end


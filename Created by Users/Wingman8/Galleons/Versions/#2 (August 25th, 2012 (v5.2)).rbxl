--2011/8/21 Initial version (based on ScriptLakes-v2.3)(by Wingman8)
--2011/9/18 v1.3 Add checks for ship gen pads, go not put islands there


local RaceScape = script.Parent


local APEX_MIN = 100
local APEX_MAX = 150

local DIAS_STEP_MIN = 70
local DIAS_STEP_MAX = 150

local SEA_LEVEL_BIAS = -15

local SEA_LEVEL = 12


-- The vertical distance that a trimmed triangle extends below the water line.
local SHORE_CUTOFF = SEA_LEVEL-10


local TREES_NUM = 50

local ProtoTree

local faceCount = 0

-- This model holds all the island parts this script generates
local Islands


-- Forward references
local finit, fnew, fPopCf, fCreate

----------------------------------------------------------------------------------------------------------------
-- Racescape dimensions.

-- The game physical layout is:

--          neg Z
--            ^
--            |                       Pit Stop Area                          |
--            |--------------------------------------------------------------|
--            |                       Starting Area                          |
--  neg X&lt;----+--------------------------------------------------------------+---&gt; pos X axis
--            |                                                              |
--            |                                                              |
--            |                       RaceScape area                         |
--            ~                                                              ~
--            |                                                              |
--            +--------------------------------------------------------------+
--            |
--            v
--        pos Z axis
--

local RS_SIZE = 2500
local RS_SEG_SIZE = 500
local RS_HGT = 16

----------------------------------------------------------------------------------------------------------------
----------------------------------------------------------------------------------------------------------------
--
--  Vertex numbering (x,z) (example for a 4x4 grid):                                                                                                             
--                                                                                                                         
--           (0,0)        (1,0)         (2,0)         (3,0)    
--    
--
--                 (0,1)         (1,1)         (2,1)           (3,1)
--
--
--           (0,2)        (1,2)         (2,2)         (3,2)
--
-- 
--                 (0,3)         (1,3)         (2,3)           (3,3)
--
--
-- Vertex direction from a given vertex:   
--
--                      (nw)   (ne)      
--
--                   (w)    (*)----(e)-------------&gt; pos X axis
--                           |
--                      (sw) | (se)
--                           |
--                           v
--                       pos z axis                                                                   
                                                                                                                       


local VDIMX = 12 --20
local VDIMZ = math.floor(VDIMX/.866)
local VSCALEX= 300
local VSCALEZ = VSCALEX * .866

local VREACHABLE_HGT_DIFF = 50


local FLUCT = 35&#9;-- How much a vertex can vary in height (+ or -) from adj vertex.

local vertices = {}

-------------------------------------------------------------
local function vinit()

&#9;vertices = {}
end


-----------------------------------------------------------
local function vget(vx,vz)

&#9;if (vx &lt; 0) or (vx &gt; VDIMX) or (vz &lt; 0) or (vz &gt; VDIMZ) then
&#9;&#9;error(&quot;vget: out of range, vx: &quot;..tostring(vx)..&quot; vz: &quot;.. tostring(vz))
&#9;end

&#9;local hz = vertices[vz] 

&#9;if not hz then
&#9;&#9;hz = {}
&#9;&#9;vertices[vz] = hz
&#9;end

&#9;local h = hz[vx]

&#9;if not h then
&#9;&#9;h = {vx=vx,vz=vz}
&#9;&#9;hz[vx] = h
&#9;end

&#9;return h
end


--------------------------------------------------------
-- Return the cell that is the specified direction from the specified cell.
-- Specifying a direction that would be off the grid is not an error and 
-- returns nil.

local function vGetDir(vertex, dir)

&#9;local vx = vertex.vx
&#9;local vz = vertex.vz

&#9;if (vz % 2) == 0 then

&#9;&#9;if dir == &quot;nw&quot; then
&#9;&#9;&#9;vx = vx - 1
&#9;&#9;&#9;vz = vz - 1
&#9;&#9;elseif dir == &quot;ne&quot; then
&#9;&#9;&#9;vz = vz -1
&#9;&#9;elseif dir == &quot;e&quot; then
&#9;&#9;&#9;vx = vx + 1
&#9;&#9;elseif dir == &quot;se&quot; then
&#9;&#9;&#9;vz = vz + 1
&#9;&#9;elseif dir == &quot;sw&quot; then
&#9;&#9;&#9;vx = vx - 1
&#9;&#9;&#9;vz = vz + 1
&#9;&#9;elseif dir == &quot;w&quot; then
&#9;&#9;&#9;vx = vx - 1
&#9;&#9;else
&#9;&#9;&#9;error(&quot;vertexGetDir: invalid dir: &quot;..dir)
&#9;&#9;end

&#9;else
&#9;&#9;if dir == &quot;nw&quot; then
&#9;&#9;&#9;vz = vz - 1
&#9;&#9;elseif dir == &quot;ne&quot; then
&#9;&#9;&#9;vx = vx + 1
&#9;&#9;&#9;vz = vz -1
&#9;&#9;elseif dir == &quot;e&quot; then
&#9;&#9;&#9;vx = vx + 1
&#9;&#9;elseif dir == &quot;se&quot; then
&#9;&#9;&#9;vx = vx + 1
&#9;&#9;&#9;vz = vz + 1
&#9;&#9;elseif dir == &quot;sw&quot; then
&#9;&#9;&#9;vz = vz + 1
&#9;&#9;elseif dir == &quot;w&quot; then
&#9;&#9;&#9;vx = vx - 1
&#9;&#9;else
&#9;&#9;&#9;error(&quot;vertexGetDir: invalid dir: &quot;..dir)
&#9;&#9;end
&#9;end

&#9;if (vx &lt; 0) or (vx &gt; VDIMX) or (vz &lt; 0) or (vz &gt; VDIMZ) then
&#9;&#9;return nil
&#9;end

&#9;return vget(vx,vz)
end

---------------------------------------------------------------------------------
-- Return a table of the up to 6 vertices adjacent to the given vertex.
-- May be less than 6 because point may be at grid edge.

local vDirs = {&quot;nw&quot;, &quot;ne&quot;, &quot;e&quot;, &quot;se&quot;, &quot;sw&quot;, &quot;w&quot;}

local function vAdj(vertex)

&#9;local adj = {}

&#9;for _,dir in pairs(vDirs) do

&#9;&#9;local c = vGetDir(vertex, dir)

&#9;&#9;if c then 
&#9;&#9;&#9;adj[c] = true
&#9;&#9;end
&#9;end

&#9;return adj
end

------------------------------------------------------------------------------------
-- return the world space coordinates of the specified vertex.

local function vToWorld( vertex )

&#9;-- Allow to work even if we have not filled in height yet
&#9;local y = vertex.hgt
&#9;if not y then
&#9;&#9;y = 0
&#9;end

&#9;if (vertex.vz % 2) == 0 then
&#9;&#9;return Vector3.new(vertex.vx * VSCALEX, y, vertex.vz*VSCALEZ)

&#9;else
&#9;&#9;return Vector3.new((vertex.vx*VSCALEX) + (VSCALEX/2), y, vertex.vz*VSCALEZ)
&#9;end
end

-----------------------------------------------------------------------
-- Iterate thru all the vertices.  Iterate x faster.

local function vertex_iter()

&#9;local vx = 0
&#9;local vz = 0
     
&#9;return function ()

&#9;&#9;if vz &gt;= VDIMZ then
&#9;&#9;&#9;return nil
&#9;&#9;end

&#9;&#9;local v = vget(vx,vz)

&#9;&#9;vx = vx + 1

&#9;&#9;if vx &gt;= VDIMX then
&#9;&#9;&#9;vx = 0
&#9;&#9;&#9;vz = vz + 1
&#9;&#9;end

&#9;&#9;return v
&#9;end
end

-----------------------------------------------------------------------
-- Find all the faces defined by the vertices and populate the Faces table.

local function vPopFaces()

&#9;finit()

&#9;-- Go up to and NOT including last row
&#9;for vz = 0,VDIMZ-2 do

&#9;&#9;-- If an even row
&#9;&#9;if (vz % 2) == 0 then

&#9;&#9;&#9;-- Go up and including last vertex on this row
&#9;&#9;&#9;for vx = 0, VDIMX-1 do

&#9;&#9;&#9;&#9;local v = vget(vx,vz)

&#9;&#9;&#9;&#9;-- If not the first vertex then populate the face to the south
&#9;&#9;&#9;&#9;if vx ~= 0 then
&#9;&#9;&#9;&#9;&#9;local a = vToWorld(vGetDir(v, &quot;sw&quot;))
&#9;&#9;&#9;&#9;&#9;local b = vToWorld(vGetDir(v, &quot;se&quot;))
&#9;&#9;&#9;&#9;&#9;local c = vToWorld(v)

&#9;&#9;&#9;&#9;&#9;fnew((vx*2)-1, vz, a,b,c)
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;&#9;-- If not the last vertex then populate the face to the se
&#9;&#9;&#9;&#9;if vx &lt; VDIMX-1 then
&#9;&#9;&#9;&#9;&#9;local a = vToWorld(vGetDir(v, &quot;e&quot;))
&#9;&#9;&#9;&#9;&#9;local b = vToWorld(v)
&#9;&#9;&#9;&#9;&#9;local c = vToWorld(vGetDir(v, &quot;se&quot;))

&#9;&#9;&#9;&#9;&#9;fnew(vx*2, vz, a,b,c)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end

&#9;&#9;-- Else is an odd row
&#9;&#9;else

&#9;&#9;&#9;-- Go up to and NOT including last vertex on this row
&#9;&#9;&#9;for vx = 0, VDIMX-2 do

&#9;&#9;&#9;&#9;local v = vget(vx,vz)

&#9;&#9;&#9;&#9;-- Populate face to the south
&#9;&#9;&#9;&#9;local a = vToWorld(vGetDir(v, &quot;sw&quot;))
&#9;&#9;&#9;&#9;local b = vToWorld(vGetDir(v, &quot;se&quot;))
&#9;&#9;&#9;&#9;local c = vToWorld(v)

&#9;&#9;&#9;&#9;fnew(vx*2, vz, a,b,c)

&#9;&#9;&#9;&#9;-- Populate face to the se
&#9;&#9;&#9;&#9;a = vToWorld(vGetDir(v, &quot;e&quot;))
&#9;&#9;&#9;&#9;b = vToWorld(v)
&#9;&#9;&#9;&#9;c = vToWorld(vGetDir(v, &quot;se&quot;))

&#9;&#9;&#9;&#9;fnew((vx*2)+1, vz, a,b,c)

&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end


-----------------------------------------------------------------------
-- Do the edging faces on the E and W sides.

local function vCreateEdgeFaces()

&#9;-- Do the W edge

&#9;for vz = 0, VDIMZ-3, 2 do

&#9;&#9;local a = vToWorld(vget(0, vz))
&#9;&#9;local b = vToWorld(vget(0, vz+2))
&#9;&#9;local c = vToWorld(vget(0,vz+1))

&#9;&#9;local face = {}
&#9;&#9;fPopCf(face, a,b,c)
&#9;&#9;fCreate(face)

&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;&#9;face.wpa.Material = Enum.Material.Grass
&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;face.wpb.Material = face.wpa.Material

&#9;end

&#9;-- If even number of vertx rows then do the SW corner
&#9;if VDIMZ %2 == 0 then

&#9;&#9;local b = vToWorld(vget(0, VDIMZ-2))
&#9;&#9;local c = vToWorld(vget(0, VDIMZ-3))
&#9;&#9;local a = Vector3.new(c.x, b.y, b.z)

&#9;&#9;local face = {}
&#9;&#9;fPopCf(face, a,b,c)
&#9;&#9;fCreate(face)
&#9;&#9;face.wpa.Name = &quot;Corner&quot;

&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;&#9;face.wpa.Material = Enum.Material.Grass
&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;face.wpb.Material = face.wpa.Material
&#9;end

&#9;-- Do the E edge
&#9;local ex = VDIMX-1

&#9;for vz = 1, VDIMZ-3, 2 do
&#9;&#9;local b = vToWorld(vget(ex, vz))
&#9;&#9;local a = vToWorld(vget(ex, vz+2))
&#9;&#9;local c = vToWorld(vget(ex,vz+1))

&#9;&#9;local face = {}
&#9;&#9;fPopCf(face, a,b,c)
&#9;&#9;fCreate(face)

&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;&#9;face.wpa.Material = Enum.Material.Grass
&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;face.wpb.Material = face.wpa.Material
&#9;end

&#9;-- Always do NE corner

&#9;local a = vToWorld(vget(ex, 0))
&#9;local b = vToWorld(vget(ex, 1))
&#9;local c = Vector3.new(b.x, a.y, a.z)

&#9;local face = {}
&#9;fPopCf(face, a,b,c)
&#9;fCreate(face)

&#9;face.wpa.Name = &quot;Corner&quot;

&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;face.wpa.Material = Enum.Material.Grass
&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;face.wpb.Material = face.wpa.Material

&#9;-- If odd number of vertx rows then do the SE corner
&#9;if VDIMZ %2 == 1 then

&#9;&#9;local a = vToWorld(vget(ex, VDIMZ-2))
&#9;&#9;local b = vToWorld(vget(ex, VDIMZ-1))
&#9;&#9;local c = Vector3.new(a.x, b.y, b.z)

&#9;&#9;local face = {}
&#9;&#9;fPopCf(face, a,b,c)
&#9;&#9;fCreate(face)

&#9;&#9;face.wpa.Name = &quot;Corner&quot;

&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;&#9;face.wpa.Material = Enum.Material.Grass
&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;face.wpb.Material = face.wpa.Material
&#9;end


end

-----------------------------------------------------------------------------------------------------------------------
-----------------------------------------------------------------------------------------------------------------------
-- Face management

--  Face numbering (x,z) (where each * is a  vertex):                                                                                                             
--                                                                                                                         
--            *---------*---------*---------* -----&gt; +x axis
--            |\       / \       / \       / \         
--            | \(0,0)/   \(2,0)/   \(4,0)/   \      
--            |  \   /(1,0)\   /(3,0)\   /(5,0)\       
--            v   \ /       \ /       \ /       \         
--        +z axis  *---------*---------*---------* 
--                / \       / \       / \       /        
--               /   \(1,1)/   \(3,1)/   \(5,1)/      
--              /(0,1)\   /(2,1)\   /(4,1)\   /      
--             /       \ /       \ /       \ /        
--            *---------*---------*---------*  
--             \       / \       / \       / \         
--              \(0,2)/   \(2,2)/   \(4,2)/   \      
--               \   /(1,2)\   /(3,2)\   /(5,2)\       
--                \ /       \ /       \ /       \         
--                 *---------*---------*---------* 
--                / \       / \       / \       /        
--               /   \(1,3)/   \(3,3)/   \(5,3)/      
--              /(0,3)\   /(2,3)\   /(4,3)\   /      
--             /       \ /       \ /       \ /        
--            *---------*---------*---------*        
--
-- Vertex identification:   
--
--                      b---------a                c
--                       \       /                / \ 
--                        \     /                /   \ 
--                         \   /                /     \ 
--                          \ /                /       \ 
--                           c                a---------b

-- This is the default vertex labeling.  Some vertices may be switched to insure that the 
-- perpendicular from c to line ab falls between a and b and within 1 unit from both a and b.
--
---
-- WedgePart orientation, and face CFrame.  Face CFrame is oriented the same as the wpb CFrame.
-- The wpa CFrame is oriented the same as the face CFrame rotated 180 about its Y axis.

--                           pos Y
--                              ^
--                              |       (pos X upward from Z/Y plane)
--                              c
--                             /|\ 
--                            / | \ 
--                           /  |  \    ------&gt; neg Z
--                          /wpa|wpb\ 
--                         a----+----b

                                                                                                                     
local FDIMX = (VDIMX-1) * 2
local FDIMZ = VDIMZ - 1

local F_ANGLE_THRESHOLD = 25

-- Each faces entry contains:
--
--&#9;fx : x faces coordinate
--&#9;fz : z faces coordinate
-- &#9;a,b,c : the world space Vector3 coordinates of each vertex of this face
--&#9;cf : the cframe of the middle top of the face, where pos X is the normal to the face
--&#9;angle : the angle in degrees that the face is tilted from horizontal
--&#9;active : true if face WedgeParts are to be created
-- &#9;wpa,wpb : references to the actual WedgeParts (if created)

local FACE_THICKNESS = 10

local faces = {}

local FUNIT_VERT = Vector3.new(1,0,0)

--------------------------------------------------------------------------
function finit()

&#9;faces = {}


end

-------------------------------------------------------------------------
-- For the triangle whose vertices are defined in the specified face, derive the CFrame at the 
-- center of the triangle.  Derive the angle the face makes with the horizontal.
-- The vertices a,b,c may be switched around
-- to insure that the perpendicular from c to line ab lies within ab and within one unit of 
-- both a and b.  The caller must guarantee that the plane of the face is not vertical.

-- Populate the cf, angle, a, b, c fields of the face.

function fPopCf(face,oa,ob,oc)

&#9;-- Calculate the CFrame whose origin is at vertex a whose negative Z axis goes thru
&#9;-- vertex b and where vertex c lines on the ZY plane on the positive Y side.
&#9;-- If object space c lies between and b, return the CFrame and the object space b and c.
&#9;local function cfatob( a, b, c )

&#9;&#9;local cf = CFrame.new(a,b)
&#9;&#9;local cr = cf:pointToObjectSpace(c)
&#9;&#9;local ang = math.atan2(cr.y, cr.x)
&#9;&#9;local cff = cf * CFrame.Angles(0,0,ang - math.rad(90))

&#9;&#9;-- Determine if the perpendicular thru c to line ab lies between a and b
&#9;&#9;-- and at least one unit from a and b.

&#9;&#9;local rb = cff:pointToObjectSpace(b)
&#9;&#9;local rc = cff:pointToObjectSpace(c)

&#9;&#9;if (rc.z &gt; -1) or  (rc.z &lt; (rb.z + 1)) then
&#9;&#9;&#9;return nil
&#9;&#9;end

&#9;&#9;return cff,rb,rc
&#9;end

&#9;local a,b,c = oa,ob,oc

&#9;-- Calculate face CFrame.  If we need to, rotate a,b,c either clockwise or anti-clockwise
&#9;-- to insure c lies between a and b.

&#9;local cff,rb,rc = cfatob(a,b,c)
&#9;if not cff then
&#9;&#9;a,b,c = oc,oa,ob
&#9;&#9;cff,rb,rc = cfatob(a,b,c)

&#9;&#9;if not cff then
&#9;&#9;&#9;a,b,c = ob,oc,oa
&#9;&#9;&#9;cff,rb,rc = cfatob(a,b,c)

&#9;&#9;&#9;if not cff then
&#9;&#9;&#9;&#9;error(&quot;could not find an orientation for a,b,c where c  is between a and b&quot;)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;face.a,face.b,face.c = a,b,c

&#9;-- Find the center of the triangle and translate the face CFrame to it.  
&#9;-- For us the center is the average of the z and y object space coordinates of the 3 vertices 
&#9;-- (remember vertex a is at the origin).

&#9;local ry = (rb.y + rc.y)/3
&#9;local rz = (rb.z + rc.z)/3
&#9;local fcf = cff - cff.p + cff:pointToWorldSpace(Vector3.new(0,ry,rz))

&#9;-- Find the angle the triangle makes with the horizontal (in degrees).
&#9;local n = fcf:vectorToWorldSpace(FUNIT_VERT)
&#9;face.angle = math.deg(math.acos(n.y))

&#9;face.cf = fcf
end

--------------------------------------------------------------------------
function fnew(fx, fz, a, b, c)

&#9;if (fx &lt; 0) or (fx &gt; FDIMX) or (fz &lt; 0) or (fz &gt; FDIMZ) then
&#9;&#9;error(&quot;fnew: out of range, fx: &quot;..tostring(fx)..&quot; fz: &quot;.. tostring(fz))
&#9;end

&#9;local frow = faces[fz] 

&#9;if not frow then
&#9;&#9;frow = {}
&#9;&#9;faces[fz] = frow
&#9;end

&#9;local face = {fx=fx, fz=fz}

&#9;fPopCf(face,a,b,c)

&#9;frow[fx] = face
end

---------------------------------------------------------------------------
local function fget(fx,fz)

&#9;if (fx &lt; 0) or (fx &gt; FDIMX) or (fz &lt; 0) or (fz &gt; FDIMZ) then
&#9;&#9;error(&quot;fget: out of range, fx: &quot;..tostring(fx)..&quot; fz: &quot;.. tostring(fz))
&#9;end

&#9;local frow = faces[fz] 
&#9;if not frow then
&#9;&#9;error(&quot;fget: undefined z, fx: &quot;..tostring(fx)..&quot; fz: &quot;.. tostring(fz))
&#9;end

&#9;local f = frow[fx]

&#9;if not f then
&#9;&#9;error(&quot;fget: undefined x, fx: &quot;..tostring(fx)..&quot; fz: &quot;.. tostring(fz))
&#9;end

&#9;return f

end

-----------------------------------------------------------------------
-- Iterate thru all the vertices.  Iterate x faster.

local function face_iter()

&#9;local fx = 0
&#9;local fz = 0
     
&#9;return function ()

&#9;&#9;if fz &gt;= FDIMZ then
&#9;&#9;&#9;return nil
&#9;&#9;end

&#9;&#9;local f = fget(fx,fz)

&#9;&#9;fx = fx + 1

&#9;&#9;if fx &gt;= FDIMX then
&#9;&#9;&#9;fx = 0
&#9;&#9;&#9;fz = fz + 1
&#9;&#9;end

&#9;&#9;return f
&#9;end
end


----------------------------------------------------------------------------
-- Return a table of the up to 3 faces adjacent to the specified face.  There could
-- be fewer if the specified face is against an edge.

local function fadj(face)

&#9;local faces = {}

&#9;local fx,fz = face.fx, face.fz

&#9;if fx &gt; 0 then
&#9;&#9;faces[fget(fx-1, fz)] = true
&#9;end

&#9;if fx &lt; FDIMX-1 then
&#9;&#9;faces[fget(fx+1, fz)] = true
&#9;end

&#9;-- If both fx and fz are either even or odd, there is adj above
&#9;if (fx % 2 == 0) == (fz % 2 == 0) then
&#9;&#9;if (fz &gt; 0) then
&#9;&#9;&#9;faces[fget(fx, fz-1)] = true
&#9;&#9;end
&#9;
&#9;-- Else one is even and other odd, there is adj below
&#9;else
&#9;&#9;if (fz &lt;  FDIMZ-1) then
&#9;&#9;&#9;faces[fget(fx, fz+1)] = true
&#9;&#9;end
&#9;end

&#9;return faces
end



------------------------------------------------------------------------
local function fWorldToFaceIndex( wp )

&#9;-- Translate to face coordinates
&#9;wp = wp

&#9;local wx,wz = wp.x, wp.z

&#9;-- Map to face grid index (halved for X)

&#9;local fx2 = math.floor((wx + VSCALEX/2) / VSCALEX)
&#9;local fz = math.floor(wz / VSCALEZ)

&#9;if (fx2 &lt; 0) or (fx2 &gt; FDIMX/2) or (fz &lt; 0) or (fz &gt; FDIMZ) then
&#9;&#9;--print(&quot;fWorldToFaceIndex: world coords not in face grid: &quot;..tostring(wx)..&quot;,&quot;..tostring(wz))
&#9;&#9;return nil
&#9;end


&#9;-- Make relative to the center bottom (or top if odd row) face grid
&#9;local rx = wx - (fx2 * VSCALEX)
&#9;local rz

&#9;if fz %2 == 0 then
&#9;&#9;rz = ((fz + 1)* VSCALEZ) - wz
&#9;else
&#9;&#9;rz = wz - (fz * VSCALEZ)
&#9;end

&#9;local fx

&#9;-- If the relative x,z coords slope indicates point in within triangle
&#9;if (((VSCALEX/2) - math.abs(rx)) * 1.73) &gt; rz then
&#9;&#9;fx = (fx2 * 2) - 1

&#9;-- Else outside of triangle, if to the right
&#9;elseif rx &gt; 0 then
&#9;&#9;fx = fx2 * 2

&#9;-- Else is to the left 
&#9;else
&#9;&#9;fx = (fx2 * 2) - 2
&#9;end

&#9;if (fx &lt; 0) or (fx &gt;= FDIMX) then
&#9;&#9;--print(&quot;fWorldToFaceIndex: world coords not in face grid b: &quot;..tostring(wx)..&quot;,&quot;..tostring(wz))
&#9;&#9;return nil
&#9;end

&#9;return fx,fz
end

------------------------------------------------------------------------
-- Return world space position that is the intersection of the vertical line thru wx,wz
-- that intersects the (possibly extended) top surface of face.

local function fWorldToFacePos( face, wx, wz )

&#9;local wp = Vector3.new(wx,0,wz)

&#9;local rp = face.cf:pointToObjectSpace(wp)
&#9;local rd = face.cf:vectorToObjectSpace(Vector3.new(0,-1,0))

&#9;local d = rp.x / rd.x

&#9;local rtp = rp - (rd * d)
&#9;
&#9;return face.cf:pointToWorldSpace(rtp)

end

------------------------------------------------------------------------------------------------------------------
-- Create a triangular face thru points a, b, c.  

-- The triangle is made up of 2 WedgeParts placed &quot;back to back&quot; so their 
-- 90 degree sides touch.  The top of each WedgePart (the Y object axis) is point c.

-- The caller must insure the triangle is not perpendicular to the XZ plane. 

-- Angles abc and bac must be acute.

-- If either angle abc or bac is very close to 90 degrees it is possible that one of 
-- the two WedgeParts would be thinner than the minimum 1 unit wide.
-- No special handling is done for this.

-- Vertices a,b,c must be anti-clockwise looking down toward negative Y.

local CF180Y = CFrame.Angles(0,math.rad(180), 0)

function fCreate(face)

&#9;local a,b,c = face.a, face.b, face.c
&#9;local cf = face.cf

&#9;-- Do the WedgePart whose neg Z corner is vertex B

--[[
&#9;local cf = CFrame.new(a,b)
&#9;local cr = cf:pointToObjectSpace(c)
&#9;local ang = math.atan2(cr.y, math.sqrt(cr.x^2, cr.z^2))
&#9;local cfc = CFrame.Angles(0,0,-ang + math.rad(90))

&#9;local cff = cf * cfc
--]]

&#9;-- Translate the face CFrame to vertex a.
&#9;local cff = cf - cf.p + a

&#9;-- Find object space position of center of WedgePart

&#9;local cr = cff:pointToObjectSpace(c)
&#9;local br = cff:pointToObjectSpace(b)
&#9;local cnr = Vector3.new(-FACE_THICKNESS/2, cr.y/2, (cr.z + br.z)/2)

&#9;--print(&quot;cr: &quot;, cr, &quot; br: &quot;, br, &quot; cnr: &quot;, cnr)

&#9;-- Create WedgePart

&#9;local w = Instance.new(&quot;WedgePart&quot;)
&#9;w.formFactor = 3  -- Custom
&#9;w.Size = Vector3.new(FACE_THICKNESS, cr.y, cr.z - br.z)
&#9;w.Anchored = true
&#9;w.CFrame = cff  + cff:vectorToWorldSpace(cnr)
&#9;w.Friction =1
&#9;w.Elasticity = 0
&#9;--w.Name = &quot;F-&quot;..tostring(face.fx)..&quot;x&quot;..tostring(face.fz)
&#9;w.Name = &quot;loFaceWedge&quot;
&#9;w.Parent = Islands

&#9;face.wpb = w

&#9;-- Do the WedgePart whose neg Z corner is vertex A

&#9;-- Rotate and translate the face CFrame to be b-&gt;a.
&#9;cff = (cf - cf.p) * CF180Y + b
&#9;
--[[
&#9;cf = CFrame.new(b,a)
&#9;cfc = CFrame.Angles(0,0,ang - math.rad(90))
&#9;cff = cf * cfc
--]]

&#9;-- Find object space position of center of WedgePart

&#9;cr = cff:pointToObjectSpace(c)
&#9;local ar = cff:pointToObjectSpace(a)
&#9;cnr = Vector3.new(FACE_THICKNESS/2, cr.y/2, (cr.z + ar.z)/2)

&#9;-- Create WedgePart

&#9;w = Instance.new(&quot;WedgePart&quot;)
&#9;w.formFactor = 3  -- Custom
&#9;w.Size = Vector3.new(FACE_THICKNESS, cr.y, cr.z - ar.z)
&#9;w.Anchored = true
&#9;w.CFrame = cff  + cff:vectorToWorldSpace(cnr)
&#9;w.Friction = 1
&#9;w.Elasticity = 0
&#9;w.Name = &quot;loFaceWedge&quot;
&#9;w.Parent = Islands

&#9;face.wpa = w

&#9;face.active = true
&#9;
end




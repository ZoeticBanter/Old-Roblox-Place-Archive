local function waitWarning( hint, text, duration )

&#9;wait(duration)

&#9;-- If we are still displaying the same thing, then clear warning.
&#9;if hint.Text == text then
&#9;&#9;hint.Visible = false
&#9;end
end
&#9;

local function plDisplayWarning( player, text, duration )

&#9;if player:FindFirstChild(&quot;PlayerGui&quot;) and player.PlayerGui:FindFirstChild(&quot;ScreenGui&quot;) then

&#9;&#9;local hint = player.PlayerGui.ScreenGui:FindFirstChild(&quot;Hint&quot;) 


-- Head directly for the specified torso until it no longer exists

local function shAttack(torso)

&#9;local tp = 1 -- Current target points

&#9;local count = 0

&#9;timerArm(20)

&#9;while true do

&#9;&#9;local targetFunc = function() 

&#9;&#9;&#9;if not torso.Parent then
&#9;&#9;&#9;&#9;return nil
&#9;&#9;&#9;end

&#9;&#9;&#9;local h = torso.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;if (not h) or (h.Health &lt;= 0) then
&#9;&#9;&#9;&#9;return nil
&#9;&#9;&#9;end

&#9;&#9;&#9;return Vector3.new(torso.Position.x, 0, torso.Position.z) 
&#9;&#9;end

&#9;&#9;local event,p = moveToward( targetFunc, {[&quot;ship nearby&quot;]=true})

&#9;&#9;--print(&quot;shAttack returns from moveTorward with event &quot;, event)

&#9;&#9;if event == &quot;hit&quot; then
&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;--print(&quot;shAttack: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif (event == &quot;player nearby&quot;) and (p ~= torso) then
&#9;&#9;&#9;--print(&quot;shAttack: returning event &quot;, event)
&#9;&#9;&#9;return &quot;player nearby&quot;, p

&#9;&#9;elseif event == &quot;target gone&quot; then
&#9;&#9;&#9;--print(&quot;shAttack: player gone, returning to idle&quot;)
&#9;&#9;&#9;state = &quot;idle&quot;
&#9;&#9;&#9;return

&#9;&#9;elseif event == &quot;timeout&quot; then

&#9;&#9;&#9;-- If target is out of the water then lose interest
&#9;&#9;&#9;if not torso:FindFirstChild(&quot;LifeVest&quot;) then
&#9;&#9;&#9;&#9;state = &quot;idle&quot;
&#9;&#9;&#9;&#9;return
&#9;&#9;&#9;end

&#9;&#9;&#9;count = count + 1
&#9;&#9;&#9;if count &gt; 5 then
&#9;&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;&#9;return 
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end


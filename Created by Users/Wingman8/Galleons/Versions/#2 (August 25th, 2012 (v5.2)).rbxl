-- Shark management package

local state = &quot;idle&quot;

------------------------------------------------------------------------------
-- Shark idle state: find a random target and head toward it
-- until target acquired, timeout, or some event happens

local function shIdle()

&#9;timerArm(20)

&#9;while true do

&#9;&#9;local targetPos = Vector3.new(math.random(1000,2600), 0, math.random(1000,2600))

&#9;&#9;local targetFunc = function() return targetPos end

&#9;&#9;local event,p = moveToward( targetFunc, {})

&#9;&#9;--print(&quot;shIdle returns from moveTorward with event &quot;, event)

&#9;&#9;if event == &quot;hit&quot; then
&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;--print(&quot;shIdle: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif event == &quot;player nearby&quot; then 
&#9;&#9;&#9;state = &quot;attack&quot;
&#9;&#9;&#9;--print(&quot;shIdle: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif event == &quot;ship nearby&quot; then
&#9;&#9;&#9;state = &quot;circle ship&quot;
&#9;&#9;&#9;--print(&quot;shIdle: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif event == &quot;timer&quot; then
&#9;&#9;&#9;return event, p
&#9;&#9;end
&#9;end
end

------------------------------------------------------------------------------
-- Shark circle state:  circle the ship whose keel is passed.

local function shCircleShip(keel)

&#9;local targets

&#9;-- Randomly select clockwise or anticockwise target points in Keel relative coordinates
&#9;if math.random() &gt; .5 then
&#9;&#9;-- Clockwise
&#9;&#9;targets = { Vector3.new(0,0,150), Vector3.new(-100,0,0), 
&#9;&#9;&#9;&#9;Vector3.new(0,0,-150), Vector3.new(100,0,0)}
&#9;else
&#9;&#9;--Anticlockwise
&#9;&#9;targets = { Vector3.new(100,0,0), Vector3.new(0,0,-150), 
&#9;&#9;&#9;&#9;Vector3.new(-100,0,0), Vector3.new(0,0,150)}
&#9;end

&#9;local tp = 1 -- Current target points

&#9;timerArm(20)

&#9;while true do

&#9;&#9;if not keel.Parent then
&#9;&#9;&#9;state = &quot;idle&quot;
&#9;&#9;&#9;return
&#9;&#9;end

&#9;&#9;--print(&quot;Cur target keel relative: &quot;, targets[tp])

&#9;&#9;local targetFunc = function() 
&#9;&#9;&#9;if (not keel) or (not keel.Parent) then
&#9;&#9;&#9;&#9;return nil
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;local pos = keel.CFrame:pointToWorldSpace(targets[tp]) 
&#9;&#9;&#9;&#9;return Vector3.new(pos.x, 0, pos.z)
&#9;&#9;&#9;end
&#9;&#9;end

&#9;&#9;local event,p = moveToward( targetFunc, {})

&#9;&#9;--print(&quot;shCircle returns from moveTorward with event &quot;, event)

&#9;&#9;if event == &quot;hit&quot; then
&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;--print(&quot;shCircleShip: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif event == &quot;player nearby&quot; then 
&#9;&#9;&#9;state = &quot;attack&quot;
&#9;&#9;&#9;--print(&quot;shCircleShip: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif (event == &quot;ship nearby&quot;) and (p ~= keel) then
&#9;&#9;&#9;--print(&quot;shCircleShip: returning event &quot;, event)
&#9;&#9;&#9;return event, p

&#9;&#9;elseif event == &quot;target gone&quot; then
&#9;&#9;&#9;state = &quot;idle&quot;
&#9;&#9;&#9;return
&#9;&#9;end

&#9;&#9;if (event == &quot;target acquired&quot;) or ( event == &quot;timeout&quot;) then
&#9;&#9;&#9;tp = tp + 1
&#9;&#9;
&#9;&#9;&#9;if tp &gt; 4 then
&#9;&#9;&#9;&#9;tp = 1
&#9;&#9;&#9;end

&#9;&#9;&#9;-- Reset the timer if we acquired the target.
&#9;&#9;&#9;if event == &quot;target acquired&quot; then
&#9;&#9;&#9;&#9;timerArm(20)
&#9;&#9;&#9;end

&#9;&#9;&#9;--print(&quot;shCircleShip: handling event &quot;, event)

&#9;&#9;&#9;-- Every so often just flee to break up the monotony

&#9;&#9;&#9;if math.random() &lt; .1 then
&#9;&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;&#9;return 
&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;
&#9;&#9;else
&#9;&#9;&#9;--print(&quot;shCircleShip: ignoring event  &quot;, event)
&#9;&#9;end
&#9;end
end


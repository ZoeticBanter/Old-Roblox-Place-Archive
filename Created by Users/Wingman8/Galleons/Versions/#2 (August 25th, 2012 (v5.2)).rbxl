------------------------------------------------------------------------------------------------------------------
-- Timer package

local curTimer = 0
local rearmTimer = nil

local function timerArm( timeVal)
&#9;curTimer = timeVal
&#9;rearmTimer = timeVal
end

local function timerStop()
&#9;curTimer = 0
&#9;rearmTimer = nil
end

-- This coroutine maintains the basic one second interval.

local TM_INTERVAL = 1

local function tmRun()

&#9;while true do

&#9;&#9;wait(TM_INTERVAL)

--print(&quot;timer tick&quot;)

&#9;&#9;if curTimer &gt; 0 then

&#9;&#9;&#9;curTimer = curTimer - 1

&#9;&#9;&#9;if curTimer &lt;= 0 then
&#9;&#9;&#9;&#9;if rearmTimer then
&#9;&#9;&#9;&#9;&#9;curTimer = rearmTimer
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;assert(coroutine.resume(sharkMgrCor,&quot;timeout&quot;))
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;&#9;&#9;
&#9;&#9;assert(coroutine.resume(sharkMgrCor,&quot;movement interval&quot;))
&#9;end&#9;
end

----------------------------------------------------------------------------
local vector0 = Vector3.new(0,0,0)

local function aimToward(target)

&#9;-- Aim for the target

&#9;local v1u = target - Shark.Position
&#9;v1u = Vector3.new(v1u.x,0,v1u.z).unit

&#9;BodyGyro.cframe = CFrame.new(vector0, vector0 + v1u)

&#9;BodyVelocity.velocity = v1u*20


end

----------------------------------------------------------------------------
local function shipNearby()

&#9;local nearestKeel 
&#9;local nearestDist

&#9;for k,v in pairs(Workspace:GetChildren()) do

&#9;&#9;if v.className == &quot;Model&quot; then
&#9;&#9;&#9;local keel = v:FindFirstChild(&quot;Keel&quot;)

&#9;&#9;&#9;if keel then

&#9;&#9;&#9;&#9;--print(&quot;Found ship at pos &quot;, keel.Position, &quot; dist to shark &quot;, (keel.Position - Shark.Position).magnitude )

&#9;&#9;&#9;&#9;if (not nearestKeel) or ((keel.Position - Shark.Position).magnitude &lt; nearestDist) then
&#9;&#9;&#9;&#9;&#9;nearestKeel = keel
&#9;&#9;&#9;&#9;&#9;nearestDist = (keel.Position - Shark.Position).magnitude
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;if nearestKeel and (nearestDist &lt; SHIP_DETECT_RANGE) then
&#9;&#9;return nearestKeel
&#9;end

&#9;return nil
end

-----------------------------------------------------------------------------
local function playerNearby()

&#9;local nearestPlayer
&#9;local nearestDist

&#9;--for k,pl in pairs(Workspace.FakePlayers:GetChildren()) do
&#9;for k,pl in pairs(game.Players:GetChildren()) do

&#9;&#9;local c = pl.Character

&#9;&#9;if c then
&#9;&#9;&#9;local torso = c:FindFirstChild(&quot;Torso&quot;)

&#9;&#9;&#9;--if torso then
&#9;&#9;&#9;--&#9;print(&quot;Found player and lifevest: &quot;, torso:FindFirstChild(&quot;LifeVest&quot;))
&#9;&#9;&#9;--end

&#9;&#9;&#9;-- Only consider players that have a lifevest on, this means they are in the water.
&#9;&#9;&#9;if torso and torso:FindFirstChild(&quot;LifeVest&quot;) then

&#9;&#9;&#9;&#9;if (not nearestPlayer) or ((torso.Position - Shark.Position).magnitude &lt; nearestDist) then
&#9;&#9;&#9;&#9;&#9;nearestPlayer = torso
&#9;&#9;&#9;&#9;&#9;nearestDist = (torso.Position - Shark.Position).magnitude
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;if nearestPlayer and (nearestDist &lt; PLAYER_DETECT_RANGE) then
&#9;&#9;return nearestPlayer
&#9;end

&#9;return nil
end


local RELOAD_TIME = 1

---------------------------------------------------------------------------------------------------

local function displayStatus()

&#9;local bullets = 0

&#9;local cntrBullets = Player:FindFirstChild(&quot;CntrBullets&quot;)

&#9;if cntrBullets and cntrBullets.Value then
&#9;&#9;bullets = cntrBullets.Value

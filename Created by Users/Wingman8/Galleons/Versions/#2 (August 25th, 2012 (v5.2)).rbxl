&#9;weldR.Parent = Workspace

&#9;CntrAim = Tool:FindFirstChild(&quot;CntrAim&quot;)
&#9;if not CntrAim then
&#9;&#9;print(&quot;ScriptDgunAnim: could not find CntrAim&quot;)
&#9;&#9;return
&#9;end

&#9;CntrAim.Changed:connect(handleAim)

end


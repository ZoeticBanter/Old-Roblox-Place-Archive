-- Shark hit by a bolt, flee in the same direction the bolt was going

local function shFlee(bolt)

&#9;local target

&#9;-- If Bolt specified then flee in same direction that bolt was going.  Determine this from the 
&#9;-- vector from Creator position to the Bolt position (this is because the
&#9;-- Bolt may have already bounced off and its vector  is not the original
&#9;-- firing direction).
&#9;if bolt and bolt.Parent then
&#9;&#9;local owner = bolt:FindFirstChild(&quot;Creator&quot;)
&#9;&#9;if owner and owner.Value and owner.Value.Character then
&#9;&#9;&#9;local torso = owner.Value.Character:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;if torso then
&#9;&#9;&#9;&#9;local bvu = bolt.Position - torso.Position
&#9;&#9;&#9;&#9;bvu = Vector3.new(bvu.x, 0, bvu.z).unit
&#9;&#9;&#9;&#9;target = Shark.Position + (bvu * FLEE_DISTANCE)

&#9;&#9;&#9;&#9;--print(&quot;shFlee target computed from bolt destination &quot;, target)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;-- If no target determined above then generate a random one.
&#9;if not target then
&#9;&#9;target = Vector3.new(math.random(-700,700), 0, math.random(-700,700))
&#9;&#9;--print(&quot;shFlee target computed randomly &quot;, target)
&#9;end

&#9;timerArm(60)

&#9;while true do

&#9;&#9;local targetFunc = function() return target end

&#9;&#9;local event,p = moveToward( targetFunc, {[&quot;ship nearby&quot;]=true, [&quot;player nearby&quot;]=true})

&#9;&#9;--print(&quot;shFlee returns from moveTorward with event &quot;, event)

&#9;&#9;if event == &quot;hit&quot; then
&#9;&#9;&#9;state = &quot;flee&quot;
&#9;&#9;&#9;--print(&quot;shFlee: returning event &quot;, event)
&#9;&#9;&#9;return event, p
&#9;&#9;end

&#9;&#9;if (event == &quot;target acquired&quot;) or (event == &quot;timeout&quot;) then
&#9;&#9;&#9;state = &quot;idle&quot;
&#9;&#9;&#9;--print(&quot;shFlee target acquired or timeout&quot;)
&#9;&#9;&#9;return 
&#9;&#9;end
&#9;end
end



------------------------------------------------------------------------------
-- Main coroutine that controls shark by maintaining the current 
-- shark state.

local function sharkManager()

&#9;state = &quot;idle&quot;

&#9;local event,p

&#9;while true do

&#9;&#9;--print(&quot;cur state &quot;, state, &quot; cur event &quot;, event)

&#9;&#9;if state == &quot;idle&quot; then
&#9;&#9;&#9;event,p = shIdle()

&#9;&#9;elseif state == &quot;circle ship&quot; then
&#9;&#9;&#9;event,p = shCircleShip(p)

&#9;&#9;elseif state == &quot;attack&quot; then
&#9;&#9;&#9;event,p = shAttack(p)

&#9;&#9;elseif state == &quot;flee&quot; then
&#9;&#9;&#9;event,p = shFlee(p)

&#9;&#9;else
&#9;&#9;&#9;print(&quot;Unhandled event /state&quot;, event,&quot;/&quot;, state, &quot; returning to idle state&quot;)
&#9;&#9;&#9;state = &quot;idle&quot;

&#9;&#9;end
&#9;end
end


------------------------------------------------------------------------------


-- Create the main shark manager coroutine and start it running
sharkMgrCor = coroutine.create(sharkManager)
assert(coroutine.resume(sharkMgrCor))

Shark.Touched:connect(onTouched)

-- This coroutine becomes the timer utility.
tmRun()


local arc = 1&#9;-- default high arc



-----------------------------------------------------------------------------------------------
-- Constants
local RELOAD_TIME = 1
local FUSE_TIME = 3

local COLOR_UNARMED&#9;&#9;&#9;&#9;&#9;-- Filled in when tool equipped based on player team color
local COLOR_ARMED&#9;&#9;&#9;&#9;&#9;-- Filled in when tool equipped based on player team color
local COLOR_ARMED2 = BrickColor.White()




local angle = 25
local MAX_SPEED_25 = 110
local EFFECTIVE_g25 = 50
local MAX_SPEED_45 = 110

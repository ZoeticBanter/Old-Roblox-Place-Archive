&#9;&#9;&#9;&#9;blist[e.f] = true

&#9;&#9;&#9;&#9;count = count + 1

&#9;&#9;&#9;-- Else must be a section or parition, fragment it
&#9;&#9;&#9;else

&#9;&#9;&#9;&#9;local o = e.o

&#9;&#9;&#9;&#9;if o.state == &quot;section&quot; then

&#9;&#9;&#9;&#9;&#9;--print(&quot;frag: section&quot;)


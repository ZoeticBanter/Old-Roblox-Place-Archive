&#9;&#9;&#9;&#9;self.block.Name = &quot;Fragment&quot;
&#9;&#9;&#9;&#9;self.block.Size = Vector3.new(lx,self.block.Size.y,lz)

&#9;&#9;&#9;&#9;self.block.LeftSurface = wl
&#9;&#9;&#9;&#9;self.block.RightSurface = wr
&#9;&#9;&#9;&#9;self.block.BackSurface = wb
&#9;&#9;&#9;&#9;self.block.FrontSurface = wf

&#9;&#9;&#9;&#9;self.block.CFrame  = cframe + cframe:vectorToWorldSpace( Vector3.new(ulx+(lx/2), 0, ulz+(lz/2)) )
&#9;&#9;&#9;&#9;self.fragmentList[self.block] = self.block
&#9;&#9;&#9;&#9;self.numFrags = self.numFrags + 1

&#9;&#9;&#9;&#9;veryFirstPlank = false

&#9;&#9;&#9;-- Else create a new part
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;self:oneFrag_y(ulx+x ,0, ulz+z, lx, lz, cframe, wl, wr, wb, wf)
&#9;&#9;&#9;end

&#9;&#9;&#9;x =x + lx
&#9;&#9;end

&#9;&#9;z = z + lz
&#9;end


&#9;self.model:MakeJoints()

&#9;self.state = &quot;fragmented&quot;

&#9;return self.model
end

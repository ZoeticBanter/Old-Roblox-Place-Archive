local function displayHandle()

&#9;if state == &quot;idle&quot; then

&#9;&#9;local cntrGrenades = Player:FindFirstChild(&quot;CntrGrenades&quot;)

&#9;&#9;if cntrGrenades and cntrGrenades.Value and cntrGrenades.Value &gt; 0 then

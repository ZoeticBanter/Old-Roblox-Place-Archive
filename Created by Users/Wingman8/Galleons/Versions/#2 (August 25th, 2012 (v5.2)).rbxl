-- Partition where the thickness dimension is z, length is x, width is y

function Frag:partition_z()

&#9;if self.state ~= &quot;section&quot; then
&#9;&#9;return
&#9;end

&#9;local block = self.block

&#9;local psx = partitionSize(block.Size.x, PART_SIZES_L)
&#9;local psy = partitionSize(block.Size.y, PART_SIZES_W)

&#9;--print(&quot;partition sizing: &quot;, psx,&quot; &quot;, psy)

&#9;local x,lx
&#9;local veryFirstPart = true
&#9;local cframe = block.CFrame
&#9;local tsizex = block.Size.x
&#9;local tsizey = block.Size.y

&#9;local y = 0

&#9;while y &lt; tsizey do

&#9;&#9;local ly = psy&#9;-- assumes a full width part

&#9;&#9;if tsizey - y &lt; ly then
&#9;&#9;&#9;ly = tsizey - y
&#9;&#9;end
&#9;
&#9;&#9;x = 0

&#9;&#9;while x &lt; tsizex do

&#9;&#9;&#9;local lx = psx&#9;-- Assume a full size part

&#9;&#9;&#9;if tsizex - x &lt; psx then
&#9;&#9;&#9;&#9;lx = tsizex - x
&#9;&#9;&#9;end

&#9;&#9;&#9;local nb

&#9;&#9;&#9;-- If very first part then resize the original block 
&#9;&#9;&#9;if veryFirstPart then 
&#9;&#9;&#9;&#9;local parent = block.Parent
&#9;&#9;&#9;&#9;block.Parent = nil&#9;&#9;-- I think we need to remove from world to break any connections
&#9;&#9;&#9;&#9;block.Size = Vector3.new(lx,ly,block.Size.z)
&#9;&#9;&#9;&#9;block.CFrame = cframe + cframe:vectorToWorldSpace( Vector3.new(x+(lx/2)-(tsizex/2), y-(tsizey/2)+(ly/2),0) )
&#9;&#9;&#9;&#9;block.Parent = parent
    
&#9;&#9;&#9;&#9;-- Must refresh this since block changed size
&#9;&#9;&#9;&#9;self.curMass = block:GetMass()

&#9;&#9;&#9;&#9;-- Transition our state 
&#9;&#9;&#9;&#9;self.state = &quot;partition&quot;

&#9;&#9;&#9;&#9;--print(&quot;firstPart; &quot;, x,&quot;,&quot;,y,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,ly/2, &quot; pos: &quot;, block.Position)

&#9;&#9;&#9;&#9;veryFirstPart = false

&#9;&#9;&#9;&#9;nb = block

&#9;&#9;&#9;-- Else create a new block
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;nb = Instance.new(&quot;Part&quot;)
&#9;&#9;&#9;&#9;nb.formFactor = 0
&#9;&#9;&#9;&#9;nb.Size = Vector3.new(lx,ly,block.Size.z)
&#9;&#9;&#9;&#9;nb.Color = block.Color
&#9;&#9;&#9;&#9;nb.FrontSurface = block.FrontSurface
&#9;&#9;&#9;&#9;nb.BackSurface = block.BackSurface
&#9;&#9;&#9;&#9;nb.FrontParamA = block.FrontParamA
&#9;&#9;&#9;&#9;nb.FrontParamB = block.FrontParamB
&#9;&#9;&#9;&#9;nb.BottomParamB = block.BottomParamB
&#9;&#9;&#9;&#9;nb.LeftSurface = Enum.SurfaceType.Weld
&#9;&#9;&#9;&#9;nb.RightSurface = Enum.SurfaceType.Weld
&#9;&#9;&#9;&#9;nb.TopSurface = Enum.SurfaceType.Weld
&#9;&#9;&#9;&#9;nb.BottomSurface = Enum.SurfaceType.Weld
&#9;&#9;&#9;&#9;nb.Transparency = block.Transparency
&#9;&#9;&#9;&#9;nb.Material = block.Material

&#9;&#9;&#9;&#9;nb.CFrame = cframe + cframe:vectorToWorldSpace(Vector3.new(x-(tsizex/2)+(lx/2),y-(tsizey/2)+(ly/2),0))

&#9;&#9;&#9;&#9;--print(&quot;onePart; &quot;, x,&quot;,&quot;,y,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,ly/2, &quot; pos: &quot;, nb.Position)

&#9;&#9;&#9;&#9;nb.Name = block.Name
&#9;&#9;&#9;&#9;nb.Parent = block.Parent.Parent --original part was put into a model

&#9;&#9;&#9;&#9;-- Create a new frag object to manage this part

&#9;&#9;&#9;&#9;Frag:new(nb)

&#9;&#9;&#9;end

&#9;&#9;&#9;nb:MakeJoints()

&#9;&#9;&#9;x = x + lx
&#9;&#9;end

&#9;&#9;y = y + ly
&#9;end
end


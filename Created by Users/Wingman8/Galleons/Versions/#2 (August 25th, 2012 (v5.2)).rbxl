					<ProtectedString name="Source">--------------------------------------------------------------------------------------------------------------------------
--2011/9/29 Initial version (based on ScriptShellB-v1.6 and ScriptCannonball-v2.7) (by Wingman8)
--2011/11/11 v1.1 Ignore Bolt in FindPartInRay
--2012/5/5 v1.2 Add reference to sound server

--[[
&#9;- The firing weapon clones ProtoCannonBall, aims it by setting its .CFrame, and enables this script.

&#9;- ProtoCannonBall Part has this script.

&#9;- Bolt does not actually &quot;move&quot; in a Roblox physics sense, remains anchored the whole time.

&#9;- Every wait() interval the area probed in front bolt is checked for hit event with FindPartOnRay(), 
&#9;  if nothing hit then shell is moved to new position.

&#9;- Parameter set code selects one of a fixed set of parameter configurations

--]]

local Bolt = script.Parent

-- Get bolt parameters from the library based on the parm set code in FrontParamA
local Ps = shared.glBoltParms[Bolt.FrontParamA]

local parmHardRadius = Ps.parmDefHardRadius
local parmSoftRadius = Ps.parmDefSoftRadius
local parmDefForce = Ps.parmDefForce
local parmRange = Ps.parmRange
local parmGravity = Ps.parmGravity
local parmSpeed = Ps.parmSpeed

local InitialPosition
local connection
local explosion
local fireSound
local blastPosition

local seaLevel = 0
local Sea = Workspace:FindFirstChild(&quot;Sea&quot;)
if Sea then
&#9;seaLevel = Sea.Position.y
end

local MAX_DAMAGE = 1900
local curDamage = 0


-------------------------------------------
local function waitChild( parent, childName)&#9;
&#9;while true do
&#9;&#9;local child =  parent:FindFirstChild(childName)
&#9;&#9;if child then
&#9;&#9;&#9;return child
&#9;&#9;end
&#9;&#9;print(&quot;ScriptShellB has to wait for &quot;,parent.Name, &quot;.&quot;, childName)
&#9;&#9;wait(1)
&#9;end
end

------------------------------------------------------------------------------------------------------------------
local function onHit(part,distance)

&#9;local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

&#9;if torso then
&#9;&#9;--print(&quot;torso distance &quot;, distance, &quot; parmSoftRadius &quot;, parmSoftRadius)
&#9;&#9;if distance &lt; parmSoftRadius then
&#9;&#9;&#9;shared.glBlastPlayer(torso, blastPosition, Bolt)
&#9;&#9;end

&#9;&#9;return
&#9;end

&#9;if part.Name == &quot;Bolt&quot; then
&#9;&#9;return
&#9;end

&#9;if part.Anchored then
&#9;&#9;return
&#9;end

&#9;-- If part is non-destroyable
&#9;if part.FrontParamA &lt; 0 then
&#9;&#9;return
&#9;end

&#9;-- Get the closest distance the blast is to the part we hit, use that to calculate damage 
&#9;-- rather than the distance supplied which is the distance to the center of the part.
&#9;local dist = shared.closestDist(part, blastPosition)

&#9;--print(&quot;Actual dist &quot;, distance, &quot; closest dist &quot;, dist)

&#9;local dm = shared.glHitHandler(part, dist, blastPosition, parmHardRadius, parmDefForce)

&#9;--print(&quot;onHit part.BottomParamB &quot;,part.BottomParamB, &quot; ourSide &quot;, ourSide )

&#9;-- Limit total damage done by this Bolt to MAX_DAMAGE
&#9;if curDamage + dm &gt; MAX_DAMAGE then
&#9;&#9;dm = MAX_DAMAGE - curDamage

&#9;&#9;if dm &lt;= 0 then
&#9;&#9;&#9;return
&#9;&#9;end
&#9;end

&#9;curDamage = curDamage + dm

&#9;shared.glPegDamage(Bolt, dm)

--]]

end


------------------------------------------------------------------------------------------------------------------------

local function assessHit(opos, volVec, bvec, dincr, travelled)

&#9;local blastPart
&#9;local vpos = opos + volVec



&#9;for i = 1,10 do

&#9;&#9;local pos

&#9;&#9;local vincr = bvec * dincr

&#9;&#9;local ro = Ray.new(opos, vincr)
&#9;&#9;local part,oposn = Workspace:FindPartOnRay(ro, Bolt)

&#9;&#9;local vr = Ray.new(vpos, vincr)
&#9;&#9;local vpart,vposn = Workspace:FindPartOnRay(vr, Bolt)

&#9;&#9;if part then
&#9;&#9;&#9;if vpart then
&#9;&#9;&#9;&#9;local od = (opos - oposn).magnitude
&#9;&#9;&#9;&#9;local vd = (vpos - vposn).magnitude

&#9;&#9;&#9;&#9;if od &lt;= vd then
&#9;&#9;&#9;&#9;&#9;pos = oposn
&#9;&#9;&#9;&#9;&#9;dincr = dincr - od 
&#9;&#9;&#9;&#9;&#9;travelled = travelled + od
&#9;&#9;&#9;&#9;&#9;vpos = vpos + (bvec*od)
&#9;&#9;&#9;&#9;&#9;opos = oposn
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;pos = vposn
&#9;&#9;&#9;&#9;&#9;dincr = dincr - vd
&#9;&#9;&#9;&#9;&#9;travelled = travelled + vd
&#9;&#9;&#9;&#9;&#9;vpos = vposn
&#9;&#9;&#9;&#9;&#9;opos = opos + (bvec*vd)
&#9;&#9;&#9;&#9;&#9;part = vpart
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;local od = (opos - oposn).magnitude
&#9;&#9;&#9;&#9;pos = oposn
&#9;&#9;&#9;&#9;dincr = dincr - od 
&#9;&#9;&#9;&#9;travelled = travelled + od
&#9;&#9;&#9;&#9;vpos = vpos + (bvec*od)
&#9;&#9;&#9;&#9;opos = oposn
&#9;&#9;&#9;end

&#9;&#9;elseif vpart then
&#9;&#9;&#9;&#9;local vd = (vpos - vposn).magnitude
&#9;&#9;&#9;&#9;pos = vposn
&#9;&#9;&#9;&#9;dincr = dincr - vd
&#9;&#9;&#9;&#9;travelled = travelled + vd
&#9;&#9;&#9;&#9;vpos = vposn
&#9;&#9;&#9;&#9;opos = opos + (bvec*vd)
&#9;&#9;&#9;&#9;part = vpart
&#9;&#9;
&#9;&#9;else
&#9;&#9;&#9;return false
&#9;&#9;end
&#9;&#9;&#9;

&#9;&#9;local ignore = false

&#9;&#9;-- Ignore touch with any barrel part, ours or others
&#9;&#9;if (part.CanCollide == false) or string.match(part.Name,&quot;Barrel&quot;) or 
&#9;&#9;&#9;string.match(part.Name,&quot;Shark&quot;) or (part.Name == &quot;Sea&quot;) then
&#9;&#9;&#9;ignore = true
&#9;&#9;end

&#9;&#9;if not ignore then
&#9;&#9;&#9;blastPart = part
&#9;&#9;&#9;blastPosition = pos
&#9;&#9;&#9;break
&#9;&#9;end

&#9;&#9;-- Advance slightly into the ignored part so we do not find it again on next iteration.
&#9;&#9;opos = opos + (bvec * .1)
&#9;&#9;vpos = vpos + (bvec * .1)

&#9;&#9;if dincr &lt; .1 then
&#9;&#9;&#9;return false
&#9;&#9;end
&#9;end


&#9;if not blastPart then
&#9;&#9;return false
&#9;end


--[[  -- For testing, turns target part yellow
&#9;local ocol = blastPart.BrickColor
&#9;blastPart.BrickColor = BrickColor.Yellow()
&#9;Bolt.Anchored = true
&#9;wait(5)
&#9;blastPart.BrickColor = ocol
&#9;Bolt:Remove()
--]]

&#9;-- Make blast position one unit back from contacting surface because original did 
&#9;-- something like that.
&#9;blastPosition = blastPosition - bvec

&#9;-- If this part is a powder keg then remove it and 
&#9;-- increase the blast radius and force to
&#9;-- simulate the powder keg exploding.
&#9;if string.match(blastPart.Name, &quot;PowderKeg&quot;) then
&#9;&#9;blastPosition = blastPart.Position
&#9;&#9;parmHardRadius = blastPart.RightParamA
&#9;&#9;parmSoftRadius = parmHardRadius
&#9;&#9;parmDefForce = blastPart.RightParamB
&#9;&#9;blastPart:Remove()

&#9;-- If this part has a fragmentation handler then call it before exploding.
&#9;elseif shared.FragHandler and shared.FragHandler[blastPart]  then

&#9;--print(&quot;cb touch calls frag handler on &quot;, part.Name)

&#9;&#9;shared.FragHandler[blastPart](Bolt, blastPosition)
&#9;end

&#9;--print(&quot;Bolt: contact point: &quot;, contactPoint, &quot; Bolt pos&quot;, Bolt.Position)


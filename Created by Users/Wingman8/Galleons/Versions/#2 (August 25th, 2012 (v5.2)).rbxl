-- Find closest distance from point to block

-- This table is indexed by a key consisting of leters m,z, or p (meaning minus, zero, or positive)
-- concatenated together for each of the x, y, and z dimensions (in that order).  It give a 
-- function that will return the closest distance to the point.

local distMap = {

&#9;-- Minus y slice

&#9;mmm = function(hs, p) return (Vector3.new(-hs.x,-hs.y,-hs.z) - p).magnitude end,
&#9;imm = function(hs,p) return (Vector3.new(p.x, -hs.y, -hs.z) - p).magnitude end,
&#9;pmm = function(hs, p) return (Vector3.new(hs.x,-hs.y,-hs.z) - p).magnitude end,

&#9;mmi = function(hs, p) return (Vector3.new(-hs.x,-hs.y,p.z) - p).magnitude end,
&#9;imi = function(hs, p) return -p.y - hs.y end,
&#9;pmi = function(hs, p) return (Vector3.new(hs.x,-hs.y,p.z) - p).magnitude end,

&#9;mmp = function(hs, p) return (Vector3.new(-hs.x,-hs.y,hs.z) - p).magnitude end,
&#9;imp = function(hs,p) return (Vector3.new(p.x, -hs.y, hs.z) - p).magnitude end,
&#9;pmp = function(hs, p) return (Vector3.new(hs.x,-hs.y,hs.z) - p).magnitude end,

&#9;-- Inside y slice

&#9;mim = function(hs, p) return (Vector3.new(-hs.x,p.y,-hs.z) - p).magnitude end,
&#9;iim = function(hs,p) return  -p.z - hs.z end,
&#9;pim = function(hs, p) return (Vector3.new(hs.x,p.y,-hs.z) - p).magnitude end,

&#9;mii = function(hs, p) return -p.x - hs.x end,
&#9;iii = function(hs, p) return p.magnitude end,
&#9;pii = function(hs, p) return p.x - hs.x end,

&#9;mip = function(hs, p) return (Vector3.new(-hs.x,p.y,hs.z) - p).magnitude end,
&#9;iip = function(hs,p) return p.z - hs.z end,
&#9;pip = function(hs, p) return (Vector3.new(hs.x,p.y,hs.z) - p).magnitude end,

&#9;-- Plus y slice

&#9;mpm = function(hs, p) return (Vector3.new(-hs.x,hs.y,-hs.z) - p).magnitude end,
&#9;ipm = function(hs,p) return (Vector3.new(p.x, hs.y, -hs.z) - p).magnitude end,
&#9;ppm = function(hs, p) return (Vector3.new(hs.x,hs.y,-hs.z) - p).magnitude end,

&#9;mpi = function(hs, p) return (Vector3.new(-hs.x,hs.y,p.z) - p).magnitude end,
&#9;ipi = function(hs, p) return p.y - hs.y end,
&#9;ppi = function(hs, p) return (Vector3.new(hs.x,hs.y,p.z) - p).magnitude end,

&#9;mpp = function(hs, p) return (Vector3.new(-hs.x,hs.y,hs.z) - p).magnitude end,
&#9;ipp = function(hs,p) return (Vector3.new(p.x, hs.y, hs.z) - p).magnitude end,
&#9;ppp = function(hs, p) return (Vector3.new(hs.x,hs.y,hs.z) - p).magnitude end,
}

function shared.closestDist( part, point )

&#9;local p = part.CFrame:pointToObjectSpace(point)

&#9;local hs = part.Size/2

&#9;local x = &quot;i&quot;
&#9;local y = &quot;i&quot;
&#9;local z = &quot;i&quot;

&#9;if p.x &lt; -hs.x then
&#9;&#9;x = &quot;m&quot;
&#9;elseif p.x &gt; hs.x then
&#9;&#9;x = &quot;p&quot;
&#9;end

&#9;if p.y &lt; -hs.y then
&#9;&#9;y = &quot;m&quot;
&#9;elseif p.y &gt; hs.y then
&#9;&#9;y = &quot;p&quot;
&#9;end

&#9;if p.z &lt; -hs.z then
&#9;&#9;z = &quot;m&quot;
&#9;elseif p.z &gt; hs.z then
&#9;&#9;z = &quot;p&quot;
&#9;end

&#9;return distMap[x..y..z](hs, p)

end

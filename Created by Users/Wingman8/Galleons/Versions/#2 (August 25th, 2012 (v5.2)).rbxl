--2010/3/25 v3.1 Initial version
--2010/4/10 v3.2 Rework timer
--2010/4/10 v4.0 Use BodyVelocity instead of BodyThrust and BodyForce
--2010/4/16 v4.1 Stop attacking player once LifeVest is off.
--2010/8/1 v4.2 Remove user defined global variables
--2011/9/30 v4.3 Rehome for GalleonsX4, add timeout to idle state


local Shark = script.Parent
local BodyVelocity= Shark.BodyVelocity
local BodyGyro = Shark.BodyGyro
local BodyPosition = Shark.BodyPosition

local SHIP_DETECT_RANGE = 300
local PLAYER_DETECT_RANGE = 100
local TARGET_ACQUIRED_RADIUS = 20
local FLEE_DISTANCE = 400

local MAX_SPEED = 20

local WATER_FORCE_MAX = Shark:GetMass() 
local WATER_FORCE_MIN = WATER_FORCE_MAX * .1

local SWIM_FORCE = (MAX_SPEED * WATER_FORCE_MAX) + WATER_FORCE_MIN

local Sea = Workspace:FindFirstChild(&quot;Sea&quot;)

if Sea then
&#9;BodyPosition.position = Vector3.new(0,Sea.Position.y+2,0)
end

local sharkMgrCor


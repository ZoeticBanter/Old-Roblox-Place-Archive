-- If Shark touches a player then BreakJoints() on whatever part
-- touched.

local function onTouched(part)

&#9;--print(&quot;touched &quot;, part.Name)

&#9;if not part.Parent then
&#9;&#9;return
&#9;end

&#9;if part == &quot;Sea&quot; then
&#9;&#9;return
&#9;end

&#9;if part.Parent:FindFirstChild(&quot;Torso&quot;) then
&#9;&#9;part:BreakJoints()
&#9;&#9;return
&#9;end

&#9;if (part.Name == &quot;Bolt&quot;) or (part.Name == &quot;FlintlockBall&quot;) then
&#9;&#9;assert(coroutine.resume(sharkMgrCor,&quot;hit&quot;, part))
&#9;&#9;return
&#9;end
end


----------------------------------------------------------------------------
-- Move toward target indicated by the targetFunc.  Adjust motion
-- every second and poll for events every 5 seconds.  If any
-- events are received from the yeild or polled (and not in the ignore list)
-- then return them.

local  function moveToward(targetFunc, ignoreEventList)

&#9;local t = 0

&#9;while true do
&#9;&#9;-- Determine the target position

&#9;&#9;local targetPos = targetFunc()

&#9;&#9;--print(&quot;moving toward &quot;, targetPos)

&#9;&#9;if not targetPos then
&#9;&#9;&#9;return &quot;target gone&quot;
&#9;&#9;end

&#9;&#9;aimToward(targetPos)

&#9;&#9;local event, p = coroutine.yield()

&#9;&#9;if event == &quot;movement interval&quot; then
&#9;&#9;&#9;
&#9;&#9;&#9;t = t + 1

&#9;&#9;&#9;if (targetPos - Shark.Position).magnitude &lt; TARGET_ACQUIRED_RADIUS then
&#9;&#9;&#9;&#9;return &quot;target acquired&quot;
&#9;&#9;&#9;end

&#9;&#9;&#9;-- Check other polled events at a  lower frequency

&#9;&#9;&#9;if (t &gt; 0) and ((t % 5) == 0) then

&#9;&#9;&#9;&#9;if not ignoreEventList[&quot;player nearby&quot;] then
&#9;&#9;&#9;&#9;&#9;local p = playerNearby()

&#9;&#9;&#9;&#9;&#9;if p then
&#9;&#9;&#9;&#9;&#9;&#9;return &quot;player nearby&quot;, p
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end

&#9;&#9;&#9;&#9;if not ignoreEventList[&quot;ship nearby&quot;] then
&#9;&#9;&#9;&#9;&#9;local p = shipNearby()

&#9;&#9;&#9;&#9;&#9;if p  then
&#9;&#9;&#9;&#9;&#9;&#9;return &quot;ship nearby&quot;, p
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end

&#9;&#9;else
&#9;&#9;&#9;if not ignoreEventList[event] then
&#9;&#9;&#9;&#9;return event, p
&#9;&#9;&#9;end

&#9;&#9;end
&#9;end
end





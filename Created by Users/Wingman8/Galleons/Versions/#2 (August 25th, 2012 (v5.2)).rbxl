local function throw( sourcePos, targetPos, timeLeft )

&#9;--print(&quot;throw src: &quot;, sourcePos, &quot; to &quot;, targetPos)

&#9;local speed = computeLaunchSpeed(sourcePos, targetPos, math.rad(angle), EFFECTIVE_g)

&#9;-- If we are not angled steeply enough to hit target then arbitrarity pick a speed
&#9;if not speed then
&#9;&#9;print(&quot;Cannot hit target with specified elevation&quot;)&#9;
&#9;&#9;speed = MAX_SPEED / 2
&#9;end

&#9;-- Adjustment empirically determined.   Do not know why this is needed.
&#9;speed = speed *.992&#9;

&#9;if speed &gt; MAX_SPEED then
&#9;&#9;speed = MAX_SPEED
&#9;end

&#9;-- v3 is the 3d world space launch vector
&#9;local v = targetPos - sourcePos
&#9;local v2 = Vector3.new(v.x, 0, v.z).unit * math.cos(math.rad(angle))
&#9;local v3 = Vector3.new(v2.x, math.sin(math.rad(angle)), v2.z)

&#9;local grenade = ProtoGrenade:Clone()
&#9;grenade.Name = &quot;HandGrenade&quot;
&#9;grenade.BrickColor = COLOR_ARMED

&#9;grenade.CFrame = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0) + v3)

&#9;grenade.FrontParamA = 8004&#9;&#9;&#9;-- Hand grenade parameter set
&#9;grenade.FrontParamB = speed&#9;&#9;-- Launch speed
&#9;grenade.RightParamA = timeLeft&#9;&#9;-- Fuse time
&#9;grenade.RightParamB = EFFECTIVE_g&#9;-- Effective gravity

&#9;-- Enable any script in the Bolt
&#9;for k,v in pairs(grenade:GetChildren()) do
&#9;&#9;if v.className == &quot;Script&quot; then
&#9;&#9;&#9;v.Disabled = false
&#9;&#9;end
&#9;end

&#9;grenade.Creator.Value = Player
&#9;
&#9;grenade.Parent = game.Workspace

&#9;grenade.Position = sourcePos

&#9;-- If grenade does not place where we attempted to put it 
&#9;-- this means there is something there.  This probably
&#9;-- means Player is attempting to throw &quot;thru&quot; a wall or 
&#9;-- some obstacle.  Remove grenade in this case but still
&#9;-- do reload wait.

&#9;if grenade.Position.y &gt; sourcePos.y + .5 then
&#9;&#9;grenade:Remove()
&#9;end

&#9;state = &quot;thrown&quot;

&#9;local co = coroutine.create(reload)
&#9;assert(coroutine.resume(co, grenade, timeLeft))

end



local coor = coroutine.create(monitorWhirling)
assert(coroutine.resume(coor))


local REASSESS_INTV = 20&#9;-- Time interval to reassess the sail force of the ship
local time = 0

local function main()

&#9;-- Main loop while Ship still in the world.  Other scripts can Remove() a ship 
&#9;-- and we do not want this script to keep going after ship is removed.

&#9;while Ship.Parent do

&#9;&#9;wait(MOVEMENT_INTV)

&#9;&#9;time = time + MOVEMENT_INTV
&#9;&#9;local r,m = pcall(moveShip,sail, turn)
&#9;&#9;if not r then
&#9;&#9;&#9;print(&quot;*************************ScriptNavControl failure calling moveShip: &quot;, m)
&#9;&#9;&#9;return
&#9;&#9;end
&#9;&#9;
&#9;&#9;-- If we receive test command to sink ship
&#9;&#9;if scuttle then
&#9;&#9;&#9;CntrScuttle.Value = &quot;damage&quot;
&#9;&#9;&#9;scuttleShip(false)
&#9;&#9;&#9;return 
&#9;&#9;end

&#9;&#9;-- If enough damage then sink ship
&#9;&#9;if sinkShip(CntrRatioDestroyed.Value, SINK_RATIO, SINK_UNITS) then
&#9;&#9;&#9;CntrScuttle.Value = &quot;damage&quot;
&#9;&#9;&#9;scuttleShip(false)
&#9;&#9;&#9;return
&#9;&#9;end

&#9;&#9;-- If we are directed to sink ship from game control
&#9;&#9;if CntrScuttle.Value and (CntrScuttle.Value == &quot;admin&quot;) then
&#9;&#9;&#9;scuttleShip(true)
&#9;&#9;&#9;return
&#9;&#9;end

&#9;&#9;--print(string.format(&quot;speed: %.1f, BodyForce: %.1f,%.1f, vel: %.1f,%.1f, sailF: %.1f,%.1f, waterF: %.1f, %.1f&quot;,
&#9;&#9;&#9;--speed,BodyForce.force.x, BodyForce.force.z, Keel.Velocity.x, Keel.Velocity.z,sailForce.x,sailForce.z, waterForce.x,waterForce.z))
&#9;end
end


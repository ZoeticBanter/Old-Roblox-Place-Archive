local function computeLaunchSpeed(source, target, angle, gravity)

&#9;local v = target - source
&#9;local mxz = Vector3.new(v.x, 0, v.z).magnitude

&#9;local r = (gravity * mxz * mxz) / ( 2 * math.cos(angle) * math.cos(angle) * ( (mxz * math.tan(angle)) - v.y ) )

&#9;if r &lt; 0 then
&#9;&#9;return nil
&#9;end

&#9;return math.sqrt(r)
end



local function ftrim(face, ra, rb)

&#9;local u = (rb - ra).unit

&#9;local l = (ra.y - SHORE_CUTOFF) / u.y

&#9;local p = ra - (u * l)

&#9;return p

end
---------------------------------------------------------------------------------------------------------
local function rsCreateFaces()

&#9;for face in face_iter() do

&#9;&#9;if face.active then

&#9;&#9;&#9;-- If exactly two points are underwater then trim triangle to water line

&#9;&#9;&#9;if (face.c.y &gt; 0) and (face.a.y &lt; SHORE_CUTOFF) and (face.b.y &lt; SHORE_CUTOFF) then
&#9;&#9;&#9;&#9;local a = ftrim(face, face.c, face.a)
&#9;&#9;&#9;&#9;local b = ftrim(face, face.c, face.b)
&#9;&#9;&#9;&#9;fPopCf(face, a, b, face.c)&#9;-- Recalculate cframe and center, maybe switch vertices
&#9;
&#9;&#9;&#9;elseif (face.b.y &gt; 0) and (face.a.y &lt; SHORE_CUTOFF) and (face.c.y &lt; SHORE_CUTOFF) then
&#9;&#9;&#9;&#9;local a = ftrim(face, face.b, face.a)
&#9;&#9;&#9;&#9;local c = ftrim(face, face.b, face.c)
&#9;&#9;&#9;&#9;fPopCf(face, a, face.b, c)&#9;-- Recalculate cframe and center, maybe switch vertices

&#9;&#9;&#9;elseif (face.a.y &gt; 0) and (face.b.y &lt; SHORE_CUTOFF) and (face.c.y &lt; SHORE_CUTOFF) then
&#9;&#9;&#9;&#9;local b = ftrim(face, face.a, face.b)
&#9;&#9;&#9;&#9;local c = ftrim(face, face.a, face.c)
&#9;&#9;&#9;&#9;fPopCf(face, face.a, b, c)&#9;-- Recalculate cframe and center, maybe switch vertices
&#9;&#9;&#9;end

&#9;&#9;&#9;fCreate(face)

&#9;&#9;&#9;faceCount = faceCount + 1

&#9;&#9;&#9;-- If angled steeply enough
&#9;&#9;&#9;if face.angle &gt; F_ANGLE_THRESHOLD then
&#9;&#9;&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Dark stone grey&quot;)
&#9;&#9;&#9;&#9;face.wpa.Material = Enum.Material.Slate
&#9;&#9;&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;&#9;&#9;face.wpb.Material = face.wpa.Material
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;local count = 0
&#9;&#9;&#9;&#9;if face.a.y &lt; 0 then count = count + 1 end
&#9;&#9;&#9;&#9;if face.b.y &lt; 0 then count = count + 1 end
&#9;&#9;&#9;&#9;if face.c.y &lt; 0 then count = count + 1 end

&#9;&#9;&#9;&#9;if count == 2 and face.angle &lt; 20 then
&#9;&#9;&#9;&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Brick yellow&quot;)
&#9;&#9;&#9;&#9;&#9;face.wpa.Material = Enum.Material.Concrete
&#9;&#9;&#9;&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;&#9;&#9;&#9;face.wpb.Material = face.wpa.Material

&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;face.wpa.BrickColor = BrickColor.new(&quot;Grime&quot;)
&#9;&#9;&#9;&#9;&#9;--face.wpa.BrickColor = BrickColor.new(&quot;Medium green&quot;)
&#9;&#9;&#9;&#9;&#9;face.wpa.Material = Enum.Material.Grass
&#9;&#9;&#9;&#9;&#9;face.wpb.BrickColor = face.wpa.BrickColor
&#9;&#9;&#9;&#9;&#9;face.wpb.Material = face.wpa.Material
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end



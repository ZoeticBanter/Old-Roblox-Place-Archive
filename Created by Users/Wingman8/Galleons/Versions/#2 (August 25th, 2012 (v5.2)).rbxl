local function assessHit(opos, bvec, dincr)

&#9;for i = 1,10 do

&#9;&#9;local vincr = bvec * dincr

&#9;&#9;local ro = Ray.new(opos, vincr)
&#9;&#9;local part,pos = Workspace:FindPartOnRay(ro, Bullet)

&#9;&#9;-- If not hit anything
&#9;&#9;if not part then
&#9;&#9;&#9;return &quot;none&quot;
&#9;&#9;end

&#9;&#9;-- See if hit a player&#9;&#9;&#9;
&#9;&#9;local tp = part.Parent
&#9;&#9;local torso = tp:FindFirstChild(&quot;Torso&quot;) or 
&#9;&#9;&#9;(((tp.className == &quot;Tool&quot;) or (tp.className == &quot;Hat&quot;)) and tp.Parent:FindFirstChild(&quot;Torso&quot;))

&#9;&#9;-- If hit a player
&#9;&#9;if torso then
&#9;&#9;&#9;local player = game.Players:playerFromCharacter(torso.Parent)

&#9;&#9;&#9;-- If player on other team then score a hit
&#9;&#9;&#9;if player and (player.TeamColor ~= Creator.TeamColor) then
&#9;&#9;&#9;&#9;blastPlayer(torso, pos)
&#9;&#9;&#9;&#9;return &quot;player&quot;

&#9;&#9;&#9;-- Else hit player on our team.  No TK but we absorb bullet here.
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;return &quot;absorb&quot;, pos, part
&#9;&#9;&#9;end
&#9;&#9;end


&#9;&#9;-- If part is collidable then this will stop us
&#9;&#9;if part.CanCollide then
&#9;&#9;&#9;return &quot;absorb&quot;, pos, part
&#9;&#9;end

&#9;&#9;-- Decrement our distance to probe to account for the distance already covered
&#9;&#9;dincr = dincr - (pos - opos).magnitude 

&#9;&#9;-- Lets not mess with tiny or negative distances
&#9;&#9;if dincr &lt; .1 then
&#9;&#9;&#9;return &quot;none&quot;
&#9;&#9;end

&#9;&#9;-- Reprobe again from the last hit position (advanced a little into the ignored
&#9;&#9;-- part so we do not find it again)
&#9;&#9;opos = pos + (bvec * .1)
&#9;end


&#9;return &quot;none&quot;
end


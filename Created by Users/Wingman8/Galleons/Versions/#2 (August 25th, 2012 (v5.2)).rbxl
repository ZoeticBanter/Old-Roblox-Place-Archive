local EFFECTIVE_g70 = 100

local MAX_SPEED = MAX_SPEED_25
local EFFECTIVE_g = EFFECTIVE_g25

---------------------------------------------------------------------------------------------------

local function displayStatus()

&#9;local grenade = 0

&#9;local cntrGrenades = Player:FindFirstChild(&quot;CntrGrenades&quot;)

&#9;if cntrGrenades and cntrGrenades.Value then
&#9;&#9;grenade = cntrGrenades.Value
&#9;end

&#9;shared.displayHint(&quot;Mouse down to arm, up to throw, arc: b=low, g=medium, t=high, Grenades left: &quot;..grenade,0)

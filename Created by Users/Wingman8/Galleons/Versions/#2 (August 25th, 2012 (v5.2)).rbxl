&#9;&#9;&#9;dias2 = diasStep(dias2)
&#9;&#9;end
&#9;&#9;if dias3 then
&#9;&#9;&#9;dias3 = diasStep(dias3)
&#9;&#9;end
&#9;&#9;if dias4 then
&#9;&#9;&#9;dias4 = diasStep(dias4)
&#9;&#9;end
&#9;&#9;if dias5 then
&#9;&#9;&#9;dias5 = diasStep(dias5)
&#9;&#9;end
&#9;&#9;if dias6 then
&#9;&#9;&#9;dias6 = diasStep(dias6)
&#9;&#9;end


&#9;&#9;if (not dias) and (not dias2) and (not dias3) and (not dias4) and (not dias5) and (not dias6)  then
&#9;&#9;&#9;--print(&quot;sll dias done&quot;)

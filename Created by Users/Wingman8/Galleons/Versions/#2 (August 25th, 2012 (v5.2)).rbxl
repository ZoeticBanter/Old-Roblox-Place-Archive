-- Create a new Frag object based on the indicated block
function Frag:new (block)

&#9;local o = { block = block }
    
&#9;setmetatable(o, self)
&#9;self.__index = self

&#9;o.model =  Instance.new(&quot;Model&quot;)
&#9;o.model.Parent = block.Parent

&#9;block.Parent = o.model  -- put the block into new model

&#9;o.fragmentList = {}
&#9;o.numFrags = 0

&#9;local length,width

&#9;-- Determine which dimension is thickness
&#9;if (block.Size.y &lt;= block.Size.x) and (block.Size.y &lt;= block.Size.z) then
&#9;&#9;o.thickDim = &quot;y&quot;
&#9;&#9;length = block.Size.x
&#9;&#9;width = block.Size.z

&#9;elseif  (block.Size.z &lt;= block.Size.x) and (block.Size.z &lt;= block.Size.y) then
&#9;&#9;o.thickDim = &quot;z&quot;
&#9;&#9;length = block.Size.x
&#9;&#9;width = block.Size.y

&#9;else
&#9;&#9;print(&quot;***ERROR**** do not support thickness in the x dimension. Part: &quot;, block.Name)
&#9;&#9;return nil

&#9;end
&#9;--print(&quot;len:&quot;, length, &quot; wid:&quot;, width)

&#9;-- If we are big enough then our initial state is a section
&#9;if (length &gt;= SECT_MIN_L) or (width &gt;= SECT_MIN_W) then
&#9;&#9;o.state = &quot;section&quot;

&#9;-- Else our initial state is a partition
&#9;else
&#9;&#9;o.state = &quot;partition&quot;
&#9;end

&#9;o.name = block.Name

&#9;o.curMass = block:GetMass()

&#9;o.origRelLv = (Keel.CFrame:toObjectSpace(block.CFrame)).lookVector

&#9;--print(&quot;New created with state &quot;, o.state)

&#9;-- Define a frag handler for the block in this object
&#9;defineHandler(o, block)

&#9;-- Add us to the master list of Frag objects
&#9;FragList[o] = o
     
&#9;return o
end



-- Teleport the specified ship using the specified targetCFrame.
-- This is the CFrame of where the Keel part of the ship 
-- should be located and oriented.  Then for each other part of the ship
-- derive the object space CFrame between that part and the Keel, then change that
-- parts CFrame to the target CFrame rotated and translated by that object space CFrame.
-- Lastly, position the Keel to the targetCFrame.

local function placeShip( ship, targetCFrame )

	local Keel = ship.Keel

	local sourceCFrame = Keel.CFrame

	local function rec( ship )

		for k,v  in pairs(ship:GetChildren()) do

			if (v.className == &quot;Part&quot;) or (v.className == &quot;Seat&quot;) or (v.className == &quot;SpawnLocation&quot;) or 
			    (v.className == &quot;WedgePart&quot;) or (v.className == &quot;TrussPart&quot;) then

				if v.Name ~= &quot;Keel&quot; then

					local dcf = sourceCFrame:toObjectSpace(v.CFrame)
					v.CFrame = targetCFrame * dcf
				end

			elseif v.className == &quot;Model&quot; then
				rec(v)
			end
		end
	end

	rec(ship)

	Keel.CFrame = targetCFrame

end

----------------------------------------------------------------------------------------
function paintShip( ship, col )

	for k,v in ipairs(ship:GetChildren()) do

		if ((v.className == &quot;Part&quot;) or (v.className == &quot;Seat&quot;)) and 
		   ((v.BrickColor == COLOR_RED) or (v.BrickColor == COLOR_BLUE)) then
			v.BrickColor = col
		
		elseif v.className == &quot;SpawnLocation&quot; then
			v.BrickColor = col
			v.TeamColor = col

		elseif v.className == &quot;Model&quot;  then
			paintShip(v, col)
		end
	end
end


-----------------------------------------------------------------------------------------
local function genShip(shipName, col, genCf)
	
	local protoShip = ProtoShips:FindFirstChild(shipName)

	if not protoShip then
		print(&quot;Could not find a ship named: &quot;, shipName)
		return nil
	end

	local ship = protoShip:Clone()

	placeShip(ship, genCf)
	replaceScripts(ship)
	paintShip(ship, col)

	return ship
	
end

----------------------------------------------------------------------------------------
-- We have detected that at least one ship has sunk, wait a few seconds for
-- victory party they display winning results.
function gameOver()

	local conditionRed = &quot;ok&quot;
	local conditionBlue = &quot;ok&quot;

	-- If ship is not in the world then it is sunk
	if RedShip.Parent == nil then
		conditionRed = &quot;sunk&quot;

	-- Else if CntrScuttle indicates it has sustained enough damage to eventually sink
	else
		local keel = RedShip:FindFirstChild(&quot;Keel&quot;)

		if not keel then
			conditionRed = &quot;sunk&quot;
		else
			local CntrScuttle = RedShip.Keel:FindFirstChild(&quot;CntrScuttle&quot;)
			if CntrScuttle and CntrScuttle.Value == &quot;damage&quot; then
				conditionRed = &quot;sinking&quot;
			end
		end
	end

	-- If ship is not in the world then it is sunk
	if BlueShip.Parent == nil then
		conditionBlue = &quot;sunk&quot;

	-- Else if CntrScuttle indicates it has sustained enough damage to eventually sink
	else

		local keel = BlueShip:FindFirstChild(&quot;Keel&quot;)

		if not keel then
			conditionBlue = &quot;sunk&quot;
		else
			local CntrScuttle = BlueShip.Keel:FindFirstChild(&quot;CntrScuttle&quot;)
			if CntrScuttle and CntrScuttle.Value == &quot;damage&quot; then
				conditionBlue = &quot;sinking&quot;
			end
		end
	end

	-- If neither has sunk yet then keep waiting
	if not ((conditionRed == &quot;sunk&quot;) or (conditionBlue == &quot;sunk&quot;)) then
		return false
	end

	-- If both sunk
	if (conditionRed == &quot;sunk&quot;) and (conditionBlue == &quot;sunk&quot;) then
		text = &quot;Both sides sunk! (Vote for next ship)&quot;

	elseif (conditionRed == &quot;sunk&quot;) and (conditionBlue == &quot;sinking&quot;) then
		text = &quot;Red has sunk, but Blue is no longer sea worthy (Vote for next ship)&quot;

	elseif (conditionBlue == &quot;sunk&quot;) and (conditionRed == &quot;sinking&quot;) then
		text = &quot;Blue has sunk, but Red is no longer sea worthy (Vote for next ship)&quot;

	elseif (conditionRed == &quot;sunk&quot;) and (conditionBlue == &quot;ok&quot;) then
		text = &quot;Blue has sunk Red! (Vote for next ship)&quot;	

	elseif (conditionBlue == &quot;sunk&quot;) and (conditionRed == &quot;ok&quot;) then
		text = &quot;Red has sunk Blue! (Vote for next ship)&quot;
	end

	local message = Instance.new(&quot;Message&quot;)
	message.Parent = game.Workspace
	message.Text= text

	wait(10)

	message.Parent = nil

	return true

end


----------------------------------------------------------------------------------------------------
-- Get a list of shipnames and the number of votes for each.  If there is not
-- a clear cut winner then select randomly from among all ships.

local function pickOneNewShip( col )

	local votes = _G.countPlayerVotes(col)

	local curHighestVotes = 0
	local curWinner = nil

	for s,v in pairs(votes) do

		if v &gt; curHighestVotes then
			curHighestVotes = v
			curWinner = s

		-- If we match vote count of current highest ship then neither of these ships can be the winner
		elseif v == curHighestVotes then
			curWinner = nil
		end
	end

	if curWinner then
		return curWinner, &quot;(votes: &quot;..curHighestVotes..&quot;)&quot;
	end

	local list = ProtoShips:GetChildren()

	local n = math.random(1, #list)

	local shipName = list[n].Name
	local svotes = 0

	if votes[shipName] then
		svotes = votes[shipName]
	end

	return shipName, &quot;(no majority, randomly selected)&quot;

end

-------------------------------------------------------------------------------------------------------
local function pickNewShips()

	local redShipName, redVoteStatus= pickOneNewShip(COLOR_RED)
	local blueShipName, blueVoteStatus = pickOneNewShip(COLOR_BLUE)

	local text = &quot;Vote result: Red: &quot;.. redShipName ..redVoteStatus..
					     &quot;,    Blue: &quot;..blueShipName ..blueVoteStatus

	return redShipName, blueShipName, text

end


----------------------------------------------------------------------------------------------------
local function mypcall(funcName, ...)

	local s,m = pcall(...)
	if not s then
		print(&quot;ScriptGalleonGameControl: Error returned from &quot;, funcName, &quot;: &quot;, m)
	end

end
----------------------------------------------------------------------------------------
function regenAll( timeout )

	if timeout then
		local message = Instance.new(&quot;Message&quot;)
		message.Parent = game.Workspace
		message.Text= &quot;15 minute game time exceeded. (Vote for next ship)&quot;

		wait(10)

		message.Parent = nil
	end

	if RedShip and RedShip.Parent then
		RedShip:Remove()
		RedShip = nil
	end

	if BlueShip and BlueShip.Parent then
		BlueShip:Remove()
		BlueShip = nil
	end

	-- Count the votes and pick the new ships

	local pRed, pBlue, text = pickNewShips()

	local message = Instance.new(&quot;Message&quot;)
	message.Parent = Workspace
	message.Text= text			-- Text describing results of voting

	wait(5)

	message.Parent = nil


	mypcall(&quot;_G.convertPlayersPnts&quot;, _G.convertPlayersPnts)	-- Convert players pnt to gold and zero out pnts for next game

	emptySwitchList()
	GameTime.Value = 0

	message.Parent = nil

	RedShip = genShip(pRed, COLOR_RED, genCfRed)
	BlueShip = genShip(pBlue, COLOR_BLUE, genCfBlue)

	enableScripts(RedShip)
	enableScripts(BlueShip)

	RedShip.Parent = game.Workspace
	BlueShip.Parent = game.Workspace

	RedShip:MakeJoints()
	BlueShip:MakeJoints()

	-- Delete any previous islands and make new ones such that they are not near
	-- either ship.
	mypcall(&quot;_G.makeIslands&quot;,_G.makeIslands,RedShip, BlueShip)

	wait(3)

	unAnchor(RedShip)
	unAnchor(BlueShip)

	genSharks()	-- Remove any old sharks and gen new ones

end

----------------------------------------------------------------------------------------------------

local firstTime = true


local INTERVAL = 5
local MAX_GAME_TIME =  15 * 60	-- 15 minutes

local timer

while true do

	if firstTime or gameOver() then

		firstTime = false

		regenAll(false)
		timer = MAX_GAME_TIME

	else

		if timer &gt; 0 then
			timer = timer - INTERVAL

		else
			regenAll(true)
			timer = MAX_GAME_TIME
		end
	end


	wait(INTERVAL)

	GameTime.Value = GameTime.Value + INTERVAL

end

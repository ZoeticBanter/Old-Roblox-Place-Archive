local curDebugLine = 1
local function displayDebug( line )

	if FrameDebug == nil then
		return
	end

	local tl = Instance.new(&quot;TextLabel&quot;)
	tl.BackgroundTransparency = 1
	tl.TextTransparency = 0
	
	tl.Position = UDim2.new(0, 0, 0, curDebugLine*15)
	tl.Size = UDim2.new(1, 0, 0, 15)
	tl.Text = line

	tl.ZIndex = FrameDebug.ZIndex + 1

	tl.Parent = FrameDebug

	curDebugLine = curDebugLine + 1

	FrameDebug.Visible = true
end


----------------------------------------------------------------------------------------------------

local function displayPage(frame,text)

	for k,v in pairs(frame:GetChildren()) do
		if v.className == &quot;TextLabel&quot; then
			v:Remove()
		end
	end

	local totPixels = frame.Size.X.Offset

	local linenum = 1

	for line in string.gmatch(text, &quot;[^\n]*\n&quot;) do

		local tl = Instance.new(&quot;TextLabel&quot;)
		tl.BackgroundTransparency = 1
		tl.TextTransparency = 0
		tl.ZIndex = frame.ZIndex + 1
		tl.TextXAlignment = Enum.TextXAlignment.Left
		
		tl.Position = UDim2.new(0, INDENT, 0, linenum*15)
		tl.Size = UDim2.new(1, 0, 0, 15)
		tl.Text = line

		tl.Parent = frame

		linenum = linenum + 1
	end
end




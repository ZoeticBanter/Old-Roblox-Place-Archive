					<ProtectedString name="Source">--------------------------------------------------------------------------------------------------------------------------
--2009/10/4 v1.0 Initial version based on ScriptShellBf-v3.0
--2009/10/14 Fix missed touched on player, check for ship TK
--2009/10/16 BodyForce already defined
--2009/11/8 v1.3 Make TK deductions immediately to the players Gold so player is penalized even if not assigned a color.
--2009/11/13 v1.4 Support cannonball hitting a part with Shape of sphere
--2010/1/15 Retrofit to Galleons game
--2010/1/17 try to make fly exactly like original Galleons game
--2010/1/21 peg damage instead of gold
--2010/1/22 Remove reference to Shape
--2010/3/25 v2.3 Add splash effect if hits water
--2010/3/28 v2.4 Ignore sharks
--2010/4/9 v2.5 Limit max damage  to 1900

--[[
	- Script maintained under ProtoShell Part that is already physically configured.

	- The firing weapon clones ProtoShell, aims it by setting its .CFrame, and starts this script.

	- Uses BodyForce to reduce gravity and throws the Shell by initially setting .Velocity.

	- Parameter set code selects one of a fixed set of parameter configurations

	- Every wait() interval Missile.RotVelocity changed incrementally to insure we get all touch events.

	Parameters based in the Shell part:

		bolt.CFrame 		-- Launch vector
		bolt.Velocity 		-- Any initial velocity caused by mount movement

		bolt.FrontParamA	-- Parameter set code
--]]


local Bolt = script.Parent

-- Get bolt parameters from the library based on the parm set code in FrontParamA
local Ps = _G.glBoltParms[Bolt.FrontParamA]

local parmHardRadius = Ps.parmDefHardRadius
local parmSoftRadius = Ps.parmDefSoftRadius
local parmDefForce = Ps.parmDefForce
local parmRange = Ps.parmRange
local parmGravity = Ps.parmGravity
local parmSpeed = Ps.parmSpeed

local InitialPosition
local connection
local explosion
local fireSound
local blastPosition
local bodyForce

local seaLevel = 0
local Sea = Workspace:FindFirstChild(&quot;Sea&quot;)
if Sea then
	seaLevel = Sea.Position.y
end

local MAX_DAMAGE = 1900
local curDamage = 0

------------------------------------------------------------------------------------------------------------------

local function onHit(part,distance)

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

	if torso then
		--print(&quot;torso distance &quot;, distance, &quot; parmSoftRadius &quot;, parmSoftRadius)
		if distance &lt; parmSoftRadius then
			_G.glBlastPlayer(torso, blastPosition, Bolt)
		end

		return
	end

	if part.Name == &quot;Bolt&quot; then
		return
	end

	if part.Anchored then
		return
	end

	-- If part is non-destroyable
	if part.FrontParamA &lt; 0 then
		return
	end

	-- Get the closest distance the blast is to the part we hit, use that to calculate damage 
	-- rather than the distance supplied which is the distance to the center of the part.
	local dist = _G.closestDist(part, blastPosition)

	--print(&quot;Actual dist &quot;, distance, &quot; closest dist &quot;, dist)

	local dm = _G.glHitHandler(part, dist, blastPosition, parmHardRadius, parmDefForce)

	--print(&quot;onHit part.BottomParamB &quot;,part.BottomParamB, &quot; ourSide &quot;, ourSide )

	-- Limit total damage done by this Bolt to MAX_DAMAGE
	if curDamage + dm &gt; MAX_DAMAGE then
		dm = MAX_DAMAGE - curDamage

		if dm &lt;= 0 then
			return
		end
	end

	curDamage = curDamage + dm

	_G.glPegDamage(Bolt, dm)

--]]

end


------------------------------------------------------------------------------------------------------------------------

local firstTouch = true

function onTouch(part)

	--print(&quot;Touch event with &quot;, part.Name)

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

	-- Ignore any non-collidable parts (except player parts)
	if (not torso) and (part.CanCollide == false)  then
		return
	end

	-- Ignore touch of Sharks or Sea 
	if string.match(part.Name,&quot;Shark&quot;) or (part.Name == &quot;Sea&quot;) then
		return
	end

	if firstTouch == false then

		--print(&quot;CannonBall: received subsequent touch from &quot;, part.Name)
		return
	end

	-- No more touch events for the Bolt

	firstTouch = false

	if connection then
		connection:disconnect()
		connection = nil
	end



	-- If we are CanCollide this means the main routine determined our
	-- velocity fell below the threshold where bolt is now harmless.
	if Bolt.CanCollide  then
		bodyForce.Parent = nil

		-- Not interested in any more touch events
		if connection then
			connection:disconnect() 
			connection = nil
		end

		-- Make a sound of bolt hitting wood
		local tockSound = Instance.new(&quot;Sound&quot;)
		tockSound.SoundId = &quot;rbxasset://sounds\\flashbulb.wav&quot;
		tockSound.Parent = Bolt
		tockSound.Volume = 1
		tockSound.PlayOnRemove = false
		tockSound:play()	

		wait(2)	-- Allow dead bolt  to live for a little while
		Bolt:remove()
		return
	end


	-- Derive a pseudo source and target  so we can call the findTp().  Take
	-- Current position and current Velocity and backup 2000 for source and
	-- go forward 2000 for target

	local source = Bolt.Position - (Bolt.Velocity.unit * 2000)
	local target = Bolt.Position + (Bolt.Velocity.unit * 2000)

	local direct,contactPoint,normal = _G.glFindTp(source,target, part)

	-- Make blastPosition one unit above the contacting surface
	blastPosition = contactPoint + normal.lookVector

	-- If this part is a powder keg then remove it and 
	-- increase the blast radius and force to
	-- simulate the powder keg exploding.
	if string.match(part.Name, &quot;PowderKeg&quot;) then
		blastPosition = part.Position
		parmHardRadius = part.RightParamA
		parmSoftRadius = parmHardRadius
		parmDefForce = part.RightParamB
		part:Remove()

	-- If this part has a fragmentation handler then call it before exploding.
	elseif _G.FragHandler and _G.FragHandler[part]  then

	--print(&quot;cb touch calls frag handler on &quot;, part.Name)

		_G.FragHandler[part](Bolt, blastPosition)
	end

	--print(&quot;Bolt: contact point: &quot;, contactPoint, &quot; Bolt pos&quot;, Bolt.Position)

	-- Create explosion
	explosion = Instance.new(&quot;Explosion&quot;)
	explosion.Position = blastPosition
	explosion.BlastRadius = parmHardRadius
	explosion.BlastPressure = 0
	explosion.Parent = Bolt

	explosion.Hit:connect(onHit)

	local sound = Instance.new(&quot;Sound&quot;)
	sound.SoundId = &quot;rbxasset://sounds\\collide.wav&quot;
	sound.Parent = Bolt
	sound.Volume = 1
	sound.PlayOnRemove = false
	sound:play()

--[[
	local marker = Instance.new(&quot;Part&quot;)
	marker.formFactor = 0
	marker.Shape = 0
	marker.BrickColor = BrickColor.Green()
	marker.Anchored = true
	marker.Size = Vector3.new(1,1,1)
	marker.CanCollide = false
	marker.CFrame = CFrame.new(blastPosition)
	marker.Parent = game.Workspace
--]]

	-- Since we are waiting a little for sound to play and for Hit events to be collected
	-- we mark our Bolt as invisible so it does not seem to go thru things.


	Bolt.Transparency = 1

	wait(1)

	Bolt:remove()
end

----------------------------------------------------------------------------------------------------
local function splash()

	if not firstTouch then
		wait(1)
		return
	end

	firstTouch = false

	local vu = Bolt.Velocity.unit

	-- Find exact position where bolt would enter water
	local tp = Bolt.Position - (vu * ((Bolt.Position.y - seaLevel) / vu.y))

	-- Put Bolt there and anchor it, size it to 1,1,1 and add the head mesh

	Bolt.BrickColor = BrickColor.new(&quot;Pastel light blue&quot;)
	Bolt.Size = Vector3.new(1,1,1)
	Bolt.CFrame = CFrame.new(tp)
	Bolt.Anchored = true


	local mesh = Instance.new(&quot;SpecialMesh&quot;)
	mesh.MeshType = 0 -- Head mesh
	mesh.Scale = Vector3.new(1,.1,1)
	mesh.Parent = Bolt
	
	local scl = 1

	-- Do the splash out and up animation
	for i =1,4 do

		mesh.Scale = Vector3.new(scl,.1,scl)
		scl = scl + 2.5
		wait(.1)
	end

	-- Do the fall down and slower out animation
	for i = 1,8 do

		scl = scl + .5
		Bolt.CFrame = Bolt.CFrame - Vector3.new(0,.5,0)
		mesh.Scale = Vector3.new(scl,.1,scl)
		wait(.1)
	end
end

----------------------------------------------------------------------------------------------------
function waitForChild(parent, childName)
	local child = parent:findFirstChild(childName)
	if child then return child end
	while true do
		child = parent.ChildAdded:wait()
		if child.Name==childName then return child end
	end
end
--------------------------------------------------------------------------------------------------
-------------------------------------------------------------------------------------------------

-- Insure these components have been added before continuing
bodyForce = waitForChild(Bolt, &quot;BodyForce&quot;)
local creatorPlayer = waitForChild(Bolt,&quot;Creator&quot;).Value

local direction = Bolt.CFrame.lookVector

Bolt.Anchored = false	-- In case creator did not do this.

Bolt.Transparency = 0

-- Compute the amount of body force needed to offset gravity enough to simulate the 
-- gravity we want.
local realGravity = (9.81 * 20)	-- real acceleration due to gravity in RBX units
local f = (realGravity - parmGravity) * Bolt:GetMass()	-- Amount of force we need to simulate our gravity
local gOffset = Vector3.new(0, f, 0)

InitialPosition = Bolt.Position

-- Launch the Bolt, include the Bolts residual velocity imparted by the launcher.
Bolt.Velocity = Bolt.Velocity + (direction * parmSpeed) 

-- Make a sound at firing time
fireSound = Instance.new(&quot;Sound&quot;)
fireSound.SoundId = &quot;rbxasset://sounds\\Shoulder fired rocket.wav&quot;
fireSound.Parent = Bolt
fireSound.Volume = .5
fireSound.PlayOnRemove = false
fireSound:play()


connection = Bolt.Touched:connect(onTouch)

-- The friction force is initially COEF1 times the current bolt speed.  Instead of 
-- incrementally changing this force as the bolt slows down just assign an 
-- initial one then adjust using COEF2 once the bolt falls below 1/3 its initial speed.
-- Once bolt drops to below absolute speed 15 remove all air friction plus remove 4/5 of the 
-- gravity offset so the bolt drops faster.
-- These values are purely empirical.

local AIR_FRICTION_COEF1 = 1.1
local AIR_FRICTION_COEF2 = .8
local airFriction = -(parmSpeed * AIR_FRICTION_COEF1) * Bolt.Velocity.unit
	
-- Add in air friction to the body force object.
bodyForce.force = gOffset + airFriction

local r = game:service(&quot;RunService&quot;)
local TIME_MAX = 10
local INTERVALS_PER_SECOND = 30
local t = 0

local ROT_INCR = Vector3.new(.001,.001,.001)
local stage = 0

while t &lt; TIME_MAX  do	

	local _,s = r.Stepped:wait()
	t = t + s

	-- Modify the .RotVelocity an undetectable amount in order to insure touch events occur.
	Bolt.RotVelocity = Bolt.RotVelocity + ROT_INCR

	local cv = Bolt.Velocity
	local cvm = Vector3.new(cv.x, 0, cv.z).magnitude -- the speed in just the xz dimensions

	if (cvm &lt; 100) then

		-- First stage is when bolt has dropped to below 1/3 its initial speed.
		if stage &lt; 1 then
			Bolt.BrickColor = BrickColor.Gray()
			Bolt.CanCollide = true
			bodyForce.force = gOffset - (cv.unit * 80)
			stage = stage + 1
			--print(&quot;stage 1 force: &quot;, bodyForce.force)
		end
	
		-- Second stage is when bolt has dropped to below absolute value of 15.
		if (cvm &lt; 15) and (stage &lt; 2) then
			bodyForce.force = gOffset/5
			stage = stage + 1
			--print(&quot;stage 2 force: &quot;, bodyForce.force)
		end
	end

	-- If bolt falls below sea level, and we have not touched anything yet, then
	-- do splash effect
	if Bolt.Position.y &lt; seaLevel then
		splash()
		break
	end


	--print(&quot;velocity: &quot;, Bolt.Velocity, &quot;air f &quot;, airFriction, &quot; force &quot;, bodyForce.force)
	--print(&quot;Mag &quot;, cvm)

end


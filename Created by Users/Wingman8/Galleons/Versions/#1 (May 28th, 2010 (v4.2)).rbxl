local lastKey = nil

local function onKeyDown(key)

	if Cannon == nil then
		return
	end

	if key == &quot;v&quot; then

		toggleCamera()
	end
end

--------------------------------------------------------------------------------------
local function allocateSeaLegs( torso, mount)

	local hum = torso.Parent:FindFirstChild(&quot;Humanoid&quot;)
	if not hum then
		return
	end

	-- If already seated, say on the captain chair, then do not allocate SeaLegs
	if hum.Sit then
		return
	end

	-- If not already seated, then remove any existing SeaLegs.  Not sure how this can
	-- happen, but sometimes SeaLegs get left behind then cannot sit at any other cannon.
	if SeaLegs then
		SeaLegs:Remove()
	end

	if SeaLegs == nil or SeaLegs.Parent == nil then

		SeaLegs = Instance.new(&quot;Seat&quot;)
		SeaLegs.Name = &quot;SeaLegs&quot;
		SeaLegs.Size = Vector3.new(1,1,1)
		SeaLegs.CanCollide = false
		SeaLegs.Position = Torso.Position
		SeaLegs.CFrame = Torso.CFrame - Vector3.new(0,2.5,0)

		local cf = mount.CFrame:toObjectSpace(SeaLegs.CFrame)

		PWeld = Instance.new(&quot;Weld&quot;)
		PWeld.Name = &quot;PWeld&quot;

		--print(&quot;Controller &quot;, Torso.Parent.Controller)

		PWeld.Part0 = mount
		PWeld.Part1 = SeaLegs
		PWeld.C0 = cf
		PWeld.Parent = Workspace

		local ov = Instance.new(&quot;ObjectValue&quot;)
		ov.Name = &quot;PWeldVal&quot;
		ov.Value = PWeld
		ov.Parent = SeaLegs

		local sc = SeaLegsScript:Clone()
		sc.Disabled = false
		sc.Parent = SeaLegs

		SeaLegs.Parent = Workspace
	end
end



--------------------------------------------------------------------------------------------------
local function onButton1Down()

	if Cannon then
		--local ccf =  game.Workspace.CurrentCamera.CoordinateFrame
		--local cp = Vector3.new(ccf.x, ccf.y,ccf.z)
		--local cp = Player.Character.Head.Position

		CntrAim.Value = Mouse.Hit.p -  Mouse.Origin.p
	end
end

local function onButton1Up()

	
end
--------------------------------------------------------------------------------------------------
local function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end

	Torso = script.Parent.Parent.Torso

	-- Detach from any previous cannon (there should be none)
	detach()

	local cannon, dist = findClosestCannon(Workspace, Torso)

	--print(&quot;cccc &quot;, cannon, &quot; &quot;, dist)

	if (cannon == nil) or (dist &gt; MAX_CONTROL_DISTANCE) then
		_G.displayWarning(&quot;To operate a cannon, stand near an idle (brown) cannon then select Cannon tool&quot;)
		return
	end

	local reloadTime = 5

	if Player:FindFirstChild(&quot;CntrReloadTime&quot;) and Player.CntrReloadTime.Value then
		reloadTime = Player.CntrReloadTime.Value
	end

	_G.displayHint(&quot;Left click to fire. Reload time: &quot;..reloadTime..&quot; secs (click Buy to improve)&quot;,0)

	-- Attach to this cannon

	Cannon = cannon

	attach(Cannon)

	-- If this Player does not already have SeaLegs attached, create now

	allocateSeaLegs( Torso, Cannon.Mount)
	
        mouse.Icon = CURSOR_INR

	mouse.KeyDown:connect(onKeyDown)
	mouse.Button1Down:connect(onButton1Down)
	mouse.Button1Up:connect(onButton1Up)

	Mouse = mouse

	--mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
end

----------------------------------------------------------------------------------------
local function onUnequippedLocal()

	if Cannon then
		detach()
	end

	_G.clearHint()

	Mouse = nil

	game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
end


-----------------------------------------------------------------------------------------
Tool.Equipped:connect(onEquippedLocal)
Tool.Unequipped:connect(onUnequippedLocal)

local elapsed = 0
local CONTROL_INTERVAL = .2
local CONTROL_CHECK_INTERVAL = 5

local lastVector
local curCursor = CURSOR_INR

-- Control aiming

while true do

	if Cannon then

		--local ccf =  game.Workspace.CurrentCamera.CoordinateFrame
		--local cp = Vector3.new(ccf.x, ccf.y,ccf.z)
		--local cp = Player.Character.Head.Position

		local a = Mouse.Hit.p -  Mouse.Origin.p

		local vru = Mount.CFrame:vectorToObjectSpace(a).unit 

		--print(&quot;vru &quot;, vru)

		local ea = math.asin(vru.y)
		local da = math.atan2(vru.x,-vru.z)
		local oor = false

		if da &lt; ANGLE_MAX_LEFT or da &gt; ANGLE_MAX_RIGHT then
			oor = true
		end

		if ea &lt; ANGLE_MAX_DOWN or ea &gt; ANGLE_MAX_UP then
			oor = true
		end	
	
		if oor then
			if curCursor == CURSOR_INR then
				Mouse.Icon = CURSOR_OOR
				curCursor = CURSOR_OOR
			end
		else
			if curCursor == CURSOR_OOR then
				Mouse.Icon = CURSOR_INR
				curCursor = CURSOR_INR
			end	
		end
				
		elapsed = elapsed + CONTROL_INTERVAL

		if elapsed &gt; CONTROL_CHECK_INTERVAL then

			if Cannon and Torso and (CntrOwner.Value ~= Player) then
				Cannon = nil
				CntrCmd = nil
				CntrOwner = nil
			end

			elapsed = 0

		end
	end

	wait(CONTROL_INTERVAL)
end



-- Find the creator of bolt and peg that players Pnts value
-- directly. 

local function pegDamage(bolt, damage)

	local creator = bolt:FindFirstChild(&quot;Creator&quot;)
	
	if (creator ~= nil) and (creator.Value ~= nil) then

		local pnts = damage * DAMAGE_TO_PNTS_SCALE
		
		--print(&quot;glPegPnts pegs by: &quot;, pnts)
		
		_G.pegPnts(creator.Value, pnts)
	end
end

_G.glPegDamage = pegDamage


	cannonUsable = Barrel.Position.y &gt; SEA_LEVEL

	-- Verify the Player in the CntrOwner.Value still exists and still is close enough to control cannon.

	if CntrOwner.Value then	

		if CntrOwner.Value.Parent == nil then	--If player no longer in the world
			CntrOwner.Value = nil

		else
			local char = CntrOwner.Value.Character
			if char and char:FindFirstChild(&quot;Torso&quot;) then
				if (Mount.Position - char.Torso.Position).magnitude &gt; MAX_CONTROL_DISTANCE then

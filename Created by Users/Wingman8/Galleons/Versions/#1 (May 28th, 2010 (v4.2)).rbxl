local function countTeams( player )

	local rteam = 0
	local bteam = 0
	local neutral = 0

	for k,v in pairs(game.Players:GetChildren()) do

		if v ~= player then

			if v.Neutral then
				neutral = neutral + 1

			elseif v.TeamColor == teamRed then
				rteam = rteam + 1

			elseif v.TeamColor == teamBlue then
				bteam = bteam + 1

			else
				neutral = neutral + 1


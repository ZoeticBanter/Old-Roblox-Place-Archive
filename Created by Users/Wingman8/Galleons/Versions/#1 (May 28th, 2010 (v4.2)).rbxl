-- If Shark touches a player then BreakJoints() on whatever part
-- touched.

local function onTouched(part)

	--print(&quot;touched &quot;, part.Name)

	if not part.Parent then
		return
	end

	if part == &quot;Sea&quot; then
		return
	end

	if part.Parent:FindFirstChild(&quot;Torso&quot;) then
		part:BreakJoints()
		return
	end

	if (part.Name == &quot;Bolt&quot;) or (part.Name == &quot;FlintlockBall&quot;) then
		assert(coroutine.resume(sharkMgrCor,&quot;hit&quot;, part))
		return
	end
end



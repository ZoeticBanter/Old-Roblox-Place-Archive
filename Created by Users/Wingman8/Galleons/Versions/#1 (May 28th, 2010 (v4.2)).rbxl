--2009/10/3 Initial version based on LScriptArmorDriver-v2.9
--2009/11/14 Add GUI hint


-- Features:

	-- Sends commands to ship the player is seat welded to.



local Tool = script.Parent

-- This runs while the script is still in game.Players/&lt;playername&gt;.
-- Record the object now so we can use it to send player specific
-- hints after this tool is equipped.
local Player = Tool.Parent.Parent

local Hint = Instance.new(&quot;Hint&quot;)

local Torso
local Mouse

-- Vehicle parts and parameters
local Ship = nil

local CntrCmd --to fire weapon

-- Values that represent nil for the CntrCmd value (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;



----------------------------------------------------------------------------------------------------------------
-- Find the vehicle (if any) that we are welded to.  Set up parameters for this vehicle.
-- Assumes the Seat Parent is the vehicle.  Returns the vehicle or nil.

function findShip(mod, torso)

	for k,ship in pairs(mod:GetChildren()) do

		if ship.className == &quot;Model&quot; then

			local keel = ship:FindFirstChild(&quot;Keel&quot;)
			local helm = ship:FindFirstChild(&quot;Eq-Helm&quot;)

			if  keel and helm then

				local seat = helm:FindFirstChild(&quot;Seat&quot;)

				if seat then

					local sw = seat:FindFirstChild(&quot;SeatWeld&quot;)

					if sw and (sw.Part1 == torso) then
						return ship,keel,helm
					end
				end
			end
		end
	end
	
	return nil
end
						


------------------------------------------------------------------------------------------------------------
function onButton1Down(mouse)


end

------------------------------------------------------------------------------------------------------------
function onButton1Up(mouse)

end



------------------------------------------------------------------------------------------------------------
-- Analyse the command and send to ship if appropriate.  



function onKeyDown(key)

	if Ship == nil then
		return
	end

	if key == &quot;p&quot; then
		--CntrCmd.Value = &quot;scuttle&quot;

	else
		CntrCmd.Value = key
	end

	wait(.1)	-- This is to insure the previous command gets sent before we clear our the Cntr values

	CntrCmd.Value = NIL_CMD


end



-------------------------------------------------------------------------------------------------------
local function monitorSpeed( )

	while Ship do

		Hint.Text = string.format(&quot;Speed: %.1f&quot;, Ship.Keel.Velocity.magnitude)
		Hint.Parent = Player

		wait(3)
	end

end

-------------------------------------------------------------------------------------------------------
function onEquipped(mouse)

	Torso = script.Parent.Parent.Torso

	local help,keel

	Ship,keel,helm = findShip(Workspace, Torso)

	if not Ship then
		_G.displayWarning(&quot;First sit in Captains chair, then select tool&quot;,0)
		return
	end

	_G.displayHint(&quot;Forward motion: u.  Steering: h,k.  All stop: x. &quot;,0)

	CntrCmd = keel.CntrCmd

	Mouse = mouse

        mouse.Icon = &quot;rbxasset://textures\\AnchorCursor.png&quot;

	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
        mouse.Button1Up:connect(function() onButton1Up(mouse) end)

	mouse.KeyDown:connect(onKeyDown)

	monitorSpeed()
end

-------------------------------------------------------------------------------------------------------
-- Note we do not remove our Player from CntrUser.  A Player is considered the 
-- user iff he is seatwelded to the driver seat, regardless of tool selection.

function onUnequipped()

	Ship = nil

	Hint.Parent = nil

	_G.clearHint()
end

---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)




local function onCmd( cmd )

	cmd = string.lower(cmd)

	if cmd == &quot;u&quot; then
		if sail == SAIL_FORWARD then
			sail = SAIL_STOP
			Wheel.BrickColor = COLOR_NEUTRAL
			WheelMount.BrickColor = COLOR_NEUTRAL
		else
			sail = SAIL_FORWARD
			Wheel.BrickColor = COLOR_ENGAGED
			WheelMount.BrickColor = COLOR_ENGAGED
		end

	elseif cmd == &quot;j&quot; then
		sail = SAIL_STOP
		Wheel.BrickColor = COLOR_NEUTRAL
		WheelMount.BrickColor = COLOR_NEUTRAL

	elseif cmd == &quot;h&quot; then

		if turn == TURN_LEFT then
			turn = TURN_STRAIGHT
			Left.BrickColor = COLOR_NEUTRAL
	
		else
			turn = TURN_LEFT
			Left.BrickColor = COLOR_ENGAGED
			Right.BrickColor = COLOR_NEUTRAL

		end
	
	elseif cmd == &quot;k&quot; then

		if turn == TURN_RIGHT then
			turn = TURN_STRAIGHT
			Right.BrickColor = COLOR_NEUTRAL
	
		else
			turn = TURN_RIGHT
			Right.BrickColor = COLOR_ENGAGED
			Left.BrickColor = COLOR_NEUTRAL
		end

	elseif cmd == &quot;x&quot; then
		Wheel.BrickColor = COLOR_NEUTRAL
		WheelMount.BrickColor = COLOR_NEUTRAL
		Left.BrickColor = COLOR_NEUTRAL
		Right.BrickColor = COLOR_NEUTRAL

		sail = SAIL_STOP
		turn = TURN_STRAIGHT

	elseif cmd == &quot;scuttle&quot; then

		scuttle = true
	end
end


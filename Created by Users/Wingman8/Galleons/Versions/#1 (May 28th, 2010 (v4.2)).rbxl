function toggleCamera()

	local h = &quot;&quot;

	if game.Workspace.CurrentCamera.CameraType == Enum.CameraType.Attach then
		game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Follow
		h = &quot;Follow&quot;

	elseif game.Workspace.CurrentCamera.CameraType == Enum.CameraType.Follow then
		game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
		h = &quot;Custom&quot;

	elseif game.Workspace.CurrentCamera.CameraType == Enum.CameraType.Custom then
		game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Attach
		h = &quot;Attach&quot;
	end

	_G.displayHint(&quot;View changed to &quot;.. h)


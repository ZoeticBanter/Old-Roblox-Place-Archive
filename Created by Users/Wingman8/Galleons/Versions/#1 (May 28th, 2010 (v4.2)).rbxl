-- Scuttle the ship, either in fast mode or regular

local function scuttleShip(fast)

	local sinkRate = -.4		-- Units sunk per MOVEMENT_INTV
	local sinkDuration = 90	-- Number of MOVEMENT_INTV till ship removed 

	if fast then
		sinkRate = -.5
		sinkDuration = 20
	end

	-- Do BreakJoints on all the sails
	releaseSails(Ship)

	-- Set the BodyGyro to something more or less upside down.
	
	local flip = CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(math.random(170,210))) 
		* CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(math.random(3,10))) 
	
	BodyGyro.cframe = BodyGyro.cframe * flip
	
	-- Increase the BodyGyro powers since otherwise it does not seem to flip very 
	-- aggressively.
	BodyGyro.D = BodyGyro.D * 4
	BodyGyro.P = BodyGyro.P * 4
	BodyGyro.maxTorque = BodyGyro.maxTorque * 4
	
	-- Keep lowering our keel position till end of sink duration.

	for i=1, sinkDuration do
	
		-- Keep moving the ship but with all sails and turning force off
		-- so it will slowly come to a stop via water friction.
		moveShip(SAIL_STOP, TURN_STRAIGHT)
		
		-- Slowly lower the keel position to simulate sinking
		Keel.BodyPosition.position = Keel.BodyPosition.position + Vector3.new(0, sinkRate, 0)
		
		wait(MOVEMENT_INTV)
	end
end


-- Functions to display a message in the Players hint GUI window

local function waitWarning( hint, text, duration )

	wait(duration)

	-- If we are still displaying the same thing, then clear warning.
	if hint.Text == text then
		hint.Visible = false
	end
end
	

local function plDisplayWarning( player, text, duration )

	if player:FindFirstChild(&quot;PlayerGui&quot;) and player.PlayerGui:FindFirstChild(&quot;ScreenGui&quot;) then

		local hint = player.PlayerGui.ScreenGui:FindFirstChild(&quot;Hint&quot;) 

		if hint then

			hint.Text = text
			hint.Visible = true

			local co = coroutine.create(waitWarning)
			coroutine.resume(co, hint, text, duration)
		end
	end
end

_G.plDisplayWarning = plDisplayWarning



-- Partition where the thickness dimension is y, length is x, width is z

function Frag:partition_y()



	if self.state ~= &quot;section&quot; then
		return
	end

	local block = self.block

	local psx = partitionSize(block.Size.x, PART_SIZES_L)
	local psz = partitionSize(block.Size.z, PART_SIZES_W)

	local x,lx
	local veryFirstPart = true
	local cframe = block.CFrame
	local tsizex = block.Size.x
	local tsizez = block.Size.z

	local z = 0

	while z &lt; tsizez do

		local lz = psz	-- assumes a full width part

		if tsizez - z &lt; lz then
			lz = tsizez - z
		end
	
		x = 0

		while x &lt; tsizex do

			local lx = psx	-- Assume a full size part

			if tsizex - x &lt; psx then
				lx = tsizex - x
			end

			local nb

			-- If very first part then resize the original block 
			if veryFirstPart then 
				local parent = block.Parent
				block.Parent = nil		-- I think we need to remove from world to break any connections
				block.Size = Vector3.new(lx,block.Size.y,lz)
				block.CFrame = cframe + cframe:vectorToWorldSpace(Vector3.new(x+(lx/2)-(tsizex/2), 0, z-(tsizez/2)+(lz/2)))
				block.Parent = parent

				-- Must refresh these since block changed 
				self.curMass = block:GetMass()

				-- Transition our state 
				self.state = &quot;partition&quot;

				--print(&quot;firstBlock; &quot;, x,&quot;,&quot;,z,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,lz/2, &quot; pos: &quot;, block.Position)

				veryFirstPart = false

				nb = block

			-- Else create a new block
			else
				nb = Instance.new(&quot;Part&quot;)
				nb.formFactor = 0
				nb.Size = Vector3.new(lx,block.Size.y,lz)
				nb.Color = block.Color
				nb.TopSurface = block.TopSurface
				nb.BottomSurface = block.BottomSurface
				nb.FrontParamA = block.FrontParamA
				nb.FrontParamB = block.FrontParamB
				nb.BottomParamB = block.BottomParamB
				nb.LeftSurface = Enum.SurfaceType.Weld
				nb.RightSurface = Enum.SurfaceType.Weld
				nb.FrontSurface = Enum.SurfaceType.Weld
				nb.BackSurface = Enum.SurfaceType.Weld
				nb.Transparency = block.Transparency
				nb.Material = block.Material

				nb.CFrame = cframe + cframe:vectorToWorldSpace( Vector3.new(x-(tsizex/2)+(lx/2),0, z-(tsizez/2)+(lz/2)) )

				--print(&quot;onePart; &quot;, x,&quot;,&quot;,z,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,lz/2, &quot; pos: &quot;, nb.Position)

				nb.Name = block.Name
				nb.Parent = block.Parent.Parent	--Original block was put into a model

				-- Create a new object to manage this block

				Frag:new(nb)

			end

			nb:MakeJoints()

			x =x + lx
		end

		z = z + lz
	end
end


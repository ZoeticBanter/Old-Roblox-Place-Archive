-- Shark management package

local state = &quot;idle&quot;

------------------------------------------------------------------------------
-- Shark idle state: find a random target and head toward it
-- until target acquired, timeout, or some event happens

local function shIdle()

	timerArm(20)

	while true do

		local targetPos = Vector3.new(math.random(-800,800), 0, math.random(-800,800))

		local targetFunc = function() return targetPos end

		local event,p = moveToward( targetFunc, {})

		--print(&quot;shIdle returns from moveTorward with event &quot;, event)

		if event == &quot;hit&quot; then
			state = &quot;flee&quot;
			--print(&quot;shIdle: returning event &quot;, event)
			return event, p

		elseif event == &quot;player nearby&quot; then 
			state = &quot;attack&quot;
			--print(&quot;shIdle: returning event &quot;, event)
			return event, p

		elseif event == &quot;ship nearby&quot; then
			state = &quot;circle ship&quot;
			--print(&quot;shIdle: returning event &quot;, event)
			return event, p
		end
	end
end

------------------------------------------------------------------------------
-- Shark circle state:  circle the ship whose keel is passed.

local function shCircleShip(keel)

	local targets

	-- Randomly select clockwise or anticockwise target points in Keel relative coordinates
	if math.random() &gt; .5 then
		-- Clockwise
		targets = { Vector3.new(0,0,150), Vector3.new(-100,0,0), 
				Vector3.new(0,0,-150), Vector3.new(100,0,0)}
	else
		--Anticlockwise
		targets = { Vector3.new(100,0,0), Vector3.new(0,0,-150), 
				Vector3.new(-100,0,0), Vector3.new(0,0,150)}
	end

	local tp = 1 -- Current target points

	timerArm(20)

	while true do

		if not keel.Parent then
			state = &quot;idle&quot;
			return
		end

		--print(&quot;Cur target keel relative: &quot;, targets[tp])

		local targetFunc = function() 
			if (not keel) or (not keel.Parent) then
				return nil
			else
				local pos = keel.CFrame:pointToWorldSpace(targets[tp]) 
				return Vector3.new(pos.x, 0, pos.z)
			end
		end

		local event,p = moveToward( targetFunc, {})

		--print(&quot;shCircle returns from moveTorward with event &quot;, event)

		if event == &quot;hit&quot; then
			state = &quot;flee&quot;
			--print(&quot;shCircleShip: returning event &quot;, event)
			return event, p

		elseif event == &quot;player nearby&quot; then 
			state = &quot;attack&quot;
			--print(&quot;shCircleShip: returning event &quot;, event)
			return event, p

		elseif (event == &quot;ship nearby&quot;) and (p ~= keel) then
			--print(&quot;shCircleShip: returning event &quot;, event)
			return event, p

		elseif event == &quot;target gone&quot; then
			state = &quot;idle&quot;
			return
		end

		if (event == &quot;target acquired&quot;) or ( event == &quot;timeout&quot;) then
			tp = tp + 1
		
			if tp &gt; 4 then
				tp = 1
			end

			-- Reset the timer if we acquired the target.
			if event == &quot;target acquired&quot; then
				timerArm(20)
			end

			--print(&quot;shCircleShip: handling event &quot;, event)

			-- Every so often just flee to break up the monotony

			if math.random() &lt; .1 then
				state = &quot;flee&quot;
				return 
			end
				
		else
			--print(&quot;shCircleShip: ignoring event  &quot;, event)
		end
	end
end

------------------------------------------------------------------------------
-- Head directly for the specified torso until it no longer exists

local function shAttack(torso)

	local tp = 1 -- Current target points

	local count = 0

	timerArm(20)

	while true do

		local targetFunc = function() 

			if not torso.Parent then
				return nil
			end

			local h = torso.Parent:FindFirstChild(&quot;Humanoid&quot;)
			if (not h) or (h.Health &lt;= 0) then
				return nil
			end

			return Vector3.new(torso.Position.x, 0, torso.Position.z) 
		end

		local event,p = moveToward( targetFunc, {[&quot;ship nearby&quot;]=true})

		--print(&quot;shAttack returns from moveTorward with event &quot;, event)

		if event == &quot;hit&quot; then
			state = &quot;flee&quot;
			--print(&quot;shAttack: returning event &quot;, event)
			return event, p

		elseif (event == &quot;player nearby&quot;) and (p ~= torso) then
			--print(&quot;shAttack: returning event &quot;, event)
			return &quot;player nearby&quot;, p

		elseif event == &quot;target gone&quot; then
			--print(&quot;shAttack: player gone, returning to idle&quot;)
			state = &quot;idle&quot;
			return

		elseif event == &quot;timeout&quot; then

			-- If target is out of the water then lose interest
			if not torso:FindFirstChild(&quot;LifeVest&quot;) then
				state = &quot;idle&quot;
				return
			end

			count = count + 1
			if count &gt; 5 then
				state = &quot;flee&quot;
				return 
			end
		end
	end
end

-----------------------------------------------------------------------------
-- Shark hit by a bolt, flee in the same direction the bolt was going

local function shFlee(bolt)

	local target

	-- If Bolt specified then flee in same direction that bolt was going.  Determine this from the 
	-- vector from Creator position to the Bolt position (this is because the
	-- Bolt may have already bounced off and its vector  is not the original
	-- firing direction).
	if bolt and bolt.Parent then
		local owner = bolt:FindFirstChild(&quot;Creator&quot;)
		if owner and owner.Value and owner.Value.Character then
			local torso = owner.Value.Character:FindFirstChild(&quot;Torso&quot;)
			if torso then
				local bvu = bolt.Position - torso.Position
				bvu = Vector3.new(bvu.x, 0, bvu.z).unit
				target = Shark.Position + (bvu * FLEE_DISTANCE)

				--print(&quot;shFlee target computed from bolt destination &quot;, target)
			end
		end
	end

	-- If no target determined above then generate a random one.
	if not target then
		target = Vector3.new(math.random(-700,700), 0, math.random(-700,700))
		--print(&quot;shFlee target computed randomly &quot;, target)
	end

	timerArm(60)

	while true do

		local targetFunc = function() return target end

		local event,p = moveToward( targetFunc, {[&quot;ship nearby&quot;]=true, [&quot;player nearby&quot;]=true})

		--print(&quot;shFlee returns from moveTorward with event &quot;, event)

		if event == &quot;hit&quot; then
			state = &quot;flee&quot;
			--print(&quot;shFlee: returning event &quot;, event)
			return event, p
		end

		if (event == &quot;target acquired&quot;) or (event == &quot;timeout&quot;) then
			state = &quot;idle&quot;
			--print(&quot;shFlee target acquired or timeout&quot;)
			return 
		end
	end
end



------------------------------------------------------------------------------
-- Main coroutine that controls shark by maintaining the current 
-- shark state.

local function sharkManager()

	state = &quot;idle&quot;

	local event,p

	while true do

		--print(&quot;cur state &quot;, state, &quot; cur event &quot;, event)

		if state == &quot;idle&quot; then
			event,p = shIdle()

		elseif state == &quot;circle ship&quot; then
			event,p = shCircleShip(p)

		elseif state == &quot;attack&quot; then
			event,p = shAttack(p)

		elseif state == &quot;flee&quot; then
			event,p = shFlee(p)

		else
			print(&quot;Unhandled event /state&quot;, event,&quot;/&quot;, state, &quot; returning to idle state&quot;)
			state = &quot;idle&quot;

		end
	end
end


------------------------------------------------------------------------------


-- Create the main shark manager coroutine and start it running
sharkMgrCor = coroutine.create(sharkManager)
assert(coroutine.resume(sharkMgrCor))

Shark.Touched:connect(onTouched)

-- This coroutine becomes the timer utility.
tmRun()

	





--2010/1/17 Initial Version
--2010/1/23 Allow buying at any time
--2010/3/8 v1.2 Add buying swim time

local ScreenGui = script.Parent

local Player

local ButBuy

local FrameBuy
local ButReloadTime, CurReloadTime
local ButBullets, CurBullets
local ButGrenades, CurGrenades
local ButSword, CurSwords
local ButSwimTime, CurSwimTime
local BuyMsg

local COLOR_BUTTON_SELECTED = Color3.new(1,1,0)	-- Yellow
local COLOR_BUTTON_IDLE = Color3.new(1,1,1)  -- White

local TEXT_COLOR_NORMAL = Color3.new(27/255, 42/255, 53/255)
local TEXT_COLOR_GRAYED_OUT = Color3.new(150/255, 150/255, 150/255)

local msgTime = 10

local BUYING_TIME = 120

----------------------------------------------------------------
local function display(text, duration)

	if not duration then
		duration = 5
	end

	msgTime = duration
	BuyMsg.Text = text
end

----------------------------------------------------------------------------
local function playerSeated()
	
	local char = Player.Character

	if not char then
		return false
	end

	local hum = char:FindFirstChild(&quot;Humanoid&quot;)

	if (not hum) or (not hum.Sit) then
		return false
	end

	return true
end

--------------------------------------------------------------------------
local function buttonValue(button, cur, col, itemName, units)

	button.TextColor3 = col
	button.BorderColor3 = col

	local item = Player:FindFirstChild(itemName)

	if (not item) or (not item.Value) then
		cur.Text = &quot;--&quot;
		return
	end

	cur.Text = item.Value..&quot; &quot;..units
end

----------------------------------------------------------------
-- Refresh the contents of the buy screen

local function refresh()

	local col 
	
	if playerSeated() then
		col = TEXT_COLOR_NORMAL
		display(&quot;Click on item to buy&quot;, 1000)
	else
		col = TEXT_COLOR_GRAYED_OUT
		display(&quot;Must be seated at cannon to buy&quot;, 1000)
	end

	buttonValue(ButReloadTime, CurReloadTime, col, &quot;CntrReloadTime&quot;, &quot;secs&quot;)
	buttonValue(ButBullets, CurBullets, col, &quot;CntrBullets&quot;, &quot;&quot;)
	buttonValue(ButGrenades, CurGrenades, col, &quot;CntrGrenades&quot;, &quot;&quot;)
	buttonValue(ButSword, CurSword, col, &quot;CntrSword&quot;, &quot;pnts&quot;)
	buttonValue(ButSwimTime, CurSwimTime, col, &quot;CntrSwimTime&quot;, &quot;secs&quot;)

end
	
----------------------------------------------------------------
-- Whenever msgTime goes to zero then wipe the display msg
local function checkMsgWipe()
	
	if msgTime &gt; 0 then
		msgTime = msgTime - 1
	else
		BuyMsg.Text = &quot;&quot;
	end

end
----------------------------------------------------------------
--  

local function onButPush( button, itemName, cost, incr, maxmin)

	if not playerSeated() then
		return
	end

	local item = Player:FindFirstChild(itemName)

	if (not item) or (not item.Value) then
		return
	end

	local cntrGold = Player:FindFirstChild(&quot;CntrGold&quot;)

	if (not cntrGold) or (not cntrGold.Value) then
		return
	end

	local leaderstats = Player:FindFirstChild(&quot;leaderstats&quot;)

	if not leaderstats then
		return
	end

	local gold = leaderstats:FindFirstChild(&quot;Gold&quot;)

	if (not gold) or (not gold.Value) then
		return
	end

	if cntrGold.Value &lt; cost then
		display(&quot;You do not have enough gold to buy this&quot;)
		return
	end

	-- See if we hit the limit
	if incr &gt; 0 then
		if item.Value + incr &gt; maxmin then
			display(&quot;You already reached the max on this item&quot;)
			return
		end
	else
		if item.Value + incr &lt; maxmin then
			display(&quot;You already reached the min on this item&quot;)
			return
		end
	end

	cntrGold.Value = cntrGold.Value - cost
	gold.Value = math.floor(cntrGold.Value)

	item.Value = item.Value + incr

	refresh()
end

-------------------------------------------------------------
local function onButBuyPush()

	if FrameBuy.Visible then
		FrameBuy.Visible = false

	else
		FrameBuy.Visible = true
		refresh()
	end

end


-------------------------------------------------------------

-- Insure our components are present


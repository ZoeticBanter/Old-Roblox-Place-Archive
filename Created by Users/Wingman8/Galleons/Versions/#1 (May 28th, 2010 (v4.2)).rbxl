CntrCmd.Name = &quot;CntrCmd&quot;
CntrCmd.Parent = Cannon

local CntrAim = Instance.new(&quot;Vector3Value&quot;)	-- to receive aiming vector from Tool
CntrAim.Name = &quot;CntrAim&quot;
CntrAim.Parent = Cannon

local CntrOwner = Instance.new(&quot;ObjectValue&quot;)	-- ref to owing players Player
CntrOwner.Name = &quot;CntrOwner&quot;
CntrOwner.Parent = Cannon


-- Find the Keel

local function findKeel( mod )

	for k,p in pairs(mod:GetChildren()) do

		if p.className == &quot;Model&quot; then
			local keel = findKeel(p)
		
			if keel  then
				return keel
			end

		elseif p.Name == &quot;Keel&quot; then
			return p

		end
	end


	return nil
end


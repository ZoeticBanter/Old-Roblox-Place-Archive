local function initBodyObjects( actWeight )

	-- Wait for these object to become present.  Do not why they are 
	-- not present immediately.

	BodyGyro = waitForChild(Keel,&quot;BodyGyro&quot;)
	BodyAngVel = waitForChild(Keel,&quot;BodyAngularVelocity&quot;)
	BodyPosition = waitForChild(Keel,&quot;BodyPosition&quot;)
	BodyVelocity = waitForChild(Keel, &quot;BodyVelocity&quot;)

	BodyGyro.maxTorque = Vector3.new(10, 0,10)
	BodyGyro.cframe = Keel.CFrame
	BodyGyro.P = 10
	BodyGyro.D = 500

	-- Init to the low power modes

	BodyVelocity.maxForce = BV_MAXFORCE_LOW
	BodyVelocity.P = BV_P_LOW

	BodyAngVel.maxTorque = BAV_TORQUE_LOW
	BodyAngVel.P = BAV_P_LOW

	-- Make the BodyPosition forces a little higher than the force that gravity
	-- is pulling down on the ship.  If it is a lot higher than this then players
	-- seem to bounce on the deck or the ship simply disappears.

	local f = actWeight * GRAVITY

	BodyPosition.D = f * 1.5
	BodyPosition.P = f * 1.21
	BodyPosition.maxForce = Vector3.new(0, f * 1.21, 0)

end


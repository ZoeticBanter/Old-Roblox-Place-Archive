-- Insure these components have been added before continuing
bodyForce = waitForChild(Bolt, &quot;BodyForce&quot;)
local creatorPlayer = waitForChild(Bolt,&quot;Creator&quot;).Value

-- Make a sound at firing time
fireSound = Instance.new(&quot;Sound&quot;)
fireSound.SoundId = &quot;http://www.roblox.com/asset/?id=2920959&quot;  --gunshot
fireSound.Parent = Bolt
fireSound.Volume = .5
fireSound.PlayOnRemove = false
fireSound:play()

local direction = Bolt.CFrame.lookVector

Bolt.Anchored = false	-- In case creator did not do this.

Bolt.Transparency = 0

initialPosition = Bolt.Position

connection = Bolt.Touched:connect(onTouch)

Bolt.Velocity = Bolt.Velocity + (direction * parmSpeed)

local r = game:service(&quot;RunService&quot;)
local T_MAX = 10  -- max lifetime of ball

local initPos = Bolt.Position

local t = 0
local ROT_INCR = Vector3.new(.001,.001,.001)

while t &lt; T_MAX  do

	local _,s = r.Stepped:wait()
	t = t + s

	-- Modify the .RotVelocity an undetectable amount in order to insure touch events occur.
	Bolt.RotVelocity = Bolt.RotVelocity + ROT_INCR

	-- if the ball has hit something then break out of flight loop
	if not bodyForce then
		break
	end

	if (Bolt.Position - initialPosition).magnitude &gt; parmRange then
		--print(&quot;BBB Bolt.Position &quot;, Bolt.Position, &quot; initialPosition &quot;, initialPosition, &quot; parmRange &quot;, parmRange)
		break
	end
end


-- If we hit something then allow ball to live for 2 more seconds
if not bodyForce then
	wait(2)
end


fireSound:stop()

if Bolt.Parent  then

	if connection then
		connection:disconnect()
	end

	Bolt:remove()
end

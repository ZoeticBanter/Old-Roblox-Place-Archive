local Tool = script.Parent

-- This runs while the script is still in game.Players/&lt;playername&gt;.
local Player = Tool.Parent.Parent

local SeaLegsScript = game.Workspace.GlobalScripts.SeaLegsScript

local MAX_CONTROL_DISTANCE = 9

local SeaLegs = nil

local Mouse

--Terminology:

	-- intact: cannon has all its parts, i.e. is not blown up
	-- available - no one else is attached to this cannon

-----------------------------------------------------------------------------------------------
-- Package variables identifying currently attached cannon (if any)
local Cannon = nil
local CntrAim = nil
local CntrOwner = nil
local Mount = nil

-- The following parameters depend on Cannon subtype (TopParamA):

local ANGLE_MAX_UP		--(positive)  degrees barrel can elevate up
local ANGLE_MAX_DOWN	--(negative)  degrees barrel can elevate down
local ANGLE_MAX_LEFT		--(negative) degrees barrel can turn to left
local ANGLE_MAX_RIGHT		--(positive) degrees barrel can turn to right


-- Package variable identifying our Player Torso
local Torso 

local CURSOR_OOR = &quot;rbxasset://textures\\ArrowCursor.png&quot;
local CURSOR_INR = &quot;rbxasset://textures\\GunCursor.png&quot;

----------------------------------------------------------------------------------------------
local function Intact(cannon)

	if cannon:FindFirstChild(&quot;Barrel&quot;) and 
		cannon:FindFirstChild(&quot;Mount&quot;) and
		cannon:FindFirstChild(&quot;CntrCmd&quot;) and 
		cannon:FindFirstChild(&quot;CntrOwner&quot;) then

		--print(&quot;sttt: &quot;..cannon:FindFirstChild(&quot;Barrel&quot;)..m..m:FindFirstChild(&quot;Weld&quot;)..cannon:FindFirstChild(&quot;CntrCmd&quot;)..cannon:FindFirstChild(&quot;CntrOwner&quot;))

		--print(&quot;operation true&quot;)
		return true
	else
		--print(&quot;Intact false&quot;)
		return false
	end
end



-------------------------------------------------------------------------------------------------
local function attach(cannon)

	CntrCmd = cannon.CntrCmd
	CntrAim = cannon.CntrAim
	CntrOwner = cannon.CntrOwner
	CntrOwner.Value = Player
	Mount = cannon.Mount


	ANGLE_MAX_UP = math.rad(15)
	ANGLE_MAX_DOWN = math.rad(-30)
	ANGLE_MAX_LEFT = math.rad(Mount.LeftParamA)
	ANGLE_MAX_RIGHT = math.rad(Mount.RightParamA)
end


-----------------------------------------------------------------------------------------------
local function detach()

	if Cannon == nil then
		return
	end

	-- If still attached to us,  release cannon
	if CntrOwner.Value == Player then

		CntrOwner.Value = nil
	end

	Cannon = nil
	CntrCmd = nil
	CntrOwner = nil
	
end
----------------------------------------------------------------------------------------------
-- Recursively search specified Model and sub-Models for the 
-- the Cannon closest to us that is also with the MAX_CONTROL_DISTANCE.

local function findClosestCannon(model, torso)

	local cannon,dist	

	for k,v in pairs(model:GetChildren()) do

		local c,ndist

		if (string.match(v.Name,&quot;Cannon&quot;)) and (v.className == &quot;Model&quot;)  then


			-- If Intact and not attached to anyone else
			if Intact(v) and (v.CntrOwner.Value == nil  or v.CntrOwner.Value == torso) then
				ndist = (v.Mount.Position - Torso.Position).magnitude
				c = v
			end


		elseif v.className == &quot;Model&quot; then
			c,ndist = findClosestCannon(v, torso)
		end
		
		if c and (dist == nil or ndist &lt; dist) then
			cannon = c
			dist = ndist
		end
	end

	return cannon,dist
end


-- Partition where the thickness dimension is z, length is x, width is y

function Frag:partition_z()

	if self.state ~= &quot;section&quot; then
		return
	end

	local block = self.block

	local psx = partitionSize(block.Size.x, PART_SIZES_L)
	local psy = partitionSize(block.Size.y, PART_SIZES_W)

	--print(&quot;partition sizing: &quot;, psx,&quot; &quot;, psy)

	local x,lx
	local veryFirstPart = true
	local cframe = block.CFrame
	local tsizex = block.Size.x
	local tsizey = block.Size.y

	local y = 0

	while y &lt; tsizey do

		local ly = psy	-- assumes a full width part

		if tsizey - y &lt; ly then
			ly = tsizey - y
		end
	
		x = 0

		while x &lt; tsizex do

			local lx = psx	-- Assume a full size part

			if tsizex - x &lt; psx then
				lx = tsizex - x
			end

			local nb

			-- If very first part then resize the original block 
			if veryFirstPart then 
				local parent = block.Parent
				block.Parent = nil		-- I think we need to remove from world to break any connections
				block.Size = Vector3.new(lx,ly,block.Size.z)
				block.CFrame = cframe + cframe:vectorToWorldSpace( Vector3.new(x+(lx/2)-(tsizex/2), y-(tsizey/2)+(ly/2),0) )
				block.Parent = parent
    
				-- Must refresh this since block changed size
				self.curMass = block:GetMass()

				-- Transition our state 
				self.state = &quot;partition&quot;

				--print(&quot;firstPart; &quot;, x,&quot;,&quot;,y,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,ly/2, &quot; pos: &quot;, block.Position)

				veryFirstPart = false

				nb = block

			-- Else create a new block
			else
				nb = Instance.new(&quot;Part&quot;)
				nb.formFactor = 0
				nb.Size = Vector3.new(lx,ly,block.Size.z)
				nb.Color = block.Color
				nb.FrontSurface = block.FrontSurface
				nb.BackSurface = block.BackSurface
				nb.FrontParamA = block.FrontParamA
				nb.FrontParamB = block.FrontParamB
				nb.BottomParamB = block.BottomParamB
				nb.LeftSurface = Enum.SurfaceType.Weld
				nb.RightSurface = Enum.SurfaceType.Weld
				nb.TopSurface = Enum.SurfaceType.Weld
				nb.BottomSurface = Enum.SurfaceType.Weld
				nb.Transparency = block.Transparency
				nb.Material = block.Material

				nb.CFrame = cframe + cframe:vectorToWorldSpace(Vector3.new(x-(tsizex/2)+(lx/2),y-(tsizey/2)+(ly/2),0))

				--print(&quot;onePart; &quot;, x,&quot;,&quot;,y,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,ly/2, &quot; pos: &quot;, nb.Position)

				nb.Name = block.Name
				nb.Parent = block.Parent.Parent --original part was put into a model

				-- Create a new frag object to manage this part

				Frag:new(nb)

			end

			nb:MakeJoints()

			x = x + lx
		end

		y = y + ly
	end
end


local function chose(player, selectedTeam)

	-- If player is not neutral then this is an old event, ignore

	if (not player.Neutral) and  ((player.TeamColor == teamRed) or
		(player.TeamColor == teamBlue)) then
		
		return
	end

	-- Count current teams (except our player)
	local rteam, bteam, neutral = countTeams(player)

	-- Add in our player to the neutral count
	neutral = neutral + 1

	local total = rteam + bteam + neutral

	--print(&quot;chose: rteam &quot;, rteam, &quot; bteam &quot;, bteam, &quot; neutral &quot;, neutral)

	if selectedTeam == teamRed then

		--print(&quot;red&quot;)

		if rteam &lt; bteam + neutral then	

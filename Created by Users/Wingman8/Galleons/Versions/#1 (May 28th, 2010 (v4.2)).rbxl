	-- Plus y slice

	mpm = function(hs, p) return (Vector3.new(-hs.x,hs.y,-hs.z) - p).magnitude end,
	ipm = function(hs,p) return (Vector3.new(p.x, hs.y, -hs.z) - p).magnitude end,
	ppm = function(hs, p) return (Vector3.new(hs.x,hs.y,-hs.z) - p).magnitude end,

	mpi = function(hs, p) return (Vector3.new(-hs.x,hs.y,p.z) - p).magnitude end,
	ipi = function(hs, p) return p.y - hs.y end,
	ppi = function(hs, p) return (Vector3.new(hs.x,hs.y,p.z) - p).magnitude end,

	mpp = function(hs, p) return (Vector3.new(-hs.x,hs.y,hs.z) - p).magnitude end,
	ipp = function(hs,p) return (Vector3.new(p.x, hs.y, hs.z) - p).magnitude end,
	ppp = function(hs, p) return (Vector3.new(hs.x,hs.y,hs.z) - p).magnitude end,
}

function _G.closestDist( part, point )

	local p = part.CFrame:pointToObjectSpace(point)

	local hs = part.Size/2

	local x = &quot;i&quot;
	local y = &quot;i&quot;
	local z = &quot;i&quot;

	if p.x &lt; -hs.x then
		x = &quot;m&quot;
	elseif p.x &gt; hs.x then
		x = &quot;p&quot;
	end

	if p.y &lt; -hs.y then
		y = &quot;m&quot;
	elseif p.y &gt; hs.y then
		y = &quot;p&quot;
	end

	if p.z &lt; -hs.z then
		z = &quot;m&quot;
	elseif p.z &gt; hs.z then
		z = &quot;p&quot;
	end

	return distMap[x..y..z](hs, p)

end
	



--------------------------------------------------------------------------------------------------------------------
local function fly(part, blastPos)

	if part then

		local v = (part.Position - blastPos).unit

		part.Velocity = (v * 30) + Vector3.new(0,70,0)
		part.RotVelocity = Vector3.new(math.random(10), math.random(10), math.random(10))

	end

end


-------------------------------------------------------------------------------------------------------------------
-- Do BreakJoints() on torso and fly parts connected to torso.  
-- Check for TK.  Honor force field.  No check for player in vehicle. 

local function blastPlayer(torso, blastPos, bolt)

	-- Check for TK

	local targetPlayer = game.Players:getPlayerFromCharacter(torso.Parent)
	if not targetPlayer then
		return
	end

	local creatorPlayer = bolt.Creator.Value
	if not creatorPlayer then
		return
	end


	if targetPlayer.TeamColor == creatorPlayer.TeamColor then
		local creatorChar = creatorPlayer.Character
		if creatorChar then
			creatorChar.torso:BreakJoints()
			return
		end
	end

	local char = torso.Parent

	if not char:FindFirstChild(&quot;ForceField&quot;) then

		torso:BreakJoints()

		fly(torso, blastPos)
		fly(char:FindFirstChild(&quot;Head&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Left Arm&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Right Arm&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Left Leg&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Right Leg&quot;), blastPos)

	end
end

_G.glBlastPlayer = blastPlayer

-------------------------------------------------------------------------
local function hitSingle( part, distance, blastPos, blastRadius, blastForce)

--print(&quot;hit single  part &quot;, part.Name)

	local d = 0

--[[
	if distance &gt; blastRadius then 

	elseif distance &lt; blastRadius/2 then
		d = ((blastRadius - distance)/blastRadius) * blastForce

	else 
		d = blastForce/2
	end

	if d &lt; blastForce/4 then
		d = blastForce/4
	end
--]]

	if distance &gt; blastRadius then 
		return 0
	end

	d = ((blastRadius - distance)/blastRadius) * blastForce

	local orig = part.FrontParamB

	part.FrontParamB = part.FrontParamB - d

	local cur = part.FrontParamB
	local max = part.FrontParamA

	--print(&quot;Part &quot;, part.Name, &quot; damage: &quot;, d, &quot; assessed, parmA: &quot;, part.FrontParamA, &quot; parmB: &quot;, part.FrontParamB)

	-- If strength is gone, release the part

	if cur &lt; 0 then
		part:BreakJoints()
		part.CanCollide = false

		fly(part, blastPos)

	-- If down to 1/3 original strength then change to inlets

	elseif (orig &gt; max/3) and (cur &lt;= max/3) then

		part.BackSurface = 4
		part.FrontSurface = 4
		part.LeftSurface = 4
		part.RightSurface = 4
		part.TopSurface = 4
		part.BottomSurface = 4


	-- If down to 2/3 original strength then change to glue

	elseif (orig &gt; max*2/3) and (cur &lt;= max*2/3) then

		part.BackSurface = 1
		part.FrontSurface = 1
		part.LeftSurface = 1
		part.RightSurface = 1
		part.TopSurface = 1
		part.BottomSurface = 1

	end

	return d
end
-------------------------------------------------------------------------
-- Handle a hit from an explosion centered on blastPos with force of
-- blastForce, on the specified part.  Caller must insure part is 
-- destroyable.  Returns how much damage was done.

local function hitHandler(part, distance, blastPos, blastRadius, blastForce)

	--print(&quot;hit handler  part &quot;, part.Name)
	local dm = 0

	if _G.FragHandler and _G.FragHandler[part] then
	
		-- Only fragment for the bigger explosions
		if blastRadius &gt;= 10 then					-- look hard coded constant

			--print(&quot;hit handler calls frag handler on &quot;, part.Name)
			
			local fm = _G.FragHandler[part](part, blastPos)

			-- If the part fragmented into fragments, then blast any of these
			-- that are within range.
			if fm then

			--print(&quot;fm on &quot;) for k,v in pairs(fm:GetChildren()) do print(&quot;fm part &quot;, v.Name) end

				for k,v in pairs(fm:GetChildren()) do
					local dist = (blastPos-v.Position).magnitude
					if dist &lt; blastRadius then
						dm = dm + hitSingle(v, dist, blastPos, blastRadius, blastForce)
					end
				end

			else
				--print(&quot;no fm on &quot;, part.Name)
			end
		end
				
	-- else a single part
	else
		dm = hitSingle(part, distance, blastPos, blastRadius, blastForce)
	end

	return dm
end

_G.glHitHandler = hitHandler


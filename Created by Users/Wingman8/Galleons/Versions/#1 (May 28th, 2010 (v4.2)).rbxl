-- Find the point on the surface of p that an object going from s to t would
-- hit.  The part p must be a sphere:
--
--	-- Boolean: true if there is direct hit, false if not
--	-- The point on or closest to the surface as defined above
--	-- The normal to this surface.


local function findTpSphere(s, t, p)

	local c = p.Position

	local a = (t.x - s.x)^2 + (t.y - s.y)^2 + (t.z - s.z)^2
	local b = 2 *(((t.x - s.x) * (s.x - c.x)) + ((t.y - s.y)*(s.y - c.y)) + ((t.z - s.z) * (s.z - c.z)))
	local cc = c.x^2 + c.y^2 + c.z^2 + s.x^2 + s.y^2 + s.z^2 - 
			(2 * ((c.x*s.x) + (c.y*s.y) + (c.z*s.z))) - ((p.Size.x/2)^2)

	local q = (b*b) - (4*a*cc)

	-- If no solution then line does not intersect sphere, as a quick and dirty
	-- just return center of sphere and a normal from center to s.

	if q &lt; 0 then
		return false, c, CFrame.new(c, s)

	else

		local u = ((-b) + math.sqrt( q )) / (2*a)

		--print(&quot;a &quot;, a, &quot; b &quot;, b, &quot; cc &quot;, cc, &quot; q &quot;, q, &quot; u &quot;, u)

		local x = s.x + (u * (t.x - s.x))
		local y = s.y + (u * (t.y - s.y))		
		local z = s.z + (u * (t.z - s.z))

		local p1 = Vector3.new(x,y,z)

		u = ((-b) - math.sqrt( q )) / (2*a)

		local x = s.x + (u * (t.x - s.x))
		local y = s.y + (u * (t.y - s.y))		
		local z = s.z + (u * (t.z - s.z))

		local p2 =  Vector3.new(x,y,z)

		-- Find which of the two intersection points is closest to the source

		local p

		if (p1 - s).magnitude &gt; (p2 - s).magnitude then
			p = p2
		else
			p = p1
		end

		return true, p, CFrame.new(p, p + (p - c))
	end

end

_G.glFindTpSphere = findTpSphere


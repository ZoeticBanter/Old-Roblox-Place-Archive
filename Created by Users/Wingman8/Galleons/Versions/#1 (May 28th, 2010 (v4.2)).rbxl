-- Give barrel an initial upward tilt, and whatever initial direction angle is
-- encoded in TopParamB.
adjustBarrel(math.rad(Mount.TopParamB), math.rad(10))


-- Create Smoke object.   Do not put into world immediately since too many of these, 
-- even when disabled seem to create smoke lag.
Smoke = Instance.new(&quot;Smoke&quot;)
Smoke.Enabled = false
Smoke.RiseVelocity = 20
Smoke.Size = 5

CntrCmd.Changed:connect(handleCmd)
CntrAim.Changed:connect(handleAim)
CntrOwner.Changed:connect(handleOwnerChange)

-- Monitor for ourselves to stop being operational, 
-- and to determine if Player abandoned us

local count = 0

while true do

	wait(5)
	count = count + 1

	if (not Barrel.Parent) or (not Mount.Parent) then
		Cannon:Remove()
		return
	end

	-- Determine if we are under water (this check should work whether on ship or not)
	cannonUsable = Barrel.Position.y &gt; SEA_LEVEL

	-- Verify the Player in the CntrOwner.Value still exists and still is close enough to control cannon.

	if CntrOwner.Value then	

		if CntrOwner.Value.Parent == nil then	--If player no longer in the world
			CntrOwner.Value = nil

		else
			local char = CntrOwner.Value.Character
			if char and char:FindFirstChild(&quot;Torso&quot;) then
				if (Mount.Position - char.Torso.Position).magnitude &gt; MAX_CONTROL_DISTANCE then
					CntrOwner.Value = nil
				end
			else
				CntrOwner.Value = nil			
			end

		end

	-- If not owned then make sure Smoke is no longer in the world.
	else
		if Smoke.Parent then
			Smoke.Parent = nil
		end
	end

	setStatusColor()
end

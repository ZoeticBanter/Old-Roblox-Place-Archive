-- When a Player welds to the Captains chair check if it is on same team as ship.
-- If not, Health = 0.

function onChildAdded(part)

	if part.className == &quot;Weld&quot; then
		local torso = part.Part1

		if torso then

			local humanoid = torso.Parent:FindFirstChild(&quot;Humanoid&quot;)

			if  humanoid then

				local Player = game.Players:GetPlayerFromCharacter(torso.Parent)
					
				if Player and (Player.TeamColor ~= CaptainChair.BrickColor) then
					humanoid.Health = 0
				end
			end
		end
	end
end


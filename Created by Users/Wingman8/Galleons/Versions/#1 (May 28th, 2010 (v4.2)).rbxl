--2010/3/25 v3.1 Initial version
--2010/4/10 v3.2 Rework timer
--2010/4/10 v4.0 Use BodyVelocity instead of BodyThrust and BodyForce
--2010/4/16 v4.1 Stop attacking player once LifeVest is off.


local Shark = script.Parent
local BodyVelocity= Shark.BodyVelocity
local BodyGyro = Shark.BodyGyro
local BodyPosition = Shark.BodyPosition

local SHIP_DETECT_RANGE = 300
local PLAYER_DETECT_RANGE = 100
local TARGET_ACQUIRED_RADIUS = 20
local FLEE_DISTANCE = 400

local MAX_SPEED = 20

local WATER_FORCE_MAX = Shark:GetMass() 
local WATER_FORCE_MIN = WATER_FORCE_MAX * .1

local SWIM_FORCE = (MAX_SPEED * WATER_FORCE_MAX) + WATER_FORCE_MIN

local Sea = Workspace:FindFirstChild(&quot;Sea&quot;)

if Sea then
	BodyPosition.position = Vector3.new(0,Sea.Position.y+2,0)
end

local sharkMgrCor

------------------------------------------------------------------------------------------------------------------
------------------------------------------------------------------------------------------------------------------
-- Timer package

local curTimer = 0
local rearmTimer = nil

local function timerArm( timeVal)
	curTimer = timeVal
	rearmTimer = timeVal
end

local function timerStop()
	curTimer = 0
	rearmTime = nil
end

-- This coroutine maintains the basic one second interval.

local TM_INTERVAL = 1

function tmRun()

	while true do

		wait(TM_INTERVAL)

--print(&quot;timer tick&quot;)

		if curTimer &gt; 0 then

			curTimer = curTimer - 1

			if curTimer &lt;= 0 then
				if rearmTimer then
					curTimer = rearmTimer
				end
				assert(coroutine.resume(sharkMgrCor,&quot;timeout&quot;))
			end
		end
				
		assert(coroutine.resume(sharkMgrCor,&quot;movement interval&quot;))
	end	
end

----------------------------------------------------------------------------
local vector0 = Vector3.new(0,0,0)

local function aimToward(target)

	-- Aim for the target

	local v1u = target - Shark.Position
	v1u = Vector3.new(v1u.x,0,v1u.z).unit

	BodyGyro.cframe = CFrame.new(vector0, vector0 + v1u)

	BodyVelocity.velocity = v1u*20


end

----------------------------------------------------------------------------
local function shipNearby()

	local nearestKeel 
	local nearestDist

	for k,v in pairs(Workspace:GetChildren()) do

		if v.className == &quot;Model&quot; then
			keel = v:FindFirstChild(&quot;Keel&quot;)

			if keel then

				--print(&quot;Found ship at pos &quot;, keel.Position, &quot; dist to shark &quot;, (keel.Position - Shark.Position).magnitude )

				if (not nearestKeel) or ((keel.Position - Shark.Position).magnitude &lt; nearestDist) then
					nearestKeel = keel
					nearestDist = (keel.Position - Shark.Position).magnitude
				end
			end
		end
	end

	if nearestKeel and (nearestDist &lt; SHIP_DETECT_RANGE) then
		return nearestKeel
	end

	return nil
end

-----------------------------------------------------------------------------
local function playerNearby()

	local nearestPlayer
	local nearestDist

	--for k,pl in pairs(Workspace.FakePlayers:GetChildren()) do
	for k,pl in pairs(game.Players:GetChildren()) do

		local c = pl.Character

		if c then
			local torso = c:FindFirstChild(&quot;Torso&quot;)

			--if torso then
			--	print(&quot;Found player and lifevest: &quot;, torso:FindFirstChild(&quot;LifeVest&quot;))
			--end

			-- Only consider players that have a lifevest on, this means they are in the water.
			if torso and torso:FindFirstChild(&quot;LifeVest&quot;) then

				if (not nearestPlayer) or ((torso.Position - Shark.Position).magnitude &lt; nearestDist) then
					nearestPlayer = torso
					nearestDist = (torso.Position - Shark.Position).magnitude
				end
			end
		end
	end

	if nearestPlayer and (nearestDist &lt; PLAYER_DETECT_RANGE) then
		return nearestPlayer
	end

	return nil
end


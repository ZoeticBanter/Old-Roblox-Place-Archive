--2010/1/19 Initial version
--2010/1/20 Use CntrBullets 

-- Uses ProtoFlintlockBall object


local Tool = script.Parent
local Player = script.Parent.Parent.Parent

local ProtoFlintlockBall


-----------------------------------------------------------------------------------------------

local reloading = false	-- currently reloading

-- Package variable identifying our Player Torso
local Torso 
local Mouse

-- Constants
local RELOAD_TIME = 1

---------------------------------------------------------------------------------------------------

local function displayStatus()

	local bullets = 0

	local cntrBullets = Player:FindFirstChild(&quot;CntrBullets&quot;)

	if cntrBullets and cntrBullets.Value then
		bullets = cntrBullets.Value
	end

	_G.displayHint(&quot;Bullets remaining: &quot;..bullets..&quot; (click Buy to buy more)&quot;,0)
end

-------------------------------------------------------------------------------------------------
local function decrBullets()

	local cntrBullets = Player:FindFirstChild(&quot;CntrBullets&quot;)

	if cntrBullets and cntrBullets.Value then

		if cntrBullets.Value &gt; 0 then

			cntrBullets.Value = cntrBullets.Value - 1

			return true
		end
	end

	return false

end

-------------------------------------------------------------------------------------------------
-- Separate co-routine to mark flintlock as no longer reloading after a timeout
function reload()

	wait(RELOAD_TIME)

	if reloading then
		reloading = false
		Mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;
	end
end
------------------------------------------------------------------------------------------------
-- Create a FlintLockBall, position it just passed end of barrel, and 
-- set its Velocity to a unit vector in the direction the Barrel appears to point.
-- Attach and enable the FlintLockBall script to manage its trajectory and impact.

function fire( sourcePos, targetPos )

	local bolt = ProtoFlintlockBall:Clone()
	bolt.Name = &quot;FlintlockBall&quot;

	local v = (targetPos - sourcePos).unit
	local sp = sourcePos + (v * 5)

	bolt.Velocity = Torso.Velocity

	bolt.CFrame =  CFrame.new(sp,  sp + v)

	bolt.Transparency = 0

	-- Enable any script in the Bolt
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = Player
	
	bolt.Parent = game.Workspace

	reloading = true

	Mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;

	local co = coroutine.create(reload)
	assert(coroutine.resume(co))
end
----------------------------------------------------------------------------------

local b1down = false

function onButton1Down()

	if not reloading then

		if decrBullets() then
			fire(script.Parent.Handle.Position, Mouse.Hit.p)
		end
	end

	displayStatus()

end



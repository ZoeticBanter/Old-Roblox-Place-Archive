-- When a Player touches the CaptainChair we check to see if it is on same team as
-- ship.  If not, Health = 0.

function onTouched(part)

	local humanoid = part.Parent:FindFirstChild(&quot;Humanoid&quot;)

	if  humanoid then

		local Player = game.Players:GetPlayerFromCharacter(part.Parent)
			
		if Player and (Player.TeamColor ~= CaptainChair.BrickColor) then
			humanoid.Health = 0
		end
	end

end

----------------------------------------------------------------------------------------------------
-- When a Player welds to the Captains chair check if it is on same team as ship.
-- If not, Health = 0.

function onChildAdded(part)

	if part.className == &quot;Weld&quot; then
		local torso = part.Part1

		if torso then

			local humanoid = torso.Parent:FindFirstChild(&quot;Humanoid&quot;)

			if  humanoid then

				local Player = game.Players:GetPlayerFromCharacter(torso.Parent)
					
				if Player and (Player.TeamColor ~= CaptainChair.BrickColor) then
					humanoid.Health = 0
				end
			end
		end
	end
end


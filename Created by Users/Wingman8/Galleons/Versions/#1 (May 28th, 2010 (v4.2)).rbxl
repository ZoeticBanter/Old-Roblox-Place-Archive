--------------------------------------------------------------------------------------------------------------------
-- Frag class

local Frag = {}

----------------------------------------
-- Create a new Frag object based on the indicated block
function Frag:new (block)

	local o = { block = block }
    
	setmetatable(o, self)
	self.__index = self

	o.model =  Instance.new(&quot;Model&quot;)
	o.model.Parent = block.Parent

	block.Parent = o.model  -- put the block into new model

	o.fragmentList = {}
	o.numFrags = 0

	local length,width

	-- Determine which dimension is thickness
	if (block.Size.y &lt;= block.Size.x) and (block.Size.y &lt;= block.Size.z) then
		o.thickDim = &quot;y&quot;
		length = block.Size.x
		width = block.Size.z

	elseif  (block.Size.z &lt;= block.Size.x) and (block.Size.z &lt;= block.Size.y) then
		o.thickDim = &quot;z&quot;
		length = block.Size.x
		width = block.Size.y

	else
		print(&quot;***ERROR**** do not support thickness in the x dimension. Part: &quot;, block.Name)
		return nil

	end
	--print(&quot;len:&quot;, length, &quot; wid:&quot;, width)

	-- If we are big enough then our initial state is a section
	if (length &gt;= SECT_MIN_L) or (width &gt;= SECT_MIN_W) then
		o.state = &quot;section&quot;

	-- Else our initial state is a partition
	else
		o.state = &quot;partition&quot;
	end

	o.name = block.Name

	o.curMass = block:GetMass()

	o.origRelLv = (Keel.CFrame:toObjectSpace(block.CFrame)).lookVector

	--print(&quot;New created with state &quot;, o.state)

	-- Define a frag handler for the block in this object
	defineHandler(o, block)

	-- Add us to the master list of Frag objects
	FragList[o] = o
     
	return o
end


----------------------------------------------------------------------------------------------------------------
-- Partition where the thickness dimension is z, length is x, width is y

function Frag:partition_z()

	if self.state ~= &quot;section&quot; then
		return
	end

	local block = self.block

	local psx = partitionSize(block.Size.x, PART_SIZES_L)
	local psy = partitionSize(block.Size.y, PART_SIZES_W)

	--print(&quot;partition sizing: &quot;, psx,&quot; &quot;, psy)

	local x,lx
	local veryFirstPart = true
	local cframe = block.CFrame
	local tsizex = block.Size.x
	local tsizey = block.Size.y

	local y = 0

	while y &lt; tsizey do

		local ly = psy	-- assumes a full width part

		if tsizey - y &lt; ly then
			ly = tsizey - y
		end
	
		x = 0

		while x &lt; tsizex do

			local lx = psx	-- Assume a full size part

			if tsizex - x &lt; psx then
				lx = tsizex - x
			end

			local nb

			-- If very first part then resize the original block 
			if veryFirstPart then 
				local parent = block.Parent
				block.Parent = nil		-- I think we need to remove from world to break any connections
				block.Size = Vector3.new(lx,ly,block.Size.z)
				block.CFrame = cframe + cframe:vectorToWorldSpace( Vector3.new(x+(lx/2)-(tsizex/2), y-(tsizey/2)+(ly/2),0) )
				block.Parent = parent
    
				-- Must refresh this since block changed size
				self.curMass = block:GetMass()

				-- Transition our state 
				self.state = &quot;partition&quot;

				--print(&quot;firstPart; &quot;, x,&quot;,&quot;,y,&quot;,&quot;,0,&quot;,&quot;,lx/2,&quot;,&quot;,ly/2, &quot; pos: &quot;, block.Position)

				veryFirstPart = false

				nb = block

			-- Else create a new block
			else
				nb = Instance.new(&quot;Part&quot;)
				nb.formFactor = 0
				nb.Size = Vector3.new(lx,ly,block.Size.z)
				nb.Color = block.Color
				nb.FrontSurface = block.FrontSurface
				nb.BackSurface = block.BackSurface

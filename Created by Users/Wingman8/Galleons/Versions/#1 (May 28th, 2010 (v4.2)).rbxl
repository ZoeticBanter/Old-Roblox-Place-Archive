local function releaseSails(mod)

	for k,v in pairs(mod:GetChildren()) do

		if v.className == &quot;Model&quot; then
			releaseSails(v)

		elseif v.Name == &quot;Sail&quot; then
			v:BreakJoints()
		end
	end
end


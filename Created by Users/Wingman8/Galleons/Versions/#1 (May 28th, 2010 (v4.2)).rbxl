					<ProtectedString name="Source">-------------------------------------------------------------------------------
--2010/3/4 Initial version 


-- Go thru the whole ship and weld anything with  &quot;WeldTo&quot; starting its
-- name.  The string after the &quot;WeldTo&quot; in the name identifies the name of 
-- part to weld to.  Go thru the whole ship looking for it.  The first part
-- described is Part1, the second part is Part0.


local Ship = script.Parent

local Keel = Ship.Keel

------------------------------------------------------------------------------------------
-- Go thru while ship looking for a part with the specified name

local function findWelder( mod, name )

	for k,v in pairs(mod:GetChildren()) do

		if v.className == &quot;Model&quot; then

			local welder = findWelder( v, name )
			if welder then
				return welder
			end

		else
			if v.Name == name then

				return v
			end
		end		
	end

	return nil
end


-----------------------------------------------------------------------------------------

local function rec( mod )

	for k,v in pairs(mod:GetChildren()) do

		if v.className == &quot;Model&quot; then
			rec( v, keel )

		else
			if string.match(v.Name, &quot;^WeldTo&quot;) then

				local welderName = string.sub(v.Name, 7)

				local welder = findWelder(Ship, welderName)

				if not welder then
					print(&quot;Could not finder welder for &quot;, v.Name)
				
				else
					-- Make sure Welder is surface welded to anything it is
					-- supposed to be attached to.

					welder:MakeJoints()

					local cf = welder.CFrame:toObjectSpace(v.CFrame)

					local w = Instance.new(&quot;Weld&quot;) 
					w.Parent = Workspace
					w.Part0 = welder 
					w.Part1 = v
					w.C0 = cf
				end
			end
		end		
	end
end

--------------------------------------------------------------------------------------

rec (Ship, Keel)











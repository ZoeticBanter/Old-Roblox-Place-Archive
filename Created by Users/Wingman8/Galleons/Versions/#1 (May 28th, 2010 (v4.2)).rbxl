local coor = coroutine.create(monitorWhirling)
assert(coroutine.resume(coor))


local REASSESS_INTV = 20	-- Time interval to reassess the sail force of the ship
local time = 0

local function main()

	-- Main loop while Ship still in the world.  Other scripts can Remove() a ship 
	-- and we do not want this script to keep going after ship is removed.

	while Ship.Parent do

		wait(MOVEMENT_INTV)

		time = time + MOVEMENT_INTV
		local r,m = pcall(moveShip,sail, turn)
		if not r then
			print(&quot;*************************ScriptNavControl failure calling moveShip: &quot;, m)
			return
		end
		
		-- If we receive test command to sink ship
		if scuttle then
			CntrScuttle.Value = &quot;damage&quot;
			scuttleShip(false)
			return 
		end

		-- If enough damage then sink ship
		if sinkShip(CntrRatioDestroyed.Value, SINK_RATIO, SINK_UNITS) then
			CntrScuttle.Value = &quot;damage&quot;
			scuttleShip(false)
			return
		end

		-- If we are directed to sink ship from game control
		if CntrScuttle.Value and (CntrScuttle.Value == &quot;admin&quot;) then
			scuttleShip(true)
			return
		end

		--print(string.format(&quot;speed: %.1f, BodyForce: %.1f,%.1f, vel: %.1f,%.1f, sailF: %.1f,%.1f, waterF: %.1f, %.1f&quot;,
			--speed,BodyForce.force.x, BodyForce.force.z, Keel.Velocity.x, Keel.Velocity.z,sailForce.x,sailForce.z, waterForce.x,waterForce.z))
	end
end


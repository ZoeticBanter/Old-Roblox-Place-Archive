--2009/3/18 Initial version
--2009/6/25 Make a non-local script because changing team color does not seem to work in local script
--2010/1/23 v1.0 Use _G.plDisplayWarning

Tool = script.Parent

-- This runs while the script is still in game.Players.&lt;playername&gt;.
-- Record the object now so we can use it to send player specific
-- hints after this tool is equipped.
local Player = Tool.Parent.Parent

local Hint = Instance.new(&quot;Hint&quot;)

--Hint.Text = &quot;Player really is &quot;..Player.Name
--Hint.Parent = Player


local teamGreen = BrickColor.new(&quot;Medium green&quot;)

--------------------------------------------------------------------------------------------
local function respawnPlayer(player)

	local character = player.Character

	if character then
		local humanoid = character.Humanoid

		if humanoid then
			humanoid.Health = 0
		end
	end

end

--------------------------------------------------------------------------------------------------------------
function onActivated(mouse)

	if Player == nil then 
		return 
	end

	if Player.Neutral or Player.TeamColor == teamGreen then

		--Hint.Text = &quot;Player neutral &quot;..tostring(Player.Neutral)..&quot; or green &quot;..Player.TeamColor.Number
		--Hint.Parent = Player
	
		return
	end

	local TeamSwitchList = game.Workspace:FindFirstChild(&quot;TeamSwitchList&quot;)
	
	if not TeamSwitchList then

		--Hint.Text = &quot;TeamSwitchList not found&quot;
		--Hint.Parent = Player

		return
	end

	for k,v in pairs(TeamSwitchList:GetChildren()) do

		if v.Name == &quot;TeamSwitcher&quot; then
			if v.Value == Player then

				_G.plDisplayWarning(Player,&quot;Can only switch teams once per game&quot;,5)

				return
			end
		end
	end


	local GameTime = game.Workspace:FindFirstChild(&quot;GameTime&quot;)

	if not GameTime then
		--Hint.Text = &quot;GameTime not found&quot;
		--Hint.Parent = Player
		return
	end

	if GameTime.Value &gt; 60 then
		_G.plDisplayWarning(Player,&quot;Can only switch teams at start of new game&quot;,5)
		return
	end

	local TeamSwitcher = Instance.new(&quot;ObjectValue&quot;)
	TeamSwitcher.Name = &quot;TeamSwitcher&quot;
	TeamSwitcher.Value = Player
	TeamSwitcher.Parent = TeamSwitchList

	Player.Neutral = false
	Player.TeamColor = teamGreen

	respawnPlayer(Player)

end


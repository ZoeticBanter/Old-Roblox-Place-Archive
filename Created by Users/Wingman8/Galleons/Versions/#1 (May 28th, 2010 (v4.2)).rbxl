local function setStatusColor()

	if CntrOwner.Value == nil then
		Mount.BrickColor =  COLOR_UNATTACHED 

	elseif reloading then
		Mount.BrickColor =  COLOR_RELOAD 

	elseif not cannonUsable then
		Mount.BrickColor =  COLOR_OOS
	
	else
		Mount.BrickColor =  COLOR_READY
	end
end



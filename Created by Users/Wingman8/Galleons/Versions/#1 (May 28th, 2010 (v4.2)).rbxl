-- Move toward target indicated by the targetFunc.  Adjust motion
-- every second and poll for events every 5 seconds.  If any
-- events are received from the yeild or polled (and not in the ignore list)
-- then return them.

local  function moveToward(targetFunc, ignoreEventList)

	local t = 0

	while true do
		-- Determine the target position

		local targetPos = targetFunc()

		--print(&quot;moving toward &quot;, targetPos)

		if not targetPos then
			return &quot;target gone&quot;
		end

		aimToward(targetPos)

		local event, p = coroutine.yield()

		if event == &quot;movement interval&quot; then
			
			t = t + 1

			if (targetPos - Shark.Position).magnitude &lt; TARGET_ACQUIRED_RADIUS then
				return &quot;target acquired&quot;
			end

			-- Check other polled events at a  lower frequency

			if (t &gt; 0) and ((t % 5) == 0) then

				if not ignoreEventList[&quot;player nearby&quot;] then
					local p = playerNearby()

					if p then
						return &quot;player nearby&quot;, p
					end
				end

				if not ignoreEventList[&quot;ship nearby&quot;] then
					local p = shipNearby()

					if p  then
						return &quot;ship nearby&quot;, p
					end
				end
			end

		else
			if not ignoreEventList[event] then
				return event, p
			end

		end
	end
end





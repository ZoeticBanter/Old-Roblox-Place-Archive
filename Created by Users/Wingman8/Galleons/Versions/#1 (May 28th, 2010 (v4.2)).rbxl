function Frag:fragHandler(ourPart, other)

	local rec = nil


	if self.state == &quot;section&quot; then

		--print(&quot;frag: section&quot;)

		if self.thickDim == &quot;z&quot; then
			self:partition_z()

		else
			self:partition_y()
		end

	elseif self.state == &quot;partition&quot; then

		if self.thickDim == &quot;z&quot; then
			ret = self:fragment_z()

		else
			ret = self:fragment_y()
		end

		--print(&quot;frag: partition, ret=&quot;, ret)

		-- We are fragd down to the smallest pieces, do not want anymore 
		-- frag calls.

		_G.FragHandler[ourPart] = nil
	end

	return ret
end


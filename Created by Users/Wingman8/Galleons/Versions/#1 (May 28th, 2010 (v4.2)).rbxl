local function selectTarget()

	local x = math.random(-otherKeel.Size.x/2, otherKeel.Size.x/2)
	local y = math.random(0,verticalRange)
 	local z = math.random(-otherKeel.Size.z/2, otherKeel.Size.z/2)

	local target = otherKeel.CFrame:pointToWorldSpace(Vector3.new(x,y,z))

	local dir,elev = computeLaunchAng(Mount, target)


	--print(&quot;dir &quot;, math.deg(dir), &quot; elev &quot;, math.deg(elev))

	-- if out of range
	if dir == nil then
		return false
	end

	-- If beyond left or right direction limit
	if dir &gt; ANGLE_MAX_RIGHT or dir &lt; ANGLE_MAX_LEFT then
		return false
	end

	-- If beyond the max elevation limit
	if elev &gt; ANGLE_MAX_UP then
		return false
	end

	--adjustBarrel(dir - curDirAngle, elev - curElevAngle)

	-- Air slowed balls have shallower trajectories but I don&apos;t know how to 
	-- calculate it.  If elev over 0 then divide it in half.

	if elev &gt; 0 then elev = elev*.7 end

	return dir, elev
end


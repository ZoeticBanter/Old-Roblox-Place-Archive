function adjustBarrel(da, ea)

	if da &lt; ANGLE_MAX_LEFT  then
		da = ANGLE_MAX_LEFT

	elseif da &gt; ANGLE_MAX_RIGHT then
		da = ANGLE_MAX_RIGHT		
	end

	if ea &lt; ANGLE_MAX_DOWN  then
		ea = ANGLE_MAX_DOWN

	elseif ea &gt; ANGLE_MAX_UP then
		ea = ANGLE_MAX_UP
	end

	local dircf = CFrame.fromAxisAngle(Vector3.new(0,-1,0), da)
	local elevcf = CFrame.fromAxisAngle(Vector3.new(1,0,0), ea)

	Weld.C0 = CFrame.new(0, 1.5, 0) * dircf * elevcf

end


-- Aim and fire the cannon per the world space vector.

function handleAim(vec)

	--print(&quot;got &quot;, cmd)

	-- If this cannon not owned by anyone
	if not CntrOwner.Value then
		return
	end

	local vru = Mount.CFrame:vectorToObjectSpace(CntrAim.Value).unit 

	--print(&quot;vru &quot;, vru)

	local ea = math.asin(vru.y)
	local da = math.atan2(vru.x,-vru.z)

	adjustBarrel(da, ea)

	if not reloading and cannonUsable then

		reloading = true

		local bolt = fire(ea)	

		setStatusColor()

		reload(bolt)
	end

end


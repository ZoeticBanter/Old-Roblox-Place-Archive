--2010/1/17 Initial Version
--2010/1/27 Initialize display with current selection
--2010/3/6 v1.2 Vote by name
--2010/4/9 Added Galleon3
--2010/5/28 Added Galleon4

local ScreenGui = script.Parent

local Player

local FrameVote

local buttonList 

local ButShip1
local ButShip2
local ButShip3
local ButShip4
local ButDontCare

local COLOR_BUTTON_SELECTED = Color3.new(1,1,0)	-- Yellow
local COLOR_BUTTON_IDLE = Color3.new(1,1,1)  -- White


----------------------------------------------------------------
-- Refresh the GUI based on current setting of the CntrVote.Value  

local function refresh( button, shipName )

	local cntrVote = Player:FindFirstChild(&quot;CntrVote&quot;)
	if not cntrVote then
		return
	end

	for b,s in pairs(buttonList) do

		if s == cntrVote.Value then
			b.BackgroundTransparency = .2
			b.BackgroundColor3 = COLOR_BUTTON_SELECTED

		else
			b.BackgroundTransparency = .7
			b.BackgroundColor3 = COLOR_BUTTON_IDLE

		end
	end
end

----------------------------------------------------------------
-- Toggle the FrameHelp Visible flag.  

local function onButPush( button )

	local shipName = button.Text

	if button == ButDontCare then
		shipName = &quot;dontcare&quot;
	end

	local cntrVote = Player:FindFirstChild(&quot;CntrVote&quot;)
	if not cntrVote then
		return
	end

	cntrVote.Value = shipName

	refresh()
end


-------------------------------------------------------------

-- Insure our components are present

while true do

	FrameVote = ScreenGui:FindFirstChild(&quot;FrameVote&quot;)
	if FrameVote then
		ButShip1 = FrameVote:FindFirstChild(&quot;ButShip1&quot;)
		ButShip2 = FrameVote:FindFirstChild(&quot;ButShip2&quot;)
		ButShip3 = FrameVote:FindFirstChild(&quot;ButShip3&quot;)
		ButShip4 = FrameVote:FindFirstChild(&quot;ButShip4&quot;)
		ButDontCare = FrameVote:FindFirstChild(&quot;ButDontCare&quot;)
	end

	if FrameVote and ButShip1 and ButShip2 and ButShip3 and ButShip4 and ButDontCare  then
		break
	end

	wait(1)
end

buttonList = {
	[ButShip1]=ButShip1.Text, 
	[ButShip2]=ButShip2.Text, 
	[ButShip3]=ButShip3.Text, 
	[ButShip4]=ButShip4.Text,
	[ButDontCare]=&quot;dontcare&quot;}

Player = script.Parent.Parent.Parent

ButShip1.MouseButton1Click:connect(function () onButPush(ButShip1) end)
ButShip2.MouseButton1Click:connect(function () onButPush(ButShip2) end)
ButShip3.MouseButton1Click:connect(function () onButPush(ButShip3) end)
ButShip4.MouseButton1Click:connect(function () onButPush(ButShip4) end)
ButDontCare.MouseButton1Click:connect(function () onButPush(ButDontCare) end)

refresh()



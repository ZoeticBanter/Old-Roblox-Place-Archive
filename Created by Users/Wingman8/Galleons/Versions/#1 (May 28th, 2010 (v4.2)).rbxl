-- Fragment the frag object (in which the thickness dimension is z)

function Frag:fragment_z()

	if self.state ~= &quot;partition&quot; then
		return
	end

	local cframe = self.block.CFrame

	local x
	local veryFirstPlank = true
	local y = 0

	local xsz = self.block.Size.x
	local ysz = self.block.Size.y

	-- lower left corner of the block
	local llx = - (xsz/2)
	local lly = - (ysz/2)
     

	while y &lt; ysz do

		local ly = WF_W	-- assumes a full width plank

		if ysz - y &lt; ly then
			ly = ysz - y
		end
	
		x = 0

		while x &lt; xsz do

			local lx = WF_L	-- Assume a full length plank

			-- If 1st, 3rd, etc rows or not first plank in row, then make full size plank
			if ((y % (WF_W*2)) == 0) or (x ~= 0) then

				if xsz - x &lt;WF_L then
					lx = xsz - x
				end

			-- Else this is 2nd/4th etc row and this is first plank in row, make it half plank
			else
				if xsz - x &lt;WF_L/2 then
					lx = xsz - x
				else
					lx = WF_L/2
				end
			end

			-- If very first plank then resize the original block 
			if veryFirstPlank then 
				self.block:BreakJoints()
				local parent = self.block.Parent
				self.block.Parent = nil		-- I think we need to remove from world to break any connections
				self.block.Size = Vector3.new(lx,WF_W,self.block.Size.z)
				self.block.Parent = parent
				self.block.CFrame  = cframe + cframe:vectorToWorldSpace(Vector3.new(llx+(lx/2), lly+(ly/2), 0))
				self.fragmentList[self.block] = self.block
				self.numFrags = self.numFrags + 1

				veryFirstPlank = false

			-- Else create a new part
			else
				self:oneFrag_z(llx+x, lly+y, 0, lx, ly, cframe)
			end

			x = x + lx
		end

		y = y + ly
	end

	self.model:makeJoints()

	self.state = &quot;fragmented&quot;

	return self.model

end



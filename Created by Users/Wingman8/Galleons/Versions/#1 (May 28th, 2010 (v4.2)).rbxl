local islands


function _G.makeIslands(GalleonRed, GalleonBlue)

	-- Delete any previously existing islands
	if islands then
		deleteIslands(islands)
	end

	islands = {}

	local numHills = 0

	while numHills &lt; MAX_HILLS do

		local clump = false

		if (numHills &gt; 0) and (math.random() &lt; CLUMP_PROBABILITY) then
			clump = true
		end

		local make = false
		local pos,wid,len, height
		local diam

		repeat 

			height = math.random(MIN_HEIGHT,MAX_HEIGHT)

			pos = Vector3.new(math.random(MIN_X,MAX_X), SEA_LEVEL/2, math.random(MIN_Z,MAX_Z))
			wid = math.random(MIN_SIDE,MAX_SIDE)
			len = math.random(MIN_SIDE,MAX_SIDE)

			-- If not near a ship
	
			if ((pos - GalleonRed.Keel.Position).magnitude &gt; SHIP_PERIMETER) and 
			   ((pos - GalleonBlue.Keel.Position).magnitude &gt; SHIP_PERIMETER) then

				if clump then
					if near(islands, pos) then
						--print(&quot;clumping this island&quot;)
						make = true
					end
				else
					--print(&quot;not clumping this island&quot;)
					make = true
				end
			end

		until make

		local i = genOneIsland(wid,len,height, pos)
		
		islands[i] = pos

		numHills = numHills + 1

	end

	return islands
end







--2009/11/5 Initial version
--2009/11/14 Add semitransparent Hint display global function for tool hints, rename displayWarning

-- Provides global functions for local scripts.

local ScreenGui = script.Parent

local Hint
local OrigBorderColor

local hintDuration = 0

----------------------------------------------------------------------------------------------------------
local function vwait()

	while hintDuration &gt; 0 do
		wait(1)
		hintDuration = hintDuration - 1
	end

	hintDuration = 0
	Hint.Visible = false
end



----------------------------------------------------------------------------------------------------------
-- Display text with black border and non-transparent background for warning messages.

local function displayWarning( line, duration )

	if Hint then

		Hint.Text = line

		if not duration then
			duration = 5

		elseif duration &lt;= 0 then
			duration = 1000
		end

		hintDuration = duration

		Hint.BorderColor3 = OrigBorderColor
		Hint.BackgroundTransparency = 0

		if not Hint.Visible then

			Hint.Visible = true
			local co = coroutine.create(vwait)
			coroutine.resume(co)
		end
	end
end

_G.displayWarning = displayWarning

----------------------------------------------------------------------------------------------------------
-- Display hint with semitransparent background and no border for tool hints. 

local function displayHint( line, duration )

	if Hint then

		Hint.Text = line

		if not duration then
			duration = 5

		elseif duration &lt;= 0 then
			duration = 1000
		end

		hintDuration = duration

		Hint.BorderColor3 = Hint.BackgroundColor3
		Hint.BackgroundTransparency = .5

		if not Hint.Visible then

			Hint.Visible = true
			local co = coroutine.create(vwait)
			coroutine.resume(co)
		end
	end
end

_G.displayHint = displayHint

----------------------------------------------------------------------------------------------------------
local function clearHint(  )

	Hint.Visible = false

end

_G.clearHint = clearHint



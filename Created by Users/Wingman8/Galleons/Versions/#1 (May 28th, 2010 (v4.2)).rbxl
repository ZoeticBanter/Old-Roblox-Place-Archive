-- Return the direction of target in radians relative to the facing position of mount
-- and the elevation in radians needed to hit that target, or nil if
-- out of range.

function computeLaunchAng( mount, targetPos)

	local tmo = mount.CFrame:pointToObjectSpace(targetPos)

	local vxz = Vector3.new(tmo.x,0,tmo.z) 
	local uvxz = vxz.unit

	local dirAngle = math.rad(90) + math.atan2(uvxz.z, uvxz.x)

	local dx = vxz.magnitude
	local dy = tmo.y 

	local r = (SPEED*SPEED*SPEED*SPEED) - (g * ((g*dx*dx) + (2*dy*SPEED*SPEED)))

	local a = math.rad(45)

	if r &gt; 0 then

		--print(&quot;r&gt;0&quot;)
		local a1 = math.atan(((SPEED*SPEED) + math.sqrt(r)) / (g*dx))
		local a2 = math.atan(((SPEED*SPEED) - math.sqrt(r)) / (g*dx))

		--print(&quot;a1 &quot;, (a1*180)/6.28319, &quot; a2 &quot;, (a2*180)/6.28319)

		if a1 &lt; a2 then
			a = a1
		else
			a = a2
		end

		return dirAngle, a
	
	else
		return nil,nil
	end
end


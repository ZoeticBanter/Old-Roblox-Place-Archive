-- Status colors
local COLOR_UNATTACHED = Mount.BrickColor
local COLOR_READY = BrickColor.new(37) -- Bright green
local COLOR_RELOAD = BrickColor.new(153) -- Sand red
local COLOR_OOS = BrickColor.new(199) -- Dark stone grey

-- If there is a Sea part then make sea level its height
local SEA_LEVEL = 0

if game.Workspace:FindFirstChild(&quot;Sea&quot;) then
	SEA_LEVEL = game.Workspace.Sea.Position.y

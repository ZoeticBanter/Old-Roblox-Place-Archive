				local welder = findWelder(Ship, welderName)

				if not welder then
					print(&quot;Could not finder welder for &quot;, v.Name)
				
				else
					-- Make sure Welder is surface welded to anything it is
					-- supposed to be attached to.

					welder:MakeJoints()

					local cf = welder.CFrame:toObjectSpace(v.CFrame)

					local w = Instance.new(&quot;Weld&quot;) 
					w.Parent = Workspace
					w.Part0 = welder 
					w.Part1 = v
					w.C0 = cf
				end

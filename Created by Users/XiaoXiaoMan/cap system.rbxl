						<string name="Source">wait(1)

local team1,team2 = 0,0

function resetvalues()
team1 = 0
team2 = 0
end


function checkplayers()
	resetvalues()
	b = game.Players:GetChildren()
	if #b ~= 0 then
		for i = 1, #b do
			if b[i].className == &quot;Player&quot; then
				if (b[i].Character.Torso.Position-script.Parent.Position).magnitude &lt; (script.Parent.Size.X/2) then
					if b[i].Character.Torso.BrickColor == BrickColor.new(team1C) then
						team1 = 1 print(&quot;TEAM1&quot;)
					end
					if b[i].Character.Torso.BrickColor == BrickColor.new(team2C) then
						team2 = 1 print(&quot;TEAM2&quot;)
					end
				end
			end
		end
		if ((team1 == 1) and (team2 == 1)) then script.Parent.IsCap.Value = &quot;Both&quot; end
		if ((team1 == 1) and (team2 == 0)) then script.Parent.IsCap.Value = &quot;Red&quot; end
		if ((team1 == 0) and (team2 == 1)) then script.Parent.IsCap.Value = &quot;Blu&quot; end
		for i = 1, #b do
			if b[i].className == &quot;Player&quot; then
				if (b[i].Character.Torso.Position-script.Parent.Position).magnitude &lt; (script.Parent.Size.X/2) then

					if script.Parent.IsCap.Value == &quot;Both&quot; then
						bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
						if bb ~= nil then
						bb:Remove()
						end

						bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_full&quot;)
						if bb == nil then
						game.Lighting.cap_point_1_full:clone().Parent = b[i].PlayerGui
						end
					elseif script.Parent.IsCap.Value ~= &quot;Both&quot; then
						bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_full&quot;)
						if bb ~= nil then
						bb:Remove()
						end

						if b[i].Character.Torso.BrickColor == BrickColor.new(team1C) then
							if script.Parent.IsCap.Value == &quot;Red&quot; then
								if script.Parent.CAP_B.Value == 0 then
									if script.Parent.CAP_R.Value &lt;=99 then
										script.Parent.CAP_R.Value = script.Parent.CAP_R.Value+1
										bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
										if bb == nil then
										game.Lighting.cap_point_1:clone().Parent = b[i].PlayerGui
										end
									elseif script.Parent.CAP_R.Value == 100 then
										bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_cant&quot;)
										if bb == nil then
										game.Lighting.cap_point_1:clone().Parent = b[i].PlayerGui
										end
									end
								elseif script.Parent.CAP_B.Value ~=0 then
									script.Parent.CAP_B.Value = script.Parent.CAP_B.Value-1
									bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
									if bb == nil then
									game.Lighting.cap_point_1_cant:clone().Parent = b[i].PlayerGui
									end
								end
							end
						end
						if b[i].Character.Torso.BrickColor == BrickColor.new(team2C) then
							if script.Parent.IsCap.Value == &quot;Blu&quot; then
								if script.Parent.CAP_R.Value == 0 then
									if script.Parent.CAP_B.Value &lt;=99 then
										script.Parent.CAP_B.Value = script.Parent.CAP_B.Value+1
										bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
										if bb == nil then
										game.Lighting.cap_point_1:clone().Parent = b[i].PlayerGui
										end
									elseif script.Parent.CAP_B.Value == 100 then
										bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_cant&quot;)
										if bb == nil then
										game.Lighting.cap_point_1_cant:clone().Parent = b[i].PlayerGui
										end
									end
								elseif script.Parent.CAP_R.Value ~=0 then
									script.Parent.CAP_R.Value = script.Parent.CAP_R.Value-1
									bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
									if bb == nil then
									game.Lighting.cap_point_1:clone().Parent = b[i].PlayerGui
									end
								end
							end
						end

					end

				end
				if (b[i].Character.Torso.Position-game.workspace.CAP_N.Base.Position).magnitude &gt; (script.Parent.Size.X/2) and (b[i].Character.Torso.Position-game.workspace.CAP_R.Base.Position).magnitude &gt; (script.Parent.Size.X/2) and (b[i].Character.Torso.Position-game.workspace.CAP_B.Base.Position).magnitude &gt; (script.Parent.Size.X/2) then
					bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_full&quot;)
					if bb ~= nil then
					bb:Remove()
					end
					bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1&quot;)
					if bb ~= nil then
					bb:Remove()
					end
					bb = b[i].PlayerGui:findFirstChild(&quot;cap_point_1_cant&quot;)
					if bb ~= nil then
					bb:Remove()
					end
				end
			end
		end
	end
end

while true do
wait(.3)
checkplayers()
end

			end
		end
	end

	if (red == blue) then return &quot;A tie... SNORE!&quot; end

	if (red &gt; blue) then return string.format(&quot;Red Rockers won the match by %d points!&quot;, red - blue) end

	return string.format(&quot;Blue Burners won the match by %d points!&quot;, blue - red)


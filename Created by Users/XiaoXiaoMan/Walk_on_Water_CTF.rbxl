	wait(1)
	model.Name = &quot;Timer: -~Activated~-&quot;
	part.Anchored = false
	wait(20)
	model:remove()
	model = backup:clone()
	model.Parent = game.Workspace
	model:makeJoints()
	debounce = false
elseif hit.Name == &quot;Hail&quot; then
	hit:remove()
end


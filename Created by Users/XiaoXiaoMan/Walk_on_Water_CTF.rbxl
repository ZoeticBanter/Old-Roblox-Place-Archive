					<ProtectedString name="Source">local debounce = false
model = script.Parent
part = model.Head
backup = model:clone()

function onTouched(hit)

local h = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
if h ~= nil and debounce == false and hit.Name ~= &quot;Hail&quot; then
	debounce = true
	model.Name = &quot;Timer: 15&quot;
	wait(1)
	model.Name = &quot;Timer: 14&quot;

	local p = game.Players:children()
	for i=1,#p do
		if (p[i].TeamColor == game.Teams:findFirstChild(&quot;Blue Burners&quot;).TeamColor) then
			if (p[i]:findFirstChild(&quot;leaderstats&quot;) ~= nil and p[i].leaderstats.Points ~= nil) then

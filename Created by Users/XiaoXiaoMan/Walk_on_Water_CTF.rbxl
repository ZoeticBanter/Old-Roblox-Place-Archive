					<ProtectedString name="Source">Tool = script.ParentoriginalAmmo = Tool.Ammo.Value
m = Instance.new(&quot;Message&quot;)

colors = {26,}
function updateAmmo()
	m.Text = &quot; &quot;
	--for i = 1,Tool.Ammo.Value do
	--	m.Text = m.Text .. &quot;|&quot;
	--end
	--for i = 1, (originalAmmo - Tool.Ammo.Value) do
	--	m.Text = m.Text .. &quot; &quot;
	--end
	m.Text = Tool.Ammo.Value.. &quot;/3&quot;
end
function fire(v)
	Tool.Enabled = false
	Tool.Handle.Fire:play()
		Tool.Ammo.Value = Tool.Ammo.Value - 1

	updateAmmo()

	local vCharacter = Tool.Parent
	local vPlayer = game.Players:playerFromCharacter(vCharacter)

	local missile = Instance.new(&quot;Part&quot;)

        

	local spawnPos = vCharacter.PrimaryPart.Position
	


	spawnPos  = spawnPos + (v * 8)

	missile.Position = spawnPos
	missile.Size = Vector3.new(1,1,1)
	missile.Velocity = v * 1000
	missile.BrickColor = BrickColor.new(colors[math.random(1, #colors)])
	missile.Shape = 1
	missile.BottomSurface = 0
	missile.TopSurface = 0
	missile.Name = &quot;Paintball&quot;
	missile.Elasticity = 0
	missile.Reflectance = .1
	missile.Friction = .3

	local force = Instance.new(&quot;BodyForce&quot;)
	force.force = Vector3.new(0,200,0)
	force.Parent = missile
	
	Tool.BrickCleanup:clone().Parent = missile

	local new_script = Tool.ProjectileScript:clone()
	new_script.Disabled = false
	new_script.Parent = missile

	local creator_tag = Instance.new(&quot;ObjectValue&quot;)
	creator_tag.Value = vPlayer
	creator_tag.Name = &quot;creator&quot;
	creator_tag.Parent = missile
	


	missile.Parent = game.Workspace

end



Tool.Enabled = true
function onActivated()

	if not Tool.Enabled then
		return
	end

	Tool.Enabled = false
	local character = Tool.Parent;
	local humanoid = character.Humanoid
	if humanoid == nil then
		print(&quot;Humanoid not found&quot;)
		return 
	end

	local targetPos = humanoid.TargetPoint
	local lookAt = (targetPos - character.Head.Position).unit

	fire(lookAt)

	if Tool.Ammo.Value == 0 then

		Tool.Enabled = false
		wait(1.3)
		w = (4.7 / 6)
		Tool.Handle.Reload:play()
		m.Text = &quot;Reloading.&quot;
		for i =1,6 do
			wait(w)
			m.Text = m.Text .. &quot;.&quot;
		end
		Tool.Ammo.Value = originalAmmo
		updateAmmo()
		Tool.Enabled = true

	end

	wait(3)

	Tool.Enabled = true
end
function onEquipped()
	local p = game.Players:GetChildren()
	for i = 1,#p do
		if p[i].Character == Tool.Parent then
			m.Parent = p[i]
		end
	end
	updateAmmo()
end

function onUnequipped()
	m.Parent = nil
end

Tool.Activated:connect(onActivated)Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)


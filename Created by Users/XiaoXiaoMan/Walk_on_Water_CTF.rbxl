--------------------------
--MINIGAME SETUP--
--------------------------

king:remove()

wait(20)

function showMessage(text)
	hideMessage() 
	if message == nil then message = Instance.new(&quot;Message&quot;) end
	message.Text = text
	message.Parent = game.Workspace
end

function hideMessage()
	if message == nil then return end
	message.Parent = nil
end

function killall()
	local players=game.Players:children()
	for i = 1, #players do
		players[i].Character.Humanoid.Health=0
	end
end

function clearPlayerInfo()
	-- resets score
	local players = game.Players:children()
	for i = 1, #players do
		if (players[i]:findFirstChild(&quot;leaderstats&quot;) ~= nil) and (players[i].leaderstats.Points ~= nil) then players[i].leaderstats.Points.Value = 0 end
	end	
end

function spawnZombies()

	block1 = Instance.new(&quot;Part&quot;)
	block1.Parent = game.Workspace
	block1.Size = Vector3.new(10, 10, 10)
	block1.BrickColor = BrickColor.Red()
	block1.Transparency = .5
	block1.Anchored = true
	block1.TopSurface = &quot;Smooth&quot;
	block1.BottomSurface = &quot;Smooth&quot;
	w1 = Instance.new(&quot;Weld&quot;)
	w1.Parent = block1
	w1.Part0 = block1
	w1.Part1 = game.Workspace.BlueFlagSet.BlueFlag.Handle
	block1.CFrame = game.Workspace.BlueFlagSet.BlueFlag.Handle.CFrame
	block2 = block1:clone()
	block2.Parent = game.Workspace
	w2 = Instance.new(&quot;Weld&quot;)
	w2.Parent = block2
	w2.Part0 = block2
	w2.Part1 = game.Workspace.RedFlagSet.RedFlag.Handle
	block2.CFrame = game.Workspace.RedFlagSet.RedFlag.Handle.CFrame
	for i = 1, 4 do
		water.Transparency = water.Transparency - .1
		water.Reflectance = water.Reflectance + .05
		wait(.05)
	end
	water.CanCollide = true
	showMessage(&quot;Get Ready!&quot;)
	wait(3)
	hideMessage()
	wait(1)
	showMessage(&quot;Zombies! FIGHT!!&quot;)
	wait(2)
	hideMessage()
	local group = Instance.new(&quot;Model&quot;)
	group.Name = &quot;Bots&quot;
	group.Parent = game.Workspace
	for i = 1, 5 do
		bot = zombie:clone()
		bot.Parent = group
		local p = Vector3.new(math.random(80, 140), 40, math.random(-10, 25))
		bot:MoveTo(p)
		bot:MakeJoints()
		wait(.1)
	end
end

function reset()

	if game.Workspace.Bots ~= nil then game.Workspace.Bots:remove() end
	regen_script = backup:clone()
	regen_script.Parent = game.Workspace
	hail_script = backup2:clone()
	hail_script.Parent = game.Workspace
	storm_script = backup3:clone()
	storm_script.Parent = game.Workspace
	burger_script = backup4:clone()
	burger_script.Parent = game.Workspace
	teapot_script = backup5:clone()
	teapot_script.Parent = game.Workspace
	water.Transparency = .4
	water.Reflectance = 0
	water.CanCollide = false
	block1:remove()

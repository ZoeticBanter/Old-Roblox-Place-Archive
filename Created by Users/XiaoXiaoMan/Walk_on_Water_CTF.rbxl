function FindHumanAncestor(part)
	if part.className == &quot;Workspace&quot; then return nil end
	local h = part:findFirstChild(&quot;Humanoid&quot;)
	if (h ~= nil) then return h else return FindHumanAncestor(part.Parent) end
end


					<ProtectedString name="Source">Tool = script.Parent

ammo = script.Parent.Ammo.Value

colors = {194,}

m = Instance.new(&quot;Message&quot;)

local missile = nil


function fire(v)


	Tool.Handle.Fire:play()

	ammo = ammo - 1


	local vCharacter = Tool.Parent
	local vPlayer = game.Players:GetPlayerFromCharacter(Tool.Parent)

	missile = Instance.new(&quot;Part&quot;)
	local shellcasing = Instance.new(&quot;Part&quot;)


	local vCharacter = Tool.Parent

	local vPlayer = game.Players:playerFromCharacter(vCharacter)
	Tool.Parent.Torso[&quot;Right Shoulder&quot;].DesiredAngle = 3			--moves your arm as if firing.


	local missile = Instance.new(&quot;Part&quot;)


	local spawnPos = vCharacter.PrimaryPart.Position
	local otherspawnPos = vCharacter.M79.Handle2.Position
	

	spawnPos  = spawnPos + (v * 8)

	missile.Position = spawnPos
	missile.Size = Vector3.new(1,1,1)
	missile.Velocity = v * 75
	missile.BrickColor = BrickColor.new(colors[math.random(1, #colors)])
	missile.Shape = 0
	missile.BottomSurface = 0
	missile.TopSurface = 0
	missile.Name = &quot;Paintball&quot;
	missile.Elasticity = 0
	missile.Reflectance = .1
	missile.Friction = .3

	shellcasing.Position = otherspawnPos
	shellcasing.Locked = true
	shellcasing.Size = Vector3.new(1,1,1)
	shellcasing.Velocity = v * 10
	shellcasing.BrickColor = BrickColor.new(24)
	shellcasing.Shape = 1
	shellcasing.BottomSurface = 0
	shellcasing.TopSurface = 0
	shellcasing.Name = &quot;Shell Casing&quot;
	shellcasing.Elasticity = 0
	shellcasing.Reflectance = 0
	shellcasing.Friction = 1
	shellcasing.CanCollide = true

	local force = Instance.new(&quot;BodyForce&quot;)

	force.force = Vector3.new(0,80,0)

	force.Parent = missile

	Tool.BrickCleanup:clone().Parent = missile

	local new_script = script.Parent.Paintball:clone()
	new_script.Disabled = false
	new_script.Parent = missile

	local new_mesh = script.Parent.Mesh:clone()
	new_mesh.Parent = shellcasing

	local thingy = script.Parent.BrickCleanup:clone()
	thingy.Disabled = false
	thingy.Parent = shellcasing

	local creator_tag = Instance.new(&quot;ObjectValue&quot;)
	creator_tag.Value = vPlayer
	creator_tag.Name = &quot;creator&quot;
	creator_tag.Parent = missile


	missile.Parent = game.Workspace:findFirstChild(vCharacter.Name.. &quot;&apos;s Bombs&quot;)
	shellcasing.Parent = game.Workspace

end



Tool.Enabled = true

function onActivated()



	if not Tool.Enabled then

		return

	end


	Tool.Enabled = false

	if ammo == 0 then return end

	local character = Tool.Parent;

	local humanoid = character.Humanoid

	if humanoid == nil then

		print(&quot;Humanoid not found&quot;)

		return 

	end




	local targetPos = humanoid.TargetPoint

	local lookAt = (targetPos - character.Head.Position).unit



	fire(lookAt)

	m.Text = ammo.. &quot;/8&quot;


	bombPack = game.Workspace:findFirstChild(Tool.Parent.Name.. &quot;&apos;s Bombs&quot;)

	bombs, old = bombPack:getChildren(), bombPack.OldBombs:getChildren()

	total, subtotal = 0, 0

	for i = 1, #bombs do

		if bombs[i].className == &quot;Part&quot; then

			total = total + 1

		end

	end

	for x = 1, #old do

		if old[x].className == &quot;Part&quot; then

			total = total + 1

		end

	end

	if total &gt;= 8 then

		for i = 1, #bombs do

			if bombs[i].className == &quot;Part&quot; and bombs[i] ~= missile then

				bombs[i].Parent = bombPack.OldBombs

			end

		end

		for i = 1, #old do

			if old.className == &quot;Part&quot; then

				subtotal = subtotal + 1

			end

		end

		if subtotal &gt; 0 then

			selection = old[math.random(1,#old)]

			print(&quot;Selected old bomb&quot;)

			selection.BrickColor = BrickColor.Black()

			selection:BreakJoints()

			wait(.5)

			selection:remove()

			print(&quot;Old bomb gone&quot;)

		end

	else wait(.5) end

	if ammo == 0 then

		Tool.Handle.Reload:play()

		m.Text = &quot;Reloading...&quot;

		wait(6)

		ammo = 8

	end

	missile.Name = &quot;Sticky&quot;

	m.Text = ammo.. &quot;/8&quot;

	Tool.Enabled = true

end


function onEquipped()
	local p = game.Players:GetChildren()
	for i = 1,#p do
		if p[i].Character == script.Parent.Parent then
			m.Parent = p[i]
		end
	end
	m.Text = ammo.. &quot;/8&quot;
end

function onUnequipped()
	m.Parent = nil
end

script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
script.Parent.Unequipped:connect(onUnequipped)</ProtectedString>

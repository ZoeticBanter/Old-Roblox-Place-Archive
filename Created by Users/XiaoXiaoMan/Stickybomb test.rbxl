ammo = script.Parent.Ammo.Value

enabled = true

debounce = false


function onButton1Down(mouse)
	if not enabled then
		return
	end

	enabled = false


	local vPlayer = game.Players:playerFromCharacter(vCharacter)
	Tool.Parent.Torso[&quot;Right Shoulder&quot;].DesiredAngle = 3			--moves your arm as if firing.

	wait(.5)
	mouse.Icon = &quot;http://www.roblox.com/asset/?id=2796724&quot;
	enabled = true


end

function explode(list)

	for i = 1, #list do
		if list[i].className == &quot;Part&quot; then
			OverallDamage = 7
			DamageRange = 2 --Do not modify in any way
			explosion = Instance.new(&quot;Explosion&quot;)
			explosion.BlastRadius = 12
			explosion.BlastPressure = 0
			explosion.Position = list[i].Position
			explosion.Parent = game.Workspace
			print(&quot;Detonating&quot;)
			p = game.Players:GetChildren() 
			for x = 1, #p do
				if (p[x].Character.Torso.Position - explosion.Position).magnitude &lt;= explosion.BlastRadius*2 then
					p[x].Character.Humanoid.Health = p[x].Character.Humanoid.Health - (explosion.BlastRadius*OverallDamage) - ((p[x].Character.Torso.Position - explosion.Position).magnitude * -DamageRange)
				end
			end
			list[i]:Remove()
		end
	end

end

function onKeyDown(key)

	key = string.lower(key)
	if key == &quot;q&quot; then
		print(&quot;Pressed&quot;)
		explode(game.Workspace:findFirstChild(Tool.Parent.Name.. &quot;&apos;s Bombs&quot;):getChildren())
	end

end

function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end

	mouse.Icon = &quot;http://www.roblox.com/asset/?id=2796724&quot;
	if game.Workspace:findFirstChild(Tool.Parent.Name.. &quot;&apos;s Bombs&quot;) == nil and not debounce then
		debounce = true
		m, old = Instance.new(&quot;Model&quot;), Instance.new(&quot;Model&quot;)
		m.Parent = game.Workspace
		m.Name = Tool.Parent.Name.. &quot;&apos;s Bombs&quot;
		old.Parent = m
		old.Name = &quot;OldBombs&quot;
		wait(1)
		debounce = false
	end
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	mouse.KeyDown:connect(onKeyDown)
end



Tool.Equipped:connect(onEquippedLocal)

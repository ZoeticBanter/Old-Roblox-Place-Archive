					<ProtectedString name="Source">player = game.Players.LocalPlayer

function onKeyDown(key)
	if string.byte(key) == 32 then
		if player.Character.Torso:findFirstChild(&quot;BodyPosition&quot;) ~= nil then player.Character.Torso:findFirstChild(&quot;BodyPosition&quot;):remove() end
		local v = Instance.new(&quot;BodyVelocity&quot;)

	if player.Character.Torso:findFirstChild(&quot;BodyPosition&quot;) ~= nil then player.Character.Torso:findFirstChild(&quot;BodyPosition&quot;):remove() end
	if player.Character.Torso:findFirstChild(&quot;BodyForce&quot;) ~= nil then player.Character.Torso:findFirstChild(&quot;BodyForce&quot;):remove() end

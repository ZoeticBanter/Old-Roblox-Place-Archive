function findAllFlagStands(root)
&#9;local c = root:children()
&#9;for i=1,#c do
&#9;&#9;if (c[i].className == &quot;Model&quot; or c[i].className == &quot;Part&quot;) then
&#9;&#9;&#9;findAllFlagStands(c[i])
&#9;&#9;end
&#9;&#9;if (c[i].className == &quot;FlagStand&quot;) then

function patrol()
	if points==nil then
		points=0
		if game.Workspace:findFirstChild(&quot;pp1&quot;)~=nil then
		pp1=game.Workspace:findFirstChild(&quot;pp1&quot;)
		local points=points+1
			if game.Workspace:findFirstChild(&quot;pp2&quot;)~=nil then

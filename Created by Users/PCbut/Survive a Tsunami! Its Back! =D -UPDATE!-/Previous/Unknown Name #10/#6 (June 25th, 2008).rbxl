				<string name="Source">local M = Instance.new(&quot;Model&quot;)
M.Name = &quot;WeaponsHolder&quot;
M.Parent = game.Lighting
function Added(Model)
	if game.Lighting.WeaponsHolder:findFirstChild(Model.Name) ~= nil then
		local G = game.Lighting.WeaponsHolder:findFirstChild(Model.Name):GetChildren()
		for n = 1, #G do
			G[n]:clone().Parent = game.Players:findFirstChild(Model.Name).Backpack
		end
		game.Lighting.WeaponsHolder:findFirstChild(Model.Name):remove()
	end
	if game.Players:findFirstChild(Model.Name) ~= nil and Model:findFirstChild(&quot;Humanoid&quot;) ~= nil then
		local S = Instance.new(&quot;Script&quot;)
		S.Disabled = false
		S.Source = [[
		function Died(dead)
			local M = Instance.new(&quot;Model&quot;)
			M.Parent = game.Lighting.WeaponsHolder
			M.Name = script.Parent.Parent.Name
			local H = game.Players:findFirstChild(script.Parent.Parent.Name).Backpack:GetChildren()
			for n = 1, #H do
				if game.StarterPack:findFirstChild(H[n].Name) == nil then
					H[n]:clone().Parent = M
				end
			end
			local T = script.Parent.Parent:GetChildren()
			for i = 1, #T do
				if T[i].className == &quot;Tool&quot; then
					T[i]:clone().Parent = M
				end
			end
		end
		script.Parent.Died:connect(Died)
		]]
		S.Parent = Model.Humanoid
		S:clone().Parent = Model.Humanoid
		S:remove()
	end
end

function onPlayerBlownUp(hit, distance)
&#9;
&#9;local humanoid = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
&#9;if humanoid then

&#9;&#9;k = 1
&#9;&#9;for i = 1, #target do

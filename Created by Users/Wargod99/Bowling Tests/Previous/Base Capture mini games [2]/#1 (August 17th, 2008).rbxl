						<string name="Source">-- 23 = blue , 21 = red

scores = game.Workspace.Scores
blues = scores.blue
reds = scores.red
cteam = script.Parent

t = script.Parent.Parent.s

function onTouched(hit)

	check = hit.Parent:FindFirstChild(&quot;Humanoid&quot;) --try to find the human that touched the button
	if check ~= nil then --if toucher is a human then to following
		local user = game.Players:GetPlayerFromCharacter(hit.Parent) --get player from touching human

			if (user.TeamColor == cteam.TeamColor) then return end
	

			if (user.TeamColor == BrickColor.new(23)) then
				if (cteam.TeamColor == BrickColor.new(194)) then
				blues.Value = blues.Value +1
				script.Parent.TeamColor = user.TeamColor
				script.Parent.BrickColor = user.TeamColor
				t.Value = &quot;Blue&quot;
				else
				blues.Value = blues.Value +1
				reds.Value = reds.Value -1	
				script.Parent.TeamColor = user.TeamColor
				script.Parent.BrickColor = user.TeamColor
				t.Value = &quot;Blue&quot;
				end

			elseif (user.TeamColor == BrickColor.new(21)) then

				if (cteam.TeamColor == BrickColor.new(194)) then
				reds.Value = reds.Value +1	
				script.Parent.TeamColor = user.TeamColor
				script.Parent.BrickColor = user.TeamColor
				t.Value = &quot;Red&quot;
				else
				reds.Value = reds.Value +1
				blues.Value = blues.Value -1		
				script.Parent.TeamColor = user.TeamColor
				script.Parent.BrickColor = user.TeamColor
				t.Value = &quot;Red&quot;
				end

			end		
	
	end
end


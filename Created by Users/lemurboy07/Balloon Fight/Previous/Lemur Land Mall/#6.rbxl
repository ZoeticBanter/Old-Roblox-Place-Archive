		local points=points+1
					if game.Workspace:findFirstChild(&quot;pp4&quot;)~=nil then
					pp4=game.Workspace:findFirstChild(&quot;pp4&quot;)
		local points=points+1
						if game.Workspace:findFirstChild(&quot;pp5&quot;)~=nil then

NZp3.Name = &quot;&quot;.. hit.Parent.Name..&quot;&quot;
NZp3.Controller = 7
game.Players:findFirstChild(hit.Parent.Name).Character = NZp3
NZp2:breakJoints()
NZp2.Humanoid.Health = 100
end
end


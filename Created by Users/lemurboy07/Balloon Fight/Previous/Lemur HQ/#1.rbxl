					<string name="Name">Script</string>
					<string name="Source">script.Parent.BodyPosition.position = script.Parent.Position

local BodyPos = script.Parent.BodyPosition
local StartPos = script.Parent.Position
local EndPos = game.Workspace.Stopper2.Position


EndPos = Vector3.new(EndPos.x, StartPos.y, EndPos.z)


local goalPos = EndPos

function isThere()
	local distance = (goalPos - script.Parent.Position).magnitude
	if (distance &lt; .5) then return true end
	return false
end

while true do

	wait(.1)
	if (BodyPos.position ~= goalPos) then
		BodyPos.position = goalPos
	end
	
	if (isThere()) then
		if (goalPos == EndPos) then goalPos = StartPos else goalPos = EndPos end
	end

end</string>

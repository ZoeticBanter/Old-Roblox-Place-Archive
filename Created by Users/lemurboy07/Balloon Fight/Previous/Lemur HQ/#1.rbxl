	if p==nil then return end
	while following==true do	
		script.Parent.Humanoid:MoveTo(p.Torso.Position, p.Torso)
		wait(0.5)
		for i=1, 3 do
			if p.Humanoid.Health&lt;1 then following=false; stopmoving() return end
			wait(0.5)
		end
	end
end


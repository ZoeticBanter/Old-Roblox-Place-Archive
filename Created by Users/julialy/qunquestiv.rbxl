local strike = false

function Do_Airstrike(mouse)
	if strike then return end
	if not mouse.Target then return end
	strike = true
	player.leaderstats.PP.Value = player.leaderstats.PP.Value - 10
	local exp = game.Lighting.Airstrike:clone()
	exp.CFrame = CFrame.new(mouse.Hit.p.x + math.random(-2, 2), 0.5, mouse.Hit.p.z + math.random(-2, 2))
	exp.Parent = game.Workspace
	for _, ob in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if (ob.Pos.Value - Vector3.new(exp.Position.x, 0, exp.Position.z)).magnitude &lt; 5 then
			if (ob.Health.Value - 5) &lt;= 0 then
				ob:remove()
				else
				ob.Health.Value = ob.Health.Value - 5
			end
		end
	end
	for i = 0, 3.14, 0.2 do 
		wait()
		local s = math.sin(i)
		exp.Mesh.Scale = Vector3.new(s*8, s*5, s*8)
	end
	exp:remove()
	Tool_Type = 0
	Tool_Mode = 1	
	strike = false
end

function Do_Heal(mouse)
	if not mouse.Target then return end
	if not mouse.Target:FindFirstChild(&quot;Health&quot;) then return end
	if mouse.Target.TypeId.Value:sub(1,4) ~= &quot;STRC&quot; then return end
	if mouse.Target.Team.Value ~= player.TeamColor then return end
	if mouse.Target.Health.Value == mouse.Target.MaxHealth.Value then return end
	--
	player.leaderstats.PP.Value = player.leaderstats.PP.Value - 5
	if mouse.Target.TypeId.Value:sub(6,-1) == &quot;Main&quot; then 
		if mouse.Target.Health.Value + 50 &gt; 100 then
			mouse.Target.Health.Value = 100
			else
			mouse.Target.Health.Value = mouse.Target.Health.Value + 50
		end
		else
		mouse.Target.Health.Value = mouse.Target.MaxHealth.Value
	end
	Tool_Type = 0
	Tool_Mode = 2
end


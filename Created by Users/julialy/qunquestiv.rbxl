			if TypeOf(mouse.Target) == &quot;STRC&quot; and ClassOf(mouse.Target) ~= &quot;Main&quot; then
				if mouse.Target.Team.Value == GetTeam() then
					mouse.Target:remove()
				end
			end
		end
		--==click and drag
		for i = 1, 3 do
			if not Mouse_Down then return end
			wait(0.1)
		end
		if mouse.Target and Tool_Type == 3 then
			Drag_State = 1
			Drag_Start = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)
			while Mouse_Down do
				wait()
				Show_Select.Parent = nil
				local disp = (Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z) - Drag_Start).magnitude

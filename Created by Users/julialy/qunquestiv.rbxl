				<ProtectedString name="Source">--[[
			===  Map_Main  ===
Main management of game objects, and players
]]


--== objects cached, to speed up power search
local Cache = {}
_G.Ch = Cache

--==optimization
local game = game

--== resources
function TypeOf(obj)
	if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
		return obj.TypeId.Value:sub(1, 4)
	end
end

function ClassOf(obj)
	if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
		return obj.TypeId.Value:sub(6, -1)
	end
end

--==player entered
game.Players.ChildAdded:connect(function(p)
	local l = Instance.new(&quot;IntValue&quot;)
	l.Name = &quot;leaderstats&quot;
	l.Parent = p
	local m = Instance.new(&quot;IntValue&quot;) --cost points
	m.Value = 0
	m.Name = &quot;CP&quot;
	m.Parent = l
	local r = Instance.new(&quot;IntValue&quot;) -- power points
	r.Name = &quot;PP&quot;
	r.Parent = l
	local u = Instance.new(&quot;IntValue&quot;) --number of units
	u.Name = &quot;NumUnits&quot;
	u.Parent = l
	local n = Instance.new(&quot;BoolValue&quot;) --ingame
	n.Name = &quot;InGame&quot;
	n.Parent = p
	n.Value = false

	for i = 1, 30 do --remove game tool when they enter, put it back through button when they join game
		wait(1)
		if p.Backpack:FindFirstChild(&quot;Game&quot;) then
			--p.Backpack.Game:remove()
			break
		end
	end
end)

--==player left
game.Players.ChildRemoved:connect(function(p)
	for _, o in pairs(game.Workspace.Conquest.Show:GetChildren()) do
		if o.creator.Parent == nil then
			o:remove()
		end
	end
	for _, o in pairs(game.Workspace.Conquest.Struct:GetChildren()) do	
		if o.Team.Value == p.TeamColor then
			o:remove()
		end
	end
	for _, o in pairs(game.Workspace.Conquest.Unit:GetChildren()) do	
		if o.Team.Value == p.TeamColor then
			o:remove()
		end
	end	
end)

--==cache an object
function CacheObject(ob)
	Cache[ob] = {}
	for _, o in ipairs(game.Workspace.Conquest.Connect:GetChildren()) do
		if o.EndA.Value == ob then
			Cache[ob][o.EndB.Value] = 1
			Cache[o.EndB.Value][ob] = 1
			elseif o.EndB.Value == ob then
			Cache[ob][o.EndA.Value] = 1
			Cache[o.EndA.Value][ob] = 1
		end
	end
end 

--==remove object from cache
function UncacheObject(de)
	Cache[de] = nil
	--remove references
	for _, tb_con in pairs(Cache) do
		tb_con[de] = nil
	end
end

--== update number of units for player ==--

function UpdateNumUnit()
	for _, p in pairs(game.Players:GetChildren()) do
		local num_u = 0
		for _, u in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
			if u.Team.Value == p.TeamColor then
				num_u = num_u + 1
			end
		end
		p.leaderstats.NumUnits.Value = num_u
	end
end

--===== function to check power ======

function PowerCheck()
	--pcall(function()
	--table of things to check, pylons to start
	local to_check = {}
	--add pylons to table, ans de-power others
	for _, ob in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if ob.TypeId.Value:sub(6, -1) == &quot;Pylon&quot; or ob.TypeId.Value:sub(6, -1) == &quot;Main&quot; then
			ob.Checked.Value = false
			table.insert(to_check, ob)
			else
			ob.Checked.Value = false
			ob.HasPower.Value = false
			ob.Transparency = 0.4
		end
	end
	--continue while there are objects in the que
	while to_check[1] do
		--localize
		local this_ob = to_check[1]
		--if it&apos;s been checked allready ignore it
		if not this_ob.Checked.Value then
			--give this object power
			this_ob.Transparency = 0
			this_ob.HasPower.Value = true
			this_ob.Checked.Value = true
			--if the object is in the cache yet
			if Cache[this_ob] then
				--yes, &quot;ob, _&quot; is right
				for to_ob, _ in pairs(Cache[this_ob]) do
					--if it doesn&apos;t have power add it to the que
					if not to_ob.HasPower.Value then
						to_check[#to_check + 1] = to_ob
					end
				end
			end
		end
		--remove this_ob from que
		table.remove(to_check, 1)
	end
	--end)
end

--start the thread
--coroutine.resume(coroutine.create(PowerCheck))

--======= thread to make turrets attack ======--

function ShowShot(pos1, pos2)
	local s = game.Lighting.Shot:clone()
	s.Mesh.Scale = Vector3.new(0.15, 0.15, (pos1 - pos2).magnitude)
	s.CFrame = CFrame.new((pos1+pos2)/2, pos1)
	s.Parent = game.Workspace.Conquest.Shot
end

function TurretAttack(o)
while true do
wait(1)
	if o.Parent == nil then return end
	if o.HasPower.Value then
		local trg = nil
		for _, un in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
			if un.Team.Value ~= o.Team.Value then
				local dist = (un.Position - o.Position).magnitude
				if dist &lt; 10 then
					trg = un
					break
				end
			end
		end
		if trg then
			o.CFrame = CFrame.new(o.Position, Vector3.new(trg.Position.x, o.Position.y, trg.Position.z)) * CFrame.fromEulerAnglesXYZ(0, math.pi/2, 0)
			trg.Health.Value = trg.Health.Value - 2
			ShowShot(o.Position, trg.Position)
			if trg.Health.Value &lt;= 0 then 
				trg.Target.Value = nil
				trg:remove()
			end
		end
	end
end
end	

--======= thread to move units =======--

function UnitAttack(o)
	--print(o, &quot;-&gt; Attack(&quot;, o.Target.Value, &quot;)&quot;)
	local cls = ClassOf(o)
	local t = o.Target.Value
	if cls == &quot;Trooper&quot; then
		ShowShot(o.Position, t.Position)
		o.NextAttack.Value = time() + 1
		t.Health.Value = t.Health.Value - 1
		if t.Health.Value &lt;= 0 then
			t:remove()
			o.Target.Value = nil
		end
		elseif cls == &quot;Engineer&quot; then
		o.NextAttack.Value = time() + 1
		if TypeOf(t) == &quot;STRC&quot; then
			t.Health.Value = t.Health.Value - 2
			else
			t.Health.Value = t.Health.Value - 1
		end
		if t.Health.Value &lt;= 0 then
			t:remove()
			o.Target.Value = nil
		end	
		elseif cls == &quot;Tank&quot; then
		ShowShot(o.Position, t.Position)
		o.NextAttack.Value = time() + 1.5
		t.Health.Value = t.Health.Value - 2
		if t.Health.Value &lt;= 0 then
			t:remove()
			o.Target.Value = nil
		end		
	end
end

--==unit thread
function UnitThread(o)
wait(0.1)
local IsTank = false
if ClassOf(o) == &quot;Tank&quot; then IsTank = true end
while true do
	if o.Parent == nil then return end
	--
	local attack = false
	local mov = false
	--
	if (o.Pos.Value - o.Dest.Value).magnitude &gt; 2 then
		mov = true
	end
	--check for death of target
	if o.Target.Value then
		if o.Target.Value.Parent == nil then
			o.Target.Value = nil
		end
	end
	--check attack nessesary
	if (o.Target.Value) then
		local disp = 100
		if TypeOf(o.Target.Value) == &quot;STRC&quot; then
			disp = (o.Target.Value.Position - o.Pos.Value).magnitude - o.Target.Value.Radius.Value
			else
			disp = (o.Target.Value.Position - o.Pos.Value).magnitude
		end
		if disp &gt; 2 then
			if IsTank then
				if disp &gt; 5 then
					o.Dest.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Target.Value.Pos.Value).lookVector * (disp - 5)
				end
				else
				o.Dest.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Target.Value.Pos.Value).lookVector * (disp)
			end
			mov = true
			if (disp &lt; 8 and IsTank) then
				attack = true
			end
			else
			attack = true
		end
	end
	for i = 1, 5 do
		wait(0.15)
		if o.Parent == nil then return end
		--==move
		if mov then
			o.Pos.Value = o.Pos.Value + CFrame.new(o.Pos.Value, o.Dest.Value).lookVector * 0.6
			o.CFrame = CFrame.new(o.Pos.Value, o.Dest.Value) + Vector3.new(0, 0.7, 0)
			if (o.Pos.Value - o.Dest.Value).magnitude &lt; 0.6 then
				mov = false
			end
		end
		--==attack
		if attack then
			local t = o.Target.Value
			if (t or game):FindFirstChild(&quot;TypeId&quot;) ~= nil then
				local disp = 100
				--
				if TypeOf(t) == &quot;STRC&quot; then
					disp = (o.Pos.Value - t.Pos.Value).magnitude - t.Radius.Value
					elseif TypeOf(t) == &quot;UNIT&quot; then
					disp = (o.Pos.Value - t.Pos.Value).magnitude
				end
				if disp &lt; 2.5 or (IsTank and disp &lt; 8) then
					--attack
					if time() &gt; o.NextAttack.Value then
						UnitAttack(o)
					end
				end
			end
		end
	end
end
end
		
game.Workspace.Conquest.Unit.ChildAdded:connect(function(de)
	print(de, &quot;-&gt; UnitAdded()&quot;)
	delay(0.2, UpdateNumUnit)
	UnitThread(de)
end)		

game.Workspace.Conquest.Unit.ChildRemoved:connect(function(de)
	wait(0.2)
	UpdateNumUnit()
end)		

--== object added, cache it
game.Workspace.Conquest.Struct.ChildAdded:connect(function(de)
	--wait for replications
	wait()
	--if it&apos;s not an object return
	if not de:FindFirstChild(&quot;TypeId&quot;) then return end
	print(de, &quot;--&gt; Added()&quot;)
	wait(0.2)
	--add to cache
	CacheObject(de)
	wait()
	PowerCheck()
	--if it&apos;s a turret run turret thread for it
	if de.TypeId.Value ~= &quot;STRC_Turret&quot; then return end
	print(de, &quot;--&gt; TurretAdded()&quot;)
	TurretAttack(de)
end)

--==object rmoved, clean up connector parts and remove from cache
game.Workspace.Conquest.Struct.ChildRemoved:connect(function(de)
	print(de, &quot;--&gt; Cleanup()&quot;)
	--remove connector parts
	for _, cn in pairs(game.Workspace.Conquest.Connect:GetChildren()) do
		if cn.EndA.Value == de or cn.EndB.Value == de then
			cn:remove()
		end
	end
	--uncache object
	UncacheObject(de)
	wait()
	PowerCheck()
end)

--== remove and fade shots
game.Workspace.Conquest.Shot.ChildAdded:connect(function(s)
	wait(0.1)
	s.Transparency = 0.2
	wait(0.1)
	s.Transparency = 0.4
	wait(0.1)
	s.Transparency = 0.6
	wait(0.1)
	s.Transparency = 0.8
	wait(0.1)
	s:Remove()
end)















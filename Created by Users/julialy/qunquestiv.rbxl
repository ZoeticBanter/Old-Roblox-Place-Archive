			end
			Show_Select.Parent = nil
			for _, un in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
				if un.Team.Value == GetTeam() then
					local disp = (Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z) - Drag_Start).magnitude
					if (un.Pos.Value - Drag_Start).magnitude &lt; disp then
						local selected = false
						for _, sel in pairs(Selection) do
							if sel == un then
								selected = true
								break
							end
						end
						if not selected then
							table.insert(Selection, un)
						end
					end
				end
			end
		end
	end)
	mouse.Button1Up:connect(function()
		Mouse_Down = false
	end)
	mouse.Move:connect(function()
		Show_Rad.Parent = nil
		Show_Ring.Parent = nil
		Show_Stat.Parent = nil
		if game.Workspace.Game_State.Value == 0 then
			mouse.Icon = &quot;http://www.roblox.com/asset/?id=9701910&quot;
			return
			elseif GetPlayer().InGame.Value == false then
			mouse.Icon = &quot;http://www.roblox.com/asset/?id=9701912&quot;
			return
		end	
		SelectCursor(mouse)
		if Tool_Type == 1 then
			ShowBuild(mouse)
			elseif Tool_Type == 2 then
			ShowRecruit(mouse)
		end
		if Tool_Type ~= 0 then
			Show_StatModel(mouse)
		end
	end)
	mouse.Idle:connect(function()
		Show_Rad.Parent = nil
		Show_Ring.Parent = nil
		Show_Stat.Parent = nil
		if game.Workspace.Game_State.Value == 0 then
			mouse.Icon = &quot;http://www.roblox.com/asset/?id=9701910&quot;
			return
			elseif GetPlayer().InGame.Value == false then
			mouse.Icon = &quot;http://www.roblox.com/asset/?id=9701912&quot;
			return
		end
		SelectCursor(mouse)
		if Tool_Type == 1 then
			ShowBuild(mouse)
			elseif Tool_Type == 2 then
			ShowRecruit(mouse)
		end
		if Tool_Type ~= 0 then
			Show_StatModel(mouse)
		end
	end)
	mouse.KeyDown:connect(function(k)
		if k == &quot;q&quot; then --cycle left
			if Tool_Type == 0 or Tool_Type == 1 then
				Tool_Mode = ShiftDown(Tool_Mode, 3)
				elseif Tool_Type == 2 then
				Tool_Mode = ShiftDown(Tool_Mode, 2)
			end
			elseif k == &quot;e&quot; then --cycle right
			if Tool_Type == 0 or Tool_Type == 1 then
				Tool_Mode = ShiftUp(Tool_Mode, 3)
				elseif Tool_Type == 2 then
				Tool_Mode = ShiftUp(Tool_Mode, 2)
			end
			elseif string.byte(k) == 27 then --escape current tool mode	
			--==clear modes and selection
			Show_Select.Parent = nil
			Tool_Type = 0
			Tool_Mode = 0
			Selection = {}
		end
	end)
end)


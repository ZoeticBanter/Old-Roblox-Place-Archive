function ShowRecruit(mouse)
	--== set cost for current building
	if Tool_Mode == 0 then --build trooper
		Needed_Money = 1
		elseif Tool_Mode == 1 then -- build tank
		Needed_Money = 3
		elseif Tool_Mode == 2 then -- build engineer
		Needed_Money = 2
	end	
	--== set icon
	if Tool_Mode == 0 or Tool_Mode == 2 then
		if ClassOf(mouse.Target) == &quot;Barracks&quot; then
			if mouse.Target.Team.Value == GetTeam() and mouse.Target.HasPower.Value then
				mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549494&quot;
				else
				mouse.Icon = (Recruit_Cursor[Tool_Mode] or &quot;http://www.roblox.com/asset/?id=9549492&quot;)
			end
			else
			mouse.Icon = (Recruit_Cursor[Tool_Mode] or &quot;http://www.roblox.com/asset/?id=9549492&quot;)
		end
		elseif Tool_Mode == 1 then
		if ClassOf(mouse.Target) == &quot;Factory&quot; then
			if mouse.Target.Team.Value == GetTeam() and mouse.Target.HasPower.Value then
				mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549494&quot;
				else
				mouse.Icon = (Recruit_Cursor[Tool_Mode] or &quot;http://www.roblox.com/asset/?id=9549492&quot;)
			end
			else
			mouse.Icon = (Recruit_Cursor[Tool_Mode] or &quot;http://www.roblox.com/asset/?id=9549492&quot;)
		end
	end
	--== return state
	if GetStat(&quot;PP&quot;) &gt;= Needed_Money then
		return true
		else
		mouse.Icon = (Recruit_Cursor[Tool_Mode] or &quot;http://www.roblox.com/asset/?id=9549492&quot;)
		return false
	end
end

function TryBuild(mouse)
	Show_Rad.Parent = nil
	Show_Ring.Parent = nil
	--check if can build
	local test = ShowBuild(mouse)
	if not test then return end
	--do building
	--==subtract money
	local mn = GetStat(&quot;CP&quot;)
	SetStat(&quot;CP&quot;, GetStat(&quot;CP&quot;) - Needed_Money)
	wait()
	if GetStat(&quot;CP&quot;) == mn or GetStat(&quot;CP&quot;) &gt; 40 then
		local m = Instance.new(&quot;Message&quot;)
		m.Text = script.Parent.Parent.Parent.Name..&quot; is attempting to hack, he/she is being removed from the game&quot;
		m.Parent = game.Workspace
		wait(2)
		m:remove()
		GetPlayer():remove()
		return
	end
	--==place building
	if Tool_Mode == 0 then --pylon
		local b = game.Lighting.Struct.Pylon:clone()
		b.Team.Value = GetTeam()
		b.BrickColor = GetTeam()
		b.Pos.Value = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)
		b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
		b.Parent = game.Workspace.Conquest.Struct
		CreateConnect(b)
	elseif Tool_Mode == 1 then --factory
		local b = game.Lighting.Struct.Factory:clone()
		b.Team.Value = GetTeam()
		b.BrickColor = GetTeam()
		b.Pos.Value = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)
		b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
		b.Parent = game.Workspace.Conquest.Struct
		CreateConnect(b)
	elseif Tool_Mode == 2 then --barracks
		local b = game.Lighting.Struct.Barracks:clone()
		b.Team.Value = GetTeam()
		b.BrickColor = GetTeam()
		b.Pos.Value = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)
		b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
		b.Parent = game.Workspace.Conquest.Struct
		CreateConnect(b)
	elseif Tool_Mode == 3 then --turret
		local b = game.Lighting.Struct.Turret:clone()
		b.Team.Value = GetTeam()
		b.BrickColor = GetTeam()
		b.Pos.Value = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)
		b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
		b.Parent = game.Workspace.Conquest.Struct
		CreateConnect(b)
	end
end

function ShowBuild(mouse)
	local CanBuild = true
	--== set radius for current building and cost for current building
	if Tool_Mode == 0 then --build pylon
		Needed_Rad = 2 
		Needed_Money = 2
		elseif Tool_Mode == 1 then -- build factory
		Needed_Rad = 4
		Needed_Money = 4
		elseif Tool_Mode == 2 then -- build barracks
		Needed_Rad = 3
		Needed_Money = 3
		elseif Tool_Mode == 3 then -- build turret
		Needed_Rad = 1 
		Needed_Money = 1
	end
	Show_Rad.Mesh.Scale = Vector3.new(Needed_Rad*2, 0.2, Needed_Rad*2)
	--== check radius and money
	if GetStat(&quot;CP&quot;) &lt; Needed_Money then CanBuild = false end
	if not CheckRoomFor(Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z), Needed_Rad) then CanBuild = false end
	--== check for other building nearby
	if NumMyBuildings() &gt; 0 then
		if not CheckBuildNearby(Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z)) then CanBuild = false end
	end
	--== set pad color
	if CanBuild then
		Show_Rad.BrickColor = BrickColor.new(Color3.new(0, 255, 0))
		else
		Show_Rad.BrickColor = BrickColor.new(21)
	end
	--== put Show_Rad in workspace
	Show_Rad.CFrame = CFrame.new(mouse.Hit.p)
	Show_Ring.CFrame = CFrame.new(mouse.Hit.p)
	Show_Rad.Parent = game.Workspace
	Show_Ring.Parent = game.Workspace
	return CanBuild
end

script.Parent.Selected:connect(function(mouse)
	if GetPlayer().Character then
		GetPlayer().Character:Remove()

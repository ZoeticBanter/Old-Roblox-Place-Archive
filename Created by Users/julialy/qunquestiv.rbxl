function IsSelected(obj)
	for _, o in pairs(Selection) do
		if o == obj then return true end
	end
	return false
end

function SelectCursor(mouse)
	if Tool_Type == 0 then --- select tool type
		mouse.Icon = (Mode_Cursor[Tool_Mode] or &quot;&quot;)
		elseif Tool_Type == 1 then -- build
		mouse.Icon = (Build_Cursor[Tool_Mode] or &quot;&quot;)
		elseif Tool_Type == 2 then -- recriut
		--TODO, unit menu
		mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549492&quot;
		elseif Tool_Type == 3 then --command
		mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549492&quot;
		if #Selection &gt; 0 then
			if mouse.Target == game.Workspace.Game_Base then
				mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549485&quot;
			end
			if TypeOf(mouse.Target) == &quot;UNIT&quot; then
				if mouse.Target.Team.Value == GetTeam() then
					if not IsSelected(mouse.Target) then
						mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549494&quot;
					end
					else
					mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549481&quot;
				end
				elseif TypeOf(mouse.Target) == &quot;STRC&quot; then
				if mouse.Target.Team.Value ~= GetTeam() then
					mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549481&quot;
				end
			end
		end
		elseif Tool_Type == 4 then --remove
		mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549492&quot;
		if TypeOf(mouse.Target) == &quot;STRC&quot; or TypeOf(mouse.Target) == &quot;UNIT&quot; then
			if mouse.Target.Team.Value == GetTeam() then
				mouse.Icon = &quot;http://www.roblox.com/asset/?id=9549494&quot;
			end
		end
	end
end


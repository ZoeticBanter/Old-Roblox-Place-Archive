				<ProtectedString name="Source">--                  === GAME_MAIN ===
--runs the game cycle and other related cycles


--==cache game
local game = game

--== votes
local vote_on = false
Votes = {}

--== do-vote function
function DoVote()
	if game.Workspace.Game_State.Value ~= 0 then return end --game going
	if vote_on == true then return end--voting in progress
	--setup
	Votes = {}
	vote_on = true
	--add messages
	for _, pl in pairs(game.Players:GetChildren()) do
		local h = Instance.new(&quot;Message&quot;)
		h.Name = &quot;Vote&quot;
		h.Parent = pl
	end
	--countdown
	for nm = 20, 1, -1 do
		for _, pl in pairs(game.Players:GetChildren()) do
			if not pl:FindFirstChild(&quot;Vote&quot;) then
				local h = Instance.new(&quot;Message&quot;)
				h.Name = &quot;Vote&quot;
				h.Parent = pl		
			end
			if Votes[pl] == nil then
				pl.Vote.Text = nm..&quot; Say \&quot;yes\&quot; to start,\&quot;no\&quot; to wait for more players&quot;
				else
				pl.Vote.Text = nm..&quot; [ You have allready voted ] &quot;
			end
		end
		all_vote = true
		for _, p in pairs(game.Players:GetChildren()) do
			if Votes[p] == nil then all_vote = false end
		end
		if all_vote then break end
		wait(1)
	end
	--tally votes
	local vote_for = 0
	local vote_against = 0
	for _, p in pairs(game.Players:GetChildren()) do
		if Votes[p] == true then
			vote_for = vote_for + 1
			elseif Votes[p] == false then
			vote_against = vote_against + 1
		end
	end
	--find result
	if vote_for &gt; vote_against then
		for _, p in pairs(game.Players:GetChildren()) do
			--remove message
			if p:FindFirstChild(&quot;Vote&quot;) then
				p.Vote:remove()
			end
			--put in game
			p.InGame.Value = true
		end
		game.Workspace.Game_State.Value = 1 --build mode
		--declare game start
		local m = Instance.new(&quot;Message&quot;)
		m.Text = &quot;Vote to start game was &quot;..vote_for..&quot; for VS. &quot;..vote_against..&quot; against&quot;
		m.Parent = game.Workspace
		wait(5)
		m.Text = &quot;Game starting now&quot;
		wait(5)
		m:remove()
	else
		for _, p in pairs(game.Players:GetChildren()) do
			--remove message
			if p:FindFirstChild(&quot;Vote&quot;) then
				p.Vote:remove()
			end
		end
		local m = Instance.new(&quot;Message&quot;)
		m.Text = &quot;Vote to start game was &quot;..vote_for..&quot; for VS. &quot;..vote_against..&quot; against&quot;
		m.Parent = game.Workspace
		wait(5)
		m:remove()
	end
	vote_on = false
end

--==chat main
game.Players.ChildAdded:connect(function(p)
	--print(p, &quot;-&gt;Enter()&quot;)
	p.Chatted:connect(function(msg, res)
		--print(p, &quot;-&gt;Chat(&quot;, msg, &quot;)&quot;)
		if msg:lower() == &quot;start&quot; or msg:lower() == &quot;startgame&quot; then
			--print(&quot;DoVote()&quot;)
			DoVote()
			elseif msg:lower() == &quot;yes&quot; or msg:lower() == &quot;y&quot; then
			if vote_on and Votes[p] == nil then
				Votes[p] = true
			end
			elseif msg:lower() == &quot;no&quot; or msg:lower() == &quot;n&quot; then
			if vote_on and Votes[p] == nil then
				Votes[p] = false
			end
		end
	end)
end)

--== player eliminated
function TeamDeath(team)
	--remove structures
	for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if st.Team.Value == team then st:remove() end
	end
	--remove units
	for _, st in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
		if st.Team.Value == team then st:remove() end
	end
	--out of game
	for _, p in pairs(game.Players:GetChildren()) do
		if p.TeamColor == team then
			p.InGame.Value = false
		end
	end
end

--== check if a player has lost
function CheckLoss(player)
	--tally buildings
	local num_strc = 0
	for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if st.Team.Value == player.TeamColor then
			num_strc = num_strc + 1
		end
	end
	--return val
	if num_strc == 0 then
		return true
		else
		return false
	end
end

--== give PP to player
function GivePP(player)
	--tally pylons
	local num_py = 0
	for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if st.Team.Value == player.TeamColor then
			if st.TypeId.Value:sub(6, -1) == &quot;Pylon&quot; then
				num_py = num_py + 1
			end
		end
	end
	--give pp
	player.leaderstats.PP.Value = player.leaderstats.PP.Value + math.ceil(num_py/2)
	return
end

--== calculate for players
function DoPlayers()
	for _, p in pairs(game.Players:GetChildren()) do
		if p.InGame.Value then
			GivePP(p)
			if CheckLoss(p) then 
				TeamDeath(p.TeamColor)
			end
		end
	end
end

--== how many people are left ingame
function Winner()
	local ingame = {}
	for _, p in pairs(game.Players:GetChildren()) do
		if p.InGame.Value then
			table.insert(ingame, p)
		end
	end
	if #ingame == 1 then
		return ingame[1]
	end
	return nil
end

--== game empty
function GameIsEmpty()
	local n_ingame = 0
	for _, p in pairs(game.Players:GetChildren()) do
		if p.InGame.Value then
			n_ingame = n_ingame + 1
		end
	end
	if n_ingame == 0 then
		return true
	end
	return false
end

local StartPos = {
	Vector3.new(60, 0, 60),
	Vector3.new(60, 0, -60),
	Vector3.new(-60, 0, -60),
	Vector3.new(-60, 0, 60),
	Vector3.new(0, 0, 35),
	Vector3.new(0, 0, -35)
}

--== main loop, to check for a win / switch building VS game on
function DoGame()
	--give players CP and start building
	for inx, p in pairs(game.Players:GetChildren()) do
		if StartPos[inx] then
			p.leaderstats.CP.Value = 40
			p.leaderstats.PP.Value = 0
			--put in main building
			local b = game.Lighting.Struct.Main:clone()
			b.Team.Value = p.TeamColor
			b.BrickColor = p.TeamColor
			b.Pos.Value = StartPos[inx]
			b.CFrame = CFrame.new(b.Pos.Value + Vector3.new(0, 0.9, 0)) * CFrame.fromEulerAnglesXYZ(0, math.random(0, math.pi*10)/5, 0)
			b.Parent = game.Workspace.Conquest.Struct
		end
	end
	--first, count down to building mode off
	local h = Instance.new(&quot;Hint&quot;)
	h.Parent = game.Workspace
	for tm = 120, 1, -1 do
		if GameIsEmpty() then
			h:remove()
			local m = Instance.new(&quot;Message&quot;)
			m.Text = &quot;No players left ingame, ending the game&quot;
			m.Parent = game.Workspace
			wait(5)
			m:remove()
			return
		--[[elseif Winner() then
			h:remove()
			local m = Instance.new(&quot;Message&quot;)
			m.Text = &quot;Only one player left ingame, ending the game&quot;
			m.Parent = game.Workspace
			wait(5)
			m:remove()
			return]]		
		end
		if tostring(tm-math.floor(tm/60)*60):len() &lt; 2 then
			h.Text = &quot;Building mode : Build your city (&quot;..math.floor(tm/60)..&quot;:&quot;..(tm-math.floor(tm/60)*60)..&quot;0 left until battle starts)&quot;
			else
			h.Text = &quot;Building mode : Build your city (&quot;..math.floor(tm/60)..&quot;:&quot;..(tm-math.floor(tm/60)*60)..&quot; left until battle starts)&quot;
		end
		wait(1)
	end
	--change mode to battle
	h.Text = &quot;Battle Mode : Make units and attack!&quot;
	--while true do, check for a win and give players PP
	while true do
		wait(15)
		DoPlayers()
		local w = Winner()
		if w then
			TeamDeath(w.TeamColor)
			h:remove()
			local m = Instance.new(&quot;Message&quot;)
			m.Text = w.Name..&quot; has won the game!&quot;
			m.Parent = game.Workspace
			wait(5)
			m:remove()
			return
		end
		if GameIsEmpty() then
			h:remove()
			local m = Instance.new(&quot;Message&quot;)
			m.Text = &quot;No players left ingame, ending the game&quot;
			m.Parent = game.Workspace
			wait(5)
			m:remove()
			return
		end
	end
end

game.Workspace.Game_State.Changed:connect(function()
	if game.Workspace.Game_State.Value == 1 then
		DoGame()
		--end game
		game.Workspace.Game_State.Value = 0
		for _, p in pairs(game.Players:GetChildren()) do
			p.leaderstats.CP.Value = 0
			p.leaderstats.PP.Value = 0
			p.InGame.Value = false
		end
		--remove structures
		for _, st in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
			st:remove()
		end
		--remove units
		for _, st in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
			st:remove()
		end
		--clear cache
		for inx, _ in pairs(Ch) do
			Ch[inx] = nil
		end
	end
end)

function _G.CMD_Start()
	game.Players:GetChildren()[1].InGame.Value = true
	game.Workspace.Game_State.Value = 1
end





















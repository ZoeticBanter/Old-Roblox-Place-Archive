function DoCommand(mouse)
	--clean out the selection
	local s = {}
	for _, o in pairs(Selection) do
		if o:FindFirstChild(&quot;Health&quot;) then
			if o.Health.Value &gt; 0 then
				table.insert(s, o)
			end
		end
	end
	Selection = s
	--gaurds
	if mouse.Target == nil then return end
	if not mouse.Target:FindFirstChild(&quot;TypeId&quot;) then
		print(&quot;no type&quot;)
		if mouse.Target == game.Workspace.Game_Base then
			print(&quot;Move selection&quot;)
			for _, o in pairs(Selection) do --selection move
				o.Target.Value = nil
				o.Dest.Value = Vector3.new(mouse.Hit.p.x, 0, mouse.Hit.p.z) + Vector3.new(math.random(-math.sqrt(#Selection-1), math.sqrt(#Selection-1)), 0, math.random(-math.sqrt(#Selection-1), math.sqrt(#Selection-1)))
			end		
		end
		return 
	end
	--prep values
	local target = mouse.Target
	local num_select = #Selection
	local target_select = false
	for _, o in pairs(Selection) do
		if o == target.Parent then target_select = true; break; end
	end
	--do commands
	if TypeOf(target) == &quot;UNIT&quot; then
		if target.Team.Value == GetTeam() then
			if not target_select then --select unit
				print(&quot;Select unit&quot;)
				table.insert(Selection, target)
			end
			else
			print(&quot;Attack!!!&quot;)
			AttackTarget(target, Selection)
		end
		elseif TypeOf(target) == &quot;STRC&quot; then
		if target.Team.Value ~= GetTeam() then
			for _, o in pairs(Selection) do --selection attack struct
				o.Target.Value = target
			end		
		end
	end
end


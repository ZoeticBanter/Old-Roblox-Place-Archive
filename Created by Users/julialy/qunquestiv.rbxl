					<ProtectedString name="Source">local game = game

function _G.GetPlayer()
	return script.Parent.Parent.Parent
end

function _G.GetTeam()
	return GetPlayer().TeamColor
end

function _G.TypeOf(obj)
	if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
		return obj.TypeId.Value:sub(1, 4)
	end
end

function _G.ClassOf(obj)
	if (obj or game):FindFirstChild(&quot;TypeId&quot;) then
		return obj.TypeId.Value:sub(6, -1)
	end
end

function _G.IsTeam(object)
	if not object then return false end
	local t = object:FindFirstChild(&quot;Team&quot;)
	if t then
		if t.Value == GetTeam() then
			return true
		end
	end
	return false
end

function _G.ShiftDown(num, max)
	num = num - 1
	if num &lt; 0 then
		return max
		else
		return num
	end
end

function _G.ShiftUp(num, max)
	num = num + 1
	if num &gt; max then
		return 0
		else
		return num
	end
end

function _G.GetStat(name)
	local lead = GetPlayer():FindFirstChild(&quot;leaderstats&quot;)
	if lead then
		local st = lead:FindFirstChild(name)
		if st then
			return st.Value
		end
	end
end

function _G.SetStat(name, val)
	local lead = GetPlayer():FindFirstChild(&quot;leaderstats&quot;)
	if lead then
		local st = lead:FindFirstChild(name)
		if st then
			st.Value = val
		end
	end
end

function MakeConnect(obj1, obj2)
	local c = game.Lighting.Connect:clone()
	c.EndA.Value = obj1
	c.EndB.Value = obj2
	c.CFrame = CFrame.new((obj1.Position + obj2.Position)/2, obj1.Position)
	c.Mesh.Scale = Vector3.new(0.1, 0.1, (obj1.Position - obj2.Position).magnitude)
	c.Parent = game.Workspace.Conquest.Connect
end

function _G.CreateConnect(obj1)
	for _, o in ipairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if o.Team.Value == obj1.Team.Value then
			if (o.Pos.Value - obj1.Pos.Value).magnitude &lt; 10 then
				MakeConnect(obj1, o)
			end
		end
	end
end

function _G.CheckBuildNearby(pos)
	for _, o in ipairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if (o.Pos.Value - pos).magnitude &lt; 10 then
			if o.Team.Value == GetTeam() then
				return true
			end
		end
	end
	return false
end

function _G.CheckRoomFor(pos, rad)
	for _, o in pairs(game.Workspace.Conquest.Struct:GetChildren()) do
		local min_dist = o.Radius.Value + rad
		if (o.Pos.Value - pos).magnitude &lt; min_dist then
			return false
		end
	end
	for _, o in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
		if (o.Pos.Value - pos).magnitude &lt; rad + 1 then
			return false
		end
	end
	return true
end

function _G.NumMyBuildings()
	local num_b = 0
	for _, o in ipairs(game.Workspace.Conquest.Struct:GetChildren()) do
		if o.Team.Value == GetTeam() then
			num_b = num_b + 1
		end
	end
	return num_b
end

function _G.NumMyUnits()
	local num_b = 0
	for _, o in ipairs(game.Workspace.Conquest.Unit:GetChildren()) do
		if o.Team.Value == GetTeam() then
			num_b = num_b + 1
		end
	end
	return num_b
end

function _G.AttackTarget(target, selection)
	--get mod table
	local mod_select = {}
	for _, o in pairs(selection) do
		if o:FindFirstChild(&quot;Health&quot;) then
			if o.Health.Value &gt; 0 then
				table.insert(mod_select, o)
			end
		end
	end
	--get table of enemy targets nearby
	local near = {}
	for _, e in pairs(game.Workspace.Conquest.Unit:GetChildren()) do
		if e.Team.Value == target.Team.Value then
			if (e.Pos.Value - target.Pos.Value).magnitude &lt; math.sqrt(#mod_select) then
				table.insert(near, e)
			end
		end
	end
	--assign targets
	for inx, o in pairs(mod_select) do
		if near[inx] then
			o.Target.Value = near[inx]
			if (o.Pos.Value - near[inx].Pos.Value).magnitude &lt; 5 and near[inx].Target.Value == nil then
				near[inx].Target.Value = o
			end
			else
			o.Target.Value = near[math.random(1, #near)]
		end
	end
end













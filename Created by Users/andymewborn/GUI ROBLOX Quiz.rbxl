function Won()
wFrame.Visible = true
game:GetService(&quot;BadgeService&quot;):AwardBadge(player.userId, 31636382)
end

function Start()
screen.Visible = true
for i = 1, 0, -0.05 do
wait()
screen.Transparency = i
end
screen.Transparency = 0
wait(1)
sFrame.Visible = false
qFrame.Visible = true
if q == maxQ+1 then
Won()
end
nextQuestion()
wait(1)
for i = 0, 1, 0.05 do
wait()
screen.Transparency = i
end
screen.Transparency = 1
screen.Visible = false
end

okButton.MouseButton1Down:connect(function()
if q == 2 then
if answerBox.Text == &quot;builderman&quot; or answerBox.Text == &quot;erik.cassel&quot; then
Start()
return
end
end
if q == 6 then
if answerBox.Text == &quot;50000&quot; or answerBox.Text == &quot;50,000&quot; then
Start()
return
end
end
if q == 9 then
if answerBox.Text == &quot;26000&quot; or answerBox.Text == &quot;26,000&quot; then
Start()
return
end
end
if answerBox.Text == correctAnswer then
Start()
else
iFrame.Visible = true
end
end)


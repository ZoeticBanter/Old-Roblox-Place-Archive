					<string name="Source">--jediknightkrazy
bin = script.Parent
local msg
local updateMessage = true

function onButton1Down(mouse)
	local player = game.Players.LocalPlayer
	if player == nil then return end
	if (mouse.Target ~= nil and mouse.Target.Name == &quot;ToolCase&quot;) then
		local tool = mouse.Target
		if (mouse.Target ~= nil) then
			state = GetWeaponState(tool, player)
			if (state == &quot;Unowned&quot;) then
				local money = player.leaderstats:FindFirstChild(&quot;Points&quot;)
				money.Value = money.Value - tool.Cost.Value
				wepCat = player.Unlocks:FindFirstChild(tool.Type.Value)
				game.Lighting:FindFirstChild(tool.Type.Value):FindFirstChild(tool.ToolName.Value):Clone().Parent = wepCat
				wepCat.Value = tool.ToolName.Value
				msg.Parent = player
				msg.Text = &quot;Purchased &quot; .. tool.FriendlyName.Value .. &quot;!&quot;
				updateMessage = false
				delay(3, function() updateMessage = true
					msg.Parent = nil
				end)
			elseif (state == &quot;Owned&quot;) then
				wepCat = player.Unlocks:FindFirstChild(tool.Type.Value)
				wepCat.Value = tool.ToolName.Value
				msg.Parent = player
				msg.Text = &quot;Equipped &quot; .. tool.FriendlyName.Value
				updateMessage = false
				delay(3, function() updateMessage = true
					msg.Parent = nil
					end)
			end
		end
	end
	mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
end

function GetWeaponState(tool, player)
	local type = tool.Type.Value
	local name = tool.ToolName.Value
	local unlocks = player:FindFirstChild(&quot;Unlocks&quot;)
	if (unlocks ~= nil) then
		local wepCat = unlocks:FindFirstChild(type)
		if (wepCat ~= nil) then
			if (wepCat.Value == name) then
				return &quot;Equipped&quot;
			elseif (wepCat:FindFirstChild(name) ~= nil) then
				return &quot;Owned&quot;
			else
				local money = player.leaderstats:FindFirstChild(&quot;Points&quot;)
				local cost = tool.Cost.Value
				if (money ~= nil and money.Value &gt;= cost) then
					return &quot;Unowned&quot;
				else
					return &quot;CantAfford&quot;
				end
			end
		end
	end
end

function onDeselected()
	msg.Parent = nil
end

function onMouseMove(mouse)
	if (updateMessage ~= true) then return end
	local player = game.Players.LocalPlayer
	if player == nil then return end
	if (mouse.Target == nil or mouse.Target.Name ~= &quot;ToolCase&quot;) then
		msg.Parent = nil
	else
		local tool = mouse.Target
		if (tool ~= nil) then
			local name = tool.FriendlyName.Value
			local state = GetWeaponState(tool, player)
			if (state == &quot;Equipped&quot;) then
				msg.Text = name .. &quot; is already equipped&quot;
			elseif (state == &quot;Owned&quot;) then
				msg.Text = &quot;Equip &quot; .. name
			elseif (state == &quot;Unowned&quot;) then
				msg.Text = &quot;Buy &quot; .. name .. &quot; for &quot; .. tool.Cost.Value
			else 
				msg.Text = &quot;You can&apos;t afford &quot; .. name
			end
			msg.Parent = player
		end
	end
end

function onSelected(mouse)
	mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	mouse.Move:connect(function() onMouseMove(mouse) end)
	msg = Instance.new(&quot;Message&quot;)
	msg.Name = &quot;ShopMessage&quot;
end

bin.Selected:connect(onSelected)
bin.Deselected:connect(onDeselected)</string>

function GetStat(player)
	if (player == nil) then return nil end
	--If it&apos;s a character, try to get the player
	if(player.className == &quot;Model&quot;) then
		realPlayer = game.Players:GetPlayerFromCharacter(player)
		if (realPlayer ~= nil) then return GetStat(realPlayer) end
	elseif (player:FindFirstChild(&quot;leaderstats&quot;) ~= nil and
		player.leaderstats:FindFirstChild(Currency) ~= nil) then
		return player.leaderstats[Currency]
	end
end

public.GetMoney = function(player)
	stat = GetStat(player)
	if (stat ~= nil) then
		return stat.Value
	end
end

public.SetMoney = function(player, money)
	stat = GetStat(player)
	if (stat ~= nil) then
		stat.Value = money
	end
end

public.ChangeMoney = function(player, money)
	stat = GetStat(player)
	if (stat ~= nil) then
		stat.Value = stat.Value + money
	end
end


				<string name="Source">function bountyMgr() return _G[game.Workspace.BountyManager] end

function onPlayerRespawned(newPlayer)
	game.Workspace.CleanMessages:Clone().Parent = newPlayer.Character
	--wait(0.5)
	--bountyMgr().AddPlayer(newPlayer.Character)
end

function onPlayerEntered(newPlayer)
	newPlayer.Changed:connect(function (property)
		if (property == &quot;Character&quot;) then
			onPlayerRespawned(newPlayer)
		end
	end)
end

game.Players.PlayerAdded:connect(onPlayerEntered)

				<string name="Source">_G[script] = {}
public = _G[script]

local CATEGORIES = {
	&quot;Weapon&quot;,
	&quot;Defense&quot;,
	&quot;Active&quot;,
	&quot;Passive&quot;
}

function onPlayerRespawned(newPlayer)
	--wait(0.5)
	--bountyMgr().AddPlayer(newPlayer.Character)
end

function removePreviews(backpack)
	local c = backpack:GetChildren()
	for i, j in ipairs(c) do
		if (j:FindFirstChild(&quot;IsDummy&quot;)) then
			j:Remove()
		end
	end
end

public.MakePreviews = function(player)
	local bp = player.Backpack
	removePreviews(bp)
	c = player:FindFirstChild(&quot;Unlocks&quot;)
	if (c ~= nil) then
		c = c:GetChildren()
		for i, j in ipairs(c) do
			name = j.Value
			wep = j:FindFirstChild(name)
			if (wep ~= nil) then
				hb = Instance.new(&quot;HopperBin&quot;)
				hb.Name = wep.Name
				hb.TextureId = wep.TextureId
				id = script.IsDummy:Clone()
				id.Disabled = false
				id.Parent = hb
				hb.Parent = bp
			end
		end
	end
end

function onPlayerEntered(newPlayer)
	local unlocks = Instance.new(&quot;IntValue&quot;)
	unlocks.Name = &quot;Unlocks&quot;
	for i, j in ipairs(CATEGORIES) do
		local col = Instance.new(&quot;StringValue&quot;)
		col.Name = j
		col.Parent = unlocks
		col.Changed:connect(function()
			public.MakePreviews(newPlayer)
		end)
	end
	unlocks.Parent = newPlayer
end

public.GiveUnlocks = function(player)
	local backpack = player.Backpack
	removePreviews(backpack)
	if (backpack:FindFirstChild(&quot;Equip&quot;) ~= nil) then
		backpack:FindFirstChild(&quot;Equip&quot;):Remove()
	end
	c = player:FindFirstChild(&quot;Unlocks&quot;)
	if (c ~= nil) then
		c = c:GetChildren()
		for i, j in ipairs(c) do
			name = j.Value
			wep = j:FindFirstChild(name)
			if (wep ~= nil) then
				wep:Clone().Parent = backpack
			end
		end
	end
end

game.Players.PlayerAdded:connect(onPlayerEntered)

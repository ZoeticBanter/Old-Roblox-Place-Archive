		<Item class="Script" referent="RBX13">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">BountyManager</string>
				<string name="Source">_G[script] = {}
public = _G[script]

botMgr = function() return _G[game.Workspace.BotManager] end
msgMgr = function() return _G[game.Workspace.MessageManager] end
moneyMgr = function() return _G[game.Workspace.MoneyManager] end

local players = {}

function getPlayer(player)
	for i=1, #players do
		if (players[i] ~= nil and players[i].player == player) then
			return players[i]
		end
	end
	return nil
end

function GetTarget(player)
	playerR = getPlayer(player)
	if (playerR ~= nil) then
		return playerR.target
	else return nil end
end
public.GetTarget = GetTarget

function removePlayer(player)
	for i=1, #players do
		if (players[i] ~= nil and players[i].player == player) then
			table.remove(players, i)
			break
		end
	end
end

function AddDetector(player)
	playerV = getPlayer(player)
	if (player == nil) then return end
	if (playerV == nil) then return end
	local detector
	if (player:FindFirstChild(&quot;HotColdDetector&quot;) ~= nil) then
		detector = player.HotColdDetector
	else
		detector= game.Workspace.HotColdDetector:Clone()
		detector.Parent = player
	end
	detector.Target.Value = playerV.target
end

function wasKilledBy(target, player)
	if (target == nil or target.Parent == nil) then return false end
	if (target:FindFirstChild(&quot;Humanoid&quot;) == nil) then return false end
	c = target.Humanoid:GetChildren()
	for i=1, #c do
		if(c[i].Name == &quot;creator&quot;) then
			if (c[i].Value ~= nil and c[i].Value.Character == player) then
				return true
			end
		end
	end
	return false
end

function onTargetKilled(target, player)
	local m = Instance.new(&quot;Message&quot;)
	if (wasKilledBy(target, player)) then 
		m.Text = &quot;Target down!&quot;
		msgMgr().ShowLocalMessage(&quot;+10 to &quot; .. player.Name .. &quot; for target killed&quot;, target.Torso.Position)
		moneyMgr().ChangeMoney(player, 10)
	else
		m.Text = &quot;Your target was killed...&quot;	
	end
	m.Parent = game.Players:GetPlayerFromCharacter(player)
	public.FindTarget(player)
	delay(3, function() m:Remove() end)
end

function onHunterKilled(player)
	removePlayer(player)
	for i=1, #players do
		if (players[i].target ~= player and wasKilledBy(player, players[i].player)) then
			local m = Instance.new(&quot;Message&quot;)
			m.Text = player.Name .. &quot; was not &quot; .. players[i].player.Name .. &quot;&apos;s target!&quot;
			m.Parent = game.Workspace
			delay(2, function() m:Remove() end)
			msgMgr().ShowLocalMessage(&quot;-15 from &quot; .. players[i].player.Name .. &quot; for civilian killed&quot;, player.Torso.Position)
			moneyMgr().ChangeMoney(players[i].player, -15)
		end
	end
end

function FindTarget(player)
	playerR = getPlayer(player)
	if (playerR == nil) then return nil end
	playerR.target = nil
	local target = nil
	--It&apos;s a for so it can time out
	for error=1, 50 do
		bots = botMgr().GetNumBots()
		chooseBot = math.random(bots + #players)
		if (math.random(bots + #players) &lt; bots / 2) then
			temp = botMgr().GetRandomBot()
			if (temp ~= nil and temp.Parent ~= nil) then
				target = temp
				break
			end
		else
			temp = players[math.random(1, #players)]
			if (temp ~= nil and temp.player ~= player) then
				target = temp.player
				break
			end
		end
	end
	playerR.target = target
	AddDetector(player)

function AddWayPoint(newPoint)
	table.insert(waypoints, newPoint)
end
public.AddWayPoint = AddWayPoint

function GetRange(range, target)
	targetPos = target.Position
	torsoPos = torso.Position
	pos = ((torsoPos - targetPos).unit * range) + targetPos
	return pos
end

function Get2DDistance(pos1, pos2)
	pos12d = Vector3.new(pos1.x, 0, pos1.z)
	pos22d = Vector3.new(pos2.x, 0, pos2.z)
	return math.abs((pos12d - pos22d).magnitude)
end

function GetCurrentWaypoint()
	result = nil
	if (waypoints[1] ~= nil) then
		result = waypoints[1]
		if (Get2DDistance(waypoints[1] , torso.Position) &lt; 3) then
			table.remove(waypoints, 1)
			result = GetCurrentWaypoint()
		end
	end
	return result
end
public.GetCurrentWaypoint = GetCurrentWaypoint

function Move(pos)
	humanoid:MoveTo(pos, debug)
	debug.Position = pos
end



function GetRandomBot()
	for i=1, 50 do
		bot = bots[math.random(#bots)]
		if (bot ~= nil and bot.Parent ~= nil) then
			return bot
		end
	end
	return nil
end

public.GetRandomBot = GetRandomBot

function remove(bot)
	for i=1, #bots do
		if (bot ~= nil and bots[i].Name == bot.Name) then
			table.remove(bots, i)
			break
		end
	end
end

function CleanUpBots()
	for i=1, #bots do
		if (bots[i] == nil or bots[i].Parent == nil) then
			remove(bot)
		end
	end
end

function botKilled(bot)
	remove(bot)
	for i=1, #listeners do
		listeners[i](bot)
	end
end



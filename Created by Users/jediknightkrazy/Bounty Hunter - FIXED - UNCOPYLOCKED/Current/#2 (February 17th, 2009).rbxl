	--Obstacle avoidance
	if  ((target ~= nil or GetCurrentWaypoint() ~= nil) and
		(Get2DDistance(torso.Position, lastPos) &lt; (humanoid.WalkSpeed * waitTime / 2))) then
		timeStuck = timeStuck + waitTime
		if (timeStuck &gt;= stuckThreshold) then
			print(&quot;Bot is stuck. Adding waypoints...&quot;)
			backUp = (torso.CFrame.lookVector * Vector3.new(-7, 0, 0)) + torso.Position
			turnedAngle = torso.CFrame * CFrame.fromAxisAngle(Vector3.new(0, 1, 0).unit, math.deg(90))
			turnAround = (turnedAngle.lookVector * Vector3.new(0, 0, 7)) + backUp
			table.insert(waypoints, 1, backUp)
			table.insert(waypoints, 2, turnAround)
			timeStuck = 0
		end
	else
		timeStuck = 0
	end

	local currentTarget = nil
	currentTarget = GetCurrentWaypoint()
	if (currentTarget == nil and target ~= nil) then
		currentTarget = GetRange(desiredRange, target)
	end
	if (currentTarget == nil) then
		currentTarget = torso.Position
	end
	Move(currentTarget)
	lastPos = torso.Position
	wait(waitTime)

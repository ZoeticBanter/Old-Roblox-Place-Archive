itlh=miked.Torso:findFirstChild(&quot;Left Hip&quot;)
itlh.Part0=miked.Torso
itlh.Part1=miked:findFirstChild(&quot;Left Leg&quot;)
itlh.C0=CFrame.new(-1, -1, 0, 0, 0, -1, 0, 1, 0, 1, 0, 0)


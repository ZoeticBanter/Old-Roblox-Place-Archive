				<ProtectedString name="Source">local function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end


local mine = script.Parent
local size
if mine.Name == &quot;MineSmall&quot; then size = 1
elseif mine.Name == &quot;MineMedium&quot; then size = 2
elseif mine.Name == &quot;MineLarge&quot; then size = 3
else print(&quot;Error mien bad namee&quot;) --wat
end

local mineColor = mine.MainPart.BrickColor
local mineReference = nil
local mineHealth = mine:FindFirstChild(&quot;Health&quot;)

local locList = game.Workspace.MainScript.GameLocations:GetChildren()
for i = 1, #locList do
&#9;local vec = (mine.MainPart.Position - locList[i].Value)
&#9;if vec.Magnitude &lt; 20 then
&#9;&#9;mineReference = locList[i]
&#9;&#9;break
&#9;end
end
if not mineReference then
&#9;print(&quot;ERROR Couldnt find matching mine reference&quot;)
end

mineReference.Team.Value = mineColor
&#9;


function RecursiveUnanchor(Model)
&#9;if Model ~= nil then
&#9;&#9;if Model:IsA(&quot;BasePart&quot;) and not Model:IsA(&quot;Terrain&quot;) then
&#9;&#9;&#9;Model.Anchored=false
&#9;&#9;end

&#9;&#9;for i, v in ipairs(Model:GetChildren()) do
&#9;&#9;&#9;RecursiveUnanchor(v)
&#9;&#9;end
&#9;end
end

function checkHealth()
&#9;if not mineHealth or mineHealth.Value &lt;= 0 then
&#9;&#9;print(mineColor.Name, &quot;lost mine at &quot;, mineReference.Value)
&#9;&#9;RecursiveUnanchor(mine)
&#9;&#9;mine:BreakJoints()
&#9;&#9;if mineReference.Team.Value == mineColor then mineReference.Team.Value = BrickColor.new(&quot;White&quot;) end
&#9;&#9;script:Destroy()
&#9;&#9;game.Debris:AddItem(mine, 2)
&#9;end
end

mineHealth.Changed:connect(checkHealth)

local mineRate = 10
local currencyname = &quot;Gold&quot;
local currencygive = size * 2.5


delay(2, function()
&#9;local Owner = waitForChild(mine, &quot;Owner&quot;)
&#9;local leaderstats = Owner.Value:FindFirstChild(&quot;leaderstats&quot;)
&#9;

					<ProtectedString name="Source">--[[
&#9;
&#9;// File Name: Leaderboard.LUA - the scripting kiddies be mad 
&#9;// Date: 04/23/2012
&#9;// Created by: Sorcus 

&#9;// Designed to be an in game plugin 
&#9;// Of course this one assumes that you will have the GUIs in place
&#9;// Eventually might create the GUIs by itself though


&#9;// If you find any bugs please PM Sorcus on ROBLOX or @Canavus on Twitter 

&#9;// TODO: Clean up the magic numbers and fix part of the code 

]]

-- Global vars 

local function waitForChild(parent, instance, delay)
&#9;local startTime = tick() 
&#9;while parent:FindFirstChild(instance) == nil do 
&#9;&#9;if delay and tick() - startTime &gt; delay then 
&#9;&#9;&#9;return nil 
&#9;&#9;end 
&#9;&#9;wait()
&#9;end 
&#9;return parent[instance]
end 

local Gui = {} -- Class, that contains functions to create various gui objects

-- Generic object Create function, which I am using to create Gui&apos;s 
-- Thanks to Stravant! 
function Gui:Create(guiType)
&#9;return function(data)
&#9;&#9;local obj = Instance.new(guiType)
&#9;&#9;for k, v in pairs(data) do
&#9;&#9;&#9;if type(k) == &apos;number&apos; then
&#9;&#9;&#9;&#9;v.Parent = obj
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;obj[k] = v
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;return obj
&#9;end
end 

function Gui:Clone(obj)
&#9;return function(data)
&#9;&#9;local cloneObj = obj:Clone()
&#9;&#9;for k, v in pairs(data) do 
&#9;&#9;&#9;if type(k) == &quot;number&quot; then 
&#9;&#9;&#9;&#9;v.Parent = cloneObj 
&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;cloneObj[k] = v
&#9;&#9;&#9;end
&#9;&#9;end 
&#9;&#9;return cloneObj
&#9;end
end

------------------------------------------------------------------
-- This works under the assumption that such a ScreenGui exists in the first place 
-- Else GG NO RE 

local RBX_Players = game.Players 
local Main_ScreenGui = script.Parent  
local Leaderboard_Frame = waitForChild(Main_ScreenGui, &quot;LeaderboardFrame&quot;)
local RBX_Teams = game.Teams 
local RBX_Debris = game:GetService(&quot;Debris&quot;)

--[[

&#9;// Here is the thing - if the Leaderboard frame is split into multiple frames 
&#9;// we assume that each of those frames represent different teams and 
&#9;// that they are positioned correctly 
]]

local TeamsExist = false 

local TeamFrames_List = {} 

-- The team color thing is annoying, ideally I want the frames 
-- to be named by the team names instead of ridiculous team color things 
-- But in this case, the frames are named after the brick color string
local lChildren = Leaderboard_Frame:GetChildren() 
for i = 1, #lChildren do 
&#9;if lChildren[i] then 
&#9;&#9;TeamsExist = true 
&#9;&#9;local t = {} &#9;&#9;
&#9;&#9;t[lChildren[i]] = {} 
&#9;&#9;local lCh = lChildren[i]:GetChildren() 
&#9;&#9;for j = 1, #lCh do 
&#9;&#9;&#9;table.insert(t[lChildren[i]], lCh[j]) 
&#9;&#9;end 
&#9;&#9;table.insert(TeamFrames_List, t)&#9;&#9;
&#9;&#9;
&#9;end 
end 

local PlayerFields_List = {} -- This one stores all the fields to help in removal 

-- It will be nice to use the existing leaderstats system, right? 
-- Also, here is another problem, its way easier to just clear the entire leaderboard
-- And repopulate vs trying to adjust the existing ones and moving them up and down
-- And its not that expensive, in fact its almost not expensive and can be done 
-- with little resources, so I decided that well, lets just repopulate everytime eh?
-- Coding the other one will be a little extra effort for what is in my opinion 
-- a marginal gain

local function PopulateLeaderboard()
&#9;-- Magic 
&#9;if TeamsExist then
&#9;&#9;Leaderboard_Frame:ClearAllChildren()
&#9;&#9;for _, t in pairs(TeamFrames_List) do 
&#9;&#9;&#9;for frame, children in pairs(t) do 
&#9;&#9;&#9;&#9;frame.Parent = Leaderboard_Frame 
&#9;&#9;&#9;&#9;for i = 1, #children do 
&#9;&#9;&#9;&#9;&#9;children[i].Parent = frame 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;end

&#9;local players = RBX_Players:GetPlayers()&#9;
&#9;local playerName_List = {} 
&#9;for i = 1, #RBX_Teams:GetChildren() do 
&#9;&#9;playerName_List[tostring(RBX_Teams:GetChildren()[i].TeamColor)] = {} 
&#9;end 
&#9;for i = 1, #players do 
&#9;&#9;if players[i] then  &#9;&#9;&#9;
&#9;&#9;&#9;local xOffset = 0 
&#9;&#9;&#9;local yOffset = 0
&#9;&#9;&#9;local leaderstats = waitForChild(players[i],&quot;leaderstats&quot;)
&#9;&#9;&#9;local lChildren = leaderstats:GetChildren()

&#9;&#9;&#9;local playerName_TextLabel = Gui:Create&quot;TextLabel&quot;{
&#9;&#9;&#9;&#9;Name = players[i].Name,
&#9;&#9;&#9;&#9;BackgroundTransparency = 1.0,
&#9;&#9;&#9;&#9;Font = Enum.Font.Arial,
&#9;&#9;&#9;&#9;FontSize = Enum.FontSize.Size14,
&#9;&#9;&#9;&#9;Text = players[i].Name,
&#9;&#9;&#9;&#9;TextColor3=Color3.new(1,1,1),
&#9;&#9;&#9;&#9;Size = UDim2.new(0, 0, 0, 0),
&#9;&#9;&#9;&#9;ZIndex = 2,
&#9;&#9;&#9;&#9;TextXAlignment = Enum.TextXAlignment.Left
&#9;&#9;&#9;}

&#9;&#9;&#9;if TeamsExist and Leaderboard_Frame:FindFirstChild(tostring(players[i].TeamColor)) ~= nil  then
&#9;&#9;&#9;&#9;local parentFrame = Leaderboard_Frame[tostring(players[i].TeamColor)]  
&#9;&#9;&#9;&#9;playerName_TextLabel.Parent = parentFrame
&#9;&#9;&#9;&#9;table.insert(playerName_List[tostring(players[i].TeamColor)], playerName_TextLabel)
&#9;&#9;&#9;&#9;yOffset = ((#playerName_List[tostring(players[i].TeamColor)]-1)*20)+95
&#9;&#9;&#9;&#9;playerName_TextLabel.Position = UDim2.new(0,40,0,yOffset)
&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;playerName_TextLabel.Parent = Leaderboard_Frame&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;yOffset = ((i-1)*20)+95
&#9;&#9;&#9;&#9;playerName_TextLabel.Position = UDim2.new(0,40,0,yOffset)
&#9;&#9;&#9;end 

&#9;&#9;&#9;local counter = 0 
&#9;&#9;&#9;for _, stat in pairs(lChildren) do 
&#9;&#9;&#9;&#9;if stat:IsA(&quot;IntValue&quot;) or stat:IsA(&quot;IntConstrainedValue&quot;) or stat:IsA(&quot;NumberValue&quot;) then 
&#9;&#9;&#9;&#9;&#9;local stat_TextLabel = Gui:Clone(playerName_TextLabel){&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;&#9;Text = stat.Value,
&#9;&#9;&#9;&#9;&#9;&#9;TextColor3=Color3.new(1,1,1),
&#9;&#9;&#9;&#9;&#9;&#9;FontSize = Enum.FontSize.Size14,
&#9;&#9;&#9;&#9;&#9;&#9;Size = UDim2.new(0, 0, 0, 0),
&#9;&#9;&#9;&#9;&#9;&#9;Position = UDim2.new(0,270+(counter*60),0,playerName_TextLabel.Position.Y.Offset),
&#9;&#9;&#9;&#9;&#9;&#9;ZIndex = 2,
&#9;&#9;&#9;&#9;&#9;&#9;TextXAlignment = Enum.TextXAlignment.Center,
&#9;&#9;&#9;&#9;&#9;&#9;Parent = playerName_TextLabel.Parent&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;}
&#9;&#9;&#9;&#9;&#9;counter = counter + 1
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;end 
end 

PopulateLeaderboard()

Spawn(function()
&#9;while true do
&#9;&#9;PopulateLeaderboard() 
&#9;&#9;wait(1)
&#9;end
end)


</ProtectedString>

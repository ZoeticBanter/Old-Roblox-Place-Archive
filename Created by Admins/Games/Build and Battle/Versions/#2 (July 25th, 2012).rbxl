					<string name="Name">Script</string>
					<ProtectedString name="Source">--Stickmasterluke


sp=script.Parent

for xi=1,sp.Size.x/4 do
&#9;for yi=1,sp.Size.y/4 do
&#9;&#9;for zi=1,sp.Size.z/4 do
&#9;&#9;&#9;local pos=game.Workspace.Terrain:WorldToCell((sp.CFrame*CFrame.new(((xi-.5)*4)-(sp.Size.x/2),((yi-.5)*4)-(sp.Size.y/2),((zi-.5)*4)-(sp.Size.z/2))).p)
&#9;&#9;&#9;game.Workspace.Terrain:SetCell(pos.x,pos.y,pos.z,Enum.CellMaterial.Cement,0,0)
&#9;&#9;end
&#9;end
end
for xi=0,(sp.Size.x/4)+1 do
&#9;for yi=0,(sp.Size.y/4)+1 do
&#9;&#9;for zi=0,(sp.Size.z/4)+1 do
&#9;&#9;&#9;if xi~=0 or xi~=(sp.Size.x/4)+1 or yi~=0 or yi~=(sp.Size.y/4)+1 or zi~=0 or zi~=(sp.Size.z/4)+1 then
&#9;&#9;&#9;&#9;local pos=game.Workspace.Terrain:WorldToCell((sp.CFrame*CFrame.new(((xi-.5)*4)-(sp.Size.x/2),((yi-.5)*4)-(sp.Size.y/2),((zi-.5)*4)-(sp.Size.z/2))).p)
&#9;&#9;&#9;&#9;local material=game.Workspace.Terrain:GetCell(pos.x,pos.y,pos.z)
&#9;&#9;&#9;&#9;if material==Enum.CellMaterial.Grass or material==Enum.CellMaterial.Sand then
&#9;&#9;&#9;&#9;&#9;game.Workspace.Terrain:AutowedgeCell(pos.x,pos.y,pos.z)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

sp:remove()

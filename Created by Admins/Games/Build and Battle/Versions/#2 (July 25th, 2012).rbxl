&#9;&#9;&#9;&#9;local hitFace = 0

&#9;&#9;&#9;&#9;if modelInfo then hitFace = modelTargetSurface(modelInfo.Parent, game.Workspace.CurrentCamera.CoordinateFrame.p, Mouse.Hit.p) end

&#9;&#9;&#9;&#9;-- are we stamping TO an unstampable surface?

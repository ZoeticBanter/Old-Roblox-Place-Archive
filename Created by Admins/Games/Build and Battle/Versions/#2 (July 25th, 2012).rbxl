							<ProtectedString name="Source">function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance[name]
end

function FireConcurrent(f, arg1)
&#9;if f~= nil then
&#9;&#9;local event = Instance.new(&apos;BindableEvent&apos;,script)
&#9;&#9;event.Event:connect(f)
&#9;&#9;event:Fire(arg1)
&#9;&#9;event.Parent = nil
&#9;end
end

local textBox = script.Parent
--local resources = textBox.Parent:FindFirstChild(&quot;Resources&quot;)
local lstats = waitForChild(game.Players.LocalPlayer, &quot;leaderstats&quot;)
local res = waitForChild(lstats, &quot;Gold&quot;)
--local res = resources.Value

textBox.Text = res.Value

function ShowIncome(ammount)
&#9;local incomeImage = waitForChild(textBox,&quot;IncomeImage&quot;)
&#9;local tempImage = incomeImage:Clone()
&#9;tempImage.Parent = textBox
&#9;tempImage.Text = &quot;+  &quot; .. ammount
&#9;tempImage.Visible = true
&#9;local frames = 16
&#9;for i = 1, frames do
&#9;&#9;wait(.05)
&#9;&#9;tempImage.Position = UDim2.new(.5,0,1,20 - 2* i)
&#9;&#9;tempImage.TextTransparency = (i)/frames
&#9;&#9;tempImage.TextStrokeTransparency = (i)/frames
&#9;end
&#9;tempImage.Parent = nil
end

local isIdle = true
local previousResCount = res.Value
res.Changed:connect(function() 
&#9;if isIdle then
&#9;&#9;isIdle = false
&#9;&#9;textBox.Text = res.Value
&#9;&#9;wait(.1)
&#9;&#9;local ammount = res.Value - previousResCount
&#9;&#9;previousResCount = res.Value
&#9;&#9;if ammount &gt; 0 then FireConcurrent(ShowIncome, ammount) end
&#9;&#9;isIdle = true
&#9;end
end)

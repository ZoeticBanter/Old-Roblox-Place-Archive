--Inside radius is checked within a sphere, outside check ignores height difference
function checkDistance(vecObj, pos, size)
&#9;local vec = vecObj.Value - pos
&#9;local vec2 = size/2
&#9;local hVec = vec * Vector3.new(1,0,1)
&#9;local hVec2 = vec2 * Vector3.new(1,0,1)
&#9;if vecObj.Name == &quot;Base1&quot; or vecObj.Name == &quot;Base2&quot; then
&#9;&#9;if vec.magnitude - vec2.magnitude &lt; baseInRadius then
&#9;&#9;&#9;--print(&quot;Too Close&quot;)
&#9;&#9;&#9;return &quot;Too Close&quot;
&#9;&#9;elseif hVec.magnitude + hVec2.magnitude &lt; baseOutRadius then
&#9;&#9;&#9;return &quot;Good&quot;
&#9;&#9;end
&#9;else
&#9;&#9;if vec.magnitude - vec2.magnitude &lt; mineInRadius then
&#9;&#9;&#9;--print(&quot;Too Close&quot;)
&#9;&#9;&#9;return &quot;Too Close&quot;
&#9;&#9;elseif hVec.magnitude + hVec2.magnitude &lt; mineOutRadius then
&#9;&#9;&#9;return &quot;Good&quot;
&#9;&#9;end
&#9;end
&#9;return &quot;Far&quot;
end


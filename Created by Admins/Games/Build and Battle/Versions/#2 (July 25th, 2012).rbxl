					<ProtectedString name="Source">--Stickmasterluke

sp=script.Parent

rate=1/30
plr=game.Players.localPlayer
check=true
page=1

function waitfor(parent,name)
&#9;while true do
&#9;&#9;local child=parent:FindFirstChild(name)
&#9;&#9;if child~=nil then
&#9;&#9;&#9;return child
&#9;&#9;end
&#9;&#9;wait()
&#9;end
end

waitfor(sp,&quot;Opening&quot;)
waitfor(sp.Opening,&quot;NextButton&quot;)
page1=waitfor(sp.Opening,&quot;Page1&quot;)
page2=waitfor(sp.Opening,&quot;Page2&quot;)

pages={
&#9;page1,
&#9;page2,
}

if plr~=nil then
&#9;if plr:FindFirstChild(&quot;OpeningTag&quot;)==nil then
&#9;&#9;sp.Opening.Visible=true
&#9;&#9;wait(3)
&#9;&#9;sp.Opening:TweenPosition(UDim2.new(.5,-391,.5,-266),&quot;Out&quot;,&quot;Quad&quot;,3,true)
&#9;end
end

sp.Opening.NextButton.MouseButton1Click:connect(function()
&#9;if check then
&#9;&#9;check=false
&#9;&#9;if page&gt;=#pages then
&#9;&#9;&#9;sp.Opening:TweenPosition(UDim2.new(.5,-391,1,10),&quot;Out&quot;,&quot;Quad&quot;,1,true)
&#9;&#9;&#9;local ot=Instance.new(&quot;StringValue&quot;)
&#9;&#9;&#9;ot.Name=&quot;OpeningTag&quot;
&#9;&#9;&#9;ot.Parent=plr
&#9;&#9;&#9;wait(2)
&#9;&#9;&#9;sp.Opening.Visible=false
&#9;&#9;else
&#9;&#9;&#9;sp.Opening:TweenPosition(UDim2.new(.5,-391,1,10),&quot;Out&quot;,&quot;Quad&quot;,1,true)
&#9;&#9;&#9;wait(1.5)
&#9;&#9;&#9;pages[page].Visible=false
&#9;&#9;&#9;page=page+1
&#9;&#9;&#9;pages[page].Visible=true
&#9;&#9;&#9;sp.Opening.Position=UDim2.new(.5,-391,0,-543)
&#9;&#9;&#9;sp.Opening:TweenPosition(UDim2.new(.5,-391,.5,-266),&quot;Out&quot;,&quot;Quad&quot;,1,true)
&#9;&#9;&#9;if page&gt;=#pages then
&#9;&#9;&#9;&#9;sp.Opening.NextButton.Image=&quot;http://www.roblox.com/asset/?id=83406842&quot;
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;check=true
&#9;end
end)



while sp.Opening.Visible do
&#9;wait(1)
end
frames=3/rate
for i=1,frames do
&#9;sp.Background.BackgroundTransparency=i/frames
&#9;wait(rate)
end
sp.Background.Visible=false




</ProtectedString>

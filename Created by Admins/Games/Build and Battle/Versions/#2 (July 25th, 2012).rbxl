&#9;&#9;&#9;&#9;upgrader = 4
&#9;&#9;&#9;&#9;if (time - creationTime) &gt;= twelvemin then
&#9;&#9;&#9;&#9;&#9;lightUp(5)
&#9;&#9;&#9;&#9;&#9;upgrader = 5
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;--]]
end





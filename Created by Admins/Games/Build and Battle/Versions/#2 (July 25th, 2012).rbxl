&#9;&#9;&#9;&#9;&#9;if clusterTag:IsA(&quot;Vector3Value&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;root = setupFakeTerrainPart(clusterTag.Value.X, terrainShape, clusterTag.Value.Z)
&#9;&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;&#9;root = setupFakeTerrainPart(clusterTag.Value, terrainShape, 0)

sp=script.Parent



placeablecolor=BrickColor.new(&quot;Cyan&quot;)
notplaceablecolor=BrickColor.new(&quot;Bright red&quot;)

  
plr=game.Players.LocalPlayer

terra=game.Workspace.Terrain
equipped=false
object=nil
objecttype=&quot;Terrain&quot;
objectsize=Vector3.new(1,1,1)

function FindHUD()
&#9;if plr.Character~=nil then
&#9;&#9;hud=plr.Character:FindFirstChild(&quot;HUD&quot;)
&#9;&#9;if hud==nil then
&#9;&#9;&#9;hud=Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;hud.Name=&quot;HUD&quot;
&#9;&#9;&#9;hud.Parent=plr.Character
&#9;&#9;end
&#9;&#9;return hud
&#9;end
end

function checkifnegativeinc(a,b)
&#9;if b&lt;a then
&#9;&#9;return -1
&#9;end
&#9;return 1
end

function checkregionempty(p1,p2)
&#9;local xinc=checkifnegativeinc(p1.x,p2.x)
&#9;local yinc=checkifnegativeinc(p1.y,p2.y)
&#9;local zinc=checkifnegativeinc(p1.z,p2.z)
&#9;p1=p1+Vector3.new(xinc*.05,yinc*.05,zinc*.05)
&#9;p2=p2+Vector3.new(-xinc*.05,-yinc*.05,-zinc*.05)
&#9;for ix=p1.x,p2.x,xinc do
&#9;&#9;for iy=p1.y,p2.y,yinc do
&#9;&#9;&#9;for iz=p1.z,p2.z,zinc do
&#9;&#9;&#9;&#9;if terra:GetCell(ix,iy,iz).Value~=0 then
&#9;&#9;&#9;&#9;&#9;return false
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return true
end
--[[
function checkregionempty(point1,point2)
&#9;for ix = point1.x, point2.x, checkifnegativeinc(point1.x, point2.x) do
&#9;&#9;for iy = point1.y, point2.y, checkifnegativeinc(point1.y, point2.y) do
&#9;&#9;&#9;for iz = point1.z, point2.z, checkifnegativeinc(point1.z, point2.z) do
&#9;&#9;&#9;&#9;if terra:GetCell(ix, iy, iz) ~= 0 then
&#9;&#9;&#9;&#9;&#9;return false
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return true
end
]]

function updatepos()
&#9;FindHUD()
&#9;if themouse~=nil and hud~=nil then
&#9;&#9;local ph=hud:FindFirstChild(&quot;PlaceHolder&quot;)
&#9;&#9;if ph==nil then
&#9;&#9;&#9;ph=Instance.new(&quot;Model&quot;)
&#9;&#9;&#9;ph.Name=&quot;PlaceHolder&quot;
&#9;&#9;&#9;ph.Parent=hud
&#9;&#9;&#9;local fakecell=Instance.new(&quot;Part&quot;)
&#9;&#9;&#9;fakecell.Name=&quot;Invisible&quot;
&#9;&#9;&#9;fakecell.formFactor=&quot;Custom&quot;
&#9;&#9;&#9;fakecell.Anchored=true
&#9;&#9;&#9;fakecell.CanCollide=false
&#9;&#9;&#9;fakecell.Transparency=.5
&#9;&#9;&#9;fakecell.BrickColor=placeablecolor
&#9;&#9;&#9;fakecell.Size=Vector3.new(4,4,4)
&#9;&#9;&#9;fakecell.Parent=ph
&#9;&#9;end
&#9;&#9;local phc=ph:GetChildren()
&#9;&#9;if #phc&gt;=1 then
&#9;&#9;&#9;if phc[1].className==&quot;Part&quot; then
&#9;&#9;&#9;&#9;phc[1].CFrame=CFrame.new((terra:WorldToCellPreferEmpty(themouse.Hit.p)*Vector3.new(4,4,4))+Vector3.new(2,2,2))
&#9;&#9;&#9;&#9;if checkregionempty(terra:WorldToCell(phc[1].Position-(phc[1].Size*.5)),terra:WorldToCell(phc[1].Position+(phc[1].Size*.5))) then
&#9;&#9;&#9;&#9;&#9;phc[1].BrickColor=placeablecolor
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;phc[1].BrickColor=notplaceablecolor
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

function findplacement(pos)
&#9;local cellpos=terra:WorldToCellPreferEmpty(pos)
&#9;local height=0
&#9;while not checkregionempty(cellpos+Vector3.new(0,height,0),cellpos+(objectsize-Vector3.new(1,1,1))+Vector3.new(0,height,0)) do
&#9;&#9;height=height+1
&#9;&#9;wait()
&#9;end
&#9;return cellpos+Vector3.new(0,height,0),cellpos+(objectsize-Vector3.new(1,1,1))+Vector3.new(0,height,0)
end

function selected(mouse)
&#9;if mouse~=nil then
&#9;&#9;FindHUD()
&#9;&#9;themouse=mouse
&#9;&#9;equipped=true
&#9;&#9;mouse.Button1Down:connect(function()
&#9;&#9;&#9;cellpos=terra:WorldToCellPreferEmpty(mouse.Hit.p)
&#9;&#9;&#9;canbeplace=false
&#9;&#9;&#9;--if checkregionempty(terra:WorldToCell(cellpos.Position-(phc[1].Size*.5)),terra:WorldToCell(cellpos.Position+(phc[1].Size*.5))) then
&#9;&#9;&#9;&#9;canbeplaced=true
&#9;&#9;&#9;--end
&#9;&#9;&#9;if canbeplaced then
&#9;&#9;&#9;&#9;print(cellpos)
&#9;&#9;&#9;&#9;terra:SetCell(cellpos.x,cellpos.y,cellpos.z,1,0,0)
&#9;&#9;&#9;end
&#9;&#9;&#9;updatepos()
&#9;&#9;end)
&#9;&#9;mouse.Move:connect(updatepos)
&#9;end
end

&#9;&#9;


function deselected()
&#9;equipped=false
end
sp.Selected:connect(selected)
sp.Deselected:connect(deselected)

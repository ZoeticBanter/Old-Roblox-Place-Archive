function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance[name]
end



sp=script.Parent
local mainPart = waitForChild( sp, &quot;MainPart&quot;)
local objectColor = mainPart.BrickColor

while sp:FindFirstChild(&quot;Gun&quot;) == nil do wait() end 
local Gun = sp.Gun 

Gun.Changed:connect(function()
&#9;if not Gun.Parent then 
&#9;&#9;sp:BreakJoints() &#9;
&#9;&#9;script:Destroy()
&#9;end 
end) 


range=75
burst=5
damage=5
chancetobreakjoints=1/5

barrelpos=CFrame.new(0,.5,-3.5)

rate=1/10
debris=game:GetService(&quot;Debris&quot;)

function rayignorecheck(hit3)
&#9;if hit3 then
&#9;&#9;if hit3.Transparency&gt;=1 or hit3.Name==&quot;Effect&quot; or hit3.Name==&quot;Rocket&quot; or hit3.Name==&quot;Handle&quot; or hit3:IsDescendantOf(sp) then
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end
&#9;return false
end

function raycast(spos,vec,dist)
&#9;local hit2,pos2=game.Workspace:FindPartOnRay(Ray.new(spos+(vec*.01),vec*dist),sp)
&#9;if hit2~=nil and pos2 then
&#9;&#9;local dist2=dist-(pos2-spos).magnitude
&#9;&#9;if rayignorecheck(hit2) and dist2&gt;0 then
&#9;&#9;&#9;return raycast(pos2,vec,dist2)
&#9;&#9;end
&#9;end
&#9;return hit2,pos2
end

function fire(tpos)
&#9;local gun=sp:FindFirstChild(&quot;Gun&quot;)
&#9;local stand=mainPart
&#9;if gun==nil or stand==nil or stand.Anchored==false then
&#9;&#9;return
&#9;end
&#9;local startpos=(gun.CFrame*barrelpos).p
&#9;local hit,epos=raycast(startpos,(tpos-startpos).unit,500)
&#9;if hit~=nil and hit.Parent~=nil then
&#9;&#9;local hum = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;if hum then
&#9;&#9;&#9;hum:TakeDamage(damage)
&#9;&#9;&#9;--[[if math.random()&lt;chancetobreakjoints then
&#9;&#9;&#9;&#9;hit:BreakJoints()
&#9;&#9;&#9;end]]
&#9;&#9;&#9;--local health=hit.Parent:FindFirstChild(&quot;Health&quot;)
&#9;&#9;&#9;--if health then
&#9;&#9;&#9;--&#9;health.Value=health.Value-damage
&#9;&#9;&#9;--end
&#9;&#9;end
&#9;end
&#9;local effectdist=(startpos-epos).magnitude
&#9;local shotairtime=math.ceil(effectdist/100)
&#9;local p=Instance.new(&quot;Part&quot;)
&#9;p.Name=&quot;Effect&quot;
&#9;p.Anchored=true
&#9;p.CanCollide=false
&#9;p.BrickColor=BrickColor.new(&quot;New Yeller&quot;)
&#9;p.TopSurface=&quot;Smooth&quot;
&#9;p.BottomSurface=&quot;Smooth&quot;
&#9;p.formFactor=&quot;Custom&quot;
&#9;p.Size=Vector3.new(.2,.2,.2)
&#9;p.CFrame=CFrame.new(startpos,epos)*CFrame.new(0,0,-effectdist/2)
&#9;local m=Instance.new(&quot;BlockMesh&quot;)
&#9;m.Scale=Vector3.new(1,1,(effectdist*5)/shotairtime)
&#9;m.Offset=Vector3.new(0,0,(effectdist*.5)-((effectdist/shotairtime)*.5))
&#9;m.Parent=p
&#9;debris:AddItem(p,2)
&#9;p.Parent=game.Workspace
&#9;delay(rate,function()
&#9;&#9;for i=1,shotairtime-1 do
&#9;&#9;&#9;m.Offset=Vector3.new(0,0,(effectdist*.5)-((effectdist/shotairtime)*(i+1-.5)))
&#9;&#9;&#9;wait(rate)
&#9;&#9;end
&#9;&#9;p:remove()
&#9;end)
end

function lookat(pivotpoint,pos)
&#9;local gun=sp:FindFirstChild(&quot;Gun&quot;)
&#9;local stand=sp:FindFirstChild(&quot;MainPart&quot;)
&#9;if gun==nil or stand==nil or stand.Anchored==false then
&#9;&#9;return
&#9;end
&#9;gun.CFrame=CFrame.new(pivotpoint,pos)*CFrame.new(0,.5,-1.6)
end

function visibilitycheck(cpos,v)
&#9;local body=v:FindFirstChild(&quot;Torso&quot;)
&#9;if body~=nil then
&#9;&#9;local vhit,vpos=raycast(cpos,(body.Position-cpos).unit,range)
&#9;&#9;if vhit~=nil and vhit.Parent~=nil then
&#9;&#9;&#9;if vhit:IsDescendantOf(v) then
&#9;&#9;&#9;&#9;return true
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return false
end

wait(2)

while true do
&#9;local gun=sp:FindFirstChild(&quot;Gun&quot;)
&#9;local stand=sp:FindFirstChild(&quot;MainPart&quot;)
&#9;if gun==nil or stand==nil or stand.Anchored==false then
&#9;&#9;print(&quot;break1&quot;)
&#9;&#9;break
&#9;end
&#9;local pivotpoint=(stand.CFrame*CFrame.new(0,1.5,0)).p
&#9;target=nil
&#9;dist=range
&#9;local otherTeam = &quot;Blue Team&quot;
&#9;if(stand.BrickColor == BrickColor.new(&quot;Bright blue&quot;)) then
&#9;&#9;team = &quot;Red Team&quot;
&#9;else
&#9;&#9;team = &quot;Blue Team&quot;
&#9;end
&#9;for i,v in pairs(_G.Teams[team]) do
&#9;--for i,v in ipairs(game.Workspace.Bots:GetChildren()) do
&#9;&#9;local char = v.Character
&#9;&#9;local body = nil
&#9;&#9;if v.TeamColor ~= objectColor and char~= nil then body = char:FindFirstChild(&quot;Torso&quot;) end
&#9;&#9;--print(&quot;char check&quot;)
&#9;&#9;--local body=v:FindFirstChild(&quot;Body&quot;)
&#9;&#9;if body~=nil then
&#9;&#9;&#9;--print(&quot;char found&quot;)
&#9;&#9;&#9;if (body.Position-gun.Position).magnitude&lt;=dist then
&#9;&#9;&#9;&#9;--print(&quot;close enough&quot;)
&#9;&#9;&#9;&#9;if visibilitycheck(pivotpoint,char) then
&#9;&#9;&#9;&#9;&#9;--print(&quot;char visible&quot;)
&#9;&#9;&#9;&#9;&#9;target=body
&#9;&#9;&#9;&#9;&#9;dist=(body.Position-gun.Position).magnitude
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;local followtime=tick()+1+(2*math.random())
&#9;while tick()&lt;followtime do
&#9;&#9;--print(&quot;tracking&quot;)
&#9;&#9;if target~=nil and target.Parent~=nil then
&#9;&#9;&#9;lookat(pivotpoint,target.Position)
&#9;&#9;end
&#9;&#9;wait(rate)
&#9;end
&#9;local gun=sp:FindFirstChild(&quot;Gun&quot;)
&#9;local stand=sp:FindFirstChild(&quot;MainPart&quot;)
&#9;if gun==nil or stand==nil or stand.Anchored==false then
&#9;&#9;print(&quot;break2&quot;)
&#9;&#9;break
&#9;end
&#9;if target~=nil and target.Parent~=nil then
&#9;&#9;--print(&quot;still targeting&quot;)
&#9;&#9;if visibilitycheck(pivotpoint,target.Parent) then
&#9;&#9;&#9;--print(&quot;still visible&quot;)
&#9;&#9;&#9;if gun:FindFirstChild(&quot;Sound&quot;) then
&#9;&#9;&#9;&#9;gun.Sound:Play()
&#9;&#9;&#9;end
&#9;&#9;&#9;local dist=(target.Position-gun.Position).magnitude
&#9;&#9;&#9;local offset=(Vector3.new(math.random(),math.random(),math.random())-Vector3.new(.5,.5,.5))*dist*.1
&#9;&#9;&#9;local offset2=(Vector3.new(math.random(),math.random(),math.random())-Vector3.new(.5,.5,.5))*dist*.1
&#9;&#9;&#9;for i=1,burst do
&#9;&#9;&#9;&#9;lookat(pivotpoint,target.Position)
&#9;&#9;&#9;&#9;--print(&quot;firing&quot;)
&#9;&#9;&#9;&#9;fire(target.Position+offset+(offset2*(i/burst)))
&#9;&#9;&#9;&#9;local followtime=tick()+.2
&#9;&#9;&#9;&#9;while tick()&lt;followtime do
&#9;&#9;&#9;&#9;&#9;if target~=nil and target.Parent~=nil then
&#9;&#9;&#9;&#9;&#9;&#9;lookat(pivotpoint,target.Position)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;wait(rate)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

print(&quot;Broke&quot;,tick())
script:remove()

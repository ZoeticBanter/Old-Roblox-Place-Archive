&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;root = setupFakeTerrainPart(1, terrainShape, 0)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

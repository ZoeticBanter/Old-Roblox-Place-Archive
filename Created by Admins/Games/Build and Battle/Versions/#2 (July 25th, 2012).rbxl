					<ProtectedString name="Source">--The Code
--You shouldn&apos;t need to edit this if you just want a generic gun

function WaitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end

local Tool = script.Parent
local Handle = Tool.Handle
local GunData = Tool.GunData
local ShootSound = WaitForChild(Handle, &quot;Shoot&quot;)
local ReloadSound = WaitForChild(Handle, &quot;Reload&quot;)
local ExplodeSound = WaitForChild(Handle, &quot;Explode&quot;)

local Character = nil


local Workspace = game.Workspace
local Debris = game.Debris

local ShotsLeft = GunData.ShotsPerClip.ShotsLeft

local Shooting = false


local C4 = nil

function TagHumanoid(Humanoid, Player)
&#9;if Humanoid and Player then
&#9;&#9;local Tag = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;Tag.Value = Player
&#9;&#9;Tag.Name = &quot;creator&quot;
&#9;&#9;game.Debris:AddItem(Tag,3)
&#9;&#9;Tag.Parent=Humanoid
&#9;end
end

function Reload()
&#9;--Don&apos;t Reload if we are reloading
&#9;if(GunData.ReloadTime.Reloading.Value) then return end
&#9;ReloadSound:Play()

&#9;--Tell the client we are reloading
&#9;GunData.ReloadTime.Reloading.Value = true
&#9;--TODO:Play Reload Sound
&#9;wait(GunData.ReloadTime.Value)
&#9;--Reload the gun and let us continue on our merry way
&#9;ShotsLeft.Value = GunData.ShotsPerClip.Value
&#9;GunData.ReloadTime.Reloading.Value = false

end



function Explosion(Target, Radius, DamageToPlayers, DamagesTerrain, DamageToBuildings, Falloff)
&#9;Radius = Radius or 10
&#9;DamageToPlayers = DamageToPlayers or 0
&#9;DamagesTerrain = DamagesTerrain or false
&#9;DamageToBuildings = DamageToBuildings or 0
&#9;Falloff = Falloff or false

&#9;local Exp = Instance.new(&quot;Explosion&quot;)
&#9;Exp.Name = &quot;Explosion&quot;
&#9;Exp.BlastPressure = 0
&#9;Exp.BlastRadius = Radius
&#9;Exp.Position = Target&#9;&#9;
&#9;Exp.Parent = game.Workspace&#9;&#9;

&#9;local radius = 10&#9;
&#9;local checkRad = radius + 4

&#9;local position = Target

&#9;local order = {}
&#9;order[Enum.CellMaterial.Empty] &#9;&#9;= Enum.CellMaterial.Empty
&#9;order[Enum.CellMaterial.Grass] &#9;&#9;= Enum.CellMaterial.Empty
&#9;order[Enum.CellMaterial.Sand] &#9;&#9;= Enum.CellMaterial.Empty
&#9;order[Enum.CellMaterial.Brick]&#9;&#9;= Enum.CellMaterial.Gravel
&#9;order[Enum.CellMaterial.Granite]&#9;&#9;= Enum.CellMaterial.Gravel&#9;&#9;&#9;--5
&#9;order[Enum.CellMaterial.Asphalt]&#9;&#9;= Enum.CellMaterial.Asphalt
&#9;order[Enum.CellMaterial.Iron]&#9;&#9;&#9;= Enum.CellMaterial.Cement&#9;&#9;&#9;--2
&#9;order[Enum.CellMaterial.Aluminum]&#9;= Enum.CellMaterial.Iron&#9;&#9;&#9;--1
&#9;order[Enum.CellMaterial.Gold]&#9;&#9;&#9;= Enum.CellMaterial.Gold
&#9;order[Enum.CellMaterial.WoodPlank]&#9;= Enum.CellMaterial.Grass
&#9;order[Enum.CellMaterial.WoodLog]&#9;&#9;= Enum.CellMaterial.Grass
&#9;order[Enum.CellMaterial.Gravel]&#9;&#9;= Enum.CellMaterial.Empty&#9;&#9;&#9;--6
&#9;order[Enum.CellMaterial.CinderBlock]= Enum.CellMaterial.Granite&#9;&#9;--4
&#9;order[Enum.CellMaterial.MossyStone]&#9;= Enum.CellMaterial.Granite
&#9;order[Enum.CellMaterial.Cement]&#9;&#9;= Enum.CellMaterial.CinderBlock&#9;--3
&#9;order[Enum.CellMaterial.RedPlastic]&#9;= Enum.CellMaterial.RedPlastic
&#9;order[Enum.CellMaterial.BluePlastic]= Enum.CellMaterial.BluePlastic

&#9;
&#9;for x = -radius, radius, 4 do
&#9;&#9;for y = -radius, radius, 4 do
&#9;&#9;&#9;for z = -radius, radius, 4 do
&#9;&#9;&#9;&#9;local vec = Vector3.new(x,y,z)
&#9;&#9;&#9;&#9;if vec.magnitude &lt;= radius then
&#9;&#9;&#9;&#9;&#9;local cellPos = game.Workspace.Terrain:WorldToCell(position + vec)
&#9;&#9;&#9;&#9;&#9;local targetmaterial = game.Workspace.Terrain:GetCell(cellPos.x, cellPos.y, cellPos.z)
&#9;&#9;&#9;&#9;&#9;if targetmaterial ~= Enum.CellMaterial.Asphalt and targetmaterial ~= Enum.CellMaterial.Empty and targetmaterial ~= Enum.CellMaterial.Water then
&#9;&#9;&#9;&#9;&#9;&#9;game.Workspace.Terrain:SetCell(cellPos.x,cellPos.y,cellPos.z,order[targetmaterial],0,0)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;for x = -checkRad, checkRad, 4 do
&#9;&#9;for y = -checkRad, checkRad, 4 do
&#9;&#9;&#9;for z = -checkRad, checkRad, 4 do
&#9;&#9;&#9;&#9;local vec = Vector3.new(x,y,z)
&#9;&#9;&#9;&#9;if vec.magnitude &lt;= checkRad then
&#9;&#9;&#9;&#9;&#9;local cellPos = game.Workspace.Terrain:WorldToCell(position + vec)
&#9;&#9;&#9;&#9;&#9;local targetmaterial = game.Workspace.Terrain:GetCell(cellPos.x, cellPos.y, cellPos.z)
&#9;&#9;&#9;&#9;&#9;if targetmaterial ~= Enum.CellMaterial.Asphalt and targetmaterial~=Enum.CellMaterial.Empty and targetmaterial == Enum.CellMaterial.Grass  then
&#9;&#9;&#9;&#9;&#9;&#9;game.Workspace.Terrain:AutowedgeCell(cellPos.x,cellPos.y,cellPos.z)
&#9;&#9;&#9;&#9;&#9;elseif targetmaterial ~= Enum.CellMaterial.Asphalt and targetmaterial~=Enum.CellMaterial.Empty and targetmaterial == Enum.CellMaterial.Sand  then
&#9;&#9;&#9;&#9;&#9;&#9;game.Workspace.Terrain:AutowedgeCell(cellPos.x,cellPos.y,cellPos.z)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;
&#9;Exp.Hit:connect(function(part, distance)
&#9;&#9;
&#9;&#9;local PartParent = part.Parent

&#9;&#9;local DamageTags = PartParent:GetChildren()
&#9;&#9;for i = 1, #DamageTags do
&#9;&#9;&#9;if(DamageTags[i].Name == &quot;DamageOnce&quot; and DamageTags[i].Value == Exp) then return end
&#9;&#9;end

&#9;&#9;local DamageOnce = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;DamageOnce.Name = &quot;DamageOnce&quot;
&#9;&#9;DamageOnce.Value = Exp
&#9;&#9;DamageOnce.Parent = PartParent
&#9;&#9;game.Debris:AddItem(DamageOnce, .1)
&#9;&#9;
&#9;&#9;if(PartParent:FindFirstChild(&quot;Humanoid&quot;)) then --It&apos;s a player
&#9;&#9;&#9;local Humanoid = PartParent:FindFirstChild(&quot;Humanoid&quot;)

&#9;&#9;&#9;local scale = 1&#9;&#9;&#9;
&#9;&#9;&#9;if(Falloff) then scale = distance / Radius end
&#9;&#9;&#9;if(Character ~= PartParent and game.Players:GetPlayerFromCharacter(Character).TeamColor ~= game.Players:GetPlayerFromCharacter(PartParent).TeamColor) then --Build And Battle specific code
&#9;&#9;&#9;&#9;TagHumanoid(Humanoid, game.Players:GetPlayerFromCharacter(Character))
&#9;&#9;&#9;&#9;Humanoid:TakeDamage(DamageToPlayers*scale)
&#9;&#9;&#9;end
&#9;&#9;elseif(PartParent:FindFirstChild(&quot;TakeDamage&quot;)) then --It&apos;s a building
&#9;&#9;&#9;local TakeDamage = PartParent:FindFirstChild(&quot;TakeDamage&quot;)

&#9;&#9;&#9;local scale = 1&#9;&#9;&#9;
&#9;&#9;&#9;if(Falloff) then scale = distance / Radius end
&#9;&#9;&#9;if(game.Players:GetPlayerFromCharacter(Character).TeamColor ~= PartParent.MainPart.BrickColor) then
&#9;&#9;&#9;&#9;TakeDamage:Invoke(DamageToBuildings * scale, GunData.DamageType.Value)
&#9;&#9;&#9;end
&#9;&#9;end&#9;

&#9;end)

end



function ThrowC4(Position, Direction)

&#9;--Create the rocket and it&apos;s effects
&#9;C4 = Instance.new(&quot;Part&quot;)
&#9;C4.Size = Vector3.new(0.5, 3, 0.5)
&#9;C4.Transparency = 0.0
&#9;C4.Shape = &quot;Block&quot;
&#9;C4.FormFactor = 3
&#9;C4.Name =&quot;C4&quot;


&#9;local C4mesh = Instance.new(&quot;SpecialMesh&quot;)
&#9;C4mesh.MeshType = &quot;FileMesh&quot;
&#9;C4mesh.Name = &quot;Mesh&quot;
&#9;C4mesh.MeshId = &quot;http://www.roblox.com/asset/?id=16975131&quot;
&#9;C4mesh.TextureId = &quot;http://www.roblox.com/asset/?id=16975111&quot;
&#9;C4mesh.Scale = Vector3.new(1, 1, 1)
&#9;C4mesh.Parent = C4

&#9;
&#9;--Place the rocket in the world
&#9;local spawnPos = Handle.Position
&#9;spawnPos = spawnPos + Direction.unit * 5.0
&#9;
&#9;C4.CFrame = CFrame.new(spawnPos, spawnPos + Direction)&#9;&#9;
&#9;C4.Velocity = Direction.unit * 20.0&#9;
&#9;C4.Elasticity = .7
&#9;C4.Parent = game.Workspace&#9;&#9;&#9;&#9;&#9;

&#9;&#9;&#9;
&#9;C4.Touched:connect(function(part)
&#9;&#9;C4.Anchored = true
&#9;end)
&#9;&#9;

end

function DetonateC4()

&#9;delay(.5, function() 
&#9;&#9;C4:Destroy() 
&#9;&#9;

&#9;&#9;Explosion(C4.Position, 40, GunData.Damage.Value, true, GunData.Damage.Value) 
&#9;&#9;ExplodeSound:Play() 
&#9;&#9;C4 = nil 
&#9;end)


end

--Client has told us where he wants to shoot.  Oblige Him
function onFireMessage(shootTarget)
&#9;if(Shooting) then return end
&#9;
&#9;local shootingPlayer = game.Players:GetPlayerFromCharacter(Character)
&#9;
&#9;Shooting = true
&#9;local position = Tool.Handle.CFrame.p
&#9;local direction = (shootTarget - position).unit
&#9;ShotsLeft.Value = ShotsLeft.Value - 1
&#9;--Shoot the rocket

&#9;if not C4 then
&#9;&#9;ShootSound:Play()

&#9;&#9;ThrowC4(position, direction)
&#9;else
&#9;&#9;DetonateC4()
&#9;end
&#9;


&#9;wait(1 / GunData.FireRate.Value)
&#9;Shooting = false
&#9;if(ShotsLeft.Value &lt;= 0) then Reload() return end
&#9;

end



--Recieve a messages from the client
Tool.ChildAdded:connect(function(child)
&#9;if child and child:IsA(&quot;Vector3Value&quot;) and child.Name == &quot;ShootMessage&quot; then
&#9;&#9;onFireMessage(child.Value)
&#9;&#9;child:Destroy()
&#9;&#9;return
&#9;end

&#9;if child and child:IsA(&quot;BoolValue&quot;) and child.Name == &quot;ReloadMessage&quot; then
&#9;&#9;Reload()
&#9;&#9;child:Destroy()
&#9;&#9;return 
&#9;end
end)

Tool.Equipped:connect(function() 
&#9;Character = Tool.Parent
end)

Tool.Unequipped:connect(function()
&#9;Character = nil
end)

local Object = BindableFunction.Parent


local ResistTable = { Bullet = 0,
&#9;&#9;&#9;&#9;&#9;&#9;&#9; Explosion = 0,
&#9;&#9;&#9;&#9;&#9;&#9;&#9; C4 = -.5 }


function WaitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end


BindableFunction.OnInvoke = function(Damage, Type)
&#9;
&#9;local d = Damage - (Damage * (ResistTable[Type] or 0))
&#9;
&#9;local health = WaitForChild(Object, &quot;Health&quot;)
&#9;health.Value = health.Value - d

&#9;local Event = WaitForChild(Object, &quot;TakeDamageEvent&quot;)
&#9;Event.Fire(d, health.Value)

end

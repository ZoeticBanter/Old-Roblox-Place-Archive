function FindHUD()
&#9;if plr.Character~=nil then
&#9;&#9;hud=plr.Character:FindFirstChild(&quot;HUD&quot;)
&#9;&#9;if hud==nil then
&#9;&#9;&#9;hud=Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;hud.Name=&quot;HUD&quot;
&#9;&#9;&#9;hud.Parent=plr.Character
&#9;&#9;end
&#9;&#9;return hud
&#9;end
end

function checkifnegativeinc(a,b)
&#9;if b&lt;a then
&#9;&#9;return -1
&#9;end
&#9;return 1
end

function checkregionempty(p1,p2)
&#9;local xinc=checkifnegativeinc(p1.x,p2.x)
&#9;local yinc=checkifnegativeinc(p1.y,p2.y)
&#9;local zinc=checkifnegativeinc(p1.z,p2.z)
&#9;local rcheck=game.Workspace:FindPartsInRegion3(Region3.new((p1*4)+Vector3.new(.1*xinc,.1*yinc,.1*zinc),(p2*4)+Vector3.new(3.9*xinc,3.9*yinc,3.9*zinc)),hud,1)
&#9;if rcheck~=nil and #rcheck&gt;0 then
&#9;&#9;return false
&#9;end
&#9;for ix=p1.x,p2.x,xinc do
&#9;&#9;for iy=p1.y,p2.y,yinc do
&#9;&#9;&#9;for iz=p1.z,p2.z,zinc do
&#9;&#9;&#9;&#9;if terra:GetCell(ix,iy,iz)~=Enum.CellMaterial.Empty then
&#9;&#9;&#9;&#9;&#9;return false
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return true
end

--[[
function checkregionempty(point1,point2)
&#9;for ix = point1.x, point2.x, checkifnegativeinc(point1.x, point2.x) do
&#9;&#9;for iy = point1.y, point2.y, checkifnegativeinc(point1.y, point2.y) do
&#9;&#9;&#9;for iz = point1.z, point2.z, checkifnegativeinc(point1.z, point2.z) do
&#9;&#9;&#9;&#9;if terra:GetCell(ix, iy, iz) ~= 0 then
&#9;&#9;&#9;&#9;&#9;return false
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return true
end
]]

function updatepos()
&#9;FindHUD()
&#9;if themouse~=nil and hud~=nil then
&#9;&#9;local oldsize=objectsize
&#9;&#9;objectsize=sizes[currentsize]
&#9;&#9;if rotation%2==1 then
&#9;&#9;&#9;rotobjsize=Vector3.new(objectsize.z,objectsize.y,objectsize.x)
&#9;&#9;else
&#9;&#9;&#9;rotobjsize=objectsize
&#9;&#9;end
&#9;&#9;local ph=hud:FindFirstChild(&quot;PlaceHolder&quot;)
&#9;&#9;if oldsize~=objectsize and ph~=nil then
&#9;&#9;&#9;ph:remove()
&#9;&#9;&#9;ph=nil
&#9;&#9;end
&#9;&#9;if ph==nil then
&#9;&#9;&#9;ph=Instance.new(&quot;Model&quot;)
&#9;&#9;&#9;ph.Name=&quot;PlaceHolder&quot;
&#9;&#9;&#9;ph.Parent=hud
&#9;&#9;&#9;local fakecell=Instance.new(&quot;Part&quot;)
&#9;&#9;&#9;fakecell.Name=&quot;Invisible&quot;
&#9;&#9;&#9;fakecell.formFactor=&quot;Custom&quot;
&#9;&#9;&#9;fakecell.Anchored=true
&#9;&#9;&#9;fakecell.CanCollide=false
&#9;&#9;&#9;fakecell.TopSurface=&quot;Smooth&quot;
&#9;&#9;&#9;fakecell.BottomSurface=&quot;Smooth&quot;
&#9;&#9;&#9;fakecell.Transparency=.5
&#9;&#9;&#9;fakecell.BrickColor=placeablecolor
&#9;&#9;&#9;fakecell.Size=rotobjsize*4
&#9;&#9;&#9;fakecell.Parent=ph
&#9;&#9;end
&#9;&#9;local phc=ph:GetChildren()
&#9;&#9;if #phc&gt;=1 then
&#9;&#9;&#9;if phc[1].className==&quot;Part&quot; then
&#9;&#9;&#9;&#9;local pos1,pos2=findplacement(themouse.Hit.p)
&#9;&#9;&#9;&#9;phc[1].Size=(Vector3.new(math.abs(pos1.x-pos2.x),math.abs(pos1.y-pos2.y),math.abs(pos1.z-pos2.z))+Vector3.new(1,1,1))*4
&#9;&#9;&#9;&#9;phc[1].CFrame=CFrame.new(((pos1+((pos2-pos1)*.5))*4)+Vector3.new(2,2,2))
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

function findplacement(pos)
&#9;local cellpos=terra:WorldToCellPreferEmpty(pos)
&#9;local height=0
&#9;while not checkregionempty(cellpos+Vector3.new(0,height,0),cellpos+(rotobjsize-Vector3.new(1,1,1))+Vector3.new(0,height,0)) do
&#9;&#9;height=height+1
&#9;end
&#9;return cellpos+Vector3.new(0,height,0),cellpos+(rotobjsize-Vector3.new(1,1,1))+Vector3.new(0,height,0)
end

function selected(mouse)
&#9;if mouse~=nil then
&#9;&#9;FindHUD()
&#9;&#9;themouse=mouse
&#9;&#9;equipped=true
&#9;&#9;mouse.Button1Down:connect(function()
&#9;&#9;&#9;local pos1,pos2=findplacement(mouse.Hit.p)
&#9;&#9;&#9;terra:SetCells(Region3int16.new(Vector3int16.new(pos1.x,pos1.y,pos1.z),Vector3int16.new(pos2.x,pos2.y,pos2.z)),14,0,0)
&#9;&#9;&#9;updatepos()
&#9;&#9;end)
&#9;&#9;mouse.KeyDown:connect(function(key)
&#9;&#9;&#9;key=string.byte(key)
&#9;&#9;&#9;if key==string.byte(&quot;r&quot;) then
&#9;&#9;&#9;&#9;rotation=rotation+1
&#9;&#9;&#9;&#9;updatepos()
&#9;&#9;&#9;elseif key==string.byte(&quot;e&quot;) then
&#9;&#9;&#9;&#9;currentsize=currentsize+1
&#9;&#9;&#9;&#9;if currentsize&gt;#sizes then
&#9;&#9;&#9;&#9;&#9;currentsize=1
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;updatepos()
&#9;&#9;&#9;elseif key==string.byte(&quot;q&quot;) then
&#9;&#9;&#9;&#9;currentsize=currentsize-1
&#9;&#9;&#9;&#9;if currentsize&lt;1 then
&#9;&#9;&#9;&#9;&#9;currentsize=#sizes
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;updatepos()
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;mouse.Move:connect(updatepos)
&#9;end
end

&#9;&#9;


function deselected()
&#9;equipped=false
&#9;FindHUD()
&#9;if hud~=nil then
&#9;&#9;for i,v in ipairs(hud:GetChildren()) do
&#9;&#9;&#9;v:remove()
&#9;&#9;end
&#9;end
end
sp.Selected:connect(selected)
sp.Deselected:connect(deselected)

					<ProtectedString name="Source">--Stickmasterluke


sp=script.Parent


function waitfor(p,n)
&#9;while p:FindFirstChild(n)==nil do
&#9;&#9;wait()
&#9;end
&#9;return p:FindFirstChild(n)
end

lbf=waitfor(sp,&quot;LeaderboardFrame&quot;)

blueframe=waitfor(lbf,&quot;Bright blue&quot;)
bluehealth=waitfor(blueframe,&quot;BlueHP&quot;)

redframe=waitfor(lbf,&quot;Bright red&quot;)
redhealth=waitfor(redframe,&quot;RedHP&quot;)


function renderhp(core)
&#9;print(&quot;Render core&quot;,tick())
&#9;if core then
&#9;&#9;local mainpart=core:FindFirstChild(&quot;MainPart&quot;)
&#9;&#9;local health=core:FindFirstChild(&quot;Health&quot;)
&#9;&#9;if mainpart~=nil and health~=nil then
&#9;&#9;&#9;local percent=health.Value/health.MaxValue
&#9;&#9;&#9;if mainpart.BrickColor==BrickColor.new(&quot;Bright blue&quot;) then
&#9;&#9;&#9;&#9;bluehealth.Size=UDim2.new(0,358*percent,0,43)
&#9;&#9;&#9;&#9;bluehealth.Position=UDim2.new(0,10+(358*(1-percent)),0,10)
&#9;&#9;&#9;elseif mainpart.BrickColor==BrickColor.new(&quot;Bright red&quot;) then
&#9;&#9;&#9;&#9;redhealth.Size=UDim2.new(0,358*percent,0,43)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

game.Workspace.ChildAdded:connect(function(item)
&#9;if item.Name==&quot;Core&quot; then
&#9;&#9;print(&quot;1Connected core&quot;,tick())
&#9;&#9;local h=waitfor(item,&quot;Health&quot;)
&#9;&#9;print(&quot;2Connected core&quot;,tick())
&#9;&#9;h.Changed:connect(function()
&#9;&#9;&#9;renderhp(item)
&#9;&#9;end)
&#9;end
end)
for i,v in ipairs(game.Workspace:GetChildren()) do
&#9;if v and v~=nil and v.Name==&quot;Core&quot; then
&#9;&#9;print(&quot;3Connected core&quot;,tick())
&#9;&#9;local h=waitfor(v,&quot;Health&quot;)
&#9;&#9;print(&quot;4Connected core&quot;,tick())
&#9;&#9;h.Changed:connect(function()
&#9;&#9;&#9;renderhp(v)
&#9;&#9;end)
&#9;end
end



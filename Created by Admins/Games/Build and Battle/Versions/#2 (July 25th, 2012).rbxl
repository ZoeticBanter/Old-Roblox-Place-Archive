					<ProtectedString name="Source">--The Code
--You shouldn&apos;t need to edit this if you just want a generic gun

function WaitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end

local Tool = script.Parent
local Handle = Tool.Handle
local GunData = Tool.GunData
local ShootSound = WaitForChild(Handle, &quot;Shoot&quot;)
local ReloadSound = WaitForChild(Handle, &quot;Reload&quot;)

local Character = nil


local Workspace = game.Workspace
local Debris = game.Debris

local ShotsLeft = GunData.ShotsPerClip.ShotsLeft

local Shooting = false


function TagHumanoid(Humanoid, Player)
&#9;if Humanoid and Player then
&#9;&#9;local Tag = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;Tag.Value = Player
&#9;&#9;Tag.Name = &quot;creator&quot;
&#9;&#9;game.Debris:AddItem(Tag,3)
&#9;&#9;Tag.Parent=Humanoid
&#9;end
end

function drawlaser(p1, p2, clr, fadetime)
&#9;local dis=(p1-p2).magnitude
&#9;local laser=Instance.new(&quot;Part&quot;)
&#9;laser.Name=&quot;Effect&quot;
&#9;laser.Anchored=true
&#9;laser.CanCollide=false
&#9;laser.Shape=&quot;Block&quot;
&#9;laser.formFactor=&quot;Custom&quot;
&#9;laser.Size=Vector3.new(.2,.2,.2)
&#9;laser.Material=Enum.Material.Plastic
&#9;laser.Locked=true
&#9;laser.TopSurface=0
&#9;laser.BottomSurface=0
&#9;laser.BrickColor=clr
&#9;laser.CFrame=CFrame.new(p2,p1)*CFrame.new(0,100,-dis/2)
&#9;local mesh=Instance.new(&quot;BlockMesh&quot;)
&#9;mesh.Scale=Vector3.new(.5,.5,dis/(.2))
&#9;mesh.Offset=Vector3.new(0,-100,0)
&#9;mesh.Parent=laser
&#9;Debris:AddItem(laser,2)
&#9;laser.Parent=game.Workspace
&#9;delay(.1,function()
&#9;
&#9;&#9;local fadeframes=math.floor(fadetime*10)
&#9;&#9;for ffs=1,fadeframes do
&#9;&#9;&#9;if laser~=nil and laser.Parent~=nil then
&#9;&#9;&#9;&#9;laser.Transparency=ffs/fadeframes
&#9;&#9;&#9;&#9;wait(fadetime/fadeframes)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;if laser~=nil and laser.Parent~=nil then
&#9;&#9;&#9;laser:remove()
&#9;&#9;end
&#9;end)
end


function Reload()
&#9;--Don&apos;t Reload if we are reloading
&#9;if(GunData.ReloadTime.Reloading.Value) then return end
&#9;ReloadSound:Play()

&#9;--Tell the client we are reloading
&#9;GunData.ReloadTime.Reloading.Value = true
&#9;
&#9;wait(GunData.ReloadTime.Value)
&#9;--Reload the gun and let us continue on our merry way
&#9;ShotsLeft.Value = GunData.ShotsPerClip.Value
&#9;GunData.ReloadTime.Reloading.Value = false

end


--Client has told us where he wants to shoot.  Oblige Him
function onFireMessage(shootTarget)
&#9;if(Shooting) then return end
&#9;print(&quot;Server: Shooting&quot;)
&#9;
&#9;Shooting = true
&#9;
&#9;ShootSound:Play()

&#9;local shootingPlayer = game.Players:GetPlayerFromCharacter(Character)
&#9;local position = Tool.Handle.CFrame.p
&#9;local direction = (shootTarget - position).unit
&#9;ShotsLeft.Value = ShotsLeft.Value - 1
&#9;--Shoot the gun
&#9;local hit, hitPos = Workspace:FindPartOnRay(Ray.new(position, (direction * GunData.Falloff.Value)), Tool.Parent)
&#9;drawlaser(position, hitPos, shootingPlayer.TeamColor, GunData.FadeTime.Value)
&#9;if(hit) then
&#9;&#9;&#9;--Check if we hit a player
&#9;&#9;if(GunData.DamagesPlayers.Value and hit.Parent:FindFirstChild(&quot;Humanoid&quot;)) then
&#9;&#9;&#9;local humanoid = hit.Parent
&#9;&#9;&#9;local hitPlayer = game.Players:GetPlayerFromCharacter(humanoid)
&#9;&#9;&#9;if(GunData.DamagesNeutrals.Value and hitPlayer.Neutral) then
&#9;&#9;&#9;&#9;humanoid.Humanoid:TakeDamage(GunData.Damage.Value)
&#9;&#9;&#9;elseif(hitPlayer.TeamColor ~= shootingPlayer.TeamColor) then
&#9;&#9;&#9;&#9;TagHumanoid(humanoid.Humanoid, shootingPlayer)
&#9;&#9;&#9;&#9;humanoid.Humanoid:TakeDamage(GunData.Damage.Value)
&#9;&#9;&#9;end

&#9;&#9;--Check if we hit an object with a health value&#9;
&#9;&#9;elseif(GunData.DamagesObjects.Value and hit.Parent:FindFirstChild(&quot;TakeDamage&quot;)) then
&#9;&#9;&#9;local building = hit.Parent
&#9;&#9;&#9;if building:FindFirstChild(&quot;MainPart&quot;).BrickColor ~= shootingPlayer.TeamColor then
&#9;&#9;&#9;&#9;&#9;hit.Parent.TakeDamage:Invoke(GunData.Damage.Value, GunData.DamageType.Value)
&#9;&#9;&#9;end
&#9;&#9;end

&#9;end
&#9;
&#9;wait(1 / GunData.FireRate.Value)

&#9;if(ShotsLeft.Value &lt;= 0) then Reload() return end

&#9;Shooting = false

end



--Recieve a messages from the client
Tool.ChildAdded:connect(function(child)
&#9;if child and child:IsA(&quot;Vector3Value&quot;) and child.Name == &quot;ShootMessage&quot; then
&#9;&#9;onFireMessage(child.Value)
&#9;&#9;child:Destroy()
&#9;&#9;return
&#9;end

&#9;if child and child:IsA(&quot;BoolValue&quot;) and child.Name == &quot;ReloadMessage&quot; then
&#9;&#9;Reload()
&#9;&#9;child:Destroy()
&#9;&#9;return 
&#9;end
end)

Tool.Equipped:connect(function() 
&#9;Character = Tool.Parent
end)

Tool.Unequipped:connect(function()
&#9;Character = nil
end)

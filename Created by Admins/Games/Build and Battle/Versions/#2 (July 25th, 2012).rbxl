function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end


local Tool = script.Parent
local GunData = waitForChild(Tool, &quot;GunData&quot;)

local Workspace = game.Workspace
local LocalPlayer = game.Players.LocalPlayer

local Fireing = false

local ShotsLeft = waitForChild(waitForChild(GunData, &quot;ShotsPerClip&quot;), &quot;ShotsLeft&quot;).Value
local Reloading = waitForChild(waitForChild(GunData, &quot;ReloadTime&quot;), &quot;Reloading&quot;).Value

local PlayerGui = waitForChild(LocalPlayer, &quot;PlayerGui&quot;)
local ObjHealth = waitForChild(PlayerGui, &quot;ObjHealth2&quot;)
local HealthBarFrame2 = waitForChild(ObjHealth, &quot;HealthBar&quot;)

local ChangeWeapon = waitForChild(Tool.Handle, &quot;ChangeWeapon&quot;)


local cursorReloading = &quot;rbxasset://textures\\GunWaitCursor.png&quot;
local cursorNormal = &quot;rbxasset://textures\\GunCursor.png&quot;



function UpdateAmmo(Count)
&#9;Count = Count or &quot;err&quot;
&#9;LocalPlayer.PlayerGui.AmmoGui.Ammo.Text = Count .. &quot; / &quot; .. GunData.ShotsPerClip.Value
end

GunData.ShotsPerClip.ShotsLeft.Changed:connect(function(value) print(&quot;Updated Shots from &quot;, ShotsLeft, &quot; to &quot;, value) ShotsLeft = value end)
GunData.ReloadTime.Reloading.Changed:connect(function(value) print(&quot;Updated Reload from &quot;, Reloading, &quot; to &quot;, value) Reloading = value end)

local Mouse = nil



function drawHealth(mouse)
&#9;ObjHealth = PlayerGui:FindFirstChild(&quot;ObjHealth2&quot;)
&#9;HealthBarFrame2 = ObjHealth:FindFirstChild(&quot;HealthBar&quot;)&#9;
&#9;blue2 = HealthBarFrame2:FindFirstChild(&quot;blue&quot;)
&#9;if mouse ~= nil and mouse.Target ~= nil and mouse.Target.Parent:FindFirstChild(&quot;Health&quot;) ~= nil and (mouse.Target.Parent.Health.className == &quot;IntConstrainedValue&quot; or mouse.Target.Parent.Health.className == &quot;IntValue&quot;) then
&#9;&#9;local obj = mouse.Target
&#9;&#9;HealthBarFrame2.Visible = true
&#9;&#9;local objHealth = obj.Parent:FindFirstChild(&quot;Health&quot;)
&#9;&#9;local objCurrentHealth = objHealth.Value
&#9;&#9;local objMaxHealth = objHealth.MaxValue
&#9;&#9;local healthPercent = objCurrentHealth/objMaxHealth
&#9;&#9;local mouseX = mouse.X
&#9;&#9;local mouseY = mouse.Y
&#9;&#9;local offset = 20
&#9;&#9;HealthBarFrame2.Position = UDim2.new(0,mouseX+offset,0,mouseY-offset) --(scaleX,offsetX,scaleY,offsetY)
&#9;&#9;blue2.Size = UDim2.new(healthPercent, -2, 1, -2)
&#9;else
&#9;&#9;HealthBarFrame2.Visible = false
&#9;end
end


function Fire(mouse)
&#9;local ShootMessage = Instance.new(&quot;Vector3Value&quot;)
&#9;ShootMessage.Name = &quot;ShootMessage&quot;
&#9;local kb = GunData.Kickback.Value
&#9;ShootMessage.Value = mouse.Hit.p  + Vector3.new(((math.random() * 2) -1) * kb, ((math.random() * 2) -1) * kb, ((math.random() * 2) -1) * kb)
&#9;ShootMessage.Parent = Tool

&#9;
&#9;ShotsLeft = ShotsLeft - 1

&#9;UpdateAmmo(ShotsLeft)

end

function RequestReload()
&#9;--Inform the server we want to reload our weapon
&#9;local ReloadMessage = Instance.new(&quot;BoolValue&quot;)
&#9;ReloadMessage.Name = &quot;ReloadMessage&quot;
&#9;ReloadMessage.Value = true
&#9;ReloadMessage.Parent = Tool
&#9;
&#9;Mouse.Icon = cursorReloading

&#9;--Predict the server reload process
&#9;Reloading = true
&#9;wait(GunData.ReloadTime.Value)
&#9;Mouse.Icon = cursorNormal
&#9;ShotsLeft = GunData.ShotsPerClip.Value
&#9;Reloading = false

end


function onFireDown(mouse)
&#9;Fireing = true
&#9;
&#9;while(Fireing and not Reloading) do
&#9;&#9;if(ShotsLeft &lt;= 0) then
&#9;&#9;&#9;RequestReload()
&#9;&#9;&#9;break
&#9;&#9;end
&#9;&#9;Fire(mouse)
&#9;&#9;
&#9;&#9;wait(1/GunData.FireRate.Value)
&#9;end
&#9;
&#9;
end

function onFireUp()
&#9;Fireing = false
end

function onKeyDown(key)
&#9;key = string.byte(key)
&#9;if(key == string.byte(&quot;r&quot;)) then RequestReload() end

&#9;if key == string.byte(&quot;l&quot;) then
&#9;&#9;print(&quot;GUI!&quot;)
&#9;&#9;local Lgui = LocalPlayer.PlayerGui:FindFirstChild(&quot;LeaderboardGui&quot;)
&#9;&#9;local Lframe = waitForChild(Lgui, &quot;LeaderboardFrame&quot;)
&#9;&#9;if Lframe.Visible == false then
&#9;&#9;&#9;Lframe.Visible = true
&#9;&#9;&#9;LocalPlayer.PlayerGui.MapSystem.Minimap.Visible = false 
&#9;&#9;&#9;if LocalPlayer.PlayerGui.MapSystem.Map.Visible == true then
&#9;&#9;&#9;&#9;print(&quot;minimapcheck setting true&quot;)
&#9;&#9;&#9;&#9;minimapcheck = true
&#9;&#9;&#9;&#9;LocalPlayer.PlayerGui.MapSystem.Map.Visible = false
&#9;&#9;&#9;end&#9;&#9;&#9;
&#9;&#9;&#9;LocalPlayer.PlayerGui.ResourceGui.ImageLabel.Visible = false
&#9;&#9;else
&#9;&#9;&#9;Lframe.Visible = false
&#9;&#9;&#9;LocalPlayer.PlayerGui.MapSystem.Minimap.Visible = true
&#9;&#9;&#9;if minimapcheck == true then
&#9;&#9;&#9;&#9;print(&quot;should be okay&quot;)
&#9;&#9;&#9;&#9;LocalPlayer.PlayerGui.MapSystem.Minimap.Visible = false
&#9;&#9;&#9;&#9;LocalPlayer.PlayerGui.MapSystem.Map.Visible = true
&#9;&#9;&#9;&#9;minimapcheck = false
&#9;&#9;&#9;end
&#9;&#9;&#9;LocalPlayer.PlayerGui.ResourceGui.ImageLabel.Visible = true
&#9;&#9;end
&#9;end
&#9;&#9;
&#9;

end


function onEquipped(mouse)
&#9;ChangeWeapon:Play()
&#9;Mouse = mouse
&#9;mouse.Button1Down:connect(function () onFireDown(mouse) end)
&#9;mouse.Button1Up:connect(function () onFireUp() end )
&#9;mouse.KeyDown:connect(onKeyDown)

&#9;mouse.Icon = cursorNormal
&#9;mouse.Move:connect(function()
&#9;&#9;&#9;drawHealth(mouse)
&#9;&#9;end)
&#9;
&#9;LocalPlayer.PlayerGui.AmmoGui.Ammo.Visible = true
&#9;UpdateAmmo(ShotsLeft)
&#9;

end

Tool.Equipped:connect(onEquipped)




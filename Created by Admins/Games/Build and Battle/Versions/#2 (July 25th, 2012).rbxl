						<ProtectedString name="Source">-- by QS
-- shows notice whenever your core is attacked or someone takes a mine

function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
&#9;return instance:FindFirstChild(name)
end

local textBox = script.Parent
local mainScript = waitForChild(game.Workspace,&quot;MainScript&quot;)
local locs = waitForChild(mainScript,&quot;GameLocations&quot;)

local textBoxUp = 0
function showText(text)
&#9;textBox.Text = text
&#9;textBox.Visible = true
&#9;textBoxUp = textBoxUp+1
&#9;wait(5)
&#9;textBoxUp = textBoxUp-1
&#9;if textBoxUp == 0 then textBox.Visible = false end
end

textBox.ChildAdded:connect(function(child)
&#9;if child.Name == &quot;UpgradedGun&quot; and child:IsA(&quot;IntValue&quot;) then
&#9;&#9;showText(&quot;Your gun has been upgraded to level &quot; .. child.Value .. &quot;!&quot;)
&#9;&#9;child:Destroy()
&#9;end
&#9;if child.Name == &quot;Update&quot; and child:IsA(&quot;StringValue&quot;) then
&#9;&#9;showText(child.Value)
&#9;&#9;child:Destroy()
&#9;end
end)

function setUpConnection(child)
&#9;--print(&quot;setUp&quot;)
&#9;if child and child.Parent and (child.Name == &quot;Mine1&quot; or child.Name == &quot;Mine2&quot; or child.Name == &quot;Mine3&quot;) then
&#9;&#9;local team = waitForChild(child, &quot;Team&quot;)
&#9;&#9;team.Changed:connect(function()
&#9;&#9;&#9;local teamColor = team.Value.Name
&#9;&#9;&#9;if teamColor == &quot;Really red&quot; then teamColor = &quot;Red&quot; end
&#9;&#9;&#9;if teamColor == &quot;Really blue&quot; then teamColor = &quot;Blue&quot; end
&#9;&#9;&#9;if teamColor == &quot;White&quot; then 
&#9;&#9;&#9;&#9;showText(&quot;A mine was destroyed&quot;)
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;showText(teamColor .. &quot; team captured a mine&quot;)
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;end
end

local locations = locs:GetChildren()
for i =1 , #locations do
&#9;setUpConnection(locations[i])
end

locs.ChildAdded:connect(setUpConnection)

function setUpCoreConnection(child)
&#9;--print(&quot;Setting up core notifier&quot;)
&#9;if child.Name ~= &quot;Core&quot; then print(&quot;core connect error&quot;) end
&#9;child.Health.Changed:connect(function()
&#9;&#9;if not (child and child.Health) or child.Health.Value &lt;= 0 then 
&#9;&#9;&#9;showText(&quot;Your core has been destroyed!&quot;)
&#9;&#9;else
&#9;&#9;&#9;showText(&quot;Your core is under attack!&quot;)
&#9;&#9;end
&#9;end)
end

local newRound = false
if not game.Workspace:FindFirstChild(&quot;Core&quot;) then newRound = true end

waitForChild(game.Workspace,&quot;Core&quot;)
wait(.4) --Wait for both cores to be in workspace



local gameObjs = game.Workspace:GetChildren()
for i = 1, #gameObjs do
&#9;if gameObjs[i].Name == &quot;Core&quot; then
&#9;&#9;--print(&quot;1Core&quot;)
&#9;&#9;waitForChild(gameObjs[i],&quot;MainPart&quot;)
&#9;&#9;if gameObjs[i].MainPart.BrickColor == game.Players.LocalPlayer.TeamColor then
&#9;&#9;&#9;setUpCoreConnection(gameObjs[i])
&#9;&#9;end
&#9;end
end

if newRound then wait(1) showText(&quot;Starting new build and battle round&quot;) end</ProtectedString>

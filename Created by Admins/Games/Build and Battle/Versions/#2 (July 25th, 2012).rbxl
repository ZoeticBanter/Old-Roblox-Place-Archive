						<ProtectedString name="Source">local core = script.Parent
local arm = core.Parent
--local creationTime = Instance.new(&quot;IntValue&quot;)
--creationTime.Parent = Part
local creationTime = tick()
--local now = tick()
--print(now)
--creationTime.Value = now

local upgrader = 1

--local threemin = 180
--local sixmin = 360
--local ninemin = 540
--local twelvemin = 960

local upgradeTimes = {60*3,60*6,60*9,60*12}

function lightUp(number)
&#9;local Top = arm:FindFirstChild(&quot;Top&quot; .. number)
&#9;if Top then
&#9;&#9;local children = Top:GetChildren()
&#9;&#9;for i = 1, #children do
&#9;&#9;&#9;if children[i].Name == &quot;TPart&quot; then 
&#9;&#9;&#9;&#9;children[i].Transparency = .6
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;children[i].Transparency = 0
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end
&#9;

core.Touched:connect(function(touchedPart)
&#9;if touchedPart and touchedPart.Parent then
&#9;&#9;local char = touchedPart.Parent
&#9;&#9;if char:FindFirstChild(&quot;Humanoid&quot;) ~= nil then
&#9;&#9;&#9;local gunTool = char:FindFirstChild(&quot;GunTool&quot;)
&#9;&#9;&#9;local player = game.Players:GetPlayerFromCharacter(char)
&#9;&#9;&#9;if player and player.TeamColor == arm.MainPart.BrickColor then
&#9;&#9;&#9;&#9;if gunTool == nil then
&#9;&#9;&#9;&#9;&#9;gunTool = player.Backpack:FindFirstChild(&quot;GunTool&quot;)
&#9;&#9;&#9;&#9;&#9;--print(&quot;gun in backpack&quot;)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;if gunTool and gunTool:FindFirstChild(&quot;upgradeVal&quot;) and gunTool.upgradeVal.Value-1 &lt; upgrader then
&#9;&#9;&#9;&#9;&#9;print(&quot;upgrading gun to &quot; .. upgrader)
&#9;&#9;&#9;&#9;&#9;gunTool.upgradeVal.Value = upgrader+1
&#9;&#9;&#9;&#9;&#9;if player.PlayerGui:findFirstChild(&quot;NotificationGui&quot;) and player.PlayerGui.NotificationGui:findFirstChild(&quot;Notification&quot;) then 
&#9;&#9;&#9;&#9;&#9;&#9;--print(&quot;notifying&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;local upVal = Instance.new(&quot;IntValue&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;upVal.Value = upgrader --displayed upgrade levels are 0,1,2,3,4,5, actual value is +1
&#9;&#9;&#9;&#9;&#9;&#9;upVal.Name = &quot;UpgradedGun&quot;
&#9;&#9;&#9;&#9;&#9;&#9;upVal.Parent = player.PlayerGui.NotificationGui.Notification
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end)

while true do
&#9;wait(1.0)
&#9;local time = tick()
&#9;if upgrader &lt; 5 and (time - creationTime) &gt;= upgradeTimes[upgrader] then
&#9;&#9;upgrader = upgrader + 1
&#9;&#9;lightUp(upgrader)
&#9;end
&#9;--[[
&#9;if (time - creationTime) &gt;= threemin then
&#9;&#9;lightUp(2)
&#9;&#9;upgrader = 2
&#9;&#9;if (time - creationTime) &gt;= sixmin then
&#9;&#9;&#9;lightUp(3)
&#9;&#9;&#9;upgrader = 3
&#9;&#9;&#9;if (time - creationTime) &gt;= ninemin then

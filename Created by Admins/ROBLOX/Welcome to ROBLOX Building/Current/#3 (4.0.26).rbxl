function needToRespawn()
&#9;&#9;if button.Parent ~= model then return true end
&#9;&#9;if topButton.Parent ~= model then return true end
&#9;&#9;if bottomBase.Parent ~= model then return true end
&#9;&#9;if button.Position.X &lt; bottomBase.Position.X - 3.2 or button.Position.X &gt; bottomBase.Position.X + 3.2 or button.Position.Z &lt; bottomBase.Position.Z - 3.2 or button.Position.Z &gt; bottomBase.Position.Z + 3.2 or button.Position.Y &gt; bottomBase.Position.Y + 3 or button.Position.Y &lt; bottomBase.Position.Y then return true end
&#9;&#9;if topButton.Position.X &lt; bottomBase.Position.X - 3.2 or topButton.Position.X &gt; bottomBase.Position.X + 3.2 or topButton.Position.Z &lt; bottomBase.Position.Z - 3.2 or topButton.Position.Z &gt; bottomBase.Position.Z + 3.2 or topButton.Position.Y &gt; bottomBase.Position.Y + 3 or topButton.Position.Y &lt; bottomBase.Position.Y then return true end
&#9;&#9;
&#9;&#9;return false
end

-- check for respawn condition every 20 seconds (if button somehow comes loose)
-- also reset isPressed if it got stuck in &quot;true&quot; position
local keepAlive = true
while keepAlive do
&#9;wait(20)
&#9;if needToRespawn() then model.Parent = nil modelClone.Parent = game.Workspace.BaseplateClearButtons modelClone:MakeJoints() keepAlive = false end
&#9;if button.Position.Y &gt;= (upY - threshold) and isPressed.Value then isPressed.Value = false turnOff(player) end
end


&#9;&#9;instance.ChildAdded:wait()
&#9;end
end

waitForChild(script.Parent, &quot;Jeep1&quot;)
local jeep = script.Parent.Jeep1
jeep.Parent = script.Parent.Parent -- keep outside of jeepspawner model

waitForChild(script.Parent, &quot;Sign&quot;)

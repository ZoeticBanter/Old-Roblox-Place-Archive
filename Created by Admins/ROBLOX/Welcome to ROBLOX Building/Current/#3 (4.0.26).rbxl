function deleteNode(node)
&#9;local nodeName = node.Name
&#9;local neighbors = network[nodeName]
&#9;costs[nodeName] = nil

&#9;-- for each neighbor, we need to find the node corresponding to node-to-be-deleted, and delete corresponding network and cost table entries
&#9;for n = 1, #neighbors do
&#9;&#9;local links = network[neighbors[n]]
&#9;&#9;local prices = costs[neighbors[n]]

&#9;&#9;for l = 1, #links do
&#9;&#9;&#9;if links[l] == nodeName then
&#9;&#9;&#9;&#9;table.remove(links, l)
&#9;&#9;&#9;&#9;table.remove(prices, l)
&#9;&#9;&#9;&#9;break
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;network[node.Name] = nil
&#9;node.Parent = nil
end


&#9;stamperToolWasEquipped = false
&#9;unequipStamperToop(player)  -- This can be run even if they don&apos;t have it equipped.
&#9;-- Get all parts
&#9;local parts  = buildingArea.PlayerArea:GetChildren()
&#9;-- Remove all parts except BasePlate and Player.
&#9;for i=1, #parts do
&#9;&#9;local type = parts[i].className
&#9;&#9;if     type == &quot;BoolValue&quot;&#9;&#9;&#9;then --
&#9;&#9;elseif type == &quot;BrickColorValue&quot;&#9;then --
&#9;&#9;elseif type == &quot;CFrameValue&quot;&#9;&#9;then --
&#9;&#9;elseif type == &quot;Color3Value&quot;&#9;&#9;then --
&#9;&#9;elseif type == &quot;IntValue&quot;      &#9;then --
&#9;&#9;elseif type == &quot;NumberValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;ObjectValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;StringValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;Vector3Value&quot; &#9;then --
&#9;&#9;elseif parts[i].Name==&quot;BasePlate&quot; then
&#9;&#9;&#9;-- we remove the baseplate and replace it with a new one!
&#9;&#9;&#9;parts[i]:Remove()
&#9;&#9;&#9;local newBaseplate = spareBaseplates:FindFirstChild(buildingArea.Name..&quot;SpareBaseplate&quot;)
&#9;&#9;&#9;if newBaseplate then
&#9;&#9;&#9;&#9;local newBaseplateClone = newBaseplate:Clone()
&#9;&#9;&#9;&#9;newBaseplateClone.Name = &quot;BasePlate&quot;
&#9;&#9;&#9;&#9;newBaseplateClone.Parent = buildingArea.PlayerArea
&#9;&#9;&#9;end
&#9;&#9;else
&#9;&#9;&#9;parts[i]:Remove()

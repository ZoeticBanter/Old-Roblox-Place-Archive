function showTutorialWindow()
&#9;hideAllPages()
&#9;hideArrows()
&#9;hideButtons()
&#9;tutorial.Visible = false
&#9;local page = bkg
&#9;-- shrink frame a little bit
&#9;local startSize = page.Size
&#9;local startPosition = page.Position
&#9;page.Size = startSize - UDim2.new(0, shrinkAmount, 0, shrinkAmount)
&#9;page.Position = startPosition + UDim2.new(0, shrinkAmount/2, 0, shrinkAmount/2)
&#9;tutorial.Visible = true
&#9;page.Visible = true
&#9;-- animate frame back to original size
&#9;page:TweenSizeAndPosition(startSize,startPosition,Enum.EasingDirection.In,Enum.EasingStyle.Quad,tweenTimeIn,true)

&#9;wait(tweenTimeIn)

&#9;page.Size = startSize
&#9;page.Position = startPosition
end

function hideTutorialWindow()
&#9;hideAllPages()
&#9;hideArrows()
&#9;hideButtons()
&#9;local page = bkg
&#9;local startSize = page.Size
&#9;local startPosition = page.Position
&#9;local endSize = &#9;UDim2.new(0,80,0,40)
&#9;local endPosition = UDim2.new(startPosition.X.Scale * 0.25,startPosition.X.Offset * 0.25,startPosition.Y.Scale * 0.25,startPosition.Y.Offset * 0.25)
&#9;page:TweenSizeAndPosition(endSize,endPosition,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,tweenTimeOut,true)
&#9;
&#9;wait(tweenTimeOut)
&#9;-- hide page
&#9;tutorial.Visible = false
&#9;-- put page back to normal size.
&#9;page.Size = startSize
&#9;page.Position = startPosition
end


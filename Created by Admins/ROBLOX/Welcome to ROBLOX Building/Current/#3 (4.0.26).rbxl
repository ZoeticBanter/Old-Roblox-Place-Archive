&#9;local maxPartVect = part.Position + transformedPartSize/2

&#9;-- we check to see if it boundingbox extends at all outside basePlate
&#9;return (maxPartVect.X &gt; maxPlateVect.X + 1 or maxPartVect.Z &gt; maxPlateVect.Z + 1 or minPartVect.X &lt; minPlateVect.X - 1 or minPartVect.Z &lt; minPlateVect.Z - 1) and not part:FindFirstChild(&quot;AnchorInsteadOfGarbageCollecting&quot;)


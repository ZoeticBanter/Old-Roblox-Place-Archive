					<string name="Name">PeriodicSaver</string>
					<ProtectedString name="Source">-- this script saves people&apos;s places periodically
function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
end

local saveFrequency = 60 -- many seconds we wait inbetween each save

waitForChild(game.Workspace, &quot;GarbageParts&quot;)
waitForChild(game.Workspace, &quot;GarbageHatPointers&quot;)
waitForChild(game.Workspace, &quot;GuestScriptContainer&quot;)
local garbageModel = game.Workspace.GarbageParts
local garbageHatsModel = game.Workspace.GarbageHatPointers
local guestScriptContainer = game.Workspace.GuestScriptContainer

wait(saveFrequency)
while true do
&#9;local takenAreas = game.Workspace.BuildingAreas:GetChildren()
&#9;for i = 1, #takenAreas do
&#9;&#9;&#9;local doSave = true
&#9;&#9;&#9;local player

&#9;&#9;&#9;if takenAreas[i]:FindFirstChild(&quot;Player&quot;) == nil then doSave = false end

&#9;&#9;&#9;if doSave then
&#9;&#9;&#9;&#9;player = takenAreas[i].Player.Value
&#9;&#9;&#9;&#9;if player == nil or player == &quot;&quot; then doSave = false end
&#9;&#9;&#9;end

&#9;&#9;&#9;if doSave then
&#9;&#9;&#9;&#9;player = game.Players:FindFirstChild(player)
&#9;&#9;&#9;&#9;if player == nil then doSave = false end
&#9;&#9;&#9;end

&#9;&#9;&#9;if doSave then
&#9;&#9;&#9;&#9;-- make sure we don&apos;t have auto-save turned off (in case of failed loading)
&#9;&#9;&#9;&#9;if player:FindFirstChild(&quot;AutoSave&quot;) == nil then doSave = false end
&#9;&#9;&#9;end

&#9;&#9;&#9;if doSave then
&#9;&#9;&#9;&#9;--[[local topHint = player.PlayerGui:FindFirstChild(&quot;topHint&quot;)
&#9;&#9;&#9;&#9;if topHint then
&#9;&#9;&#9;&#9;&#9;waitForChild(topHint,&quot;Add&quot;)
&#9;&#9;&#9;&#9;&#9;waitForChild(topHint.Add,&quot;Label&quot;)
&#9;&#9;&#9;&#9;&#9;topHint.Add.Label.Value = &quot;Cleaning up and autosaving your place... (using &quot; .. tostring(takenAreas[i].DataCost) .. &quot; of 45000 max)&quot;
&#9;&#9;&#9;&#9;&#9;topHint.Add.Width.Value = 600
&#9;&#9;&#9;&#9;&#9;topHint.Add.Time.Value = 5
&#9;&#9;&#9;&#9;&#9;topHint.Add.Disabled = true
&#9;&#9;&#9;&#9;&#9;topHint.Add.Disabled = false
&#9;&#9;&#9;&#9;end]]

&#9;&#9;&#9;&#9;if doSave and takenAreas[i]:FindFirstChild(&quot;PlayerArea&quot;) then
&#9;&#9;&#9;&#9;&#9;-- store point information for next load
&#9;&#9;&#9;&#9;&#9;if takenAreas[i].PlayerArea:FindFirstChild(&quot;InitialPoints&quot;) == nil then
&#9;&#9;&#9;&#9;&#9;&#9;local initialPoints = Instance.new(&quot;IntValue&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;initialPoints.Name = &quot;InitialPoints&quot;
&#9;&#9;&#9;&#9;&#9;&#9;initialPoints.Value = math.max(0,player.PointsUsed.Value)
&#9;&#9;&#9;&#9;&#9;&#9;initialPoints.Parent = takenAreas[i].PlayerArea
&#9;&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;&#9;takenAreas[i].PlayerArea:FindFirstChild(&quot;InitialPoints&quot;).Value = math.max(0,player.PointsUsed.Value)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;--Save his plate
&#9;&#9;&#9;&#9;&#9;local success, error = pcall(function() player:SaveInstance(&quot;PlayerArea&quot;, takenAreas[i].PlayerArea) end)
&#9;&#9;&#9;&#9;&#9;--[[if not success then
&#9;&#9;&#9;&#9;&#9;&#9;player.PlayerGui.Popup.Title.Value = &quot;Saving your Creation Failed&quot;
&#9;&#9;&#9;&#9;&#9;&#9;player.PlayerGui.Popup.Text.Value = tostring(error)
&#9;&#9;&#9;&#9;&#9;&#9;player.PlayerGui.Popup.Style.Value = &quot;error&quot;
&#9;&#9;&#9;&#9;&#9;&#9;player.PlayerGui.Popup.Add.Disabled = false
&#9;&#9;&#9;&#9;&#9;end]]
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end

&#9;end

&#9;-- TODO: Get Rid of this when we have janitor bot
&#9;-- these moved outside of above for loop to attempt to fix &quot;all garbage deletion systems breaking eventually&quot; bug-- possible cause could be that doSave might&apos;ve become always false somehow?
&#9;-- this fix untested...
&#9;local garbage = garbageModel:GetChildren()
&#9;for i = 1, #garbage do
&#9;&#9;garbage[i]:remove()
&#9;end

&#9;local garbageHats = garbageHatsModel:GetChildren()
&#9;for i = 1, #garbageHats do
&#9;&#9;if garbageHats[i].className == &quot;ObjectValue&quot; and garbageHats[i].Value and garbageHats[i].Value.Parent == game.Workspace then garbageHats[i].Value:Remove() end
&#9;&#9;garbageHats[i]:Remove()
&#9;end

&#9;local guestScripts = guestScriptContainer:GetChildren()
&#9;for i = 1, #guestScripts do
&#9;&#9;if not guestScripts[i]:FindFirstChild(&quot;Player&quot;) then guestScripts[i]:Remove()
&#9;&#9;elseif guestScripts[i].Player.Value == nil then guestScripts[i]:Remove() end
&#9;end

&#9;wait(saveFrequency)

end

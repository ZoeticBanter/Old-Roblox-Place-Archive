&#9;&#9;end
&#9;end
end

function makeNewStatue(char)

&#9;-- delete shirt, pants, and &quot;roblox&quot; from Torso
&#9;-- change all BodyColors to gold
&#9;-- recursively look thru all meshes and change VertexColor to (1, 1, 0)
&#9;-- either give them a shirt &amp; pants with a gold texture  OR (if body has details) we set the body texture to a new one which has been &quot;goldenized&quot;

&#9;local&#9;newStatue = blankBot:Clone()
&#9;if newStatue == nil then return end

&#9;copyCharacter(char, newStatue)

&#9;local torso = newStatue:FindFirstChild(&quot;Torso&quot;)
&#9;if torso == nil then return nil end

&#9;-- remove torso icon
&#9;local torsoIcon = torso:FindFirstChild(&quot;roblox&quot;)
&#9;if torsoIcon ~= nil then torsoIcon:Remove() end

&#9;--moveAndAnchor(statuePlace.p - newStatue.Torso.CFrame.p + Vector3.new(0, (magnify-1)*3, 0), newStatue)
&#9;moveAndAnchor(statuePlace.p - newStatue.Torso.CFrame.p + Vector3.new(0, -(magnify)*3, 0), newStatue)

&#9;setBodyColors(BrickColor.new(&quot;CGA brown&quot;), newStatue)

--&#9;goldifyMeshes(newStatue)

&#9;for j = 1, #smokerBlocks do
&#9;&#9;local newSmoke = Instance.new(&quot;Smoke&quot;)
&#9;&#9;newSmoke.Color = Color3.new(.4,.4,.4)
&#9;&#9;newSmoke.RiseVelocity = 5
&#9;&#9;newSmoke.Size = 3*magnify
&#9;&#9;newSmoke.Parent = smokerBlocks[j]
&#9;&#9;debris:AddItem(newSmoke, 12)
&#9;end

&#9;newStatue.Name = &quot;Statue of &quot; .. char.Name
&#9;newStatue.Parent = game.Workspace

&#9;-- make statue &quot;grow&quot;  [end at (magnify-1)*3...  start at -3*2*magnify
&#9;for k = 1, 12 do
&#9;&#9;moveAndAnchor(Vector3.new(0, magnify/2 - .25, 0), newStatue)
&#9;&#9;wait(.5)
&#9;end

&#9;wait(1)

&#9;for j = 1, #smokerBlocks do
&#9;&#9;local oldSmoke = smokerBlocks[j]:FindFirstChild(&quot;Smoke&quot;)
&#9;&#9;if oldSmoke then oldSmoke.Enabled = false end
&#9;end

&#9;return newStatue
end

while true do
&#9;wait(waitTime)
&#9;local playaList = game.Players:GetPlayers()
&#9;local bestExp = 2000 -- to get a statue, must have at least 2000 exp points...  gold will be at least 3000 [values chosen arbitrarily for now]
&#9;local bestChar = nil

&#9;for i = 1, #playaList do
&#9;&#9;local playa = playaList[i]
&#9;&#9;local stats = playa:FindFirstChild(&quot;leaderstats&quot;)
&#9;&#9;if stats ~= nil then
&#9;&#9;&#9;local expObj = stats:FindFirstChild(&quot;Exp&quot;)
&#9;&#9;&#9;if expObj ~= nil then
&#9;&#9;&#9;&#9;local exp = expObj.Value
&#9;&#9;&#9;&#9;local char = playa.Character
&#9;&#9;&#9;&#9;if exp &gt; bestExp and char then
&#9;&#9;&#9;&#9;&#9;bestExp = exp
&#9;&#9;&#9;&#9;&#9;bestChar = char
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;if bestChar ~= nil and (statue == nil or statue.Name ~= &quot;Statue of &quot; .. bestChar.Name) then
&#9;&#9;if statue ~= nil then explodeStatue() statue.Parent = nil end
&#9;&#9;--builderBot.Parent = nil
&#9;&#9;statue = makeNewStatue(bestChar)

&#9;elseif bestChar == nil and statue ~= nil then
&#9;&#9;explodeStatue()
&#9;&#9;statue.Parent = nil
&#9;&#9;--builderBot.Parent = game.Workspace
&#9;&#9;statue = nil
&#9;end

end

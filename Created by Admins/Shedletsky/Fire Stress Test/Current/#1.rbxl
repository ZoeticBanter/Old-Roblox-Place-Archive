				<ProtectedString name="Source">fire = game.Workspace.FirePart:Clone()
game.Workspace.FirePart:Remove()



while true do
	
	local f = fire:Clone()
	f.Position = Vector3.new(0,5,0)
	f.Velocity = Vector3.new(math.random(-60,60), math.random(100,300), math.random(-60,60))
	f.Parent = game.Workspace

	wait(script.SpawnRate.Value)

	if (script.Reset.Value == true) then
		script.Reset.Value = false
		local c = game.Workspace:GetChildren()
		for i=1,#c do
			if (c[i].Name == &quot;FirePart&quot;) then c[i]:Remove() end
		end
	end
	

end

